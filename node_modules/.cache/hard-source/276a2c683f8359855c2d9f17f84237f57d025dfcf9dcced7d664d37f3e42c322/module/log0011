: activeLocale,\n            attribution,\n            guestEmail,\n            gaClientId\n        });\n    }\n    catch (err) {\n        return \"Your cart was rejected\";\n    }\n}\n\n/**\n * Saves cart to customer\n * @param {Object} cart\n * @param {Number} customerId\n */\n\nexport async function saveUserCart(cart, customerId) {\n    try {\n        return saveMagentoCart({\n            cart,\n            customerId\n        });\n    }\n    catch (err) {\n        console.info(err);\n        return err;\n    }\n}\n\n/**\n * Returns the total number of items in the cart\n * @param {Object} lineItems\n * @param {Object} products\n */\n\nexport function totalLineItems(lineItems, products) {\n    let total = 0;\n\n    Object.keys(lineItems).forEach(lineItem => {\n        const cartItem = lineItems[lineItem];\n        const product = products[cartItem.parentId] || null;\n        const variant = product && product.variants && cartItem.variant && product.variants.length > 1 ? getVariantKey(cartItem.variant.id, product) : null;\n  \n        if(isValidItem(product, variant)) \n            total = total + parseInt(cartItem.quantity);\n    });\n\n    return total;\n}\n\n/**\n * Returns whether or not item is eligible to show in cart\n * @param {Object} product\n * @param {Object} variant\n */\n\nexport function isValidItem (product = null, variant = null) {\n    if(product) {\n        const inStock = variant ? variant.inStock : product.inStock;\n\n        return inStock ? true : false;\n    }\n    return false;\n}\n\n/**\n * Calculates the total of all the products\n * currently in the cart considering also\n * that the product is still available and in stock.\n * @param {Object} lineItems\n * @param {Object} products\n */\n\nexport function calculateTotal(lineItems, products) {\n    let total = 0;\n\n    for (var i = 0, len = Object.keys(lineItems).length; i < len; i++) {\n        const cartItem = lineItems[Object.keys(lineItems)[i]];\n        const product = products[cartItem.parentId] || null;\n        const variant = product && product.variants && cartItem.variant && product.variants.length > 1 ? getVariantKey(cartItem.variant.id, product) : null;\n\n        if(product) {\n            if(isValidItem(product, variant)) {\n                    let price = cartItem.dynamicPrice ? cartItem.dynamicPrice : cartItem.price;\n                    total += price * cartItem.quantity;\n            }\n        }\n    };\n    return total;\n}\n\n\n/**\n * \n * Pre-validates the cart items prior to checkout\n * to make sure they are in stock and eligible to be passed\n * on. Returns only valid items.\n * @param {Object} lineItems\n * @param {Object} products\n */\n\nexport function validateCheckoutItems(lineItems, products) {\n    let total = 0;\n    let validItems = {};\n\n    for (var i = 0, len = Object.keys(lineItems).length; i < len; i++) {\n        const cartItem = lineItems[Object.keys(lineItems)[i]];\n        const product = products[cartItem.parentId] || null;\n\n        if(product) {\n            const variant = product.variants.length > 1 ? \n                getVariantKey(cartItem.variant.id, product) : null;\n\n            if(isValidItem(product, variant)) {\n                validItems[cartItem.id] = {\n                    title: product.title,\n                    sku: variant ? variant.sku : product.sku,\n                    quantity: cartItem.quantity,\n                    bundled: cartItem.bundled || null,\n                    // hint for promos. This tells magento this is a promo.\n                    isPromo: cartItem.isPromo ? true : false,\n                };\n            }\n        }\n    };\n\n    if(isEmpty(validItems))\n        return false;\n\n    return validItems;\n}\n\n/**\n * Sorts the product list by date (newest first)\n * @param {Array} items\n */\n\n\nexport function sortByDate(items) {\n    return items.sort((a,b) => \n        new Date(b.dateAdded).getTime() - new Date(a.dateAdded).getTime()\n    );\n}\n\n/**\n * Merges two cart objects\n * The item with the larger quantity wins\n * @param {Object} existingCart\n * @param {Object} newCart\n */\n\nexport function mergeCarts (existingCart = {}, newCart = {}) {\n    if (isEmpty(newCart) && isEmpty(existingCart)) return {};\n    else if (isEmpty(newCart)) return existingCart;\n    else if (isEmpty(existingCart)) return newCart;\n\n    function _safeQuantity(value) {\n        const int = getSafe(() => value());\n        return int || 0;\n    }\n    \n    function _safeItem(item) {\n        if (item && item.quantity) return item;\n    }\n\n    function _biggerQuantity(oldValue, newValue) {\n        if (_safeQuantity((() => oldValue.quantity)) >= _safeQuantity(() => newValue.quantity)) return _safeItem(oldValue);\n        else return _safeItem(newValue);\n    }\n\n    return mergeWith(existingCart, newCart, _biggerQuantity);\n}"]}},"_buildHash":"044ab104be26c014c71c005b1ac38b18","hash":"a623be587271628d17d490dd22170bd2","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"RequireHeaderDependency","range":[161,168],"loc":{"start":{"line":8,"column":12},"end":{"line":8,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/keys","range":[169,204],"loc":{"start":{"line":8,"column":12},"end":{"line":8,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[280,307],"loc":{"start":{"line":12,"column":19},"end":{"line":12,"column":55}}},{"type":"RequireHeaderDependency","range":[272,279],"loc":{"start":{"line":12,"column":19},"end":{"line":12,"column":55}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/asyncToGenerator","range":[403,443],"loc":{"start":{"line":16,"column":25},"end":{"line":16,"column":74}}},{"type":"RequireHeaderDependency","rang