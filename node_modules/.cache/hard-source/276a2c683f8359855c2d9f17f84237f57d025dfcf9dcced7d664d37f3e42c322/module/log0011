zed: false }, true);\n        });\n    }\n\n    @action updateAttribution = () => {\n        setAttribution(this.consultantShopping, this.socialId);\n    }\n\n    @action setConsultant = (consultant) => {\n        if (!userStore.isConsultant && !userStore.isEmployee) {\n            const newConsultant = cloneDeep(this.processConsultantData(consultant));\n            this.consultant = newConsultant;\n        }\n    }\n    \n    @action lookupPWS = async ({ key, id }) => {\n        try {\n            const result = await requestPWS({ key, id });\n            if (result && result.data && result.data.id && result.data.active) {\n                const data = { \n                    ...result.data,\n                    key,\n                };\n                this.setConsultant(data);\n                return cloneDeep(this.processConsultantData(data));\n            }\n        }\n        catch(err) {\n            console.log(err);\n            return false;\n        }\n    }\n\n    @action getSocials = (id) => {\n        return getSocials(id, localeStore.activeLocale.phoenixCountry);\n    }\n\n    @action searchConsultants = async (entry, limit) => {\n        const result = await searchConsultantNames(entry, limit, localeStore.activeLocale.phoenixCountry);\n\n        if (result) {\n            analyticsStore.consultantsSearched(result, entry);\n            return {\n                ...result,\n                items: result.items ? uniqBy(result.items, 'accountID') : [],\n            };\n        }\n    }\n\n    @action searchConsultantsByLocation = async (entry, limit) => {\n        const result = await searchConsultantsByLocation(entry, limit);\n        if (result) {\n            analyticsStore.consultantsSearched(result, entry);\n            return {\n                ...result,\n                items: result.items ? uniqBy(result.items, 'accountID') : [],\n            };\n        }\n    }\n    \n    @action hydrateAttribution = (attribution) => {\n        if (attribution.sponsorid && !this.consultantShopping && !this.isOrderPage) {\n            this.lookupPWS({ id: attribution.sponsorid });\n        }\n        this.sponsor = attribution.sponsorid || false;\n    }\n\n    @action processConsultantData = (consultant) => {\n        const socialId = this.consultant.socialId || consultant.socialId;\n\n        return {\n            ...consultant,\n            socialId,\n            fullName: consultant.fullName || `${consultant.firstName} ${consultant.lastName}`,\n            url: `/${consultant.pwsKey}` || '/',\n        };\n    }\n\n    @action goToPWS = async (id, contact = false) => {\n        interfaceStore.showProgress('page-load');\n        const PWS = await this.lookupPWS({ id });\n\n        const errorMsg = () => messagingStore.addMessage(`Sorry, no page was found for that Consultant.`, { type: 'error' });\n\n        try {\n            if (PWS && PWS.url) navigationStore.to({ url: PWS.url + (contact ? this.pwsContactURL : '') });\n            else errorMsg();\n        }\n        catch(e) {\n            console.error(e);\n            errorMsg();\n        }\n        interfaceStore.hideProgress('page-load');\n    }\n\n    @computed get pwsContactURL() {\n        return getSafe(() => contentStore.content.configGeneral[0].pwsContactUrl)\n    }\n\n    @action reset = () => {\n        this.consultant = {};\n        this.sponsor = false;\n        sessionStore.saveKey('consultant', {}, { localized: false }, true);\n    }\n\n    @action resetOverride = () => {\n        const pws = sessionStore.getKey('pws-consultant', { localized: false }, true);\n        const id = pws ? pws.id : toJS(this.sponsor);\n        if (id) {\n            this.lookupPWS({ id });\n        }\n        else {\n            this.reset();\n        }\n        sessionStore.saveKey('override-consultant', {}, { localized: false }, true);\n        sessionStore.saveKey('pws-consultant', {}, { localized: false }, true);\n    }\n\n    @computed get consultantShopping() {\n        return getSafe(() => this.consultant.id && this.consultant.id !== 1);\n    }\n\n    @computed get socialId() {\n        return getSafe(() => this.consultant.socialId);\n    }\n\n    @computed get shoppingWithHQ() {\n        if (this.consultant && this.consultant.id === 1) return true;\n    }\n\n    @computed get isOrderPage() {\n        return window.location.pathname.includes(routes.ORDER_CONFIRM);\n    }\n\n    @computed get consultantInfo() {\n        return getSafe(() => toJS(this.consultant));\n    }\n\n    @action setSocial = (id) => {\n        this.consultant.socialId = id;\n        this.consultant = cloneDeep(this.consultant);\n    }\n}\n\nconst attributionStore = new AttributionStore();\n\nexport default attributionStore;\nexport { AttributionStore };\n"]}},"_buildHash":"884bb395be003bb02c59660740d86f2b","hash":"dc6eaf09446a5042ea50e55e7b97e9ca","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","reques