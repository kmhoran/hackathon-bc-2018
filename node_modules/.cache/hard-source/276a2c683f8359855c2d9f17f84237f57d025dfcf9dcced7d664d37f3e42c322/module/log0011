tSafe(() => toJS(attributionStore.consultant).fullName);\n\n        return {\n            category,\n            consultant_id,\n            consultant_name,\n            social_id,\n            currency: this.currency,\n            locale: getSafe(() => localeStore.activeLocale.code),\n            time: Date.now(),\n            chnl: 'online',\n            application: 'tiamat'\n        };\n    }\n\n    constructor() {\n        this.viewedCheckoutStepOne = false;\n        this.currency = null;\n        this.title = null;\n\n        __BROWSER__ && requestAnimationFrame(() => {\n            autorun(() => {\n                userStore.user;\n                attributionStore.consultant;\n                attributionStore.socialId;\n                attributionStore.consultantShopping;\n\n                const { consultant_id, consultant_name, social_id } = this.properties;\n\n                traits({ consultant_id, consultant_name, social_id });\n\n                if (userStore.user.hasOwnProperty('email')) {\n                    const { magentoid, email, encoreid, groupid, firstname, lastname } = userStore.user;\n                    const id = magentoid ? magentoid : encoreid;\n                    identify(id, {\n                        email,\n                        username: email,\n                        magento_id: magentoid,\n                        encore_id: encoreid,\n                        group_id: groupid,\n                        firstName: firstname,\n                        lastName: lastname,\n                        name: `${firstname} ${lastname}`,\n                        ...this.properties\n                    });\n                }\n            });\n\n            autorun(() => {\n                localeStore.activeLocale;\n\n                if (localeStore.activeLocale.code === 'en-US') this.currency = 'USD';\n                else this.currency = 'CAD';\n            });\n        });\n    }\n\n    @action productAdded(product, incrementButton = false) {\n        const { quantity } = product;\n        track('Product Added', {\n            label: incrementButton ? 'Increment Button' : 'Add To Bag',\n            ...productsStore.segmentObject[product.id],\n            quantity,\n            ...this.properties\n        });\n    }\n\n    @action productRemoved(product, decrementButton = false) {\n        const { quantity } = product;\n        track('Product Removed', {\n            label: decrementButton ? 'Decrement Button' : 'Remove All',\n            ...productsStore.segmentObject[product.id],\n            quantity,\n            ...this.properties\n        });\n    }\n\n    @action async orderCompleted({ orderId }, cart) {\n        if (!cart) return false;\n        const { subtotal, grandtotal, tax, shipping_amount, discount_amount, promoid, promocode } = await getOrderDetails(orderId);\n        const fromCheckout = __BROWSER__ && getSafe(() => JSON.parse(window.sessionStorage.getItem('fromCheckout'))) || {};\n        const total = fromCheckout.total || null;\n        const { skus, quantity } = this.getSkusQty(cart.products);\n        this.trackCheckoutStep();\n        const properties = {\n            label: 'Purchase',\n            order_id: orderId,\n            total: fromCheckout && fromCheckout.total,\n            value: fromCheckout && fromCheckout.total && fromCheckout.total.toFixed(2),\n            shipping: shipping_amount,\n            tax,\n            discount: discount_amount,\n            coupon: promocode,\n            skus,\n            products: cart.products,\n            quantity,\n            ...this.properties,\n            consultant_id: (fromCheckout && fromCheckout.consultantId) || 1\n        };\n        this._orderProperties = properties;\n        track('Order Completed', properties);\n        track('Order Completed - Facebook', properties);\n\n        if (__BROWSER__ && window.optimizely) {\n            requestAnimationFrame(() => {\n                window.optimizely.push({\n                    type: 'event',\n                    eventName: 'purchase',\n                    tags: {\n                    revenue: total && (total * 100)\n                    }\n                });\n            });\n        }\n    }\n\n    @action checkoutAttribution(attribution) {\n        const { total } = cartStore.cart;\n        const { skus, quantity } = this.getSkusQty(cartStore.cartItems);\n        __BROWSER__ && window.sessionStorage.setItem('fromCheckout', JSON.stringify({\n            consultantId: attribution && attribution.consultantId,\n            total\n        }));\n        this.trackCheckoutStep(3);\n        if (attribution && attribution.consultantId) {\n            track('Consultant Attributed In Checkout', {\n                label: 'Continue',\n                total,\n                value: total && total.toFixed(2),\n                skus,\n                quantity,\n                products: cartStore.cartItems,\n                ...this.properties,\n                consultantId: attribution && attribution.consultantId\n            });\n        }\n    }\n\n    @action checkoutStarted() {\n        const { total } = cartStore.cart;\n        const { skus, quantity } = this.getSkusQty(cartStore.cartItems);\n        track('Checkout Started', {\n            label: 'Checkout',\n            total,\n            value: total && total.toFixed(2),\n            skus,\n            quantity,\n            products: cartStore.cartItems,\n            ...this.properties\n        });\n    }\n\n    @action trackCheckoutStep(step) {\n        const viewed = 'Checkout Step Viewed',\n              completed = 'Checkout Step Completed';\n\n        switch (step) {\n            case 1:\n                this.checkoutStarted();\n                track(viewed, { step, label: step, ...this.properties });\n                this.viewedCheckoutStepOne = true;\n                break;\n            case 2:\n                if (!this.viewedCheckoutStepOne) {\n                    this.viewedCheckoutStepOne = true;\n                    this.checkoutStarted();\n                    track(viewed, { step: 1, label: 1, ...this.properties });\n                }\n                track(completed, { step: 1, label: 1, ...this.properties });\n                track(viewed, { step, label: step, ...this.properties });\n                break;\n            case 3:\n                track(completed, { step: 2, label: 2, ...this.properties });\n                track(viewed, { step, label: step, ...this.properties });\n                break;\n            default:\n                track(completed, { step: 3, label: 3, ...this.properties });\n                break;\n        }\n    }\n\n    @action cartViewed() {\n        const { total } = cartStore.cart;\n        const { skus, quantity } = this.getSkusQty(cartStore.cartItems);\n        track('Cart Viewed', {\n            label: 'Bag Icon',\n            total,\n            value: total && total.toFixed(2),\n            skus,\n            quantity,\n            products: cartStore.cartItems,\n            ...this.properties\n        });\n    }\n\n    @action linkClicked(destinationType, props) {\n        const { to, name, children } = props;\n        track('Link Clicked', {\n            label: name || (typeof children === 'string' ? children : null),\n            destination: to,\n            destinationType,\n            ...this.properties\n        });\n    }\n\n    @action buttonClicked(props) {\n        const { to, children } = props;\n        track('Button Clicked', {\n            label: getSafe(() => this.getChildString(children)),\n            destination: to,\n            ...this.properties\n        });\n    }\n\n    @action productsSearched(searchResults, params) {\n        const results = getSafe(() => searchResults.map(obj => ({ id, sku, handle })));\n        track('Products Searched', {\n            results,\n            numberOfResults: results && results.length,\n            query: params,\n            ...this.properties\n        });\n    }\n\n    @action consultantsSearched(result, query) {\n        const results = result && result.items ? uniqBy(result.items, 'accountID') : [];\n        track('Consultants Searched', {\n            results,\n            numberOfResults: results && results.length,\n            query,\n            ...this.properties\n        });\n    }\n\n    @action quickViewToggled(url, variant) {\n        const { id } = productsStore.productsBySlug[url];\n        track('Quick View Toggled', {\n            label: getSafe(() => product.name),\n            ...productsStore.segmentObject[id],\n            ...this.properties\n        });\n    }\n\n    @action productListViewed(category, title) {\n        const productsInCategory = getSafe(() => category && productsStore.productsByCategoryId[category.magentoId]);\n        const products = getSafe(() => productsInCategory && productsInCategory.map(p => productsStore.segmentObject[p.id]));\n        const skus = getSafe(() => products.map(p => p.sku));\n        track('Product List Viewed', {\n            label: getSafe(() => category.name),\n            list_id: getSafe(() => category.magentoId),\n            products,\n            skus,\n            ...this.properties,\n            category: title\n        });\n    }\n\n    @action productViewed(id) {\n        const product = productsStore.segmentObject[id];\n        track('Product Viewed', {\n            label: getSafe(() => product.name),\n            ...product,\n            ...this.properties\n        });\n    }\n\n    @action pwsLookup(consultant, social) {\n        track('PWS Lookup', {\n            label: consultant && consultant.fullName,\n            lookupResult: consultant && consultant.id,\n            social,\n            ...this.properties\n        });\n    }\n\n    @action promoItemChanged(id, quantity, promoPack = null) {\n        const product = productsStore.segmentObject[id];\n        track('Promo Item Changed', {\n            label: getSafe(() => product.name),\n            products: [product],\n            skus: [product.sku],\n            quantity,\n            promoPack,\n            ...this.properties\n        });\n    }\n\n    @action promoItemRemoved(id, promoPack = null) {\n        const product = productsStore.segmentObject[id];\n        track('Promo Item Removed', {\n            label: getSafe(() => product.name),\n            products: [product],\n            skus: [product.sku],\n            promoPack,\n            ...this.properties\n        });\n    }\n\n    @action pageViewed(title) {\n        if (!(__BROWSER__ && window.location && title)) return false;\n        const path = window.location.pathname;\n        const label = this.getPageDetails(path);\n        const newTitle = window.location.pathname === '/' ? 'Home' : title;\n        const productsIndex = getSafe(() => path.split('/').indexOf(routes.PRODUCTS.substring(1)));\n\n        if (productsIndex && productsIndex !== -1) {\n            const slug = getSafe(() => path.split('/')[productsIndex + 1]);\n            const category = slug && getSafe(() => categoryStore.categoriesBySlug[slug]);\n\n            if (category) this.productListViewed(category, newTitle);\n        }\n\n        page(undefined, {\n            label,\n            ...this.properties,\n            category: newTitle\n        });\n\n        if (__BROWSER__ && window.optimizely && window.location.pathname.includes('/summer-gift-with-purchase')) {\n            window.optimizely.push({\n                type: 'event',\n                eventName: 'promoBannerPageView'\n            });\n        }\n    }\n\n    @action signUp(email, source) {\n        const { code, name, magentoStoreView } = localeStore.activeLocale;\n        track('Sign Up', {\n            label: email,\n            Language: code.toUpperCase(),\n            countryId: name === 'FR' ? 'CA' : name,\n            accountType: 'Prospect',\n            source,\n            storeId: magentoStoreView,\n            ...this.properties\n        });\n    }\n\n    @action enrollmentStarted() {\n        track('Enrollment Started', {\n            label: 'Join Now',\n            ...this.properties\n        });\n    }\n\n    @action reset() {\n        reset();\n    }\n\n    @action getChildString(children) {\n        if (!children) return null;\n        if (typeof children === 'string') return children;\n        if (Array.isArray(children)) {\n            for (let i = 0; i < children.length; i++) {\n                if (children[i] && children[i].props && children[i].props.children && typeof children[i].props.children === 'string') {\n                    return children[i].props.children;\n                }\n            }\n        }\n        return null;\n    }\n\n    @action getSkusQty(products) {\n        const skus = getSafe(() => products.map(product => product.sku));\n        const quantity = getSafe(() => products.reduce((accum, product) => accum += product.quantity, 0));\n        return { skus, quantity };\n    }\n\n    @action getPageDetails(path) {\n        if (!path) return;\n        let label;\n        const params = path.split('/');\n\n        params.shift();\n        params[0] = '/' + params[0];\n        if (params[params.length - 1] === '') params.length--;\n\n        if (params[0] === routes.PRODUCTS) {\n            label = getSafe(() => categoryStore.categorySlugs[params[1]].name);\n        }\n\n        else if (params[0] === routes.PRODUCT) {\n            const product = getSafe(() => productsStore.productsBySlug[params[1]]);\n\n            if (product && product.parentCategoryIds) {\n                for (let i = 0; i < product.parentCategoryIds.length; i++) {\n                    if (categoryStore.categoryIds[product.parentCategoryIds[i]]) {\n                        label = getSafe(() => categoryStore.categoryIds[product.parentCategoryIds[i]].name);\n                        break;\n                    }\n                }\n            }\n        }\n\n        else if (params[0] === routes.PWS || '/' + params[1] === attributionStore.pwsContactURL) {\n            label = params[0] === routes.PWS ? params[1] : params[0].substring(1);\n        }\n\n        return label;\n    }\n}\n\nconst analyticsStore = new AnalyticsStore();\n\nexport default analyticsStore;\nexport { AnalyticsStore };\n"]}},"_buildHash":"8e0e28780a60017e309312afe09741c6","hash":"1339465348053168ed3d9f54e7254ecc","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/define-property","range":[149,195],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[141,148],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[296,303],"loc":{"start":{"line":12,"column":32},"end":{"line":12,"column":99}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[304,362],"loc":{"start":{"line":12,"column":32},"end":{"line":12,"column":99}}},{"type":"RequireHeaderDependency","range":[468,475],"loc":{"start":{"line":16,"column":17},"end":{"line":16,"column":64}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/json/stringify","range":[476,514],"loc":{"start":{"line":16,"column":17},"end":{"line":16,"column":64}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[600,627],"loc":{"start":{"line":20,"column":19},"end":{"line":20,"column":55}}},{"type":"RequireHeaderDependency","range":[592,599],"loc":{"start":{"line":20,"column":19},"end":{"line":20,"column":55}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/asyncToGenerator","range":[723,763],"loc":{"start":{"line":24,"column":25