 that should redirect\n                const redirects = getSafe(() => stores.contentStore.content.productDetail[0].redirects);\n                if (redirects) {\n                    for (let i = 0; i < Object.keys(redirects).length; i++) {\n                        const redirectSKU = Object.keys(redirects)[i];\n                        if (SKU == redirectSKU) {\n                            return {\n                                redirect: redirects[redirectSKU],\n                                replace: localize(redirects[redirectSKU])\n                            };\n                        }\n                    }\n                }\n\n                // Standard product?\n                const [ ProductContainer, memberConfig ] = await Promise.all([\n                    loadContainer('ProductContainer'),\n                    stores.contentStore.fetchContentfulConfig({\n                        ...asyncModels.memberPromotion,\n                        route: path,\n                    }),\n                ]);\n\n                await stores.interfaceStore.setHeaderStyle({\n                    transparentHeader: false,\n                    hideCategoryNavigation: false,\n                    disableHover: true,\n                    disabledCategoryBorder: false,\n                });\n\n                return {\n                    chunks: ['ProductContainer'],\n                    component: <ProductContainer\n                        slug={params.slug}\n                        selectedVariant={variant || false}\n                    />\n                };\n            }\n        }]\n    },\n\n    {\n        path: [\n            `${routes.BECOME_MEMBER}`,\n            `${routes.MEMBER_RENEWAL}`,\n        ],\n        async action({ stores, path }) {\n            stores.interfaceStore.showProgress(\"page-load\");\n            const [ MemberContainer ] = await Promise.all([\n                loadContainer('MemberContainer'),\n                stores.contentStore.fetchConte