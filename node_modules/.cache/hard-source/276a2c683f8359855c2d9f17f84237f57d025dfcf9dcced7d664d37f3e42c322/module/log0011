a/react-node-frontend","sourcesContent":["import React, { Component } from 'react';\nimport { autorun } from 'mobx';\nimport { observer, inject } from 'mobx-react';\nimport s from './PreOrderPopup.scss';\nimport cx from 'classnames';\nimport MarkdownWrapper from 'Components/MarkdownWrapper';\nimport Form from 'Common/Form';\nimport getLoginItems from 'Containers/PageContainer/LoginPage/forms/login';\nimport Link from 'Common/Link';\nimport { routes } from 'Root/config';\nimport Button from 'Common/Button';\nimport IconClose from 'Icons/Controls/Close.jsx';\nimport ClickOutHandler from 'react-onclickout';\nimport withStyles from 'isomorphic-style-loader/lib/withStyles';\nimport { getSafe } from 'Middleware/utils/object';\n\n@withStyles(s)\n@inject(\n    'userStore',\n    'messagingStore',\n    'interfaceStore'\n)\n@observer class PreOrderPopup extends Component {\n\n    state = {\n        signUpFormActive: false,\n        earlyAccessFormActive: false,\n        hideForm: false,\n        hideEarlyAccessForm: true,\n        form: {}\n    }\n\n    componentDidMount() {\n\n        // Delays displaying the popup form. Prevents flashing for logged in users.\n        this.disposer = autorun(() => {\n            if (this.props.interfaceStore.appIsLoaded) {\n                setTimeout(() => {\n                    if (!this.props.userStore.isLogged) {\n                        !this.state.signUpFormActive && this.setState({ signUpFormActive: true });\n                    }\n                }, 300);\n            }\n        });\n    }\n\n    componentWillUnmount() {\n        this.disposer();\n    }\n\n    handleChange = (item) => {\n        this.setState({ form: {\n            ...this.state.form,\n            ...item,\n        } });\n    }\n\n    parseError = (error) => {\n        const {\n            formErrorMessage\n        } = this.props.config;\n\n        if (error.includes('Invalid login or password.') || error.includes('Enter Valid Username.')) {\n            const message = formErrorMessage;\n            const linkMessage = getSafe( () => message.split('{')[1].split('}')[0] );\n            const p1 = message.split('{')[0];\n            const p2 = message.split('}')[1];\n\n            const formattedError = (\n                <span>\n                    {p1}<Link onClick={() =>\n            this.props.messagingStore.clear()} className={s.linkMessage} to={routes.FORGOT_PASSWORD}><u>{linkMessage}</u></Link>{p2}\n                </span>\n            );\n\n            return formattedError;\n        }\n        return error;\n    }\n\n    closeSignUpForm = () => {\n        this.setState({ signUpFormActive: false });\n        this.setState({ earlyAccessFormActive: false }, () => {\n            this.hideForm();\n        });\n    }\n\n    hideForm = () => {\n        if (!this.state.earlyAccessFormActive || !this.state.signUpFormActive) {\n            this.setState({ hideForm: true }, () => {\n                this.forceUpdate();\n            });\n        }\n    }\n\n    handleSubmit = async (data) => {\n        this.props.interfaceStore.showProgress(\"login\");\n        const loggedIn = await this.props.userStore.login(data);\n\n        if (loggedIn.success) {\n            this.props.messagingStore.clearErrors();\n            this.setState({\n                earlyAccessFormActive: true,\n                signUpFormActive: false,\n                hideEarlyAccessForm: false,\n            });\n\n        } else {\n            const error = loggedIn.message || \"Invalid username or password.\";\n            this.props.messagingStore.addMessage(this.parseError(error), { type: 'error' });\n        }\n        this.hideForm();\n        this.props.interfaceStore.hideProgress(\"login\");\n    }\n\n    render() {\n        if (!this.props.config) return false;\n\n        const {\n            preorderExperienceEarlyAccessPopupHeader,\n            preorderExperienceEarlyAccessContent,\n            preorderExperienceBobJoinNowButton,\n            preorderExperienceEmailInput,\n            preorderExperienceInlineContent,\n            preorderExperiencePasswordInput,\n            preorderExperienceForgotPassword,\n            preorderExperienceSignInButton,\n            preorderExperienceInlineHeader,\n            earlyAccessImage,\n            signInImage,\n            earlyAccessImageMobile,\n            signInImageMobile,\n            togglePopupModules,\n            shopNowText,\n            shopNowUrlLink,\n            earlyAccessUrlLink\n        } = this.props.config;\n\n        return (\n            <React.Fragment>\n                {/*  Sign up form popup for users who are NOT logged in */}\n                {(this.state.signUpFormActive && this.props.product.preOrder && togglePopupModules) &&\n                    <div className={cx(s.overlayModule, this.state.signUpFormActive ? s.formActive : s.formInactive)}\n                        style={{ display: this.state.hideForm ? 'none' : 'block' }}\n                        >\n                        <ClickOutHandler onClickOut={() => this.closeSignUpForm()}>\n                            <div\n                                className={cx(s.signupFormContainer)}\n                                >\n                                <div className={s.signupFormContent}>\n                                    <div className={s.signUpFormImage}>\n                                        <img src={\n                                            this.props.interfaceStore.isMobile ?\n                                            earlyAccessImageMobile && `${getSafe(() => earlyAccessImageMobile.fields.file.url)}?q=70` :\n                                            earlyAccessImage && `${getSafe(() => earlyAccessImage.fields.file.url)}?q=70`\n                                        }\n                                        />\n                                    </div>\n                                    <div className={s.signUpForm}>\n                                        <MarkdownWrapper\n                                            className={s.formHeader}\n                                            content={preorderExperienceInlineHeader}\n                                        />\n                                        <p>{preorderExperienceInlineContent}</p>\n                                        <Form\n                                            items={getLoginItems(this, s, {\n                                                emailLabel: preorderExperienceEmailInput,\n                                                passwordLabel: preorderExperiencePasswordInput,\n                                            })}\n                                            onSubmit={this.handleSubmit}\n                                            errors={this.state.errors}\n                                            showLabels={true}\n                                            onChange={items => this.handleChange(items)}\n                                            labelsPosition={'top'}\n                                            largeInput={true}\n                                            floatingLabel={true}\n                                            noMargin={true}>\n\n                                            <div className={s.forgotPasswordWrapper}>\n                                                <Link\n                                                    className={s.forgotPasswordLink}\n                                                    to={routes.FORGOT_PASSWORD}>\n                                                        {preorderExperienceForgotPassword}\n                                                </Link>\n                                            </div>\n\n                                            <div className={s.button}>\n                                                <Button isSubmit wide>\n                                                    {preorderExperienceSignInButton}\n                                                </Button>\n                                            </div>\n\n                                            <div className={cx(s.forgotPasswordWrapper, s.shopNowLink)}>\n                                                <Link\n                                                    className={s.forgotPasswordLink}\n                                                    to={shopNowUrlLink}>\n                                                        <MarkdownWrapper\n                                                            className={s.forgotPasswordLink}\n                                                            content={shopNowText}\n                                                        />\n                                                </Link>\n                                            </div>\n                                        </Form>\n                                        <IconClose className={cx(s.closeIcon, s.signInIcon)} onClick={this.closeSignUpForm}/>\n                                    </div>\n                                </div>\n                            </div>\n                        </ClickOutHandler>\n                    </div>\n                }\n\n                {/*  Early Access form popup for users who are logged in as NON-BoB members */}\n                {(this.state.earlyAccessFormActive && this.props.product.preOrder && togglePopupModules && !this.props.userStore.isConsultant) &&\n                    <div className={cx(s.overlayModule, this.state.earlyAccessFormActive ? s.formActive : s.formInactive)}\n                        style={{ display: this.state.hideEarlyAccessForm ? 'none' : 'block' }}\n                        >\n                        <ClickOutHandler onClickOut={() => this.closeSignUpForm()}>\n                            <div\n                                className={cx(s.signupFormContainer)}\n                                >\n                                <div className={cx(s.signupFormContent, s.earlyAccessFormContent)}>\n                                    <div className={s.signUpFormImage}>\n                                        <img src={\n                                            this.props.interfaceStore.isMobile ?\n                                            signInImageMobile && `${getSafe(() =>  signInImageMobile.fields.file.url)}?q=70` :\n                                            signInImage && `${getSafe(() =>  signInImage.fields.file.url)}?q=70`\n                                        } />\n                                    </div>\n\n                                    <div className={cx(s.signUpForm, s.earlyAccessForm)}>\n                                        <div className={s.earlyAccessButtonContainer}>\n                                            <IconClose className={s.earlyAccessCloseIcon} onClick={this.closeSignUpForm}/>\n                                        </div>\n                                        <h2 className={s.formHeader}>{preorderExperienceEarlyAccessPopupHeader}</h2>\n                                        <p>{preorderExperienceEarlyAccessContent}</p>\n                                        <div className={s.earlyAccessButton}>\n                                            <Link to={earlyAccessUrlLink}>\n                                                <Button isSubmit wide>\n                                                    {preorderExperienceBobJoinNowButton}\n                                                </Button>\n                                            </Link>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                        </ClickOutHandler>\n                    </div>\n                }\n            </React.Fragment>\n        )\n    }\n\n}\n\nexport default PreOrderPopup;\n"]}},"_buildHash":"ac032d51b51a9c65ae5ff7305fb23671","hash":"ab3b4a8060a67850a50660212ceee61b","_lastSuccessfulBuil