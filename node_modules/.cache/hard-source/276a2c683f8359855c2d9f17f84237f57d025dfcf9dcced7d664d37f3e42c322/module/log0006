ort CartPromotion from './CartPromotion';\nimport CartMenu from './CartMenu';\nimport CartItem from './CartItem';\n\nimport Container from 'Common/Container';\nimport Row from 'Common/Row';\nimport Column from 'Common/Column';\nimport Separator from 'Common/Separator';\nimport Button from 'Common/Button';\nimport Link from 'Common/Link';\nimport Image from 'Common/Image';\nimport IconBack from 'Icons/Controls/LeftAngle.jsx';\nimport IconCartShopping from 'Icons/CartShopping.jsx';\n\n@withStyles(s)\n@inject(\n    'cartStore',\n    'contentStore',\n    'productsStore',\n    'promoStore',\n    'interfaceStore',\n    'userStore',\n)\n@observer class Cart extends Component {\n\n    constructor(props) {\n        super(props);\n        this.totalPromoSavings = 0;\n        this.totalCost = 0;\n    }\n\n    static propTypes = {\n        cartLoaded: pt.bool,\n        cartId: pt.string,\n        products: pt.array,\n        promoStore: pt.object,\n    };\n\n    componentWillUpdate() {\n        if(this.props.cartStore.newCartItem && typeof this.refs.cartItems != \"undefined\")\n            this.refs.cartItems.scrollTop = 0;\n    };\n\n    handleCheckout = event => {\n        const { serverRequest } = this.props.cartStore;\n        if (serverRequest === false || typeof serverRequest === 'string') this.props.cartStore.prepareCheckout();\n    };\n\n    renderCartItems(items, isPromo = true, removable) {\n        if (!Array.isArray(items) && items.length <= 1) return null;\n\n        const { cartId, promoStore, cartStore, userStore } = this.props;\n\n        return items.map((cartItem, idx) => {\n            const \n                product = this.props.productsStore.productsById[cartItem.parentId] || null,\n                variant = (product && product.variants.length > 1) ? getVariantKey(cartItem.variant.id, product) : null,\n                inStock = variant ? variant.inStock : (product ? product.inStock : null);\n            const { groupId } = this.props.userStore;\n\n            if (product) {\n\n                const id = variant ? variant.id : product.id;\n                const sku = variant ? variant.sku : product.sku;\n\n                const promoPrice = promoStore.promoCartProductsByID\n                && promoStore.promoCartProductsByID[product.id] ?\n                promoStore.promoCartProductsByID[product.id].price\n                : cartItem.price;\n\n                let amount = (() => {\n                    return cartItem.price;\n                    // return variant ? variant.price: product.price;\n                })();\n\n            // if this is not a promo store it for later manipulation.\n                if (!isPromo) {\n                    promoStore.unpromofiedCartItemsById[id] = cartItem;\n                }\n\n                const compareAtPrice = (() => {\n                    if (amount < 1) return 0;\n\n                // pull the price from promo store first. if no promostore exists, then dynamically fetch price also if a consultant.\n                    if (id) {\n                        let alteredPromoPrice = promoPrice;\n                        if (promoPrice === amount && groupId && sku) {\n                            alteredPromoPrice = promoStore.applyGroupPricingRules({\n                                price: promoPrice,\n                                sku,\n                            });\n                        }\n                        const ret = amount - alteredPromoPrice;\n                        if (ret > 0) return ret;\n                        return 0;\n                    }\n                    return 0;\n                })() * cartItem.quantity;\n\n            // finsished all amount calculations that require the individual price.\n                amount *= cartItem.quantity;\n\n                this.totalCost += amount;\n\n            // adjust promo subtotal.\n                const deduction = amount - (amount - compareAtPrice);\n                if (deduction > 0) this.totalPromoSavings += deduction;\n\n                return (\n                    // <CSSTransition\n                    //     key={cartItem.id}\n                    //     classNames={{\n                    //         enter: s.cartItemEnter,\n                    //         enterActive: s.cartItemEnterActive,\n                    //         exit: s.cartItemExit,\n                    //         exitActive: s.cartItemExitActive,\n                    //     }}\n                    //     timeout={{ enter: 500, exit: 500 }}>\n                            <div key={idx}>\n                                <div className={s.cartItemContainer}>\n                                    <CartItem\n                                        product={product}\n                                        variant={variant}\n                                        cartItem={cartItem} \n                                        cartId={cartId} \n                                        promo={cartItem.promoBucket}\n                                        price={amount - deduction}\n                                        compareAtPrice={amount}\n                                        promoPrice={(() => {\n                                            if (cartItem.promoPrice) {\n                                                return cartItem.price;\n                                            }\n                                        })()}\n\t\t\t\t\t\t\t\t\t\tcanDelete={removable}\n                                    />\n                                </div>\n                                <Separator className={s.itemSeparator}/>\n                            </div>\n                    // </CSSTransition>\n                )\n            }\n            return null;\n        });\n    }\n\n    render() {\n        const { promoStore, cartStore } = this.props;\n        let { products: sourceProducts, cartId, productsById, serverRequest, count } = cartStore,\n        { products: promoProducts, activatedPromos, promoCartTotal, discountTotal, has } = promoStore,\n            config = this.props.contentStore.content.cart ? this.props.contentStore.content.cart[0] : null;\n\n        if(!config)\n            return false;\n        const cartItems = [];\n\n    // on each render we set the promo total to match the original cost as a base.\n        this.totalCost = 0;\n        this.totalPromoSavings = 0;\n\n    //make a clone of products\n        const products = JSON.parse(JSON.stringify(sourceProducts));\n\n    // extract promofied items like gwp and bogo into promo groups.\n        if (Object.keys(activatedPromos).length > 0) {\n            cartItems.push(...Object.keys(activatedPromos)\n            .filter((promoId) => \n            // check if there is an array entry.\n                activatedPromos[promoId].length > 0\n            // make sure this promo has children.\n                && activatedPromos[promoId][0].children.length > 0)\n            .map((promoId) => {\n                if (activatedPromos[promoId].length < 1) return null;\n\n                return activatedPromos[promoId].map((promo, promoIndex) => {\n\n                    const ParentItems = (() => {\n                        let productIndex = null;\n                        return [\n                            ...this.renderCartItems(\n                                promo.parents.map((id) => {\n                                    const productInfo = Object.keys(products)\n                                        .filter((i) => {\n                                            if (products[i].productId === id) {\n                                                productIndex = i;\n                                                return true;\n                                            }\n                                            if (products[i].variantId === id) {\n                                                productIndex = i;\n                                                return true;\n                                            }\n                                        })\n                                        .map(index => products[index])[0];\n\n                                    if (productInfo) {\n                                        const promoProductInfo = JSON.parse(JSON.stringify(productInfo));\n                                        \n                                    //wrap the promo info into the rendered item to give quantity selector access to promo meta.\n                                        promoProductInfo.promoBucket = {\n                                            promo,\n                                            relationship: 'parent'\n                                        };\n\n                                        promoProductInfo.quantity = promo.parentQuantity;\n\n                                    // remove quantity from product original\n                                        productInfo.quantity = productInfo.quantity - promo.parentQuantity;\n\n\n                                    // delete product from general group if this is the only one\n                                        if (productInfo.quantity < 1) {\n                                            products.splice(productIndex, 1);\n                                        }\n                                        return promoProductInfo;\n                                    }\n                                    return [];\n                                }),\n                            ),\n                        ];\n                    })();\n\n                    const ChildItems = ((removable = true) => {\n                        let productIndex = null;\n                        let count = promo.children.length;\n                        return [\n                            ...this.renderCartItems(\n                                promo.children.map((id) => {\n                                    const productInfo = Object.keys(products)\n                                        .filter((i) => {\n                                            if (products[i].productId === id) {     \n                                                productIndex = i;\n                                                return true;\n                                            }\n                                            if (products[i].variantId === id) {\n                                                productIndex = i;\n                                                return true;\n                                            }\n                                        })\n                                        .map(index => {\n                                            return products[index]\n                                        })[0];\n\n                                    if (productInfo) {\n                                        const promoProductInfo = JSON.parse(JSON.stringify(productInfo));\n\n                                    // subtract the free item from the total.\n                                        this.totalPromoSavings += promoProductInfo.price * promo.childQuantity;\n                                        this.totalCost += promoProductInfo.price * promo.childQuantity;\n\n                                    // child item is a free item.\n                                        promoProductInfo.price = 0;\n\n                                    // wrap the promo info into the rendered item to give quantity selector access to promo meta.\n                                        promoProductInfo.promoBucket = {\n                                            promo,\n                                            type: 'child'\n                                        };\n\n                                        promoProductInfo.quantity = promo.childQuantity / (promo.children.length || 1);\n\n                                        //remove quantity from product original\n                                        productInfo.quantity = productInfo.quantity - (promo.childQuantity / (promo.children.length || 1));\n\n                                        //delete product from general group if this is the only one\n                                        if (productInfo.quantity < 1) {\n                                            products.splice(productIndex, 1);\n                                        }\n                                        return promoProductInfo;\n                                    }\n                                    return [];\n                                }),\n                                true, //isPromo\n                                removable\n                            ),\n                        ];\n                    });\n\n                    const key = `${promoId}${promoIndex}`;\n                    const promoType = promoStore.getPromoType(promoId);\n                    const isShort = (() => {\n                        if (promoStore.promoTypeHasBehavior(promoType,\n                        'hasShortCartContainer')) {\n                            return 'Short';\n                        }\n                        return '';\n                    });\n                    return (\n                    // TODO: refactor animations to work correctly.\n                        // <CSSTransition\n                        // key={key}\n                        // classNames={{\n                        //     enter: s[`cartGroupEnter${isShort()}`],\n                        //     enterActive: s[`cartGroupEnterActive${isShort()}`],\n                        //     exit: s[`cartGroupExit${isShort()}`],\n                        //     exitActive: s[`cartGroupExitActive${isShort()}`],      \n                        // }}\n                        // timeout={{ enter: 500, exit: 500 }}>\n                            <div key={key} className={s.promoContainer}>\n                                {ParentItems}\n                                {ChildItems(\n                                    !promoStore.promoTypeHasBehavior(promoType,\n                                    'disablesDelete')\n                                )}\n                            </div>\n                        // </CSSTransition>\n                    );\n\n                });\n            }));\n        }\n\n\n    // render remaining promo unnattached products.\n        cartItems.push(...this.renderCartItems(products, false));\n\n        if (isEmpty(cartItems) || count == 0 || !this.props.interfaceStore.appIsMounted) {\n            return (\n                <div className={s.panel}>\n                <CartMenu config={config} />\n                <Row>\n                <Container contentArea className={s.cartEmptyArea}>\n                    <div className={s.cartEmpty}>\n                        {config.emptyMessage}\n                    </div>\n                    <Link to={routes.SHOP}>\n                        <div className={s.continueShoppingButton}> \n                            <Button>\n                                {config.continueShoppingButton}\n                            </Button>\n                        </div>\n                    </Link>\n                </Container>\n                </Row>\n                </div>\n            );\n        }\n\n        if (this.props.interfaceStore.appIsMounted)\n            return (\n                <div className={s.panel}>\n                    <CartMenu config={config} />\n                    <Row>\n                        <Column alignItems='stretch'\n                                    className={s.promotionContainer}\n                                    flowDirection='bottom'>\n                            {config.showPromo && \n                            <Container>\n                                <div className={s.promotion}>\n                                    <CartPromotion config={config} />\n                                </div>\n                            </Container>}\n                        </C