n';\nimport mixin from 'Middleware/utils/decorators/mixinComponent';\nimport { observer } from 'mobx-react';\n\nconst\n    ICON_THICKNESS = 5,\n    EMPTY_INDEX = -1;\n\n@withStyles(s)\n@onClickOutside\n@mixin(ScrollToMixin)\n@observer\nclass Select extends Component {\n    static propTypes = {\n        withIcon: pt.bool,\n        onChange: pt.func,\n        onClick: pt.func,\n        title: pt.string,\n        placeholder: pt.string,\n        showLabel: pt.bool,\n        className: pt.string,\n        wrapperClassName: pt.string,\n        wide: pt.bool,\n        frame: pt.bool,\n        required: pt.bool,\n        options: pt.array, // [{name1, selectable1}, {name2, selectable2}, ...]\n        labelPosition: pt.oneOf([\n            'left',\n            'top',\n            'right'\n        ]),\n        defaultValue: pt.string,\n        editable: pt.bool,\n        error: pt.oneOfType([\n            pt.string,\n            pt.bool,\n        ]),\n        tabindex: pt.string,\n        mode: pt.string,\n        storeEntry: pt.object,\n    };\n\n    static defaultProps = {\n        showLabel: false,\n        wide: false,\n        frame: true,\n        required: false,\n        options: [],\n        labelPosition: 'top',\n        defaultValue: '',\n        editable: true,\n        withIcon: true,\n        invalid: false,\n        error: '',\n        tabindex: '',\n        mode: 'key',\n        storeEntry: null,\n    };\n\n    constructor(props) {\n        super(props);\n\n    // the default mode of handling the options. use the key prop.\n        this.mode = 'key';\n        const defaultValue = props.storeEntry ? props.storeEntry.value : props.defaultValue;\n        let index = findIndex(props.options, { name: defaultValue });\n\n        try {\n            if (this.props.mode === 'id') {\n                index = findIndex(props.options, { id: props.defaultValue });\n                this.mode = 'id';\n            }\n        } catch (e) {}\n\n        this.state = {\n            collapsed: true,\n            index,\n        };\n    }\n\n    componentWillReceiveProps(nextProps) {\n        const\n            { index } = this.state,\n            { options } = this.props,\n            { storeEntry } = nextProps;\n        \n        const defaultValue = storeEntry ? storeEntry.value : nextProps.defaultValue;\n\n        if ((index === EMPTY_INDEX) || ((index > EMPTY_INDEX) && (defaultValue !== options[index].name))) {\n            const newIndex = findIndex(options, { name: defaultValue });\n\n            (newIndex !== index) && this.setState({ index: newIndex });\n        }\n    }\n\n    handleFocus = event => {\n        this.handleClick(event);\n    }\n\n    handleClick = event => {\n        this.props.onClick && this.props.onClick(event);\n\n        if (this.props.editable) {\n            this.setState({\n                collapsed: !this.state.collapsed\n            });\n        }\n    };\n\n    moveAdjacentOption(e, index) {\n        e.preventDefault();\n        e.stopPropagation();\n        this.setState({\n            index: index\n        });\n        if (this.props.options[index]) {\n            this.props.onChange && this.props.onChange(this.props.options[index].key || this.props.options[index].name);\n        } else {\n            this.props.onChange && this.props.onChange(EMPTY_INDEX);\n        }\n        let scrollToElem = this.refs['option' + index];\n        if (scrollToElem) {\n            scrollToElem.focus();\n            this.refs.dropdownContent.scrollTop = scrollToElem.offsetTop - scrollToElem.offsetHeight;\n        }\n        return false;\n    }\n\n    handleKeyEnter = e => {\n        switch (e.keyCode) {\n            case 13: //Enter\n                return this.handleChoose(this.state.index);\n            case 38:  // Up\n                if (this.state.index === 0) return false;\n                var index = this.state.index;\n                index--;\n                return this.moveAdjacentOption(e, index);\n            case 40: // Down\n                if (this.state.index === this.props.options.length) return false;\n                var index = this.state.index;\n                index++;\n                return this.moveAdjacentOption(e, index);\n        }\n        return true;\n    }\n\n    handleBlur = (event) => {\n        if(event.relatedTarget) {\n            if (this.props.editable) {\n                this.setState({\n                    collapsed: true\n                });\n            }\n        }\n        return true;\n    }\n\n    handleChoose(index, name) {\n        this.setState({\n            collapsed: true,\n            index: +index\n        });\n\n        if (this.props.options[index]) {\n            this.props.onChange && this.props.onChange(this.props.options[index][this.mode] || this.props.options[index].name);\n            this.props.storeEntry && this.props.storeEntry.form.handleChange(name, this.props.storeEntry.key, this.props.storeEntry.label);\n        } else {\n            this.props.onChange && this.props.onChange(EMPTY_INDEX);\n        }\n    }\n\n    handleClickOutside = event => {\n        !this.state.collapsed && this.setState({\n            collapsed: true\n        });\n    };\n\n    renderLabel() {\n        const { showLabel, required } = this.props;\n\n        const title = this.props.storeEntry ? this.props.storeEntry.label : this.props.title;\n\n        if (!showLabel) {\n            return null;\n        }\n\n        return (\n            <label className={cx(s.label, {\n                [s.hasOffset]: this.props.labelPosition !== 'top'\n            })}\n            >\n                {title}\n                {required && <span>&nbsp;*</span>}\n            </label>\n        );\n    }\n\n    renderSelectField() {\n        const\n            { index, collapsed } = this.state,\n            { frame, alwaysBordered, options, editable, invalid, showLabel, placeholder, minWidth, tabindex, uppercaseTopText } = this.props;\n\n        const title = this.props.storeEntry ? this.props.storeEntry.label : this.props.title;\n        const topText = placeholder || !showLabel && uppercaseTopText ? title.toUpperCase() : title;\n        return (\n            <div\n                ref={'selectContainer'}\n                onClick={this.handleClick}\n                onFocus={this.handleFocus}\n                onBlur={this.handleBlur.bind(this)}\n                onKeyDown={this.handleKeyEnter.bind(this)}\n                style={{ minWidth }}\n                className={cx(s.select, s.selectField, this.props.className, {\n                    [s.frame]: frame,\n                    [s.redFrame]: invalid,\n                    [s.nonEditable]: !editable,\n                    [s.disabled]: !editable,\n                    [s.open]: !collapsed,\n                    [s.alwaysBordered]: alwaysBordered,\n                })}\n                tabIndex={tabindex}\n            >\n            <div className={s.stretchOptions}>\n                {topText}\n                {this.renderOptions()}\n            </div>\n                {!collapsed &&\n                    <div className={cx(s.chosenName, s.dropdownOpen)} onClick={this.handleChoose.bind(this, -1)} >\n                        {topText}\n                    </div>\n                }\n                {collapsed && (options[index]\n                    ? <div className={s.chosenName}>\n                        {options[index].chosenName || options[index].name}\n                    </div>\n                    : <div className={s.placeholder}>\n                        {topText}\n                    </div>)}\n                {editable && (collapsed\n                    ? <IconAngleDown className={cx({\n                        [s.arrow]: alwaysBordered\n                    })} size={ICON_SIZE} thickness={ICON_THICKNESS} />\n                    : <IconAngleUp className={cx(s.openArrow, {\n                        [s.arrow]: alwaysBordered\n                    })} size={ICON_SIZE} thickness={ICON_THICKNESS} />)}\n            </div>\n        );\n    }\n\n    renderOptions() {\n        const\n            { withIcon, placeholder, showLabel, title } = this.props,\n            options = [...this.props.options];\n\n        return options.map((option, index) => (\n                <div\n                    key={index}\n                    ref={'option' + index}\n                    className={cx(s.option, this.props.className, {\n                        [s.disabled]: option.disabled,\n                        [s.selected]: index === this.state.index + 1\n                    })}\n                    onClick={option.selectable ? this.handleChoose.bind(this, index, option.name) : null}\n                >\n                    {withIcon && index === this.state.index && <div className={s.chosen}>\n                        <IconApproved size={ICON_SIZE} />\n                    </div>}\n                    {option.name}\n                </div>\n            )\n        );\n    }\n\n    renderDropdown() {\n        const\n            { collapsed } = this.state,\n            { withIcon, placeholder, showLabel, title } = this.props,\n            options = [...this.props.options];\n\n        return (\n            <div\n                className={cx(s.dropdown, {\n                    [s.collapsed]: collapsed\n                })}\n            >\n                <div ref={'dropdownContent'} className={s.dropdownContent}>\n                    {this.renderOptions()}\n                </div>\n            </div>\n        );\n    }\n\n    componentDidUpdate() {\n        if (this.props.storeEntry) {\n            const { value, type, label } = this.props.storeEntry;\n            const index = findIndex(this.props.options, { name: value });\n            if (this.state.index !== index) this.setState({ index });\n            if (this.state.index === EMPTY_INDEX && type === 'required') {\n                this.props.storeEntry.error = `${label.substring(0, label.length - 1)} is required.`;\n            } else this.props.storeEntry.error = null;\n        }\n    }\n\n    render() {\n        this.props.storeEntry && this.props.storeEntry.value;\n        const error = this.props.storeEntry ? this.props.storeEntry.error : this.props.error;\n        const invalid = this.props.storeEntry ? this.props.storeEntry.error : this.props.invalid;\n\n        return (\n            <div className={cx(s.root, this.props.wrapperClassName, {\n                [s.wide]: this.props.wide,\n                [s.leftLabelPosition]: this.props.labelPosition === 'left',\n                [s.rightLabelPosition]: this.props.labelPosition === 'right'\n            })}\n            >\n                {this.renderLabel()}\n                <div className={s.content}>\n                    {this.renderSelectField()}\n                    {this.renderDropdown()}\n                    <div className={cx(s.message, {\n                        [s.error]: !!invalid\n                    })}\n                    >\n                        {!!error && error}\n                    </div>\n                </div>\n            </div>\n        );\n    }\n}\n\nexport default Select;\n"]}},"_buildHash":"f39b632ee1795df99a847318ea963cc2","hash":"3c03117fee7ad91728d4004b7a69ba84","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/toConsumableArray","range":[117,158],"loc":{"start":{"line":7,"column":26},"end":{"line":7,"column":76}}},{"type":"RequireHeaderDependency","range":[109,116],"loc":{"start":{"line":7,"column":26},"end":{"line":7,"column":76}}},{"type":"RequireHeaderDependency","range":[257,264],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/defineProperty","range":[2