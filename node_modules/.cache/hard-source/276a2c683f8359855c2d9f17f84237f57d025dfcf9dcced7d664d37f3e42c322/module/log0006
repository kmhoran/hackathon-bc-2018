ndering } from 'mobx-react';\nimport { AsyncComponentProvider } from 'react-async-component';\nimport asyncBootstrapper from 'react-async-bootstrapper';\n\nimport Location from 'Middleware/utils/Location';\nimport router from 'Root/router';\nimport Context from 'Root/context';\nimport stores from 'Stores';\n\nimport AppContainer from 'Containers/AppContainer';\nimport { redirect } from 'Root/config';\n\n// Global (context) variables that can be easily accessed from any React component\n// https://facebook.github.io/react/docs/context.html\nconst context = new Context();\n\n// Switch off the native scroll restoration behavior and handle it manually\n// https://developers.google.com/web/updates/2015/09/history-api-scroll-restoration\nconst scrollPositionsHistory = {};\nif (window.history && 'scrollRestoration' in window.history) {\n    window.history.scrollRestoration = 'manual';\n}\n\nlet onRenderComplete = function initialRenderComplete() {\n    const elem = document.getElementById('css');\n    if (elem)\n        elem.parentNode.removeChild(elem);\n    onRenderComplete = function renderComplete(route, location) {\n        /** Update necessary tags in <head> at runtime here, ie:\n          * updateMeta('keywords', route.keywords);\n          * updateCustomMeta('og:url', route.canonicalUrl);\n          * updateCustomMeta('og:image', route.imageUrl);\n          * updateLink('canonical', route.canonicalUrl);\n          * etc.\n          */\n\n        let scrollX = 0;\n        let scrollY = 0;\n\n        const targetHash = location.hash.substr(1);\n        if (targetHash) {\n            const target = document.getElementById(targetHash);\n            if (target) {\n                scrollY = window.pageYOffset + target.getBoundingClientRect().top;\n            }\n        }\n\n        // Restore the scroll position if it was saved into the state\n        // or scroll to the given #hash anchor\n        // or scroll to top of the page\n        window.scrollTo(scrollX, scrollY);\n\n        // Google Analytics tracking. Don't send 'pageview' event after\n        // the initial rendering, as it was already sent\n        // if (window.ga) {\n        //     window.ga('send', 'pageview', createPath(location));\n        // }\n    };\n};\n\nconst container = document.getElementById('app');\nlet appInstance;\nlet currentLocation = Location;\n\n// Re-render the app when window.location changes\nasync function onLocationChange(location, action) {\n    const isInitialRender = stores.interfaceStore.initialRender;\n    // Remember the latest scroll position for the previous location\n    scrollPositionsHistory[currentLocation.key] = {\n        scrollX: window.pageXOffset,\n        scrollY: window.pageYOffset,\n    };\n\n    // Delete stored scroll position for next page if any\n    if (action === 'PUSH') {\n        delete scrollPositionsHistory[location.key];\n    }\n    currentLocation = location;\n\n    try {\n        // Traverses the list of routes in the order they are defined until\n        // it finds the first route that matches provided URL path string\n        // and whose action method returns anything other than `undefined`.\n\n        const params = {\n            context,\n            path: location.pathname,\n            localizedPath: location.pathname,\n            query: queryString.parse(location.search),\n            isServer: false,\n            stores,\n            isInitialRender,\n        };\n        const route = await router.resolve(params);\n        // Prevent multiple page renders during the routing process\n        if (currentLocation.key !== location.key) {\n            return;\n        }\n        const resolvedRoute = route.redirect ? await router.resolve({\n            ...params,\n            path: route.redirect,\n        }) : route;\n\n        if (route.replace) {\n            window.history.replaceState({}, {}, route.replace);\n        }\n        \n        const rehydrateState = window.ASYNC_COMPONENTS_STATE;\n        // Disable hydration in the local envirment due to BrowserSync and other\n        // plugins inserting DOM elements that may differ client side\n        const renderReactApp = isInitialRender ? ReactDOM.hydrate : ReactDOM.render;\n        const appInstance = (\n                <Provider key={'provider'} { ...stores }>\n                    <AppContainer \n                        context={context}\n                        path={location.pathname}\n                        key={'app'}>\n                            {resolvedRoute.component}\n                    </AppContainer>\n                </Provider>\n        );\n\n        const asyncProvider = (\n            <AsyncComponentProvider rehydrateState={rehydrateState}>\n                {appInstance}\n            </AsyncComponentProvider>\n        )\n\n        if (!isInitialRender)\n            renderReactApp(appInstance,\n                container,\n                () => onRenderComplete(route ,location)\n            );\n\n        else  {\n            useStaticRendering(true);\n            asyncBootstrapper(asyncProvider).then(() => {\n                useStaticRendering(false);\n                renderReactApp(\n                    appInstance,\n                    container,\n                    () => onRenderComplete(route ,location)\n                )\n            })\n        }\n\n        if (redirect) {\n            const redirectFilter = await import('Middleware/services/redirect');\n            redirectFilter.ipRedirect();\n        }\n\n        setTimeout(() => stores.interfaceStore.updateRenderState(), 200);\n    }\n    catch (error) {\n        if (__DEV__) {\n            throw error;\n        }\n\n        console.error(error);\n\n        // Do a full page reload if error occurs during client-side navigation\n        if (currentLocation.key === location.key) {\n             window.location.reload();\n        }\n    }\n}\n\n// Handle client-side navigation by using HTML5 History API\n// For more information visit https://github.com/mjackson/history#readme\nLocation.listen(onLocationChange);\n// onLocationChange(currentLocation);\n\nif (!window.location.origin) {\n    window.location.origin = window.location.protocol + \"//\" + window.location.hostname + (window.location.port ? ':' + window.location.port: '');\n}\n\n// Force refresh on Safari backstate to prevent state caching\n// which can cause issues in a single page app\nwindow.onpageshow = (event) => {\n    if (event.persisted) {\n        window.location.reload();\n    }\n};\n\nif ('serviceWorker' in navigator) {\n    window.addEventListener('load', () => {\n        navigator.serviceWorker.register('/service-worker.js');\n    });\n}\n\nif (module.hot) {\n    // Do a full reload on these dependencies\n    module.hot.decline([\n        'client.js',\n        'routes.js',\n        './src/stores/index.js',\n        ...Object.keys(stores).map(store => `./src/stores/${store}/${store}.js`), ]\n    );\n\n    // Else use hot reload\n    module.hot.accept();\n}\n"]}},"_buildHash":"04e7d329ca88325c52d22f3a43025881","hash":"60af292544956808fe4986614001a7f6","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/keys","range":[35,70],"loc":{"start":{"line":3,"column":12},"end":{"line":3,"column":56}}},{"type":"RequireHeaderDependency","range":[27,34],"loc":{"start":{"line":3,"column":12},"end":{"line":3,"column":56}}},{"type":"RequireHeaderDependency","range":[145,152],"loc":{"start":{"line":7,"column":26},"end":{"line":7,"column":76}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/toConsumableArray","range":[153,194],"loc":{"start":{"line":7,"column":26},"end":{"line":7,"column":76}}},{"type":"RequireHeaderDependency","range":[289,296],"loc":{"start":{"