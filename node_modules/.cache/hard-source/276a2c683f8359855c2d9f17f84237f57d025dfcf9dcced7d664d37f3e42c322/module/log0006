', 'contentStore', 'messagingStore')\n@observer\nclass ShareDirect extends Component {\n\n    constructor(props) {\n        super(props);\n\n        this.state = {\n            appIsMounted: false,\n            shareLink: '',\n            copied: false,\n            hasSocial: this.props.socialId ? true : false\n        };\n\n        this.handleSocialClick = this.handleSocialClick.bind(this);\n        this.removeSocialClick = this.removeSocialClick.bind(this);\n\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.content.configSharingPage[0]);\n    }\n\n    derriveShareLink(){\n        const navStore = this.props.navigationStore;\n\n        const pwsUrl = getSafe( () => this.props.userStore.pwsUrl) || '';\n        const deepLink = navStore.unlocalizedPath() || '';\n        const pageQuery = navStore.queryString || '';\n\n        const cleanUri = deepLink.replace(initQueryString(pageQuery), '');\n        const gotoQuery = generateQueryString({goto: [cleanUri]}, false);\n        const cleanPageQuery = omitQueryParam(pageQuery, 'goto');\n\n        const localizedPws = pwsUrl ? navStore.localizeURL(`/${pwsUrl}`) : '';\n        const socialUrl = this.state.hasSocial ? `/social/${this.props.socialId}` : '';\n        const shareLinkQuery = initQueryString(joinQueryStrings(gotoQuery, cleanPageQuery));\n\n        const socialId = this.state.hasSocial ? !!this.props.socialId : true;\n\n        if (localizedPws && shareLinkQuery && socialId) {\n            const pwsShareLink = `${liveUrl}${localizedPws}${socialUrl}${shareLinkQuery}`;\n            this.setState({shareLink: pwsShareLink});\n        } else {\n            const error = this.config.errorShareLinkFailed || 'There was an error.';\n            this.props.messagingStore.addMessage(error, {type: 'error'})\n        }\n\n    }\n\n    componentDidMount() {\n        __BROWSER__ && requestAnimationFrame(() => this.setState({ appIsMounted: true }));\n        this.derriveShareLink();\n    }\n\n    handleSocialClick(event) {\n        const { setBottomPanel } = getSafe(() => this.props.interfaceStore);\n        setBottomPanel(\n            <ShareWithSocial />\n        );\n    }\n\n    removeSocialClick() {\n        this.setState({hasSocial: false});\n    }\n\n    renderSocialLink(s){\n        const {\n            socialLink,\n            removeSocialLink,\n            socialSelectionText\n        } = this.config;\n\n        if (getSafe(() => this.props.userStore.hasOpenSocial) && !this.state.hasSocial) {\n            return (\n                <a className={s.socialLink} onClick={this.handleSocialClick} >{socialLink}</a>);\n        }\n\n        if (this.state.hasSocial) {\n            return(\n                <div className={s.selectedSocialContainer}>\n                    <MarkdownWrapper content={socialSelectionText}  className=\"socialSelectionText\"/>\n                    <div className={s.socialName}>{this.props.socialName}</div>\n                    <div><a className={s.removeSocialLink} onClick={this.removeSocialClick} >{removeSocialLink}</a></div>\n                </div>\n            );\n        }\n    }\n\n    renderCopyButton(s) {\n        const {\n            inputCta,\n            copiedCta\n        } = this.config;\n        if (getSafe(() => this.state.copied)) {\n            return (\n                <div>\n                    <Button className={s.copyButton}>\n                        <IconChecked\n                            color={COLOR_WHITE}\n                            size={SIZE_EXTRA_SMALL}\n                            thickness={THICKNESS_THICK}\n                            className={s.iconChecked}/>\n                        {copiedCta}\n                    </Button>\n                </div>\n            );\n        }\n        return (\n            <div>\n                <Button \n                    className={s.copyButton}\n                    disabled={!this.state.shareLink}\n                >\n                    {inputCta}\n                </Button>\n            </div>\n        );\n    }\n\n    render() {\n        if(!this.state.appIsMounted) return false;\n        const {\n            shareTitle,\n            inputCta,\n            copiedCta,\n            emailSubject,\n            emailBody\n        } = this.config;\n        const pageTitle = document.title;\n\n        const { shareLink } = this.state;\n\n        return (\n            <div className={s.container}>\n                <MarkdownWrapper content={shareTitle} />\n                { this.renderSocialLink(s) }\n                <div className={s.copyContainer}>\n                    <input className={s.copyInput} value={shareLink} readOnly />\n                    <CopyToClipboard\n                        text={shareLink}\n                        onCopy={() => this.setState({copied: true})}\n                    >\n                        { this.renderCopyButton(s) }\n                    </CopyToClipboard>\n                </div>\n                <div className={s.socialIcons}>\n                    <SocialNetworks\n                        productName={pageTitle}\n                        sharePage={true}\n                        sharePageUrl = {shareLink}\n                        shareThis={true}\n                        hidePinterest={true}\n                        emailSubject={emailSubject}\n                        emailBody={emailBody + ' ' + shareLink}\n                    />\n                </div>\n            </div>\n        );\n    }\n}\n\nexport default ShareDirect;\n"]}},"_buildHash":"6c7695214cda65b28b8b06acc2614429","hash":"309146d1a7e4874372f90fc847f1db00","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc"