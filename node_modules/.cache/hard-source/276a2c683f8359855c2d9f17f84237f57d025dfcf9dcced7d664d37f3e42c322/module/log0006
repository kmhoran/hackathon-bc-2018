ixin)\n@observer\nclass OverlayModal extends Component {\n\n    static propTypes = {\n        visible: pt.bool,\n        onClose: pt.func,\n        container: pt.string,\n        headerOffset: pt.number\n    };\n\n    static defaultProps = {\n        visible: false,\n        menuOffset: 50\n    };\n\n    state = {\n        appIsMounted: false,\n        height: 738,\n    }\n\n    constructor(props) {\n        super(props);\n    }\n\n    componentDidMount() {\n        __BROWSER__ && requestAnimationFrame(() => this.setState({ appIsMounted: true }));\n        this.scrollTo();\n        this.resizeListener = throttle(this.updateDimensions, 80);\n        window.addEventListener(\"resize\", this.resizeListener);\n        disableBodyScroll(this.refs.contentRef);\n        __BROWSER__ && this.updateDimensions();\n    }\n\n    componentWillUnmount(){\n        window.removeEventListener(\"resize\", this.resizeListener);\n        enableBodyScroll(this.refs.contentRef);\n        clearAllBodyScrollLocks();\n        if (this.props.interfaceStore.isPhone) document.body.style.position = '';\n    }\n\n    componentDidUpdate() {\n        if (this.props.interfaceStore.isPhone) document.body.style.position = 'fixed';    \n    }\n\n    /*\n    This method sets the height of this element to:\n      - 100% if its mobile\n      - the lesser of the parent container height or the height of the container to the end of the screen\n\n    Since this element locks scrolling on the screen, if the height of the element goes beyond the end of the screen,\n    it is not possible to see/interact with it\n    */\n    updateDimensions = () => {\n        const { menuOffset } = this.props;\n        const windowHeight = window.innerHeight;\n        if (window.innerWidth <= MEDIA_PHONE) return (this.state.height != `${windowHeight-menuOffset}px` && this.setState({height: `${windowHeight-menuOffset}px`}));\n\n        const parentElem = document.getElementById('pdp-info-container');\n        if (!parentElem) return (this.state.height != windowHeight && this.setState({height: windowHeight + 'px'}));\n\n        const {offsetTop, offsetHeight} = parentElem;\n        const desiredHeight = ((offsetTop + offsetHeight) > windowHeight) ? (windowHeight - offsetTop)  : offsetHeight;\n        const emailShareHeight = `${desiredHeight}px`;\n\n        this.state.height !== emailShareHeight && this.setState({height: emailShareHeight});\n    }\n\n    handleClickOutside = event => {\n        if (event.target.classList.contains(s.clickOutContainer)) return;\n        this.props.onClose && this.props.onClose(event);\n    };\n\n    portalWrapper(children) {\n        return this.props.interfaceStore.isPhone ? <Portal visibility={true}>{children}</Portal> : <React.Fragment>{children}</React.Fragment>\n    }\n\n    render() {\n        if(!this.state.appIsMounted) return false;\n\n        return (\n            this.portalWrapper(\n                <TransitionGroup key={s.emailShare}>\n                    <CSSTransition\n                        classNames={ {\n                            enter: s.emailShareEnter,\n                            enterActive: s.emailShareEnterActive,\n                            exit: s.emailShareExit,\n                            exitActive: s.emailShareExitActive,\n                        } }\n                        timeout={{ enter: 400, appear: 0, exit: 400 }}>\n                        <div id=\"overlay-modal-root\" ref='contentRef' className={cx(s.root)} style={{height: this.state.height}}>\n                            <div className={s.close}>\n                                <IconClose\n                                    size={{ width: 16, height: 16 }}\n                                    className={s.close}\n                                    color={'#000'}\n                                    onClick={this.handleClickOutside}\n                                />\n                            </div>\n                            <ClickOutHandler onClickOut={this.handleClickOutside}>\n                                {this.props.children}\n                            </ClickOutHandler>\n                        </div>\n                    </CSSTransition>\n                </TransitionGroup>\n            )\n        )\n    }\n\n}\n\nexport default OverlayModal;\n"]}},"_buildHash":"3c3166358697081c5e9331074f9116a9","hash":"b8338c41f938f7e63c5754e05e652753","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-prototype-of","range":[113,160],"loc":{"start":{"line":7,"column":22},"end":{"line":7,"column":78}}},{"type":"RequireHeaderDependency","range":[105,112],"loc":{"start":{"line":7,"column":22},"end":{"line":7,"column":78}}},{"type":"RequireHeaderDependency","range":[252,259],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/classCallCheck","range":[260,298],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"RequireHeaderDependency","range":[388,395],"loc":{"start":{"line":15,"column":20},"end":{"line":15,"column":64}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/createClass","range":[396,431],"loc":{"start":{"line":15,"column":20},"end":{"line":15,"column":64}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/possibleConstructorReturn","range":[537,586],"loc":{"start":{"line":19,"column":34},"end":{"line":19,"column":92}}},{"type":"RequireHeaderDependency","range":[529,536],"loc":{"start":{"line":19,"column":34},"end":{"line":19,"column":92}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/inherits","range":[703,735],"loc":{"start":{"line":23,"column":17},"end":{"line":23,"column