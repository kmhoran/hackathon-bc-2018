 sessionStore.getKey('fif-selections', { localized: true }) || {};\n        this.step = Object.keys(this.selections).length ? 6 : 0;\n        this.steps = 6;\n        this.config = {};\n        this.videoConfig = {};\n        this.product = {};\n        this.hasReviewed = this.selections || false;\n        this.locale = sessionStore.getKey('fif-locale') || toJS(localeStore.activeLocale.code);\n\n        this.mountReactions();\n    }\n\n    mountReactions() {\n        autorun(() => {\n            productsStore.productsById;\n            this.product = productsStore.productBySKU(this.sku);\n\n            // Reset on locale change\n            if (this.locale !== getSafe(() => contentStore.extensions.flawlessInFive.locale)) {\n                this.selections = sessionStore.getKey('fif-selections', { localized: true }) || {};\n                this.step = 0;\n                this.locale = toJS(localeStore.activeLocale.code);\n            }\n        });\n    }\n\n    @action async init() {\n        await contentStore.fetchContentfulConfig({ \n            ...asyncModels.flawlessInFive,\n            callback: () => this.loadConfig(),\n        });\n        this.imagePreload = this.preloadAllPreloadImages();\n    }\n\n    @action loadConfig() {\n        this.config = contentStore.extensions.flawlessInFive.data.configFlawlessInFive[0];\n        this.videoConfig = find(\n            contentStore.extensions.flawlessInFive.data.widgetVideoSelector,\n            { key: 'Video Selector - FiF Header' }\n        );\n    }\n\n    @computed get bundle() {\n        // Reference observable getter to update on this.selections changes\n        this.selections;\n\n        if (!this.product && !this.product.bundledOptions) return false;\n        productsStore.productsById;\n\n        return this.product.bundledOptions.map((product, key) => {\n            const swatches = product.bundledOptionValues.map((swatch) => {\n                return {\n                    name: swatch.name.includes(' - ') ? swatch.name.split(' - ')[1] : swatch.name,\n                    // Special display rule for Mascara (prevent duplicate \"Black\" swatches)\n                    type: product.storeViewTitle.includes('Mascara') ? swatch.name.split(' - ')[0] : '',\n                    description: swatch.fmfColorDescription,\n                    image: `${processSrc(swatch.swatchGraphic)}&q=60&w=250`,\n                    id: swatch.sku,\n                    internalId: swatch.selectionId,\n                    isRecommended: this.getRecommendation(swatch.sku),\n                    inStock: swatch.isInStock !== \"0\" && swatch.isInStock !== 0 && swatch.isInStock !== false,\n                    sku: swatch.sku,\n                    isSelected: this.isSelected(key, swatch.sku),\n                    mainImage: `${processSrc(swatch.fmfImage)}&q=60&w=700`,\n                    reviewImage: `${processSrc(swatch.fmfImageReview)}&q=60&w=600`,\n                }\n            });\n\n            const swatchesBySKU = keyBy(swatches, 'sku');\n\n            return {\n                productName: product.storeViewTitle,\n                productDescription: this.config.productDescriptions  ? this.config.productDescriptions[key+1] : '',\n                swatches,\n                swatchesBySKU\n            }\n        });\n    }\n\n    @computed get stepInfo() {\n        return this.bundle[this.step];\n    }\n\n    @computed get totalSelections() {\n        return this.selections ? Object.keys(this.selections).length : 0;\n    }\n\n    @action getStepSelection = (step) => {\n        return this.selections[step];\n    }\n\n    @computed get swatchSelection() {\n        const existing = getSafe(() => find(this.stepInfo.swatches, { id: this.selections[this.step] }));\n        \n        return this.selections[this.step] && existing ? existing : find(this.stepInfo.swatches, { inStock: true });\n    }\n\n    @computed get productSelections() {\n        this.selections;\n        \n        function getBundle (bundle) {\n           return find(bundle.swatches, { isSelected: true }) || find(bundle.swatches, { inStock: true });\n        }\n        return this.bundle.map(((bundle, key) => { \n                return { \n                    ...getBundle(bundle), \n                    step: key, \n                    productName: bundle.productName \n                }\n            })\n        );\n    }\n\n    @action isSelected = (step, id) => {\n        if (this.selections[step] == id) \n            return true;\n        return false;\n    }\n\n    SKUbyID(id) {\n        let product = false;\n        this.bundle.forEach(bundle => {\n            if(bundle.swatchesById[id]) product = bundle.swatchesById[id].sku;\n        });\n\n        return product;\n    }\n\n    @action getRecommendation = (productId) => {\n        const recommendations = this.config.recommendationMappings;\n\n        return getSafe(() => recommendations[this.selections[0]].includes(productId));\n    }\n\n    @computed get stepRecommendationTotal() {\n        let recommendations = 0;\n        this.step > 0 && this.bundle[this.step] && this.bundle[this.step].swatches.map((swatch) => {\n            if(this.getRecommendation(swatch.id)) {\n                recommendations++;\n            }\n        });\n        return recommendations;\n    }\n    \n    @action nextStep = (step) => {\n        this.goToStep({ step: this.step + 1, urlChange: true });\n    }\n\n    @action goToStep = ({ step = 0, override = false, urlChange = true } ) => {\n        if (this.totalSelections || override) {\n            urlChange && window.history.pushState({ path: '' }, '', `?step=${step+1}`);\n            this.step = step;\n            if(this.bundle[step]) {\n                this.bundle[step].swatches.map((swatch) => {\n                    if(this.totalSelections && !this.selections[step] && this.getRecommendation(swatch.id)) {\n                        swatch.inStock && this.setStepSelection(swatch.id);\n                    }\n                })\n                if (!this.selections[step]) {\n                    if (this.bundle[step].swatches) {\n                        for(let i = 0; i < this.bundle[step].swatches.length; i++) {\n                            const swatch = this.bundle[step].swatches[i];\n                            if (swatch.inStock) {\n                                this.setStepSelection(swatch.id)\n                                break;\n                            }\n                        }\n                    }\n                }\n            }\n            if (this.step == this.steps) { \n                this.hasReviewed = true;\n                sessionStore.saveKey('fif-selections', this.selections);\n            }\n\n            this.selections = JSON.parse(JSON.stringify(this.selections));\n        }\n    }\n\n    @action clearSelections = () => {\n        this.selections = {};\n        sessionStore.saveKey('fif-selections', this.selections);\n    }\n\n    @action setStepSelection = (id) => {\n        this.selections[this.step] = id;\n        this.selections = JSON.parse(JSON.stringify(this.selections));\n\n        this.hasReviewed && sessionStore.saveKey('fif-selections', this.selections, { localized: true });\n    }\n\n    @action addToCart = () => {\n        const productMap = this.bundle;\n        const selections = this.selections;\n        function processBundled(bundle) {\n            const processed = {};\n            productMap.map((product, key) => {\n                const found = getSafe(() => product.swatchesBySKU[selections[key]]) || false;\n                const defaultValue = find(product.swatches, { inStock: true });\n \n                processed[parseInt(key)+1] = found ? found.internalId.toString() : defaultValue.internalId.toString();\n            });\n            verbose && console.log('Added bundled items to cart: ', processed);\n            return processed;\n        }\n        cartStore.addProduct({ \n            product: this.product, \n            bundled: processBundled(this.selections)\n        });\n    }\n\n    @computed get price() {\n        productsStore.productsById; // observe changes\n\n        const price = this.product.price;\n\n        return localeStore.formatPrice(price);\n    }\n\n    @computed get strikeoutPrice() {\n        productsStore.productsById; // observe changes\n\n        const price = this.config.strikeoutPrice;\n\n        return price ? '(' + price.split('{')[0] + localeStore.formatPrice(parseInt(price.split('{')[1].split('}')[0])) + price.split('}')[1] + ')' : '';\n    }\n\n    preloadAllPreloadImages() {\n        const images = [];\n        this.bundle.forEach((bundle) => {\n            bundle.swatches.forEach((swatch) => {\n                swatch.mainImage && images.push(swatch.mainImage);\n                swatch.image && images.push(swatch.image);\n                swatch.reviewImage && images.push(swatch.reviewImage);\n            })\n        });\n        const reviewImage = getSafe(() => this.config.reviewPageImage.fields.file.url);\n        reviewImage && images.push(reviewImage);\n\n        return images;\n    }\n}\n\nconst flawlessStore = new FlawlessStore();\n\nexport default flawlessStore;\nexport { FlawlessStore };\n"]}},"_buildHash":"84c9bdc4dc4ea6fec97cdf7ce9fef0cd","hash":"b653c31d134157dbba736b16538195f4","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/define-property","range":[148,194],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[140,147],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[280,287],"loc":{"start":{"line":12,"column":17},"end":{"line":12,"column":64