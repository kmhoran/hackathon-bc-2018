s","sourceRoot":"/Users/millie.savalia/react-node-frontend","sourcesContent":["import { observable, computed, action, autorun, toJS } from 'mobx';\nimport { getSafe }  from 'Middleware/utils/object';\nimport { find, first, keyBy } from 'lodash';\nimport { asyncModels, magento, specialProductSKUs, verbose } from 'Root/config';\n\nimport contentStore from 'Stores/contentStore';\nimport productsStore from 'Stores/productsStore';\nimport cartStore from 'Stores/cartStore';\nimport sessionStore from 'Stores/sessionStore';\nimport interfaceStore from 'Stores/interfaceStore';\nimport localeStore from 'Stores/localeStore';\n\nclass BundleStore {\n    @observable config;\n    @observable selections;\n    @observable step;\n\n    constructor(sku, config, bundleConfig) {\n        try {\n            this.sku = sku;\n            this.handle = bundleConfig.handle;\n            this.retrieve();\n\n            const totalItems = bundleConfig.bundledItems.length;\n            this.steps = totalItems;\n            this.config = config;\n            this.bundleConfig = bundleConfig;\n            this.product = {};\n            this.hasReviewed = this.selections || false;\n            this.locale = sessionStore.getKey(`bundle-locale-${this.handle}`) || toJS(localeStore.activeLocale.code);\n\n            this.step = Object.keys(this.completions).length ? Object.keys(this.completions).length : 0;\n            if (this.step > 0) this.goToStep({ step: this.step });\n\n            this.mountReactions();\n            this.imagePreload = this.preloadAllPreloadImages();\n        }\n        catch(e) {\n            console.error(e);\n        }\n    }\n\n    mountReactions() {\n        autorun(() => {\n            productsStore.productsById;\n            this.product = productsStore.productBySKU(this.sku);\n\n            if (this.locale !== getSafe(() => contentStore.extensions.getTheLook.locale)) {\n                this.retrieve();\n                this.step = 0;\n                this.locale = toJS(localeStore.activeLocale.code);\n            }\n        });\n    }\n\n    @computed get bundle() {\n        this.selections;\n\n        if (!this.bundleConfig.bundledItems[0]) return false;\n        productsStore.productsById;\n\n        try {\n            return this.bundleConfig.bundledItems.map((product, key) => {\n                const swatches = product.fields.selectionOptions.map((item) => {\n                    const swatch = item.fields;\n                    const lookup = this.lookupSKUAttribute(swatch.sku);\n                    return {\n                        name: swatch.name || getSafe(() => `${lookup.swatches.swatchlabel}`),\n                        type: swatch.type,\n                        description: swatch.description || null,\n                        image: getSafe(() => `${swatch.smallImage.fields.file.url}?q=60&w=550`) || getSafe(() => `${lookup.images[0].src}&q=60&w=550`),\n                        id: swatch.sku,\n                        internalId: swatch.selectionId || false,\n                        isRecommended: this.getRecommendation(swatch.sku),\n                        inStock: getSafe(() => lookup ? lookup.inStock !== \"0\" && lookup.inStock !== 0 && lookup.inStock !== false : true),\n                        sku: swatch.sku,\n                        isSelected: this.isSelected(key, swatch.sku),\n                        mainImage: getSafe(() => `${swatch[interfaceStore.isPhone && swatch.selectionImageMobile ? 'selectionImageMobile' : 'selectionImage'].fields.file.url}?q=60&w=900`) || getSafe(() => `${lookup.images[0].src}&q=60&w=900`),\n                        reviewImage: getSafe(() => `${swatch.smallImage.fields.file.url}?q=60&w=900`) || getSafe(() => `${lookup.images[0].src}&q=60&w=90`),\n                        color: swatch.swatchHexColor || getSafe(() => `#${lookup.swatches.colorHexCode}`) || false,\n                        step: key,\n                    }\n                });\n\n                const swatchesBySKU = keyBy(swatches, 'sku');\n\n                return {\n                    productName: product.fields.name,\n                    productDescription: product.fields.description,\n                    shadeSelectionLabel: product.fields.shadeSelectionLabel,\n                    defaultSwatch: swatches[0] || null,\n                    swatches,\n                    swatchesBySKU,\n                    swatchColors: product.fields.swatchType == 'Color' && true,\n                    ignore: product.fields.hidden,\n                    optionalBackground: key === 0 && getSafe(() => `${this.bundleConfig.palette.fields.file.url}?q=60&w=900`) || false,\n                    ...product.fields,\n                }\n            });\n        }\n        catch(e) {\n            console.error(e)\n        }\n    }\n\n    @computed get stepInfo() {\n        return this.bundle[this.step];\n    }\n\n    @computed get totalSelections() {\n        return this.selections ? Object.keys(this.selections).length : 0;\n    }\n\n    @action getStepSelection = (step) => {\n        return this.selections[step];\n    }\n\n    @computed get swatchSelection() {\n        const existing = getSafe(() => find(this.stepInfo.swatches, { id: this.selections[this.step] }));\n        \n        return this.selections[this.step] && existing ? existing : find(this.stepInfo.swatches, { inStock: true });\n    }\n\n    @computed get productSelections() {\n        this.selections;\n        \n        try {\n            function getBundle (bundle) {\n                return find(bundle.swatches, { isSelected: true }) || find(bundle.swatches, { inStock: true });\n            }\n            return this.bundle.filter(bundle => !bundle.ignore)\n                    .map((bundle, key) => { \n                    return { \n                        ...getBundle(bundle), \n                        productName: bundle.productName \n                    }\n                })\n        }\n        catch(e) {\n            console.log(e);\n            return false;\n        }\n    }\n\n    @action isSelected = (step, id) => {\n        if (this.selections[step] == id) \n            return true;\n        return false;\n    }\n\n    SKUbyID(id) {\n        let product = false;\n        this.bundle.forEach(bundle => {\n            if (bundle.swatchesById[id]) product = bundle.swatchesById[id].sku;\n        });\n\n        return product;\n    }\n\n    @action getRecommendation = (productId) => {\n        const recommendations = this.bundleConfig.recommendationMappings;\n\n        return getSafe(() => recommendations[this.selections[0]].includes(productId));\n    }\n\n    @computed get stepRecommendationTotal() {\n        let recommendations = 0;\n        this.step > 0 && this.bundle[this.step] && this.bundle[this.step].swatches.map((swatch) => {\n            if (this.getRecommendation(swatch.id)) {\n                recommendations++;\n            }\n        });\n        return recommendations;\n    }\n    \n    @action nextStep = (step) => {\n        this.recordCompletion(this.step + 1);\n        this.goToStep({ step: this.step + 1 });\n    }\n\n    @action goToStep = ({ step = 0, override = false, urlChange = false }) => {\n        if (this.totalSelections || override) {\n            urlChange && window.history.pushState({ path: '' }, '', `?step=${step+1}`);\n            this.step = step;\n            if (this.bundle[step]) {\n                this.bundle[step].swatches.map((swatch) => {\n                    if (this.totalSelections && !this.selections[step] && this.getRecommendation(swatch.id)) {\n                        swatch.inStock && this.setStepSelection(swatch.id);\n                    }\n                })\n                if (!this.selections[step]) {\n                    if (this.bundle[step].swatches) {\n                        for (let i = 0; i < this.bundle[step].swatches.length; i++) {\n                            const swatch = this.bundle[step].swatches[i];\n                            if (swatch.inStock) {\n                                this.setStepSelection(swatch.id)\n                                break;\n                            }\n                        }\n                    }\n                }\n            }\n            if (this.step == this.steps) { \n                this.hasReviewed = true;\n                this.save();\n            }\n\n            this.selections = JSON.parse(JSON.stringify(this.selections));\n        }\n    }\n\n    @action setStepSelection = (id) => {\n        this.selections[this.step] = id;\n        this.selections = JSON.parse(JSON.stringify(this.selections));\n\n        this.hasReviewed && this.save();\n    }\n\n    @action preselectSelections = (selections) => {\n        this.reset();\n        selections.forEach((selection, key) => {\n            if (getSafe(() => this.bundle[key].swatchesBySKU[selection])) {\n                this.selections[key] = selection;\n                this.completions[key] = selection;\n            }\n        });\n    }\n\n    findInternal = (sku) => {\n        let internal = {};\n        this.product.bundledOptions && this.product.bundledOptions.map((bundle, key) => {\n            bundle.bundledOptionValues && bundle.bundledOptionValues.map((swatch) => {\n                if (swatch.sku === sku) {\n                    internal = {\n                        id: swatch.selectionId || false,\n                        optionId: bundle.optionId || false,\n                        index: key,\n                    }\n                }\n            });\n        });\n\n        return internal;\n    }\n\n    @action addToCart = () => {\n        const productMap = this.bundle;\n        const selections = this.selections;\n        const processBundled = (bundle) => {\n            const processed = {};\n            productMap.map((product, key) => {\n                try {\n                    const found = getSafe(() => product.swatchesBySKU[selections[key]]) || false;\n                    const defaultValue = find(product.swatches, { inStock: true });\n                    const internal = found ? this.findInternal(found.sku) : this.findInternal(defaultValue.sku);\n                    if (internal.id && internal.optionId) processed[internal.optionId] = internal.id;\n                }\n                catch(e) {\n                    console.log(e)\n                    return false;\n                }\n            });\n            verbose && console.log('Added bundled items to cart: ', processed);\n            return processed;\n        }\n        cartStore.addProduct({ \n            product: this.product, \n            bundled: processBundled(this.selections)\n        });\n    }\n\n    @action save = () => {\n        sessionStore.saveKey(`bundle-selections-${this.handle}`, this.selections, { localized: true });\n        sessionStore.saveKey(`bundle-completions-${this.handle}`, this.completions, { localized: true });\n    }\n\n    @action retrieve = () => {\n        this.selections = sessionStore.getKey(`bundle-selections-${this.handle}`, { localized: true }) || {};\n        this.completions = sessionStore.getKey(`bundle-completions-${this.handle}`, { localized: true }) || {};\n    }\n\n    @action recordCompletion = (step) => {\n        this.completions[step] = true;\n    }\n\n    @action reset = () => {\n        this.selections = {};\n        this.completions = {};\n        this.step = 0;\n        this.save();\n    }\n\n    @action lookupSKUAttribute = (sku, attribute) => {\n        const base = productsStore.productBySKU(sku);\n        return getSafe(() => productsStore.variantById(base.selectedVariant || base.id));\n    }\n\n    @computed get price() {\n        productsStore.productsById; // observe changes\n\n        const price = this.product.price;\n\n        return localeStore.formatPrice(price);\n    }\n\n    @computed get strikeoutPrice() {\n        productsStore.productsById; // observe changes\n\n        const price = this.config.strikeoutPrice;\n\n        return price ? '(' + price.split('{')[0] + localeStore.formatPrice(parseInt(price.split('{')[1].split('}')[0])) + price.split('}')[1] + ')' : '';\n    }\n\n    preloadAllPreloadImages() {\n        const images = [];\n        this.bundle.forEach((bundle) => {\n            bundle.swatches.forEach((swatch) => {\n                swatch.mainImage && images.push(swatch.mainImage);\n                swatch.image && images.push(swatch.image);\n                swatch.selectionImage && images.push(swatch.selectionImage);\n                swatch.reviewImage && images.push(swatch.reviewImage);\n            });\n            bundle.optionalBackground && images.push(bundle.optionalBackground);\n        });\n\n        return images;\n    }\n}\n\nexport default BundleStore;\n"]}},"_buildHash":"40eec4b90ce6c24a0e3c8c3c26961684","hash":"79cf1cb394af3963379134a4fd3c7609","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/define-property","range":[113,159],"loc":{"start":{"line":7,"column":22},"end":{"line":7,"column":77}}},{"type":"RequireHeaderDependency","range":[105,112],"loc":{"start":{"line":7,"column":22},"end":{"line":7,"column":77}}},{"type":"RequireHeaderDependency","range":[245,252],"loc":{"start":{"line":11,"column":17},"end":{"line":11,"column":64}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/json/stringify","range":[253,291],"loc":{"start":{"line":11,"column":17},"end":{"line":11,"column":64}}},{"type":"RequireHeaderDependency","range":[382,389],"loc":{"start":{"line":15,"column":32},"end":{"line":15,"column":99}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[390,448],"loc":{"start":{"line":15,"column":32},"end":{"line":