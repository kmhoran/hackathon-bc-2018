K,IAAL,CAAb;AACA,gBAAIM,MAAM,EAAV;;AAFmB,uBAGnB,oBAAYN,IAAZ,CAHmB;;AAAA;;AAAA,uBAIP;AAAA,uBAAOA,KAAKpC,GAAL,KAAaoC,KAAKpC,GAAL,EAAU6B,cAAV,CAAyB,OAAzB,CAApB;AAAA,aAJO;;AAAA;;AAAA;AAAA;;AAAA;;AAAA;;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;;AAAA,uBAKN;AAAA,uBAAOa,IAAI1C,GAAJ,IAAWoC,KAAKpC,GAAL,EAAUG,KAA5B;AAAA,aALM;;AAAA;AAAA;;AAAA;AAAA;;AAMnB,mBAAOuC,GAAP;AACH;;;4BAEqB;AAAA;;AAClB,gBAAMN,OAAO,gBAAK,IAAL,CAAb;;AADkB,uBAEX,oBAAYA,IAAZ,CAFW;;AAAA;;AAAA,uBAGN;AAAA,uBAAOA,KAAKpC,GAAL,KAAaoC,KAAKpC,GAAL,EAAU6B,cAAV,CAAyB,OAAzB,CAApB;AAAA,aAHM;;AAAA;;AAAA;AAAA;;AAAA;;AAAA;;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;;AAAA,uBAIP,mBAAO;AACV,oBAAMvD,cAAcmD,aAAaA,UAAUnD,WAAV,CAAsB,OAAK0B,GAAL,EAAUwB,IAAhC,CAAjC;AACA,oBAAIlD,WAAJ,EAAiB;AACb,yBAAK,IAAIqE,IAAI,CAAb,EAAgBA,IAAIrE,YAAY8B,MAAhC,EAAwCuC,GAAxC,EAA6C;AACzC,4BAAIrE,YAAYqE,CAAZ,EAAef,KAAf,CAAqBH,SAArB,EAAgC,CAAC,OAAKzB,GAAL,EAAUE,KAAX,EAAkB,OAAKF,GAAL,EAAUG,KAA5B,CAAhC,MAAwE,IAA5E,EAAkF;AAC9ER,oCAAQkB,KAAR,CAAc,wBAAd,EAAwC,OAAKb,GAAL,EAAUwB,IAAlD;AACA,mCAAO,KAAP;AACH;AACJ;AACJ;AACD,uBAAO,IAAP;AACH,aAfa;;AAAA;;AAAA;AAAA;;AAAA;;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;;AAElB;AAcH;;;;;;;;;;;;;;;AAGL,IAAMC,YAAY,IAAIrD,SAAJ,EAAlB;;kBAEeqD,S;QACNrD,S,GAAAA,S;QAAW0B,I,GAAAA,I","file":"formStore.js","sourceRoot":"/Users/millie.savalia/react-node-frontend","sourcesContent":["import { toJS, observable, computed, action, autorun, values, extendObservable, set } from 'mobx';\nimport { verbose } from 'Root/config';\nimport { getSafe }  from 'Middleware/utils/object';\nimport sessionStore from 'Stores/sessionStore';\nimport contentStore from 'Stores/contentStore';\nimport localeStore from 'Stores/localeStore';\nimport { validEmail, validUsZipcode, validCaZipcode } from 'Middleware/utils/validators';\n\nclass FormStore {\n\n    @observable forms;\n    @observable validations;\n\n    constructor() {\n        this.forms = {};\n        this.validations = {\n            email: [this.email, this.required],\n            zip: [this.zipcode, this.required],\n            address: [this.addressLength, this.required],\n            addressOptional: [this.addressLength],\n            required: [this.required],\n        //TODO: add additional validators for these\n            optional: [],\n            password: [this.required],\n            passwordOptional: [],\n            phone: [this.required],\n            phoneOptional: [],\n            creditCardNumber: [this.required],\n            creditCardNumberOptional: [], // my account hack type.\n            creditCardVerification: [this.required],\n        };\n\n        const storedForms = false;\n        try {\n            const storedForms = JSON.parse(sessionStore.getKey('formStore'));\n        }\n        catch(e) {\n            verbose && console.log(e);\n        }\n        if (storedForms) {\n            Object.keys(storedForms).forEach(name => {\n                this.forms[name] = new Form(name, storedForms[name], true);\n            });\n        }\n    }\n\n    @action create(options) {\n        const name = typeof options === 'string' ? options : options.key;\n        const persist = typeof options === 'object' ? options.persist : false;\n        if (this.forms[name]) return this.forms[name];\n        this.forms[name] = new Form(name, null, persist);\n        return this.forms[name];\n    }\n\n    @action get(name) {\n        return this.forms[name];\n    }\n\n    @action clear() {\n        this.forms = {};\n    }\n\n    @computed get formLabels() {\n        return getSafe(() => contentStore.extensions.forms.data.configForms[0].formLabels);\n    }\n\n    @computed get errorMessages() {\n        return getSafe(() => contentStore.extensions.forms.data.configForms[0].errorMessages);\n    }\n\n// Validators\n    @action required(label, value) {\n        if (label[label.length - 1] === '*') label = label.substring(0, label.length - 1);\n        if (value.length > 0) return true;\n        else return this.errorMessages.required.replace('${label}', label);\n    }\n    @action email(label, value) {\n        return validEmail(value) ? true : this.errorMessages.email;\n    }\n    @action zipcode(label, value) {\n        const isCA = getSafe(() => localeStore.activeLocale.code !== 'en-US');\n        if (isCA) return validCaZipcode(value) ? true : this.errorMessages.zipcodeCA;\n        else return validUsZipcode(value) ? true : this.errorMessages.zipcodeUS;\n    }\n    @action addressLength(label, value) {\n        if (value.length <= 30) return true;\n        else return {\n            error: this.errorMessages.addressLength,\n        };\n    }\n\n\n}\n\nclass Form {\n\n    @observable __persist;\n    @observable name;\n    @observable timeout;\n\n    constructor(name, storedEntries = null, persist = false) {\n        this.name = name;\n        this.__persist = persist;\n        this.timeout = null;\n\n        if (storedEntries) {\n            Object.keys(storedEntries).forEach(key => this.add(storedEntries[key]));\n        }\n    }\n\n    @action handleChange(value, key, title) {\n        let error = false, allowChange = true;\n\n        if (this[key].type) {\n            const validations = formStore && formStore.validations[this[key].type];\n            if (validations) {\n                validations.forEach(validation => {\n                    const result = validation.apply(formStore, [title, value]);\n                    if (result !== true) {\n                        this[key].error = typeof result === 'object' ? result.error : result;\n                        if (typeof result === 'object' && result.hasOwnProperty('allowChange')) allowChange = result.allowChange;\n                        error = true;\n                    }\n                });\n            }\n        }\n        if (!error) this[key].error = null;\n        if (allowChange) this[key].value = value;\n\n        if (__BROWSER__ && this.__persist) {\n            if (this.timeout) {\n                window.clearTimeout(this.timeout);\n                this.timeout = null;\n            }\n\n            this.timeout = setTimeout(() => {\n                const storedForms = JSON.parse(sessionStore.getKey('formStore'));\n                sessionStore.saveKey('formStore', JSON.stringify({\n                    ...storedForms,\n                    [this.name]: this.copy\n                }));\n            }, 1000);\n        }\n    }\n\n    @action forceValidation() {\n        const form = toJS(this);\n        Object.keys(form)\n            .filter(key => form[key] && form[key].hasOwnProperty('value'))\n            .forEach(key => {\n                this.handleChange(form[key].value, key, form[key].label);\n            });\n    }\n\n    @action forceSessionSave() {\n        if (this.timeout) {\n            window.clearTimeout(this.timeout);\n            this.timeout = null;\n        }\n        const storedForms = JSON.parse(sessionStore.getKey('formStore'));\n        sessionStore.saveKey('formStore', JSON.stringify({\n            ...storedForms,\n            [this.name]: this.copy\n        }));\n    }\n\n    @action add(options) {\n        if (!Array.isArray(options)) options = [options];\n        options.forEach(option => {\n            let label = (getSafe(() => formStore.formLabels[option.label])) || option.label || null;\n\n            try {\n                if (formStore.validations[option.type].includes(formStore.required)) label += '*';\n            } catch (e) {}\n\n            if (!this[option.key] \n            || option.replace) {\n                set(this, { \n                    [option.key]: {\n                        key: option.key,\n                        type: option.type || false,\n                        value: option.value || '',\n                        label,\n                        error: option.error || null,\n                        form: this\n                    }\n                });\n            }\n        });\n    }\n\n    @computed get copy() {\n        const form = toJS(this);\n        let obj = {};\n        Object.keys(form)