ionStore from 'Stores/attributionStore';\nimport messagingStore from 'Stores/messagingStore';\nimport userStore from 'Stores/userStore';\nimport navigationStore from 'Stores/navigationStore';\nimport analyticsStore from 'Stores/analyticsStore';\nimport contentStore from 'Stores/contentStore';\nimport Cart from 'Components/Cart';\n\nclass CartStore {\n\n    @observable cartLoaded;\n    @observable cartId;\n    @observable productsById;\n    @observable total;\n    @observable currency;\n    @observable serverRequest;\n    @observable itemLimit;\n\n    constructor(cart = []) {\n        this.cartLoaded = false;\n        this.cartId = this.generateCartId();\n        this.productsById = {};\n        this.total = 0;\n        this.currency = { name: 'USD', format: '$0' },\n        this.serverRequest = false,\n        this.itemLimit = 10;\n        this.newCartItem = false;\n\n        __BROWSER__ && requestAnimationFrame(() => {\n            autorun(() => {\n                userStore.groupId;\n                const peristed = sessionStore.getKey('cart', { localized: true });\n                !isEmpty(peristed) && this.load(peristed);\n            });\n            autorun(() => {\n                localeStore.activeLocale;\n                const peristed = sessionStore.getKey('cart', { localized: true });\n                if (isEmpty(peristed)) this.clear();\n            });\n        });\n    }\n\n    @computed get products() {\n        const total = this.total;\n        return sortByDate(values(JSON.parse(JSON.stringify(this.productsById))));\n    }\n\n    @computed get cart() {\n        const currency = this.currency.name ? this.currency.name : this.currency;\n        return toJS({\n            cartLoaded: this.cartLoaded,\n            cartId: this.cartId || this.generateCartId(),\n            total: this.total,\n            currency\n        });\n    }\n\n    @computed get cartItems() {\n        return Object.keys(this.productsById).map(id => {\n            return {\n                ...productsStore.segmentObject[id],\n                quantity: this.productsById[id].quantity\n            };\n        });\n    }\n\n    @action changeQuantity = ({ id, quantity, promo, disablePromoSearching = false }) => {\n        const productAttributes = this.getLatestProductAttributes(this.productsById[id].id);\n        if (!productAttributes) return false;\n\n        if (this.productsById[id] && this.productsById[id].quantity + quantity <= productAttributes.maxQuantity) {\n            this.productsById[id].quantity += quantity;\n            promoStore.recentlyUpdatedId = id;\n            if (promo) {\n                if (promoStore.isItemPromoEligible(id, 'GWP')\n                || promoStore.isItemPromoEligible(id, 'BOGO')) {\n                    promoStore.changeQuantity(promo, quantity, id);\n                }\n            }\n            quantity === 1 ? analyticsStore.productAdded(this.productsById[id], true) : analyticsStore.productRemoved(this.productsById[id], true);\n        }\n        else {\n            this.productsById[id].quantity = productAttributes.maxQuantity;\n        }\n        this.productsById = JSON.parse(JSON.stringify(this.productsById));\n        this.validateCart(true, { disablePromoSearching });\n    }\n\n    @action create = (data) => {\n        this.cartLoaded = true;\n    }\n\n    @action generateCartId = () => {\n        return parseInt(Math.random()*100000);\n    }\n\n    @action load(data) {\n        if (data) {\n            this.cartLoaded = true;\n            this.cartId = data.cartId || this.generateCartId();\n            this.currency = data.currency || {name: 'USD', format: '$0'};\n            this.productsById = data.productsById || {};\n        }\n        this.validateCart(true, {});\n\n        if (!this.cartLoaded) {\n            this.cartLoaded = true;\n        }\n    }\n\n    @action loadUserCart(cart) {\n        if (cart) {\n            try {\n                const parsed = JSON.parse(cart);\n                Object.keys(parsed).map((key) => {\n                    const cartInstance = parsed[key].cart;\n                    if (cartInstance) {\n                        const existingCart = sessionStore.getKey(`cart-${key}`) || this.productsById;\n                        const merged = mergeCarts(existingCart ? existingCart.productsById : {}, cartInstance ? cartInstance.productsById : {});\n                        if (merged) {\n                            if (key == localeStore.activeLocale.code) {\n                                this.productsById = merged;\n                            }\n                            else {\n                                sessionStore.saveKey(`cart-${key}`, {\n                                    cartLoaded: true,\n                                    cartId: this.generateCartId(),\n                                    currency: this.currency,\n                                    productsById: merged\n                                });\n                            }\n                        }\n                    }\n                });\n                this.validateCart(true, {});\n            }\n            catch(e) {\n                console.info(e);\n            }\n        }\n    }\n\n    @action clear = (context) => {\n        this.cartId = this.generateCartId();\n        this.productsById = {};\n        this.total = 0;\n        this.currency = 'USD';\n\n        promoStore.clear();\n        this.validateCart(true, {});\n        this.load();\n    }\n\n    @action prepareCheckout = () => {\n        if (userStore.isConsultant || userStore.isEmployee) this.checkout();\n        else {\n            const callback = () => {\n                const redirect = navigationStore.localizeURL(routes.CHECKOUT);\n                userStore.clearGuest();\n\n                /**\n                 * Because Safari has a very strict cross-domain cookie policy\n                 * We literally must redirect to Magento\n                 * in order to instantiate the cookie first\n                 * and then have it redirect back\n                 */\n                const isSafari = navigator.userAgent.search(\"Safari\") >= 0 && navigator.userAgent.search(\"Chrome\") < 0;\n\n                if (!userStore.isLogged && isSafari && magento.redirect)\n                    location.replace(`${magento.URL}/session-gateway?referral=${window.location.origin + redirect}`);\n                else\n                    navigationStore.to({\n                        url: redirect,\n                        replace: true,\n                    });\n            };\n            interfaceStore.fadeOutAll(callback);\n        }\n    }\n\n    @action checkout = async (attributionParams = false) => {\n        if (cartStore.count < 1) {\n            navigationStore.to({ url: '/' });\n            messagingStore.addMessage('You cannot checkout if your cart is empty!', { type: 'error' });\n            return false;\n        }\n        const { activeLocale } = localeStore;\n        await interfaceStore.fadeOutAll();\n\n        const validItems = cartStore.getCheckoutItems();\n\n        let attribution = false;\n        if (attributionParams && attributionParams.consultant) {\n            attribution = {\n                consultantId: attributionParams.consultantId ? attributionParams.consultantId : 1,\n                socialId: attributionStore.socialId || 0,\n            }\n        }\n\n        if (verbose && attribution) console.log('Passing in attribution to Magento: ', attribution);\n\n        if (validItems) {\n            if (__BROWSER__ && contentStore.flags('nogento')) {\n                window.sessionStorage.setItem('newCheckout', true);\n                navigationStore.to({ url: routes.CHECKOUT });\n                return;\n            }\n            const gaClientId = __BROWSER__ && window.sessionStorage.getItem('gaClientId');\n\n            addItemsToBackendCart(validItems, activeLocale, attribution, userStore.isGuest, gaClientId)\n            .then((response) => {\n                    if (!response.success)\n                        cartStore.handleCheckoutError();\n            })\n            .catch(error => this.handleCheckoutError(error));\n\n            analyticsStore.checkoutAttribution(attributionParams);\n        }\n    }\n\n    @action completeOrder = (context) => {\n        __BROWSER__ && requestAnimationFrame(() => {\n            analyticsStore.orderCompleted(context, Object.assign(toJS(this), { products: JSON.parse(JSON.stringify(this.cartItems)) }));\n            this.clear();\n        });\n    }\n\n    @action getCheckoutItems = () => {\n        return validateCheckoutItems(\n            promoStore.processCartItemsForCheckout(this.productsById),\n            productsStore.productsById\n        );\n    }\n\n    @action handleCheckoutError(error) {\n        interfaceStore.hideProgress('checkout');\n        messagingStore.addMessage(error || 'Sorry, there was an error. Try again?', { type: 'error' });\n        this.serverRequest = false;\n    }\n\n    @action addProduct({ product, quantity = 1, selectedVariant = null, variant, bundled = [], hideCart = false, callback = null, disablePromoFinding, disablePromoSearching, isPromo = false}) {\n        if (!hideCart) {\n            interfaceStore.setRightPanel(<Cart />);\n            interfaceStore.openRightPanel();\n        }\n\n        // Exception to not add a gwp type product.\n        if (quantity === 0 && callback) {\n            callback('empty-promo');\n            return true;\n        }\n\n        if (!variant) variant = product;\n\n        let data = {\n            product,\n            productId: variant && variant.id,\n            price: variant && variant.price && parseInt(variant.price),\n            quantity,\n            selectedVariant\n        }\n\n        let newProduct = {};\n        let variantObj = {};\n        let oldQuantity = 0;\n        let hitLimit;\n\n        if (data.selectedVariant && product.variants && product.variants[0].swatches)\n            variant = product.variants[getVariantId(data.selectedVariant, product)].productId; // custom variant\n        else\n            variant = product.variants[0].id;\n\n        data.id = variant;\n        data.cartId = data.id;\n        data.parentId = product.id;\n        data.variantId = variant;\n\n        if (variant) {\n            variant = find(product.variants, { productId: variant });\n            variantObj = { variant };\n        }\n        else\n            variantObj = {id: data.productId};\n\n        if (!this.total)\n            this.total = 0;\n\n        if (typeof this.productsById[data.id] != 'undefined' && typeof this.productsById[data.id].quantity != 'undefined') {\n            const tobe = data.quantity + this.productsById[data.id].quantity;\n            const attributes = this.getLatestProductAttributes(data.variantId);\n            const limit = attributes ? attributes.maxQuantity : this.itemLimit;\n            if (tobe < limit) {\n                data.quantity += this.productsById[data.id].quantity;\n            }\n            else {\n                disablePromoFinding = true;\n                data.quantity = limit;\n                hitLimit = true;\n            }\n\n            delete this.productsById[data.id];\n        }\n\n        newProduct[data.id] = {\n            ...variantObj,\n            ...data,\n            dateAdded: Date.now(),\n            product,\n            bundled,\n            isPromo, // magento hack\n        };\n\n        this.productsById = Object.assign(this.productsById, newProduct);\n        this.productsById = JSON.parse(JSON.stringify(this.productsById));\n        if (!hitLimit)\n            this.validateCart(true, { product: newProduct[data.id], operation: 'create', disablePromoFinding, disablePromoSearching });\n\n        // Notify the cart that a new item has been added once\n        // so that it scrolls to top\n        this.newCartItem = true;\n        setTimeout(() => {\n            this.newCartItem = false\n            if (callback) callback();\n        }, 300);\n\n        analyticsStore.productAdded(newProduct[data.id]);\n    }\n\n    @action removeProduct = ({product, promoPack, promoCheck = true, disablePromoFinding = true}) => {\n\n        const deleteProduct = (() => {\n            delete this.productsById[product.id];\n            if (!this.total)\n                this.total = 0;\n            this.productsById = JSON.parse(JSON.stringify(this.productsById));\n            this.validateCart(true, { product, operation: 'delete', disablePromoFinding });\n        });\n\n        if (this.productsById[product.id]) {\n            // Exception for products related to promofied items. This will remove a product\n            // such that is the same ID as another item which is attached to a gwp or autoadd type.\n            if (!promoPack && promoCheck && Object.keys(promoStore.activatedPromos).length > 0) {\n                // TODO: refactor this into checkForProductInActivePromos\n                const parentOfPromo = Object.keys(promoStore.activatedPromos)\n                    .map((key) => promoStore.activatedPromos[key])\n                    .reduce((set, bucket) => set.concat(toJS(bucket)), [])\n                    .filter((activePromo) => activePromo.parents.indexOf(product.id) !== -1)[0];\n                const childOfPromo = promoStore.checkForProductInActivePromos(product.id, 'child');\n\n                if (parentOfPromo) {\n                    promoStore.removeProductFromPromo(product, {\n                        promo: parentOfPromo,\n                        type: 'parent',\n                    });\n                } else if (childOfPromo) {\n\n\n                    if (cartStore.productsById[product.id].quantity === product.quantity) { deleteProduct(); }\n                    else { cartStore.changeQuantity({\n                            id: product.id,\n                            quantity: -product.quantity,\n                    });}\n                } else {\n                    deleteProduct();\n                }\n            }\n            else if (promoPack) {\n                promoStore.removeProductFromPromo(product, promoPack);\n            }\n            else {\n                deleteProduct();\n            }\n\n            analyticsStore.productRemoved(product);\n        }\n    }\n\n    @computed get count() {\n        return totalLineItems(this.productsById, productsStore.productsById);\n    }\n\n    @computed get totalCost() {\n        return calculateTotal(this.lineItemsProcessed, productsStore.productsById);\n    }\n\n    // Returns line items with dynamic pricing applied\n    // e.g. consultant discount\n    @computed get lineItemsProcessed () {\n        userStore.groupId; //subscribe to observable changes\n        const lineItems = this.productsById;\n        const processed = {};\n        Object.keys(lineItems).map((key) => {\n            const item = lineItems[key];\n            const product  = productsStore.productOrVariantById(item.productId);\n            const dynamicPrice = promoStore.applyGroupPricingRules({\n                price: product.price,\n                sku: product.sku,\n            });\n\n            processed[key] = {\n                ...item,\n                dynamicPrice,\n            };\n        });\n        return processed;\n    }\n\n    @computed get hasMemberOnlyItems() {\n        let has = 0;\n        Object.keys(this.productsById).map((key) => {\n            if (memberOnlySKUs.includes(this.productsById[key].product.sku)) has++;\n        })\n        return has >= 1 ? true : false;\n    }\n\n    @action getLatestProductAttributes(id) {\n        return getSafe(() => productsStore.variantById(id));\n    }\n\n    @action getItemMaxQuantity(id) {\n        const maxQuantity = getSafe(() => this.getLatestProductAttributes(id).maxQuantity);\n        const quantityInCart = this.productsById[id] ? this.productsById[id].quantity : 0;\n\n        return maxQuantity - quantityInCart;\n    }\n\n    @action toggleNewCheckout = () => {\n        this.newCheckout = !this.newCheckout;\n    }\n\n    validateCart = (save = true, { product = null, operation = 'save', disablePromoFinding = false, disablePromoSearching = false }) => {\n        // Fix for autorun init when not mounted.\n        if (promoStore && sessionStore) {\n\n            // Client-side: check for special rules for product\n            if (!disablePromoFinding && product)\n                promoStore.checkItem(product, operation, save);\n            // Server: check entire cart in promo engine\n            if (!disablePromoSearching && this.count)\n                promoStore.checkCart(this.productsById);\n\n            // Defer total processing till after promo determination\n            this.total = calculateTotal(this.lineItemsProcessed, productsStore.productsById);\n\n            save && sessionStore.saveCart(this);\n\n        }\n    }\n}\n\nconst cartStore = new CartStore();\n\nexport default cartStore;\nexport { CartStore };\n"]}},"_buildHash":"b7ae497b13f6ef11d01635a1faab6d9b","hash":"0fafa94f3511252ac56b421978764b2a","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/define-property","range":[144,190],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[136,143],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[278,285],"loc":{"start":{"line":12,"column":19},"end":{"line":12,"column":55}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[286,313],"loc":{"start":{"line":12,"column":19},"end":{"line":12,"column":55}}},{"type":"CommonJsRequireDepe