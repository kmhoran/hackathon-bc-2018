lags, serverRendering } from 'Root/config';\nimport { getSafe }  from 'Middleware/utils/object';\nimport { rehydrateState } from 'Middleware/utils/hydration';\n\nimport localeStore from 'Stores/localeStore';\nimport productsStore from 'Stores/productsStore';\nimport categoryStore from 'Stores/categoryStore';\nimport interfaceStore from 'Stores/interfaceStore';\nimport navigationStore from 'Stores/navigationStore';\nimport promoStore from 'Stores/promoStore';\nimport userStore from 'Stores/userStore';\nimport attributionStore from 'Stores/attributionStore';\n\nclass ContentStore {\n\n    @observable content;\n    @observable extensions;\n    @observable utilities;\n\n    constructor() {\n        const serverState = rehydrateState('contentStore');\n\n        this.content = serverState.content || {};\n        this.extensions = serverState.extensions || {};\n        this.updateInProgress = 1;\n        this.fetch = [];\n        this.utilities = {};\n        this.subscription;\n        if (developerTools && __BROWSER__) this.initTestUtilities();\n    }\n\n    @action get = async ({ locale = localeStore.activeLocale, page = false, showProgress = true, loadCatalog = true, firstLoad = false, reload = false }) => {\n        if (firstLoad) {\n            this.content = {};\n            this.extensions = {};\n            this.fetch = [];\n        }\n\n        /* Create an immutable copy of the locale  internally */\n        const newLocale = JSON.parse(JSON.stringify(locale));\n\n        showProgress && interfaceStore.showProgress(`content-get-${newLocale.code}`);\n\n        const path = unlocalizeURL(\n            __BROWSER__ ?\n                window.location.pathname : newLocale.path\n        );\n\n        /* Refresh the URL and page to reflect the translated version */\n        updateLocaleURL(newLocale.code);\n        if (typeof this.content.page != \"undefined\" && !isEmpty(this.content.page)) {\n            for(let pageName in this.content.page) {\n                if (pageName == path || (\"/\" + pageName) == path) {\n                    page = path;\n                }\n            }\n            // TODO: Run pages that are not found through the router again\n            // rather than defaulting\n            if (!page) page = '/';\n        }\n\n        /* Retrieve core models and page index from Contentful */\n        const fetch = [\n                getContent({\n                    locale: newLocale.code,\n                    type: 'coreComponents',\n                    drafts: this.flags('contentDrafts'),\n                    firstLoad,\n                }),\n                getContent({\n                    locale: newLocale.code,\n                    type: 'pageIndex',\n                    drafts: this.flags('contentDrafts'),\n                    firstLoad,\n                }),\n            ];\n\n        /* Retrieve data for current page (if applicable) */\n        if (page) {\n            fetch.push(getContent({\n                locale: newLocale.code,\n                type: 'singlePage',\n                page,\n                drafts: this.flags('contentDrafts'),\n                firstLoad,\n            }));\n        }\n        else fetch.push(null);\n\n        /* Retrieve product catalog */\n        if (loadCatalog) {\n            fetch.push(getCatalog({\n                locale: newLocale,\n                useCache: this.flags('catalogCaching'),\n                firstLoad,\n                drafts: this.flags('contentDrafts'),\n            }));\n        }\n        else fetch.push(null);\n\n        /* Retrieve extension data e.g. containers with CMS dependences */\n        if (Object.keys(this.extensions).length) {\n            Object.keys(this.extensions).map((extension) => {\n                const params = this.extensions[extension].params;\n                fetch.push(this.getOnce({ ...params, firstLoad }));\n            });\n        }\n\n        const response = await Promise.all(fetch);\n        const [ coreContent, pageIndex, currentPage, catalog ] =  response;\n\n        if (coreContent && coreContent && typeof coreContent.category !== \"undefined\") {\n            if (localeStore.activeLocale.code !== newLocale.code) {\n                showProgress && interfaceStore.hideProgress(`content-get-${newLocale.code}`);\n                return false;\n            }\n\n            localeStore.modifyActive(newLocale);\n\n            runInAction(\"updating content\", () => {\n\n                categoryStore.load({\n                    category: coreContent.category,\n                    navigation: getSafe(() => coreContent.header[0].categoryNavigation.fields.links)\n                });\n\n                if (loadCatalog) { \n                    productsStore.load(catalog.data.Products);\n                    categoryStore.appendFilters({\n                        subcategories: catalog.data.Categories,\n                        filters: catalog.data.Filters,\n                    });\n\n                    if (__BROWSER__ && !this.flags('catalogCaching') && this.subscription !== newLocale.code) {\n                        subscribeToCatalog(() => this.get({\n                            page: false,\n                            showProgress:  false,\n                            loadCatalog: true\n                        }), newLocale);\n                        this.subscription = newLocale.code;\n                    }\n                }\n                if (response.length > 3) {\n                    const extensions = response.slice(4);\n                    extensions.map(extension => {\n                        if (getSafe(() => extension.params.route) &&\n                        containsRoute(extension.params.route, navigationStore.unlocalizedBasePath())) {\n                            this.extensions[extension.params.id].data = cloneDeep(extension);\n                            this.extensions[extension.params.id].locale = newLocale.code;\n                            extension.params.refreshCallback();\n                        }\n                    });\n                }\n\n                function containsRoute(routes, url) {\n                    if (routes.constructor === Array)\n                        return routes.filter(route => url.includes(route)).length || routes.includes(url);\n                    return url.includes(routes) || routes.includes(url);\n                }\n\n                categoryStore.appendSortTypes({\n                    sortTypes,\n                    names: response[0].shop[0],\n                });\n\n                let pages = keyBy(pageIndex.page, 'url');\n\n                if (page && currentPage) {\n                    pages[page] = {\n                        ...cloneDeep(currentPage),\n                        activeLocale: newLocale,\n                    }\n                }\n\n                this.content = {\n                    ...coreContent,\n                    page: { ...this.content.pages, ...pages},\n                };\n                this.extensions = { ...this.extensions };\n\n                if (reload) navigationStore.to({ url: path + (location.search || '') });\n\n                showProgress && interfaceStore.hideProgress(`content-get-${newLocale.code}`);\n            })\n        }\n        else {\n            console.error('App failed to update content. Check your API request.');\n            showProgress && interfaceStore.hideProgress('content-get');\n            return false;\n        }\n    }\n    \n    @action getPage = async ({ locale, page, firstLoad }) => {\n        if (this.content.page[page] && this.content.page[page].activeLocale && localeStore.activeLocale.code === this.content.page[page].activeLocale && !this.contentDrafts) return true;\n\n        const pageContent = await getContent({\n            locale,\n            type: 'singlePage',\n            page,\n            drafts: this.flags('contentDrafts'),\n            firstLoad,\n        });\n\n        runInAction(\"updating page\", asyn