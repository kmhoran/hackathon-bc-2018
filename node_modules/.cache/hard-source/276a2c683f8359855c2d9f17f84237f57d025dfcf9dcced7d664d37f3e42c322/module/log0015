les from 'isomorphic-style-loader/lib/withStyles';\nimport { observer, inject } from 'mobx-react';\nimport { autorun, computed, toJS } from 'mobx';\n\nimport Sticky from 'react-stickynode';\nimport { getThisVariantImage, getVariantKey, preloadProductImages } from 'Middleware/services/products';\nimport { isEmpty, throttle } from 'lodash';\nimport { yotpoReviews } from 'Root/config';\nimport OnVisible, { setDefaultProps } from 'Components/Extensions/OnVisible';\nimport ProductTags from 'Components/Product/ProductTags';\nimport ProductBasics from 'Components/Product/ProductBasics';\nimport ProductImageViewer from 'Components/Product/ProductImageViewer';\nimport { getProductUrl } from 'Middleware/utils/url';\nimport { siteName } from 'Root/config';\nimport ImagePrefetch from 'Components/Extensions/ImagePrefetch';\nimport { getSafe } from 'Middleware/utils/object';\nimport LazyLoad from 'Common/LazyLoad';\n\nimport ProductInfo from './ProductInfo';\nimport InstagramGallery from './InstagramGallery';\nimport Directions from './Directions';\nimport IngredientsContainer from './IngredientsContainer';\nimport CrossSell from './CrossSell';\nimport Reviews from './Reviews';\nimport MiniWidgets from './MiniWidgets';\nimport Video from './Video';\nimport EmailPopupContainer from '../EmailPopupContainer';\nimport PreOrderPopup from './PreOrderPopup';\n\nimport Container from 'Common/Container';\nimport Image from 'Common/Image';\nimport Column from 'Common/Column';\nimport Link from 'Common/Link';\nimport { routes } from 'Root/config';\nimport Button from 'Common/Button';\nimport NavigationBreadcrumbs from 'Common/NavigationBreadcrumbs';\n\nimport { generateWidget } from 'Components/Widgets';\n\n@withStyles(s)\n@inject(\n    'productsStore',\n    'contentStore',\n    'localeStore',\n    'cartStore',\n    'userStore',\n    'promoStore',\n    'navigationStore',\n    'interfaceStore',\n    'analyticsStore'\n)\n@observer class ProductContainer extends Component {\n    static pageName = 'product';\n\n    static contextTypes = {\n        setTitle: pt.func.isRequired,\n        onSetMeta: pt.func.isRequired\n    };\n\n    static propTypes = {\n        slug: pt.string,\n        selectedVariant: pt.oneOfType([\n            pt.bool,\n            pt.string\n        ])\n    };\n\n    static defaultProps = {\n        selectedVariant: \"\",\n        selectedVariantName: false,\n    };\n\n    state = {\n        activeVariant: 0,\n        width: 1280,\n        selectedVariant: false,\n        selectedVariantId: false,\n        selectedVariantName: false,\n        form: {}\n    }\n\n    componentWillMount() {\n        const product = this.props.productsStore.productsBySlug[this.props.slug];\n\n        if (typeof product != \"undefined\") {\n            this.context.setTitle(product.title);\n            this.context.onSetMeta('description', `${product.title} ${product.sku}`);\n            this.context.onSetMeta('og:url', getProductUrl(product.handle), 'property');\n            this.context.onSetMeta('og:title', product.title, 'property');\n            this.context.onSetMeta('og:description', product.metaDescription, 'property');\n            this.context.onSetMeta('og:image', getSafe(() => product.images[0].src), 'property');\n            this.context.onSetMeta('og:image:secure_url', getSafe(() => product.images[0].src), 'property');\n            this.context.onSetMeta('og:type', 'product', 'property');\n            this.context.onSetMeta('og:site_name', siteName, 'property');\n\n            this.context.onSetMeta('twitter:title', product.title, 'name');\n            this.context.onSetMeta('twitter:description', product.metaDescription, 'name');\n            this.context.onSetMeta('twitter:image', getSafe(() => product.images[0].src), 'name');\n            this.context.onSetMeta('twitter:card', 'summary_large_image', 'name');\n            this.context.onSetMeta('twitter:site', '@beauycounterhq', 'name');\n        }\n    }\n\n    @computed get memberPromotion() {\n        return getSafe(() => this.props.contentStore.extensions.memberPromotion.data.configMemberPromotion[0]);\n    }\n\n    @computed get pageConfig() {\n        return getSafe(() => this.props.contentStore.content.productDetail[0]);\n    }\n\n    get breadcrumbLinks() {\n        const { productsStore, slug } = this.props;\n        const product = productsStore.productsBySlug[slug];\n        return([\n            productsStore.getPrimaryCategory(product),\n            productsStore.getSecondaryCategory(product),\n        ].map(item => toJS(item)))\n    }\n\n    componentDidMount() {\n        this.resizeListener = throttle(this.updateDimensions, 80);\n        window.addEventListener(\"resize\", this.resizeListener);\n\n        __BROWSER__ && this.updateDimensions();\n\n        // Re-render if a variant is queried (pre-rendered pages are set to default)\n        requestAnimationFrame(() => {\n            if (this.props.selectedVariant) {\n                this.setState({selectedVariantId: this.props.selectedVariant});\n            }\n            if (window.location.hash.length > 1) {\n                const selector = `.${window.location.hash.substr(1)}`;\n                const main =  document.querySelector(selector);\n                if (typeof main != \"undefined\" && main)\n                    window.scrollTo(0, main.getBoundingClientRect().top + window.pageYOffset);\n                else\n                    return false;\n            }\n            this.props.analyticsStore.productViewed(this.props.selectedVariant || getSafe(() => this.props.productsStore.productsBySlug[this.props.slug].id));\n        });\n    }\n\n    componentWillReceiveProps(nextProps) {\n        if (this.props.slug !== nextProps.slug || this.props.selectedVariant !== nextProps.selectedVariant) {\n            const product = this.props.productsStore.productsBySlug[this.props.slug];\n            if(typeof product != \"undefined\" && __BROWSER__) {\n                this.setState({\n                    selectedVariant: false,\n                    selectedVariantId: false,\n                    selectedVariantName: false\n                });\n                this.context.setTitle(product.title);\n            }\n        }\n\n        this.localeSwitch = autorun(() => {\n            this.props.localeStore.activeLocale;\n            if (!nextProps.productsStore.normalizedBySKU[nextProps.selectedVariant])\n                __BROWSER__ && requestAnimationFrame(() => {\n                    this.setState({\n                        selectedVariant: false,\n                        selectedVariantId: false,\n                        selectedVariantName: false\n                    });\n                });\n        });\n    }\n\n    componentWillUnmount() {\n        window.removeEventListener(\"resize\", this.resizeListener);\n    }\n\n    updateDimensions = () => {\n        const windowWidth = window.innerWidth;\n        windowWidth != this.state.width && this.setState({ width: windowWidth});\n    }\n\n    selectedVariant = (variant) => {\n        this.state.variant != variant.id && this.setState({\n            selectedVariant: variant.name,\n            selectedVariantId: variant.id,\n            selectedVariantName: variant.fullName,\n            activeVariant: variant.id\n        });\n        this.props.analyticsStore.productViewed(variant.id);\n    }\n\n    renderProductInfo(selectedProduct) {\n        const { contentStore, promoStore, productsStore } = this.props;\n        const config = this.pageConfig;\n        const memberPromotion = this.memberPromotion;\n\n        return (\n            <ProductInfo\n                product={productsStore.productsBySlug[this.props.slug]}\n                variant={selectedProduct}\n                viewportWidth={this.state.width}\n                config={config}\n                memberPromotion={memberPromotion}\n                quantityDisplay={!promoStore.isItemPromoEligible(selectedProduct.id, 'GWP')}\n                defaultVariant={this.state.selectedVariant}\n                selectedVariant={this.selectedVariant} />\n        );\n    }\n\n    render() {\n        const\n            product = this.props.productsStore.productsBySlug[this.props.slug];\n        const config = this.pageConfig;\n\n        if(typeof product != \"undefined\" && config) {\n            const { activeLocale } = this.props.localeStore;\n\n            let selectedVariant;\n\n            if (!isEmpty(product)) {\n                this.context.setTitle(product.title);\n                this.context.onSetMeta('description', `${product.title} ${product.sku}`);\n            }\n\n            if(!this.props.selectedVariant && typeof product.variants[0].swatches != \"undefined\") {\n                selectedVariant = product.variants[0].id;\n            }\n            else\n                selectedVariant = this.props.selectedVariant;\n\n            if(!this.state.selectedVariant)\n                this.state.selectedVariant = selectedVariant;\n\n            if (!product)\n                return null;\n\n            const images = getThisVariantImage(product, this.state.selectedVariant);\n            const selectedProduct = getVariantKey(this.state.selectedVariant, product);\n            const disableScroll = (this.state.width < 1000 || (images && images.length <= 1 )) ? true : false;\n            const preloadImages = preloadProductImages(product, this.props.interfaceStore.viewportWidth);\n            const hasIngredients = !!getSafe(() => selectedProduct.ingredients || product.ingredients || product.bundleIds.length > 0);\n            const hasKeyIngredients = !!getSafe(() => selectedProduct.ingredients.keyIngredients || product.ingredients.keyIngredients);\n            const ugcCarouselConf = {...getSafe(() => config.ugcCarouselWidget.fields), tag: product.sku };\n\n            return (\n\n                <div className={s.root} key={'product-page-' + product.id}>\n\n                <PreOrderPopup\n                    config={this.memberPromotion}\n                    product={this.props.productsStore.productsBySlug[this.props.slug]}\n                />\n\n                <Container\n                    hasLeftPadding={false}\n                    hasRightPadding={false}>\n                    <div className={s.hideMobile}>\n                        <NavigationBreadcrumbs\n                            className={s.navigationBreadcrumbs}\n                            title={product.title}\n                            links={this.breadcrumbLinks}\n                        />\n                    </div>\n\n                        <div id=\"pdp-sticky-container\" className={s.productHeader}>\n                            <div className={s.leftColumn}>\n                                <div className={s.infoContainerMobile}>\n                                    <ProductBasics\n                                        variant={selectedProduct}\n                                        parent={product} />\n                                </div>\n                                <ProductImageViewer\n                                    className={s.viewer}\n                                    centered\n                                    large\n                                    overlay={<ProductTags product={product} />}\n                                    images={images}\n                                    product={product.title}\n                                    activeVariant={this.state.selectedVariant} />\n                                <ImagePrefetch images={preloadImages} />\n                            </div>\n                            <div>\n                            {disableScroll ?\n                            <div id=\"pdp-info-container\" className={s.info}>\n                                {this.renderProductInfo(selectedProduct)}\n                            </div> :\n                            <Sticky top={40} bottomBoundary='#pdp-sticky-container'>\n                                <div id={'#sticky-pdp-info'} className={s.info}>\n                                    {this.renderProductInfo(selectedProduct)}\n                                </div>\n                            </Sticky>}\n                            </div>\n                        </div>\n                </Container>\n\n                {!product.preOrder && !selectedProduct.preOrder && <EmailPopupContainer viewportWidth={this.state.width} />}\n\n                {(selectedProduct.howToUse || product.howToUse) &&\n                (!hasIngredients || hasKeyIngredients) &&\n                    <OnVisible\n                        className={cx(s.slideIn, s.hidePhone)}\n                        visibleClassName={s.slideInVisible}\n                        percent={16} >\n                        <Directions\n                            config={config}\n                            directions={selectedProduct.howToUse || product.howToUse}\n                            hasIngredients={hasIngredients}\n                            hasKeyIngredients={hasKeyIngredients}\n                        />\n                    </OnVisible>\n                }\n                {hasIngredients &&\n                    <OnVisible\n                        className={s.slideIn}\n                        visibleClassName={s.slideInVisible}\n                        percent={30} >\n                        <IngredientsContainer\n                            config={config}\n                            product={product}\n                            selectedVariant={selectedProduct}\n                            hasIngredients={hasIngredients}\n                            hasKeyIngredients={hasKeyIngredients}\n                            directions={(selectedProduct.howToUse || product.howToUse) ? (selectedProduct.howToUse || product.howToUse) : false}\n                        />\n                    </OnVisible>\n                }\n                <LazyLoad>\n                    <OnVisible\n                        className={s.slideIn}\n                        visibleClassName={s.slideInVisible}\n                        percent={16} >\n                        { generateWidget('Instagram', ugcCarouselConf) }\n                    </OnVisible>\n                </LazyLoad>\n                <Video id={selectedProduct.video.videoLink || product.video.videoLink} />\n                <OnVisible\n                    className={s.slideIn}\n                    visibleClassName={s.slideInVisible}\n                    percent={40} >\n                    <MiniWidgets product={product} />\n                </OnVisible>\n                {yotpoReviews[activeLocale.name] && !yotpoReviews[activeLocale.name].hide &&\n                    <OnVisible\n                        className={s.slideIn}\n                        visibleClassName={s.slideInVisible}\n                        percent={10} >\n                        <Reviews\n                            key={'reviews-' + product.id}\n                            product={product} />\n                    </OnVisible>\n                }\n                {product.upSellIds && product.upSellIds.length &&\n                <LazyLoad>\n                    <OnVisible\n                        className={s.slideIn}\n                        visibleClassName={s.slideInVisible}\n                        percent={20} >\n                        <CrossSell\n                            products={product.upSellIds}\n                            heading={config && config.crossSellHeading}\n                            viewportWidth={this.state.width} />\n                    </OnVisible>\n                </LazyLoad>}\n                </div>\n            );\n        }\n        else {\n            return <div className={s.notFound}>\n                    {config.notFoundMessage}\n                </div>;\n        }\n    }\n}\n\nexport default ProductContainer;\n"]}},"_buildHash":"44c652652a1769a3205a32dacc998ccb","hash":"827d9ad53d45afac9a04127fb2bb87a1","_lastSuccess