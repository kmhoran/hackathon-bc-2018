      }\n            }\n\n            const [ CheckoutInterstitial, CheckoutStore ] = await Promise.all([\n                loadContainer('CheckoutInterstitialContainer'),\n                loadContainer('CheckoutStore'),\n                stores.contentStore.fetchContentfulConfig(asyncModels.checkoutInterstitial),\n            ]);\n\n            const checkoutStore = new CheckoutStore;\n            if (stores.userStore.isConsultant) await checkoutStore.checkout();\n\n            return {\n                chunks: ['CheckoutInterstitialContainer'],\n                component: (\n                    <Provider checkoutStore={checkoutStore}>\n                        <CheckoutInterstitial />\n                    </Provider>\n                ),\n            }\n        }\n    },\n\n    {\n        path: routes.EVENTS,\n        async action({ stores }) {\n            const [ EventsPage ] = await Promise.all([\n                loadContainer('EventsPage'),\n                stores.contentStore.fetchContentfulConfig(asyncModels.events),\n            ]);\n\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: false,\n                hideCategoryNavigation: false,\n                disableHover: true\n            });\n            return {\n                chunks: ['EventsPage'],\n                component: <EventsPage />,\n            }\n        }\n    },\n\n    {\n        path: [\n            `${routes.OUR_STORY}`,\n            `${routes.ABOUT_BEAUTYCOUNTER}`,\n        ],\n        async action({ stores }) {\n            stores.interfaceStore.showProgress(\"page-load\");\n\n            const [ OurStoryPage ] = await Promise.all([\n                loadContainer('OurStoryPage'),\n                stores.contentStore.fetchContentfulConfig(asyncModels.ourStory),\n            ]);\n\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disable