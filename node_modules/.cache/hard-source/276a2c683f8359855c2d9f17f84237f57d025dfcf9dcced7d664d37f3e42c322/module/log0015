    static propTypes = {\n        html: pt.string.isRequired,\n        url: pt.string.isRequired,\n    };\n\n    constructor(props) {\n        super(props);\n\n        // this.handleChange = this.handleChange.bind(this);\n        this.handleSubmit = this.handleSubmit.bind(this);\n\n        // stores the url for the current form action.\n        this.formActions = [];\n        this.fields = [];\n        this.key = ''; // the current form key.\n    \n    // references for the forms created in form store\n        this.forms = [];\n\n    //stores a copy of the fields that were submitted for rehydration.\n        this.submittedFields = [];\n\n    //original html from request and before submit.\n        this.cleanHTML = \"\";\n\n        // ref to the modified html\n        this.filledHTML = null;\n\n        this.keyEnum = 0;\n\n        this.buttonCleanStore = [];\n        this.checkBoxCleanStore = [];\n        this.inputCleanStore = [];\n\n    // stop repeat refreshes by tracking the last called url.\n        this.lastCalledPath = null;\n\n        this.keyForPage = {\n            '/v1/newsletter/manage': newsletter,\n            '/v1/customer/account/edit': information,\n            '/v1/sales/order/history': orderHistory,\n            '/v1/sales/order/view': orderHistory,\n            '/v1/customer/address/new': addressbook,\n            '/v1/customer/address/edit': addressbook,\n            '/v1/customer/address': addressbook,\n            '/v1/customer/paymentinfo': managePayment,\n            '/v1/storecredit/info': bandOfBeauty,\n            '/v1/giftcard/customer/': bandOfBeauty,\n            //... add pages here.\n        };\n\n    // tracks changes in media size to patch through component update.\n        this.isMobileTracker = null;\n\n        this.renderTrackingLink = throttle(this.renderTrackingLink, 2000);\n\n        this.state = {\n            activeStyles: (() => {\n                try {\n                    return this.keyForPage[\n                        Object.keys(this.keyForPage).filter((key) => {\n                            return props.legacyAppStore.namespacedURL(\n                                props.navigationStore.path, 'react', 'POST', true)\n                                .indexOf(props.legacyAppStore.namespacedURL(\n                                    key, 'react', 'POST', true)) > -1\n                        }\n                    )[0]];\n                } catch (e) {\n                    return '';\n                }\n            })(),\n            fields: {},\n            html: this.props.html,\n            //view specific\n            //account information\n            passwordChangeVisibility: false,\n            //payment edit page\n            cardDataChangeVisibility: false,\n            message: '',\n            contentMessage: '',\n        }\n    }\n\n    static contextTypes = {\n        setTitle: pt.func.isRequired,\n    };\n\n    componentDidUpdate() {\n        this.renderTrackingLink();\n    }\n\n    componentDidMount() {\n        const { legacyAppStore, navigationStore, interfaceStore, userStore, myAccountStore } = this.props;\n\n        interfaceStore.hideProgress(\"page-load\");\n\n        this.renderTrackingLink();\n\n        this.autoRunDisposer = autorun(() => {\n        //observering hints\n            legacyAppStore.responseMessage;\n            interfaceStore.isMobile;\n            // myAccountStore.postUrl;\n\n            if (!userStore.isLogged)\n                navigationStore.to({ url: routes.HOME });\n\n        // redirect to post page.\n        // this is commented out because if you save a page. then go to dashboard,\n        // then come back again, the forwarding is still set causing page refresh.\n            // if (myAccountStore.postUrl.length > 0 && !this.isForwarded) {\n            //     const url = legacyAppStore.namespacedURL(myAccountStore.postUrl, 'magento');\n            //     navigationStore.to({ url });\n            //     this.isForwarded = true;\n            // }\n\n            this.setState({\n                ...(() => {\n                    if (url === this.props.url)\n                    return {\n                        html: myAccountStore.previousHTML,\n                    };\n                }),\n                contentMessage: legacyAppStore && legacyAppStore.responseMessage,\n                activeStyles : (() => {\n                    try {\n                        const styleKey = this.keyForPage[\n                            Object.keys(this.keyForPage).filter((key) => {\n                                return legacyAppStore.namespacedURL(\n                                    navigationStore.path, 'react', 'POST', true)\n                                    .indexOf(legacyAppStore.namespacedURL(\n                                        key, 'react', 'POST', true)) > -1\n                            })[0]];\n                        return styleKey;\n                    } catch (e) {\n                        return '';\n                    }\n                })(),\n            });\n        });\n    }\n\n    componentWillUnmount() {\n        this.autoRunDisposer();\n    }\n\n    componentWillReceiveProps(nextProps) {\n        const { localeStore, navigationStore, url, html, legacyAppStore, c } = nextProps;\n\n        this.context.setTitle(c.name);\n\n\n        this.cleanHTML = nextProps.html;\n\n        //reset these for the new page.\n        this.buttonCleanStore = [];\n        this.checkBoxCleanStore = [];\n\n        this.setState({\n            html, message: '',\n            contentMessage: legacyAppStore.responseMessage,\n        });\n\n        this.inputCleanStore = [];\n\n        this.formActions = [];\n\n        if (this.lastLocale !== null && this.lastLocale !== localeStore.activeLocale.code) {\n            navigationStore.to({\n                url: navigationStore.localizeURL(navigationStore.path),\n            });\n        }\n        this.lastLocale = localeStore.activeLocale.code;\n     }\n\n    shouldComponentUpdate(nextProps, nextState) {\n\n        // don't update if the url doesn't match this page.\n        if (nextProps.url !== nextProps.navigationStore.path) {\n            return false;\n        }\n\n        // if (nextProps.myAccountStore.postUrl.length > 0 && !this.isForwarded) {\n        //     const url = nextProps.legacyAppStore.namespacedURL(nextProps.myAccountStore.postUrl, 'magento');\n        //     nextProps.navigationStore.to({ url });\n        //     this.isForwarded = true;\n        //     return false;\n        // }\n\n        if (nextProps.myAccountStore.previousHTML.length > 0 &&\n            this.props.html !== nextProps.myAccountStore.previousHTML\n        ) {\n            nextProps.myAccountStore.previousHTML = '';\n            nextProps.myAccountStore.isLoadingPost = false;\n            nextProps.myAccountStore.postUrl = '';\n           return true;\n        }\n\n        if (this.props.interfaceStore.isMobile !== this.isMobileTracker) {\n            this.isMobileTracker = this.props.interfaceStore.isMobile;\n            return true;\n        }\n\n        if (nextProps.myAccountStore.previousHTML.length > 0 &&\n            this.props.html !== nextProps.myAccountStore.previousHTML\n        ) {\n            nextProps.myAccountStore.previousHTML = '';\n            nextProps.myAccountStore.isLoadingPost = false;\n            nextProps.myAccountStore.postUrl = '';\n           return true;\n        }\n\n        if (this.props.interfaceStore.isMobile !== this.isMobileTracker) {\n            this.isMobileTracker = this.props.interfaceStore.isMobile;\n            return true;\n        }\n\n        if (nextProps.myAccountStore.previousHTML.length > 0 &&\n            this.props.html !== nextProps.myAccountStore.previousHTML\n        ) {\n            nextProps.myAccountStore.previousHTML = '';\n            nextProps.myAccountStore.isLoadingPost = false;\n            nextProps.myAccountStore.postUrl = '';\n           return true;\n        }\n\n\n        if (this.props.interfaceStore.isMobile !== this.isMobileTracker) {\n            this.isMobileTracker = this.props.interfaceStore.isMobile;\n            return true;\n        }\n\n        if (nextProps.myAccountStore.previousHTML.length > 0 &&\n            this.props.html !== nextProps.myAccountStore.previousHTML\n        ) {\n            nextProps.myAccountStore.previousHTML = '';\n            nextProps.myAccountStore.isLoadingPost = false;\n            nextProps.myAccountStore.postUrl = '';\n           return true;\n        }\n\n\n        if (this.props.interfaceStore.isMobile !== this.isMobileTracker) {\n            this.isMobileTracker = this.props.interfaceStore.isMobile;\n            return true;\n        }\n\n        if (this.props.navigationStore.path !== nextProps.url) return false;\n\n        // used by newsletter page.\n        // if the page is marked as no refresh and we are on that page.\n        // const accountConfig = accountLinks.filter((l) => l.to === nextProps.url)[0];\n        // if (accountConfig && accountConfig.noRefresh\n        // && this.props.url === nextProps.navigationStore.localizeURL(accountConfig.to)) {\n        //     console.log('3');\n        //     return false;\n        // }\n\n        if (this.props.myAccountStore\n        && this.props.myAccountStore.previousHTML\n        && this.props.myAccountStore.previousHTML.length > 0) {\n\n            if (this.props.html !== this.props.myAccountStore.previousHTML ) {\n                return true;\n            }\n            return false;\n        }\n\n    // avoid updating if q the html is the same but the styles are different.\n        if (this.state.activeStyles && nextState.activeStyles && Object.keys(nextState.activeStyles).length !== Object.keys(this.state.activeStyles).length\n        && nextProps.html === this.props.html) {\n            this.queuedStyles = nextState.activeStyles;\n            return false;\n    // handles the case where the user directs to another page in which styles would be undefined.\n        } else if (!nextState.activeStyles) {\n            return false;\n        }\n\n        return true;\n\n    }\n\n    async renderTrackingLink() {\n        const trackingLink = document.querySelector('.track');\n\n        if (trackingLink) {\n            const params = this.props.url.split('/');\n            if (params[params.length - 1] === '') params.length--;\n            const orderId = params[params.length - 1];\n            const { carrier, tracking_number, service_method, originZip, destination_zip } = await getTrackingDetails(orderId);\n            let carrierName, ozip = '46168';\n            if (tracking_number) {\n                switch (carrier) {\n                    case 'CAPOST': \n                        carrierName = 'canadapost';\n                        ozip = 'L9T%200A4';\n                        break;\n                    case 'PUROLA': \n                        carrierName = 'purolator';\n                        break;\n                    case 'SMPM': \n                        carrierName = 'dhlglobal';\n                        break;\n                    default: \n                        carrierName = carrier.toLowerCase();\n                        break;\n                }\n                let href = `https://beautycounter.narvar.com/beautycounter/tracking/${carrierName}?tracking_numbers=${tracking_number}&ozip=${ozip}`;\n                if (destination_zip) href += `&dzip=${destination_zip.split('-')[0]}`;\n                const service = (function(service_method) {\n                    switch(color) {\n                        case 'UPSC': // UPS - Standard\n                            return 'UG';\n                        case 'UPSS': // UPS - SurePost\n                            return 'SP';\n                        case 'UPSM': // UPS - Mail Innovations\n                            return 'MI';\n                        case 'UPS3': // UPS - 3 Day Select\n                            return 'E3';\n                        case 'UPS2': // UPS - 2nd Day Air\n                            return 'E2';\n                        case 'UPSA': // UPS - Next Day Air\n                            return 'E1';\n                        case 'FEDG': // FedEx - Ground\n                            return 'FG';\n                        case 'FEDH': // FedEx - Home Delivery\n                            return 'HD';\n                        case 'FXSP': // FedEx - Smart Post\n                            return 'SP';\n                        // case ' FED3??? ': // Fedex 3 day select\n                        //     return 'E3';\n                        case 'FED2': // FedEx - 2 Day\n                            return 'E2';\n                        case 'FEDP': // FedEx - Priority Overnight\n                            return 'E1';\n                        case 'USPF': // USPS - First Class\n                            return 'FC';\n                        // case ' USPE??? ': // USPS standard post\n                        //     return 'SP';\n                        case 'USPP': // USPS - Priority Mail\n                            return 'E2';\n                        // case ' USPF??? ': // USPS express next day\n                        //     return 'E1';\n                        default:\n                            return null;\n                    }\n                })(service_method);\n\n                if (service) href += `&service=${service}`;\n                trackingLink.href = href;\n                trackingLink.innerText = 'Track Order';\n                trackingLink.classList.remove('track');\n            } else {\n                trackingLink.innerText = 'No tracking number yet';\n            }\n        }\n    }\n\n    handleClick(e, node, formIndex) {\n        this.setState({\n            cardDataChangeVisibility: !this.state.cardDataChangeVisibility\n        }, () => {\n            if (this.state.passwordChangeVisibility === true) this.fields[formIndex - 1]['payment[sameCard]'] = 0;\n            else this.fields[formIndex - 1]['payment[sameCard]'] = 1;\n        });\n\n    }\n\n    handleChange(e, node, formIndex) {\n        if (node.attribs.name === 'change_password') {\n            this.setState({\n                passwordChangeVisibility: !this.state.passwordChangeVisibility\n            }, () => {\n                if (this.state.passwordChangeVisibility === true) this.fields.change_password = 1\n                else this.fields.change_password = 0;\n            });\n        }\n        if (node.attribs.id && node.attribs.id === 'region_id') {\n            let id = 0;\n            node.attribs.options.forEach((option) => {\n                if (option.key === e) {\n                    id = option.id;\n                }\n            });\n            this.fields[formIndex - 1]['billing[region_id]'] = id;\n            this.fields[formIndex - 1]['billing[region]'] = '';\n        }\n        let value;\n        if (e.persist) e.persist();\n        if (e.preventDefault) e.preventDefault();\n        if (e.target && e.target.value) {\n            value = e.target.value;\n            this.setState(prevState => ({\n                fields: { ...prevState.fields, [node.attribs.id]: value }\n            }));\n        }\n        else value = e;\n        if (node.attribs && node.attribs.name) {\n            this.fields[formIndex - 1][node.attribs.name] = value;\n        }\n        // this.props.legacyAppStore.updateValueForPage(e.target.value, i, 'pagename');\n    }\n\n    // wrap timeout in promise\n    timeout(ms) {\n        return new Promise(resolve => setTimeout(resolve, ms));\n    }\n\n    async handleSubmit(e, i, refresh = true) {\n        const { messagingStore, legacyAppStore: store, userStore, myAccountStore, navigationStore } = this.props;\n\n        const submitUrl = this.formActions[i];\n\n        e.preventDefault();\n\n        if (!this.forms[i].valid\n        // payment info page can have many forms. make sure to skip the validation check on edit and delete buttons.\n        && this.forms.length <= i + 1\n        ) {\n            messagingStore.addMessage('Please fill out all the fields', 'error');\n        } else if (submitUrl) {\n\n        // store fields to rehydrate\n            this.submittedFields = JSON.parse(JSON.stringify(this.fields));\n        //avoid changing password\n            // delete this.fields.change_password;\n\n            // if this form url is equal to a post form location, then look\n            // for forwarding location on that key.\n            let accountConfig = accountLinks.filter((l) => {\n                return l.api === store.namespacedURL(submitUrl, 'react', 'POST', true)\n            })[0];\n        // if no account config is found then try to select another way.\n            if (!accountConfig) {\n                accountConfig = accountLinks.filter((l) => {\n                    console.log(l.api, store.namespacedURL(submitUrl, 'react', 'POST', true));\n                    return l.api && store.namespacedURL(submitUrl, 'react', 'POST', true)\n                    .indexOf(l.api) > -1;\n                })[0];\n            }\n            if (accountConfig && accountConfig.noRefresh) {\n                refresh = false;\n            }\n        // also can set refresh false to cancel refreshing\n        //update clean html with new values\n            this.cleanHTML = this.filledHTML.innerHTML;\n\n            //preclear message.\n            // this.setState({ message: '' });\n\n            // declares to router that it should not reload this page on state update.\n            myAccountStore.isLoadingPost = true;\n            myAccountStore.isForwarded = false;\n            const cleanedUrl = store.namespacedURL(submitUrl, 'magento', 'POST');\n            myAccountStore.postUrl = cleanedUrl;\n            let html = await store.postForm(cleanedUrl, '', this.fields[i]);\n\n            // if there is a clipping error\n            if (!store.clipContent(html, 'col-main', true)\n            && accountConfig) {\n                if (accountConfig.errorCatchApi) {\n                    this.props.interfaceStore.showProgress('page-load');\n                    await this.timeout(1500);\n                    html = await store.postForm(accountConfig.errorCatchApi, '', {});\n                } else {\n                    messagingStore.addMessage(\"There was an error processing the request. Please make sure all the fields are completed.\", { type: 'error' });\n                }\n            }\n\n            this.lastCalledPath = null;\n\n        // after submitting the content then refresh user store\n            userStore.verifyLoggedIn(true);\n\n            const el = document.createElement('html');\n            el.innerHTML = html;\n            const messages = el.getElementsByClassName('messages')[0];\n            let success_message = null;\n            // this signals to the redirection code below if the logic should be skipped\n            // and we should stay on the same page.\n            let hasErrors = false;\n            // used to detect non success states on error catch pages.\n            let hasSuccess = false;\n\n            try {\n                success_message = messages.getElementsByClassName('success-msg')[0].firstChild.firstChild.firstChild.textContent;\n                messagingStore.addMessage(success_message, {type: 'success'});\n                hasSuccess = true;\n            } catch (e) {\n                try {\n                    const error_message = messages.getElementsByClassName('error-msg')[0].firstChild.firstChild.firstChild.textContent;\n                    messagingStore.addMessage(error_message, {type: 'error'});\n                    hasErrors = true;\n                } catch (e) {\n                    // this.setState({message: 'There was an error processing your request.'});\n                }\n            }\n\n            // if this address has a custom redirect then send to there.\n            let { redirectTo, errorCatchApi } = accountConfig;\n            if (redirectTo\n            && !hasErrors\n            && (!errorCatchApi || hasSuccess)\n            ) {\n            // clear previous html if there is because we wouldn't want to\n            // display a different page's html.\n                myAccountStore.previousHTML = '';\n                navigationStore.to({\n                    url: navigationStore.localizeURL(accountLinks.filter((l) => l.key === accountConfig.redirectTo)[0].to),\n                });\n            } else {\n\n                if (errorCatchApi && !hasSuccess && !hasErrors) {\n                    messagingStore.addMessage('There was an error submitting your payment information. Please check the information entered and try again.', {type: 'error'});\n                } else if (!hasErrors) { // reset all fields + update screen with new content\n                    myAccountStore.previousHTML = store.clipContent(html);\n\n                    this.fields = [];\n                    this.forms = [];\n                    this.setState({\n                        // html: ((() => {messages ? messages.innerHTML : null})() + this.cleanHTML),\n                        // fields: {},\n                        html: (() => refresh ? store.clipContent(html) : this.cleanHTML)(),\n                    });\n                }\n            }\n        } else {\n            messagingStore.addMessage('error retrieving data', 'error');\n        }\n\n    }\n\n    render() {\n        const { legacyAppStore: store, contentStore, c, navigationStore, localeStore } = this.props;\n        let { fields, html } = this.state;\n    \n    // clear the fields and forms\n        this.fields = [];\n        this.forms = [];\n\n    // remove 'your consultant is hq'\n        html = html.replace(`<div class=\"input-box\">Your consultant is <strong>Beautycounter HQ</strong></div>`, '');\n\n    // fix for getting the current state on address edit\n        let region_id = -1;\n        try {\n            const start = html.indexOf(`$('region_id').setAttribute('defaultValue',  \"`) + 46;\n            const remainder = html.slice(start, html.length);\n            region_id = remainder.slice(0,remainder.indexOf(`\"`));\n        } catch(e) {}\n\n    // fix for extracting address delete link.\n        let addressbookDeleteLink = '';\n        try {\n            const start = html.indexOf(`customer/address/delete/form_key/`);\n            const end = html.indexOf(`/id/'+addressId;`);\n            addressbookDeleteLink = html.slice(start, end);\n        } catch(e) {}\n\n    // all text fields come from dashboard configuration.\n        const updatedFields = getSafe(\n            () => contentStore.extensions['dashboard'].data.configAccount\n            // .filter((textPack) => textPack.page === c.key)[0]\n            .filter((textPack) => textPack.page === 'dashboard')[0]\n            .labels,\n        )\n        this.textFields = updatedFields ? updatedFields : this.textFields;\n\n    //clear these enumerating states on render\n        //keeps an index of the different forms so that the handlesubmit can match the right action to the right form.\n        let formCount = 0;\n\n        let buttonIndexCurrent = 0;\n        let checkBoxIndexCurrent = 0;\n        let inputIndexCurrent = 0;\n\n        const transform = (node, i) => {\n            let { attribs, type, name, children } = node;\n\n            //handle react attribute transformation\n            if (attribs !== undefined) {\n\n                attribs.key = this.keyEnum;\n                this.keyEnum = this.keyEnum + 1;\n\n                if (attribs.maxlength) {\n                    attribs.maxLength = attribs.maxlength;\n                    delete attribs.maxlength;\n                }\n                if (attribs.viewbox) {\n                    attribs.viewBox = attribs.viewbox;\n                    delete attribs.viewbox;\n                }\n                if (attribs['stroke-widt