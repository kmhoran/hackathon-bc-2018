          ], c);\n        },\n    },\n\n    {\n        path: routes.PRESS,\n        async action({ next }) {\n            return await next();\n        },\n        children: [\n            {\n                path: `/:articleID`,\n                action: data => data.route.parent.page(data)\n            },\n            {\n                path: `*`,\n                action: data => data.route.parent.page(data)\n            },\n        ],\n        page: async ({ params, stores, query, next }) => {\n\n            stores.interfaceStore.showProgress('page-load');\n            const [ pressStore, PressContainer ] = await Promise.all([\n                loadContainer('PressStore'),\n                loadContainer('PressContainer'),\n            ]);\n            await pressStore.init();\n            if (pressStore.page && pressStore.page.hide) {\n                return await next();\n            }\n            stores.interfaceStore.hideProgress('page-load');\n\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disableHover: true,\n            });\n\n            return {\n                chunks: ['PressStore', 'PressContainer'],\n                component: (\n                    <Provider pressStore={pressStore}>\n                        <PressContainer\n                            articleID={params.articleID || null}\n                            query={query}\n                        />\n                    </Provider>\n                ),\n            };\n        },\n    },\n\n    {\n        path: routes.LOGIN,\n        async action({ stores }) {\n            stores.interfaceStore.showProgress('page-load');\n            const [ LoginPage ] = await Promise.all([\n                loadContainer('LoginPage'),\n                stores.contentStore.fetchContentfulConfig(asyncModels.login),\n            ]);\n            stores.interfaceStore.hideProgress('page-load');\n\n 