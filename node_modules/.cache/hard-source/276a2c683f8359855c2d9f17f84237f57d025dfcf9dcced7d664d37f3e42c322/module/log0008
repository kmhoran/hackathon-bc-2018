}),\n                    showAddToCart && renderExtra()\n                )\n            );\n        }\n    }]);\n    return ProductItem;\n}(_react.Component), _class2.propTypes = {\n    // product: pt.object.isRequired, //TODO: empty queries will respond as a bool. so this will error.\n    onClick: _propTypes.PropTypes.func,\n    row: _propTypes.PropTypes.number,\n    showQuicklook: _propTypes.PropTypes.bool,\n    showTags: _propTypes.PropTypes.bool,\n    showSwatches: _propTypes.PropTypes.bool,\n    infoPadding: _propTypes.PropTypes.bool,\n    useAltMobileStyle: _propTypes.PropTypes.bool,\n    showAddToCart: _propTypes.PropTypes.bool,\n    className: _propTypes.PropTypes.string,\n    markFree: _propTypes.PropTypes.bool, //mark an item as being free for purchase.\n    theme: _propTypes.PropTypes.string,\n    buttonText: _propTypes.PropTypes.string,\n    disabled: _propTypes.PropTypes.bool\n}, _class2.defaultProps = {\n    product: {},\n    row: 1,\n    showQuicklook: false,\n    showTags: true,\n    showSwatches: true,\n    infoPadding: true,\n    useAltMobileStyle: false,\n    selectedVariant: false,\n    selectedVariantId: false,\n    selectedVariantName: false,\n    viewportWidth: 1280,\n    showAddToCart: false,\n    className: null,\n    markFree: false,\n    theme: 'standard',\n    buttonText: null,\n    disabled: false\n}, _temp2)) || _class) || _class) || _class);\nexports.default = ProductItem;","name":"node_modules/babel-loader/lib/index.js??ref--4!/Users/millie.savalia/react-node-frontend/src/components/Product/ProductItem/ProductItem.jsx","sourceMap":{"version":3,"sources":["src/components/Product/ProductItem/ProductItem.jsx"],"names":["ProductItem","state","activeImageIndex","selectedVariant","props","openQuicklook","product","row","variant","categoryStore","toggleQuicklook","color","setState","name","selectedVariantId","id","selectedVariantName","fullName","handleSwipeImageChange","images","index","variantId","selectedIndex","shop","contentStore","cont