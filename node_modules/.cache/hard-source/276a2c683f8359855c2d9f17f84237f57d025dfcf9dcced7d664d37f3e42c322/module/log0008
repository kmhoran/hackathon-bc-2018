sName={cx(s.root, this.props.className)} onClick={() => this.handleClick()}>\n                {(ratings || empty) && \n                <div className={cx(s.smartAlign,s.reviewContainer)}>\n                    <div className={cx(\"yotpo yotpo bottomline\", s.reviewStars)}>\n                        {stars.map((star, index) => \n                            <span \n                                key={index} \n                                className={cx(\"yotpo-icon-star pull-left\", star)}\n                            />\n                        )}\n                    </div>\n                    {!empty && reviews && !hideCount && \n                        <span style={reviewStyles} className={s.reviewCount}>\n                            <ReviewString />\n                        </span>\n                        }\n                </div>}\n            </div>\n        );\n    }\n}\n\nexport default ProductRating;"]}},"_buildHash":"5480e0c234100ace04c894f9f7eccc9b","hash":"b02a9e9affdfb6428f4943dfe5ef8ccc","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[110,137],"loc":{"start":{"line":7,"column":19},"end":{"line":7,"column":55}}},{"type":"RequireHeaderDependency","range":[102,109],"loc":{"start":{"line":7,"column":19},"end":{"line":7,"column":55}}},{"type":"RequireHeaderDependency","range":[225,232],"loc":{"start":{"line":11,"column":25},"end":{"line":11,"column":74}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/asyncToGenerator","range":[233,273],"loc":{"start":{"line":11,"column":25},"end":{"line":11,"column":74}}},{"type":"RequireHeaderDependency","range":[369,376],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","