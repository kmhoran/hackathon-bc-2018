(this);\n            else window.optimizelyCallback = this.handleOptimizelyLoaded.bind(this);\n\n            this.scrollListener = debounce(this.handleScroll,10);\n            window.addEventListener('scroll', this.scrollListener, { passive: true });\n        }\n    }\n\n    componentWillMount() {\n        this.handleScroll();\n    }\n\n    componentWillUnmount() {\n        window.removeEventListener('scroll', this.scrollListener);\n    };\n\n    handleScroll = () => {\n        if (__BROWSER__ && !this.props.interfaceStore.isMobile && this.refs.root) {\n            const classList = this.refs.root.classList;\n            if (window.pageYOffset >= 360) {\n                if (classList.contains(s.desktopScrollTopExpanded)) classList.remove(s.desktopScrollTopExpanded);\n                if (classList.contains(s.desktopScrollTopCollapsed)) classList.remove(s.desktopScrollTopCollapsed);\n                classList.add(s.desktopScroll);\n            } else if (window.pageYOffset < 360) {\n                classList.remove(s.desktopScroll);\n                if (this.state.expanded) classList.add(s.desktopScrollTopExpanded);\n                else if (!this.state.expanded) classList.add(s.desktopScrollTopCollapsed);\n            }\n        }\n    };\n\n    handleOptimizelyLoaded() {\n        if (this.config && window.optimizelyVariations.includes(this.config.key)) {\n            this.setState({ enabled: true });\n        }\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.content.promoBanner[0]);\n    }\n\n    handleLinkClick() {\n        if (__BROWSER__ && window.optimizely) {\n            window.optimizely.push({\n                type: 'event',\n                eventName: 'promoBannerLinkClick'\n            });\n        }\n        this.handleExiting(false);\n    }\n\n    handleExiting(expanded) {\n        this.setState({ exiting: true });\n        if (!this.exitTimeout) {\n            this.exitTimeout = setTimeout(() => {\n                const { isMobile } = this.props.interfaceStore;\n                this.setState({ expanded, exiting: false });\n                this.exitTimeout = null;\n                if (isMobile && expanded) this.renderMobileExpanded();\n            }, 300);\n        }\n\n        if (__BROWSER__ && window.optimizely && expanded) {\n            window.optimizely.push({\n                type: 'event',\n                eventName: 'promoBannerExpanded'\n            });\n        }\n    }\n\n    renderDesktopCollapsed() {\n\n        const {\n            smallBannerTextDesktop,\n            hexValue\n        } = this.config;\n\n        const style = { \n            backgroundColor: `#${hexValue}`\n        };\n\n        if (this.state.exiting) {\n            style.transform = 'translateX(-375px)';\n            style.transition = 'transform .3s';\n        }\n\n        return (\n            <div \n                onClick={() => this.handleExiting(true)} \n                className={s.desktopCollapsed}\n                style={style}>\n                    <div>\n                        <Gift\n                            color={this.state.fontColor}\n                            size={{ width: 18, height: 18 }} />\n                    </div>\n                    <div>\n                        {smallBannerTextDesktop}\n                    </div>\n                    <div>\n                        <RightAngle \n                            color={this.state.fontColor}\n                            size={{ width: 12, height: 12 }} />\n                    </div>\n            </div>\n        );\n    }\n\n    renderDesktopExpanded() {\n\n        const {\n            bannerTextDesktop,\n            expiresTextDesktop,\n            linkTextDesktop,\n            hexValue,\n            linkUrl\n        } = this.config;\n\n        const style = { \n            backgroundColor: `#${hexValue}`\n        };\n\n        if (this.state.exiting) {\n            style.transform = 'translateY(-40px)';\n            style.transition = 'transform .3s';\n        }\n\n        return (\n            <div \n                className={s.desktopExpanded}\n                style={style}>\n                    <div>\n                        <Gift\n                            color={this.state.fontColor}\n                            size={{ width: 18, height: 18 }} />\n                    </div>\n                    <div>\n                        {bannerTextDesktop}\n                    </div>\n                    <div>\n                        {expiresTextDesktop}\n                    </div>\n                    <div>\n                        <Link \n                            className={s.detailsLink} \n                            to={linkUrl}\n                            onClick={this.handleLinkClick} >\n                                {linkTextDesktop}\n                        </Link>\n                    </div>\n                    <div \n                        className={s.desktopClose}\n                        onClick={() => this.handleExiting(false)}>\n                            <Close \n                                color={this.state.fontColor}\n                                size={{ width: 12, height: 12 }} />\n                    </div>\n            </div> \n        );\n    }\n\n    renderMobileCollapsed() {\n\n        const { \n            smallBannerTextMobile,\n            hexValue\n        } = this.config;\n\n        const style = { \n            backgroundColor: `#${hexValue}`\n        };\n\n        const containerStyle = {};\n\n        if (this.state.exiting) {\n            containerStyle.transform = 'translateX(-375px)';\n            containerStyle.transition = 'transform .3s';\n        }\n\n        const firstMobile = this.firstMobile;\n        if (this.firstMobile) this.firstMobile = false;\n\n        return (\n            <div className={s.mobileCollapsedContainer} style={containerStyle}>\n                <div \n                    onClick={() => this.handleExiting(true)} \n                    className={s.mobileCollapsed}\n                    style={{ ...style, display: firstMobile ? 'flex' : 'none' }}>\n                        <div style={{ transform: 'translateY(-3px)' }}>\n                            <Gift\n                                color={this.state.fontColor}\n                                size={{ width: 18, height: 18 }} />\n                        </div>\n                        <div>\n                            {smallBannerTextMobile}\n                        </div>\n                        <div>\n                            <RightAngle \n                                color={this.state.fontColor}\n                                size={{ width: 12, height: 12 }} />\n                        </div>\n                </div>\n                <div\n                    onClick={() => this.handleExiting(true)}\n                    className={s.mobileCollapsedGift}\n                    style={style}>\n                        <div style={{ transform: 'translateY(-3px)' }}>\n                            <Gift\n                                color={this.state.fontColor}\n                                size={{ width: 18, height: 18 }} />\n                        </div>\n                </div>\n            </div>\n        );\n    }\n\n    renderMobileExpanded() {\n\n        if (this.state.exiting) return false;\n\n        const { setBottomPanel, openBottomPanel } = this.props.interfaceStore;\n\n        const {\n            headerTextMobile,\n            expiresTextMobile,\n            detailsTextMobile,\n            limitTextMobile,\n            linkTextMobile,\n            hexValue,\n            linkUrl\n        } = this.config;\n\n        const mobileExpandedStyle = { \n            backgroundColor: `#${hexValue}`,\n            color: this.state.fontColor\n        };\n\n        const bottomPanelOptions = {\n            key: 'promoBanner',\n            minHeight: '55%',\n            closeIconColor: this.state.fontColor,\n            onClose: () => this.handleExiting(false)\n        };\n\n        const isFR = getSafe(() => this.props.localeStore.activeLocale.code === 'fr-CA');\n\n        const component = (\n            <div \n                className={s.mobileExpanded} \n                style={mobileExpandedStyle}>\n                    <div className={s.mobileExpandedChild} key='promoGift'>\n                        <Gift\n                            color={this.state.fontColor}\n                            size={{ width: 40, height: 40 }} />\n                    </div>\n                    <div className={s.mobileExpandedChild} key='promoHeader'>\n                        {headerTextMobile}\n                    </div>\n                    <div className={cx(s.mobileExpandedChild, isFR && s.frenchSpacing)} key='promoExpires'>\n                        {expiresTextMobile}\n                    </div>\n                    <div className={cx(s.mobileExpandedChild, isFR && s.frenchSpacing)} key='promoDetails'>\n                        {detailsTextMobile}\n                    </div>\n                    <div className={cx(s.mobileExpandedChild, isFR && s.frenchSpacing)} key='promoLimit'>\n                        {limitTextMobile}\n                    </div>\n                    <div key='promoButton'>\n                        <Button\n                            to={linkUrl}\n                            color={COLOR_WHITE}\n                            wide\n                            onClick={this.handleLinkClick}>\n                                {linkTextMobile}\n                        </Button>\n                    </div>\n                </div>\n        );\n\n        setBottomPanel(component, bottomPanelOptions);\n        openBottomPanel();\n    }\n\n    render() {\n        if (!this.state.enabled) return false;\n\n        const { isMobile } = this.props.interfaceStore;\n\n        return (\n            <div \n                className={s.root} \n                style={{ color: this.state.fontColor }}\n                ref='root'>\n                    {!isMobile ? this.state.expanded ? this.renderDesktopExpanded() : this.renderDesktopCollapsed() :\n                    !this.state.expanded && this.renderMobileCollapsed()}\n            </div>\n        );\n    }\n}\n\nexport default PromoBanner;\n"]}},"_buildHash":"723a97d1f928b6018d479df29c88734c","hash":"f9baa10a8795a87e4235b09875ead4e6","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[286,293],"loc":{"start":{"line":11,"column":16},"end":{"line":11,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/extends","range":[294,325],"loc":{"start":{"line":11,"column":16},"end":{"line":11,"column":56}}},{"type":"RequireHeaderDependency","range":[403,410],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-prototype-of","range":[411,458],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/classCallCheck","range":[558,596],"loc":{"start":{"line":19,"column":23},"end":{"line":19,"column":70}}},{"type":"RequireHeaderDependency","range":[550,557],"loc":{"start":{"line":19,"column":23},"end":{"line":19,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/createClass","range":[694,729],"loc":{"start":{"line":23,"column":20