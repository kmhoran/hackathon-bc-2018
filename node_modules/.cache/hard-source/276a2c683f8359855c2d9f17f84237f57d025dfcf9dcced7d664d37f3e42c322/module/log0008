ith smallest assets \n        };\n\n        this._handleMobileNavChange = this._handleMobileNavChange.bind(this);\n        this._setBackgroundImage = this._setBackgroundImage.bind(this);\n        this._handleWaypointEnter = this._handleWaypointEnter.bind(this);\n        this._enableWaypoint = this._enableWaypoint.bind(this);\n        this._checkBreakpoint = throttle(this._checkBreakpoint.bind(this), 100);\n\n    }\n\n    componentDidMount() {\n        window.addEventListener('wheel', this._enableWaypoint, { passive: true });\n        window.addEventListener('touchmove', this._enableWaypoint, { passive: true });\n        window.addEventListener('resize', this._checkBreakpoint);\n    }\n\n    componentWillUnmount() {\n        window.removeEventListener('wheel', this._enableWaypoint);\n        window.removeEventListener('touchmove', this._enableWaypoint);\n        window.removeEventListener('resize', this._checkBreakpoint);\n    }\n\n    componentDidUpdate(prevProps, prevState) {\n        const sectionChanged = prevState.focusedSectionId !== this.state.focusedSectionId;\n        const isMobile = this.state.activeBreakpoint <= MEDIA_MOBILE;\n        if (sectionChanged && isMobile) {\n            this._handleMobileNavChange();\n        }\n    }\n\n    _handleWaypointEnter(elementId, e) {\n        if (!this.state.isWaypointDisabled) {\n            this.setState({\n                focusedSectionId: elementId,\n                waypointPosition: e.previousPosition,\n            });\n        }\n    }\n\n    _enableWaypoint() {\n        if (this.state.isWaypointDisabled) {\n            this.setState({ isWaypointDisabled: false });\n        }\n    }\n\n    _checkBreakpoint() {\n        const activeBreakpoint = throttleBreakpoints(window.innerWidth, this.state.activeBreakpoint);\n        if (!!activeBreakpoint) {\n            this.setState({ activeBreakpoint });\n        }\n    }\n\n    _setBackgroundImage(desktop, mobile) {\n        const hasBackground = mobile || desktop;\n        if (!!hasBackground) {\n            const getStyle = (url) => ({\n                backgroundImage: `url(${url}?q=55)`,\n                backgroundSize: 'cover',\n                backgroundPosition: 'center top',\n                backgroundRepeat: 'no-repeat',\n            });\n            const forBreakpoint = this.state.activeBreakpoint <= MEDIA_MOBILE\n                ? hasBackground\n                : desktop;\n\n            return getStyle(forBreakpoint);\n        }\n    }\n\n    _handleMobileNavChange() {\n        if (this.navMenu && this.focusedNavItem) {\n            const windowWidth = window.innerWidth;\n            const navItemRect = this.focusedNavItem.getBoundingClientRect();\n\n            const isLeft = navItemRect.x < 0;\n            const isRight = navItemRect.right > windowWidth;\n\n            if (isLeft || isRight) {\n                const scrollDelta = windowWidth - navItemRect.width;\n                this.navMenu.scrollLeft += isRight ? scrollDelta : -scrollDelta;\n            }\n        }\n    }\n\n    /*\n     * NOTE: scrollAnchors account for header height\n    */\n    changeViewport(anchorId) {\n        if (this.refs[anchorId]){\n            this.setState({ focusedSectionId: anchorId, isWaypointDisabled: true });\n            requestAnimationFrame(\n                () => this.refs[anchorId].scrollIntoView({ block: 'start', behavior: 'smooth' })\n            );\n        }\n    }\n\n    render() {\n        let ContentBlocks = [];\n        let NavItems = [];\n\n        const {\n            _handleWaypointEnter,\n            _handleWaypointLeave,\n            changeViewport,\n        } = this;\n\n        const {\n            injectNavFooter,\n            injectNavHeader,\n            content,\n            navMenuClassName,\n            interfaceStore\n        } = this.props;\n\n        const {\n            pageLeftOffset,\n            navTopOffset,\n            focusedSectionId,\n            activeBreakpoint,\n        } = this.state;\n\n        const getWaypoints = (isNavHeader, anchorId) => (\n            isNavHeader &&\n            <React.Fragment>\n                <div ref={anchorId} className={s.scrollToAnchor}></div>\n                <div className={s.waypointAnchor}>\n                    <Waypoint\n                        scrollableAncestor={'window'}\n                        onEnter={_handleWaypointEnter.bind(this, anchorId)}\n                        topOffset={'50%'}\n                    />\n                </div>\n            </React.Fragment>\n        )\n\n        const ContentBlock = props => (\n            <li\n                className={s.listItem}\n                key={props.id}\n            >\n                {getWaypoints(props.isNavHeader, props.anchorId)}\n                <Section\n                    className={props.blockClassName}\n                    innerClassName={s.blockInner}\n                    contentClassName={cx(s.contentBlockBody, props.classname)}\n                    customBackground={this._setBackgroundImage(props.backgroundImageUrl, props.mobileBackgroundImageUrl)}\n                >\n                    {props.children}\n                </Section>\n            </li>\n        )\n\n        const NavItem = props => (\n            <li\n                key={props.id}\n                onClick={changeViewport.bind(this, props.anchorId)}\n                className={cx(\n                    props.navItemClassName,\n                    s.navItemFixedWidth,\n                    {\n                        [props.navItemSelectedClassName]: focusedSectionId === props.anchorId\n                    }\n                )}\n                ref={(focusedNavItem) => { \n                    if (focusedSectionId === props.anchorId) {\n                            this.focusedNavItem = focusedNavItem \n                        } \n                    }\n                }\n            >\n                <span> {props.title} </span>\n            </li>\n        )\n\n        content.forEach(contentChild => {\n            ContentBlocks.push(ContentBlock(contentChild.props));\n            if (contentChild.props.isNavHeader) {\n                NavItems.push(NavItem(contentChild.props));\n            }\n        });\n\n        return (\n            <div className={s.flex}>\n                {interfaceStore.appIsMounted && <div className={s.panel}>\n                        <Sticky\n                            className={s.sticky}\n                            activeClass={s.activeNode}\n                            bottomBoundary='#page-wrapper'\n                            innerZ={6}>\n                            <div className={s.navWrapper} >\n                                <div\n                                    className={cx(s.navBlock, navMenuClassName)}\n                                    ref={(navMenu) => { this.navMenu = navMenu }}\n                                >\n                                    <div className={s.navEndcap}>\n                                        {injectNavHeader}\n                                    </div>\n                                    <ul>\n                                        {NavItems}\n                                    </ul>\n                                    <div className={s.navEndcap}>\n                                        {injectNavFooter}\n                                    </div>\n                                </div>\n                            </div>\n                        </Sticky>\n                </div>}\n\n                <div\n                    id={'page-wrapper'}\n                    className={s.pageWrapper}\n                >\n                    <ul className={cx(s.list, navMenuClassName)}>\n                        {ContentBlocks}\n                    </ul>\n                </div>\n            </div >\n        );\n    }\n}\n\nexport default StickyNavContainer;\n"]}},"_buildHash":"d3454fedeb7f5440d9781ac560bba56c","hash":"83373a6420251419fe53f700ee5abc7b","_lastSuccessfulBuildMeta