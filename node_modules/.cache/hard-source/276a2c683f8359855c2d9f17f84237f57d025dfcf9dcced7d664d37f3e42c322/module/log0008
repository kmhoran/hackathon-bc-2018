;\nimport s from './VideoSelector.scss';\nimport cx from 'classnames';\nimport withStyles from 'isomorphic-style-loader/lib/withStyles';\nimport { getSafe } from 'Middleware/utils/object';\nimport MarkdownWrapper from 'Components/MarkdownWrapper';\nimport { generateWidget } from 'Components/Widgets';\n\n\n@withStyles(s)\nclass VideoSelector extends Component {\n\n    constructor(props) {\n        super(props);\n        this.state = {\n            selectedVideo: 0,\n            defaultCaption: 0,\n        };\n    }\n\n    static defaultProps = {\n        key: '',\n        thumbnails: [],\n        videos: [],\n        captions: [],\n        derriveHeightFromParent: false,\n        disableCaption: false,\n        keepOriginalDimensions: false,\n        boxShadow: true,\n        circleThumbnails: false,\n        onClickHandler: null,\n        defaultSelectedIndex: null,\n        fadeOutUnselected: false,\n        lockAspectRatio: false,\n        thumbnailsToShow: null,\n    }\n\n    get thumbnailStyle(){\n        const k = (95 / this.props.thumbnailsToShow);\n        return ({width: `${k}%`});\n    }\n\n    _handleThumbnailClick(i, e) {\n        if(this.props.defaultSelectedIndex === null ){\n            this.setState({ selectedVideo: i });\n        }\n        this.props.onClickHandler && this.props.onClickHandler(i);\n    }\n\n    _getButton(markdown = '') {\n        return markdown.split(/\\n/).filter(line => line.includes('isButton')).join();\n    }\n\n    render() {\n        let Selection = [];\n        let ThumbnailSelection = [];\n\n        const {\n            _handleThumbnailClick,\n            _getButton,\n        } = this;\n\n        const {\n            selectedVideo,\n            defaultCaption,\n        } = this.state;\n\n        const {\n            key,\n            videos,\n            thumbnails,\n            captions,\n            derriveHeightFromParent,\n            disableCaption,\n            keepOriginalDimensions,\n            boxShadow,\n            circleThumbnails,\n            defaultSelectedIndex,\n            fadeOutUnselected,\n            lockAspectRatio,\n            thumbnailsToShow,\n        } = this.props;\n\n        const aspectRatio = lockAspectRatio || 'wide';\n\n        const selectedIndex = defaultSelectedIndex !== null \n            ? defaultSelectedIndex\n            : selectedVideo;\n\n        const markdown = getSafe(() => captions[selectedIndex].fields.body)\n            || getSafe(() => captions[defaultCaption].fields.body);\n\n        const ctaButtonMarkdown = _getButton(markdown);\n\n        thumbnails && videos && thumbnails.forEach((element, i) => {\n            const isVideoWidget = (getSafe(() => videos[i].sys.contentType.sys.id) === 'widgetVideo');\n            const selectorAssets = isVideoWidget\n                ? generateWidget('Video', videos[i].fields)\n                : <img className={s.vimeoWrapper} src={getSafe(() => element.fields.file.url)} />\n\n            ThumbnailSelection.push(\n                <div\n                    key={i} \n                    style={thumbnailsToShow && this.thumbnailStyle}\n                    className={cx(s.thumbnailWrapper, lockAspectRatio)}>\n                    <div\n                        className={cx(s.thumbnail, s.wide)}\n                        ref={(selector) => { this.selector = selector }}>\n                        <img\n                            className={cx({\n                                [[s.isTarget]]: i === selectedIndex,\n                            })}\n                            onClick={_handleThumbnailClick.bind(this, i)}\n                            src={getSafe(() => thumbnails[i].fields.file.url)}\n                        />\n                    </div>\n                </div>\n\n            );\n\n            Selection.push(\n                <div\n                    key={i}\n                    className={cx(s.vimeoWrapper, {\n                        [[s.hidden]]: i !== selectedIndex,\n                        [[s.notHidden]]: i === selectedIndex,\n                    })}\n                >\n                    {selectorAssets}\n                </div>\n            );\n        });\n\n        return (\n            <div className={cx(s.root, {\n                [[s.mobileShadow]]: !!ctaButtonMarkdown,\n                [[s.disableCaption]]: disableCaption,\n                [[s.derriveHeightFromParent]]: derriveHeightFromParent,\n                [[s.keepOriginalDimensions]]: keepOriginalDimensions,\n                [[s.boxShadow]]: boxShadow,\n                [[s.circleThumbnails]]: circleThumbnails,\n                [[s.fadeOutUnselected]]: fadeOutUnselected,\n                [[s.lockAspectRatio]]: lockAspectRatio,\n            })}>\n                {captions.length > 0 &&\n                    <div className={s.caption}>\n                        <MarkdownWrapper\n                            wrapperClassName={s.markdownWrapper}\n                            content={markdown}\n                        />\n                    </div>\n                }\n                <div className={s.selectorWrapper}>\n                    <div className={cx(s.video, s[aspectRatio])}>\n                        {Selection}\n                    </div>\n                    {/* <div className={s.test}> */}\n                        <div className={cx(s.thumbnails)}>\n                            <div className={s.absoluteWrapper}> \n                                {ThumbnailSelection}\n                            </div>\n                        </div>\n                    {/* </div> */}\n                    {ctaButtonMarkdown &&\n                        <div className={s.showOnMobile}>\n                            <MarkdownWrapper\n                                content={ctaButtonMarkdown}\n                            />\n                        </div>\n                    }\n                </div>\n            </div>\n        );\n    }\n}\n\nexport default VideoSelector;"]}},"_buildHash":"3fb067c