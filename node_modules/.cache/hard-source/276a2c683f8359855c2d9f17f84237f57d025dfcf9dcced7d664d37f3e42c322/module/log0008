mailInput, error });\n    }\n\n    handleError() {\n        const emailError = !this.isValidEmail\n            && this.props.formEmailInputValidation;\n\n        const sailthruError = this.state.sailthruError \n            && this.props.formSailthruError;\n\n        const error = emailError || sailthruError;\n\n        this.setState({ error });\n    }\n\n    toggleFormSubmit() {\n        const isFormSubmitted = !this.state.isFormSubmitted;\n        this.setState({ isFormSubmitted });\n    }\n\n    handleSubmit = async e => {\n        e.preventDefault();\n\n        if (this.isValidEmail) {\n            const formSubmissionObj = { email: this.state.emailInput };\n            this.setState({ isFormSubmitted: true });\n\n            // Send the object and form values to the sailthru API.\n            this.toggleFormSubmit();\n            popupShopSubscribe(formSubmissionObj, this.props.sailthruSubscriptionListKey)\n                .then(() => {\n                    this.setState({\n                        submitSuccess: true,\n                        sailthruError: false,\n                    });\n                })\n                .catch(e => {\n                    this.setState({\n                        sailthruError: true,\n                    });\n                })\n                .finally(\n                    this.toggleFormSubmit\n                );\n        } else {\n            this.handleError();\n        }\n    };\n\n    render() {\n        const {\n            linkLocation,\n            textLocation,\n            textPhoneNumber,\n            title,\n            body,\n            formSuccessMessage,\n            formSubmitButtonText,\n            formTitle,\n            formEmailInputLabel,\n            formEmailInputValidation,\n            mapWidget,\n            googleMapsApiUrl,\n        } = this.props;\n\n        const {\n            emailInput,\n            isFormSubmitted,\n            sailthruError,\n            submitSuccess,\n            error,\n        } = this.state;\n\n        const MapComponent = (\n            <GoogleMap googleMapsApiUrl={googleMapsApiUrl} />\n        );\n\n        const LoaderComponent = (\n            <span className={s.loader}>\n                <Loader />\n            </span>\n        );\n\n        const FormComponent = (\n            <React.Fragment>\n                <h4>{formTitle}</h4>\n                <form action={''} onSubmit={this.handleSubmit}>\n                    <ErrorField className={s.inputInvalid} errors={error} />\n\n                    <div className={s.inputWrapper}>\n                        <span className={s.input}>\n                            <Input\n                                showLabel\n                                required\n                                type={'text'}\n                                defaultValue={this.state.emailInput}\n                                onChange={this.handleInputChange}\n                                title={formEmailInputLabel}\n                            />\n                        </span>\n\n                        <span className={s.joinBtn}>\n                            <Button\n                                isSubmit\n                                disableStyleOnly={!this.isValidEmail}\n                            >\n                                {isFormSubmitted ? LoaderComponent : formSubmitButtonText}\n                            </Button>\n                        </span>\n\n                    </div>\n                </form>\n            </React.Fragment>\n        );\n\n        const SuccessMessageComponent = (\n            <div className={s.successMessage}>\n                <h4>\n                    {/* TODO: ICON add props for line color */}\n                    <span className={s.checkIcon} id=\"checkIcon--green\">\n                        <CheckedIcon />\n                    </span>\n                    {!isFormSubmitted && formSuccessMessage}\n                </h4>\n            </div>\n        );\n\n        return (\n            <section className={s.root}>\n                <div className={s.panelContent}>\n                    <section className={s.leftCol}>\n                        <h1>{title}</h1>\n\n                        <div className={s.contactInformation}>\n                            <span>\n                                <MapPin className={s.globeIcon} />\n                                <a href={linkLocation} target=\"_blank\">\n                                    {textLocation}\n                                </a>\n                            </span>\n\n                            <span>\n                                <Phone className={s.phoneIcon} />\n                                <a href={`tel:+${textPhoneNumber}`}>{textPhoneNumber}</a>\n                            </span>\n                        </div>\n\n                        <MarkdownWrapper className={s.underlineHeader} content={'---'} />\n\n                        <MarkdownWrapper className={s.body} content={body} />\n\n                        {!submitSuccess && FormComponent}\n\n                        {submitSuccess && SuccessMessageComponent}\n                    </section>\n\n                    <section className={s.googleMaps}>\n                        {MapComponent}\n                    </section>\n                </div>\n            </section>\n        );\n    }\n}\n\nexport default EventMap;\n"]}},"_buildHash":"6ece08ffa500e53d90b0c2f7d3aee6da","hash":"487bf0a8b084a19500449ea06f297719","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[110,137],"loc":{"start":{"line":7,"column":19},"end":{"line":7,"column":55}}},{"type":"RequireHeaderDependency","range":[102,109],"loc":{"start":{"line":7,"column":19},"e