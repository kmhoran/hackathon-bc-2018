LI;AAMlBgE,iBAAa,IANK;AAOlBP,uBAAmB,KAPD;AAQlBnF,qBAAiB,KARC;AASlBW,uBAAmB,KATD;AAUlBE,yBAAqB,KAVH;AAWlBkB,mBAAe,IAXG;AAYlBJ,mBAAe,KAZG;AAalBC,eAAW,IAbO;AAclBC,cAAU,KAdQ;AAelBC,WAAO,UAfW;AAgBlBE,gBAAY,IAhBM;AAiBlBC,cAAU;AAjBQ,C;kBA6OXpC,W","file":"ProductItem.jsx","sourceRoot":"/Users/millie.savalia/react-node-frontend","sourcesContent":["import React, { Component } from 'react';\nimport { PropTypes as pt } from 'prop-types';\nimport withStyles from 'isomorphic-style-loader/lib/withStyles';\nimport s from './ProductItem.scss';\nimport cx from 'classnames';\nimport { observer, inject } from 'mobx-react';\nimport { head } from 'lodash';\nimport { SIZE_EXTRA_SMALL } from 'Middleware/constants/icon';\nimport { getThisVariantImage, getVariantIdFromImage, getProductImages, getVariantKey } from 'Middleware/services/products';\nimport { MEDIA_MOBILE, MEDIA_LAPTOP, MEDIA_PHONE } from 'Middleware/constants/media';\nimport { getNativeProductUrl } from 'Middleware/utils/url';\n\nimport ProductTags from 'Components/Product/ProductTags';\nimport ProductSwatches from 'Components/Product/ProductSwatches';\nimport ProductPrice from 'Components/Product/ProductPrice';\nimport ProductRating from 'Components/Product/ProductRating';\nimport ProductAddButton from 'Components/Product/ProductAddButton';\n\nimport Image from 'Common/Image';\nimport Link from 'Common/Link';\nimport Eye from 'Icons/Eye.jsx';\nimport SwipeableViews from 'Common/SwipeableViews';\n\n@withStyles(s)\n@inject(\n    'categoryStore',\n    'contentStore',\n    'interfaceStore'\n)\n@observer class ProductItem extends Component {\n\n    static propTypes = {\n        // product: pt.object.isRequired, //TODO: empty queries will respond as a bool. so this will error.\n        onClick: pt.func,\n        row: pt.number,\n        showQuicklook: pt.bool,\n        showTags: pt.bool,\n        showSwatches: pt.bool,\n        infoPadding: pt.bool,\n        useAltMobileStyle: pt.bool,\n        showAddToCart: pt.bool,\n        className: pt.string,\n        markFree: pt.bool, //mark an item as being free for purchase.\n        theme: pt.string,\n        buttonText: pt.string,\n        disabled: pt.bool,\n    };\n\n    static defaultProps = {\n        product: {},\n        row: 1,\n        showQuicklook: false,\n        showTags: true,\n        showSwatches: true,\n        infoPadding: true,\n        useAltMobileStyle: false,\n        selectedVariant: false,\n        selectedVariantId: false,\n        selectedVariantName: false,\n        viewportWidth: 1280,\n        showAddToCart: false,\n        className: null,\n        markFree: false,\n        theme: 'standard',\n        buttonText: null,\n        disabled: false,\n    };\n\n    state = {\n        activeImageIndex: 0,\n        selectedVariant: this.props.selectedVariant || false\n    }\n\n    openQuicklook = (product, row, variant) => {\n        this.props.categoryStore.toggleQuicklook({product, row, variant});\n    }\n\n    selectedVariant = (color) => {\n        this.setState({\n            selectedVariant: color.name,\n            selectedVariantId: color.id,\n            selectedVariantName: color.fullName}\n        );\n    }\n\n    handleSwipeImageChange = index => {\n        const\n            product = this.props.product,\n            images = getProductImages(product, index);\n\n        this.setState({\n            selectedVariant: images[index].variantId,\n            selectedVariantId: images[index].variantId,\n            activeImageIndex:  index,\n            selectedIndex: index\n        });\n    };\n\n    render() {\n\n        const { product, contentStore: { content: { shop } }, interfaceStore, showQuicklook, showTags, showSwatches, showAddToCart, className, markFree, theme, viewportWidth, buttonText, disabled } = this.props;\n\n        const { selectedVariant } = this.state;\n\n        if (!product || typeof product.id === \"undefined\") return false;\n\n        let\n            images = [],\n            staticImages = [],\n            hasSwatches,\n            selected,\n            hasMultipleImages = false,\n            isDesktop = viewportWidth > MEDIA_MOBILE,\n            variantLink = \"\",\n            selectedProduct = product;\n        const isPhone = viewportWidth <= MEDIA_PHONE;\n\n        //theme sets\n        const selectorProduct = theme === 'selectorProduct' ? true : false;\n\n        if (product.variants.length > 1) {\n            hasMultipleImages = true;\n            hasSwatches = true;\n\n            if (selectedVariant) {\n                images = getProductImages(product, selectedVariant);\n                staticImages = getThisVariantImage(product, selectedVariant);\n                variantLink = selectedVariant;\n                selectedProduct = getVariantKey(selectedVariant, product);\n            }\n            else {\n                selected = this.props.selectedVariant || product.variants[0].id;\n                selectedProduct = getVariantKey(selected, product);\n                images = getProductImages(product, selected);\n                staticImages = images;\n                if (viewportWidth < MEDIA_LAPTOP)\n                    this.state.selectedVariant = selected;\n            }\n        }\n        else\n            staticImages[0] = product.images ? product.images[0] : null;\n\n        const productURL = getNativeProductUrl(product.handle, variantLink);\n        const hoverImage = (interfaceStore.appIsLoaded && !interfaceStore.isMobile && selectedProduct.images && selectedProduct.images.length > 1) ? selectedProduct.images[1] : false;\n\n        function renderExtra() {\n            return (\n                <div key={product.id} className={s.itemDetails}>\n                <ProductRating\n                    productId={product.id}\n                    hideCount  />\n                <div className={s.addContainer}>\n                    <ProductAddButton\n                        product={product}\n                        variant={selectedProduct}\n                        smallSize\n                        className={cx(\n                            {[s.button_sp1]: selectorProduct}\n                        )}\n                        dark={true}\n                        disabled={disabled}\n                    >\n                        {buttonText}\n                    </ProductAddButton>\n                    <Link underlined\n                    to={productURL}\n                    wrapperClassName={cx(\n                        {[s.details_sp1]: selectorProduct}\n                    )}\n                    >\n                        {shop[0].detailsLink}\n                    </Link>\n                </div>\n            </div>\n            )\n        }\n\n        return (\n                <div key={`product-item-${product.id}`} className={cx(s.root, className)}>\n                        <div className={cx(s.imageContainer, !showSwatches && s.imageContainerNoSwatches)}>\n                            {showTags &&\n                                <div>\n                                    <div\n                                        className={s.tagContainer}>\n                                            <ProductTags product={product} />\n                                    </div>\n                                    <div className={s.tagContainerMobile}>\n                                        <ProductTags product={product} />\n                                    </div>\n                                </div>\n                            }\n                            {showQuicklook && !product.bundledOptions &&\n                                <div\n                                    className={cx(s.quicklookButtonContainer)}\n                                    onClick={() => this.openQuicklook(product.handle, this.props.row, selectedVariant)}>\n                                        <div className={s.quicklookButton}>\n                                            <Eye\n                                     