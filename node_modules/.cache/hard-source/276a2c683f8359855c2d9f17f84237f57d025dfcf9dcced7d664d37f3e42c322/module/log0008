from 'react-slick';\n\n@withStyles(s)\n@inject(\n    'interfaceStore',\n)\n@observer class CounterCarousel extends Component {\n\n    state = {\n        currentSlideIndex: 0,\n        appIsMounted: false,\n    }\n    \n    componentDidMount() {\n        __BROWSER__ && this.setState({ appIsMounted: true });\n\n        this.scrollListener = throttle(this.handleScroll, 150);\n        this.touchListener = throttle(this.handleScroll, 200);\n        window.addEventListener('scroll', this.scrollListener, { passive: true });\n        window.addEventListener(\"touchmove\", this.touchListener, { passive: true });\n\n        this.disposer = autorun(() => {\n            if (this.props.interfaceStore.isMobile || this.props.interfaceStore.isComputer)\n                requestAnimationFrame(() => this.goToSlide(0, false));\n        })\n    }\n\n    componentWillUnmount() {\n        window.removeEventListener('scroll', this.scrollListener);\n        window.removeEventListener(\"touchmove\", this.touchListener);\n        this.disposer();\n    }\n\n    handleScroll = () => {\n        const { counterSlides } = this.props;\n        const { isMobile } = this.props.interfaceStore;\n\n        if (!this.refs.container || isMobile) return false;\n\n        this.height = this.refs.container.offsetHeight;\n        this.startingPos = this.refs.container.getBoundingClientRect().top;\n\n        let total = 0;\n        for (let i = 0; i < counterSlides.length; i++) {\n            if (this.slideInPosition(i)) {\n                this.setState({ currentSlideIndex: i });\n                break;\n            }\n        }\n    }\n\n    slideInPosition = (index) => {\n        const { counterSlides } = this.props;\n\n        return window.pageYOffset <= this.calculateSlideLocation(index);\n    }\n\n    calculateSlideLocation = (index) => {\n        const { counterSlides } = this.props;\n\n        return this.startingPos + window.pageYOffset + this.height / counterSlides.length * index + window.innerHeight / 2;\n    }\n\n    goToSlide = (key, scroll = true) => {\n        const {\n            isMobile,\n            isComputer\n        } = this.props.interfaceStore;\n\n        this.setState({ currentSlideIndex: key });\n\n        if (isComputer && scroll)\n            window.scrollTo(0, this.calculateSlideLocation(key));\n\n        if (this.refs.slider && isMobile)\n            this.refs.slider.slickGoTo(key);\n    }\n\n    nextSlide = () => {\n        this.goToSlide(this.state.currentSlideIndex + 1);\n    }\n\n    prevSlide = () => {\n        this.goToSlide(this.state.currentSlideIndex - 1);\n    }\n\n    renderCarousel() {\n        const {\n            counterSlides,\n            contentFooter,\n        } = this.props;\n\n        const { isComputer } = this.props.interfaceStore;\n\n        const { appIsMounted } = this.state;\n\n        if (appIsMounted && isComputer) return false;\n\n        const settings = {\n            infinite: false,\n            speed: 350,\n            arrows: false,\n            slidesToShow: 1,\n            slidesToScroll: 1,\n            swipe: true,\n            swipeToSlide: true,\n            touchMove: true,\n            initialSlide: 0,\n            lazyLoad: false,\n        };\n\n        return (\n            <React.Fragment>\n                {this.renderHeading()}\n                {appIsMounted && <div className={s.sliderWrapper}>\n                    <Slider ref='slider' {...settings} afterChange={(i) => this.goToSlide(i)}>\n                        {this.renderContent()}\n                    </Slider>\n                </div>}\n            </React.Fragment>\n        );\n    }\n\n    renderContent() {\n        const { counterSlides } = this.props;\n        const { currentSlideIndex } = this.state;\n\n        return counterSlides.map((slide, index) => (\n            <div key={index} className={cx([\n                s.slideWrapper,\n                index == currentSlideIndex - 1 && s.prevSlide,\n                index < currentSlideIndex - 1 && s.prevSlides,\n                index == currentSlideIndex + 1 && s.nextSlide,\n                index > currentSlideIndex + 1 && s.nextSlides,\n            ])}>\n                <div className={s.slideRoot} style={{\n                    opacity: index === currentSlideIndex ? 1 : 0,\n                    transform: index === currentSlideIndex ?\n                        'scale(1) translateY(0)' : 'scale(0.92) translateY(0)',\n                    zIndex: currentSlideIndex === index ? 3 : -1,\n                }}>\n                    <div className={s.content}>\n                        <ReactMarkdown\n                            className={s.contentBody}\n                            source={slide.fields.bodyText} \n                        />\n                        {this.renderDashes()}\n                    </div>\n                    <div className={s.slide} style={{ backgroundImage: `url(${getSafe(() => slide.fields.slideAsset.fields.file.url)})` }} />\n                </div>\n            </div>\n        ))\n    }\n\n    renderDashes() {\n        const { counterSlides } = this.props;\n\n        return (\n            <ul className={s.dashContainer}>\n                {counterSlides.map((dash, key) => (\n                    <li\n                        onClick={() => this.goToSlide(key)}\n                        key={key}\n                        className={cx(s.dash, this.state.currentSlideIndex === key && s.dashActive)} />\n                ))}\n            </ul>\n        );\n    }\n\n    renderHeading() {\n        return (\n            <div className={s.headerMarkdown}>\n                <ReactMarkdown source={this.props.heading} />\n            </div>\n        )\n    }\n\n    _testScroll() {\n        console.log(this.refs.container);\n        this.scrollTarget.scrollIntoView({ block: 'start', behavior: 'smooth' });\n    }\n\n    render() {\n        const {\n            counterSlides,\n            contentFooter,\n            key,\n            scrollText,\n        } = this.props;\n        const { isMobile } = this.props.interfaceStore;\n        const {\n            currentSlideIndex,\n            appIsMounted\n        } = this.state;\n\n        if (!counterSlides && counterSlides.length) return false;\n\n        const scrollableSlightHeight = 750;\n        const id = 'counter-carousel' + 1;\n\n        return (\n            <React.Fragment>\n                <div\n                    className={s.root}\n                    style={{\n                        height: appIsMounted && !isMobile ? counterSlides.length * scrollableSlightHeight + 'px' : '100%'\n                    }}\n                    id={id}\n                    ref='container'>\n                    <div className={s.desktopContainer}>\n                        <Sticky\n                            topBoundary={`#${id}`}\n                            bottomBoundary={`#${id}`}\n                            activeClass={s.sticky}>\n                            <div className={s.slideContainer}>\n                                {this.renderHeading()}\n                                {this.renderContent()}\n                                <div onClick={this.prevSlide} className={cx([\n                                    s.iconLeft,\n                                    currentSlideIndex > 0 && s.arrowActive\n                                ])}>\n                                    <IconLeft size={{ width: 30, height: 30 }} />\n                                </div>\n                                <div onClick={this.nextSlide} className={cx([\n                                    s.iconRight,\n                                    currentSlideIndex < counterSlides.length - 1 && s.arrowActive\n                                ])}>\n                                    <IconRight size={{ width: 30, height: 30 }} />\n                                </div>\n                                <div onClick={this._testScroll.bind(this)} className={cx([\n                                    s.iconDown,\n                                ])}>\n                                    <span className={s.downWrapper}>\n                                        <h4> {scrollText} </h4>\n                                        <IconRight className={s.rotate} size={{ width: 30, height: 30 }} />\n                                    </span>\n                                </div>\n                            </div>\n                        </Sticky>\n                    </div>\n                    {this.renderCarousel()}\n                </div>\n\n                <div className={s.scrollTarget} ref={scrollTarget => this.scrollTarget = scrollTarget}></div>\n            </React.Fragment>\n        );\n    }\n}\n\nexport default CounterCarousel;"]}},"_buildHash":"18302e08808507b1988692fe7ac9de1e","hash":"635a825e310efce5134ed7366b0105c9","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/extends","range":[107,138],"loc":{"start":{"line":7,"column":16},"end":{"line":7,"column":56}}},{"type":"RequireHeaderDependency","range":[99,106],"loc":{"start":{"line":7,"column":16},"end":{"line":7,"column":56}}},{"type":"RequireHeaderDependency","range":[216,223],"loc":{"start":{"line":11,"column":22},"end":{"line":11,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-prototype-of","range":[224,271],"loc":{"start":{"line":11,"column":22},"end":{"line":11,"column":78}}},{"type":"RequireHeaderDependency","range":[363,370],"loc":{"start":{"line":15,"column":23},"end":{"line":15,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/classCallCheck","range":[371,409],"loc":{"start":{"line":15,"column":23},"end":{"line":15,"column":70}}},{"type":"CommonJsRequireDep