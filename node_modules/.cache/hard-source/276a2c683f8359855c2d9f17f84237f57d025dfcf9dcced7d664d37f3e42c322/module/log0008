uctsStore',\n    'categoryStore',\n    'contentStore',\n    'navigationStore',\n)\n@observer class SearchBar extends Component {\n    \n    constructor(props) {\n        super(props);\n        this.filteredProducts = [];\n    }\n\n    componentDidMount() {\n        this.searchInput.focus(); \n    }\n\n    componentWillUnmount() {\n        this.searchInput.blur(); \n    }\n\n    state = {\n        value: \"\",\n        totalResults: 0,\n        categoryResults: true\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.content.header[0]);\n    }\n\n    onKeyDown = (event) => {\n        if (event.key === 'Enter' && this.state.value.length) {\n\n            if(this.state.totalResults === 1)\n                this.props.navigationStore.to({ url: this.props.productsStore.getProductLink(this.filteredProducts[0].id) });\n            else\n                this.props.navigationStore.to({ url: `${routes.PRODUCTS}?search=${this.state.value}` }) ;\n        }\n    }\n\n    handleClickOutside = event => {\n        this.props.onCloseSearch && this.props.onCloseSearch(event);\n    };\n\n    handleCloseSearch = event => {\n        this.props.onCloseSearch && this.props.onCloseSearch(event);\n    };\n\n    handleLinkClicked= event => {\n        this.props.onLinkClicked && this.props.onLinkClicked(event);\n    };\n\n    handleSearchChange = event => {\n        this.setState({\n            value: event.target.value.toLowerCase()\n        });\n    };\n\n    renderCategories(entry, render) {\n        return (\n            <div>\n                {render}\n                {this.state.totalResults > 0 && <div className={s.viewAllWrapper}>\n                        <br />\n                        {this.props.interfaceStore.isComputer ?\n                        this.state.categoryResults &&\n                        <Link\n                            to={`${routes.PRODUCTS}?search=${entry}`} \n                            className={s.viewAll} \n                            onClick={this.handleLinkClicked}\n                            key={5}>\n                            {this.config.viewAllProducts.replace('%s', this.state.totalResults)}<br />\n                        </Link> :\n                        <Link\n                            to={`${routes.PRODUCTS}?search=${entry}`} \n                            className={s.viewAll} \n                            onClick={this.handleLinkClicked}\n                            key={5}>\n                            {this.config.viewAllProducts.replace('%s', this.state.totalResults)}<br />\n                        </Link>}\n                    </div>\n                }\n            </div>\n        );\n    }\n\n    getCategories (entry, limit) {\n        const { uniqueCategoryLinksById } = this.props.categoryStore;\n\n        const releventKeywordIDs = Object.keys(uniqueCategoryLinksById)\n            .filter(keyword => \n                searchList(entry, uniqueCategoryLinksById[keyword].name)\n            );\n\n        let results = 0;\n        const render = [];\n\n        const eachKeyword = releventKeywordIDs.map((keywordID, key) => {\n            if (results < limit) {\n                results++;\n\n                render.push(\n                    <div key={keywordID}>\n                    <Link\n                        to={uniqueCategoryLinksById[keywordID].to}\n                        name={uniqueCategoryLinksById[keywordID].name}\n                        className={s.keyword}\n                        onClick={this.handleLinkClicked}\n                        key={key}\n                    />\n                    </div>)\n            }\n        });\n\n        if (!results) {\n            render.push(<div key={\"no-results\"} className={s.noResults}>{this.config.noResults}</div>);\n            this.state.categoryResults = false;\n        } else {\n            this.state.categoryResults = true;\n        }\n        return render;\n    }\n\n    renderProducts(entry, productItems) {\n        return this.state.totalResults ?\n        <div>\n            <div className={s.list}>\n                {productItems}\n            </div>\n            {this.props.interfaceStore.isComputer && \n            <div style={this.state.categoryResults ? { display: 'none' } : { marginTop: '20px' }}>\n                <Link\n                    to={`${routes.PRODUCTS}?search=${entry}`} \n                    className={s.viewAll} \n                    onClick={this.handleLinkClicked}\n                    key={5}>\n                    {this.config.viewAllProducts.replace('%s', this.state.totalResults)}<br />\n                </Link>\n            </div>}\n        </div> :\n        <span>{this.config.noResultsProducts}</span>\n    }\n\n    getProducts(entry, limit) {\n        const { products, searchProducts } = this.props.productsStore;\n        \n        this.filteredProducts = searchForProducts(products, entry);\n        \n        let results = 0;\n        let total = 0;\n\n        const productItems = this.filteredProducts.map(product => {\n                if(results < limit) {\n                    results++;\n                    total++;\n                    return ( \n                        <SearchProductPreview\n                            onClick={this.handleLinkClicked}\n                            key={product.id}\n                            product={product}\n                            onLinkClicked={this.handleLinkClicked}\n                        />\n                    )\n                }\n                else\n                    total++;\n        });\n\n        this.state.totalResults = results ? total : 0;\n        return productItems;\n    }\n\n    renderSearchResults () {\n        const entry = this.state.value;\n        const productItems = this.getProducts(entry, 3);\n        const render = this.getCategories(entry, 5);\n\n        return (\n            <div className={s.resultsContainer}>\n                <div className={s.resultsContent}>\n                   <div className={cx(s.resultsColumn, s.resultsProducts)} >\n                            <h5 className={s.resultsHeading}>Products</h5>\n                            {this.renderProducts(entry, productItems)}\n                            \n                    </div>\n                    <div className={cx(s.resultsColumn, s.resultsCategory)} >\n                            <h5 className={s.resultsHeading}>Categories</h5>\n                            {this.renderCategories(entry, render)}\n                    </div>\n                </div>\n            </div>\n        )\n    }\n\n    render() {\n        return (\n            <ClickOutHandler onClickOut={this.handleClickOutside}>\n            <div className={s.root}>\n                <input \n                    ref={(input) => { this.searchInput = input; }} \n                    className={s.input}\n                    placeholder={this.config.searchbarPlaceholder}\n                    onChange={this.handleSearchChange.bind(this)}\n                    value={this.state.value}\n                    onKeyDown={this.onKeyDown}\n                />\n                 <div className={s.closeBox} onClick={this.handleCloseSearch}>\n                        <Trashcan\n                            size={{width: 13, height: 13}}\n                            className={s.closeIcon} \n                            color={COLOR_WHITE}\n                        />\n                </div>\n            </div>\n            {this.state.value && this.renderSearchResults()}\n            </ClickOutHandler>\n        )\n    }\n}\n\nexport default SearchBar;\n"]}},"_buildHash":"b024c3f63bc78b163f94d79b862dfd59","hash":"8634a40decdd15686175446fde28e7ec","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request