d) {\n            this.updateQuantity(1);\n        }\n    }\n\n    componentWillUnmount() {\n        this.disposer();\n    }\n\n    @computed get variant() {\n        this.props.productsStore.productsById;\n        return this.props.productsStore.variantById(this.props.id);\n    }\n\n    @computed get inStock() {\n        this.props.productsStore.productsById;\n        return this.variant && this.variant.inStock;\n    }\n\n    @computed get maxQuantity() {\n        this.props.cartStore.productsById;\n        const quantityInCart = getSafe(() => this.props.cartStore.getLatestProductAttributes(id).maxQuantity) || 0;\n        return this.variant ? this.variant.maxQuantity - quantityInCart : 0;\n    }\n\n    handleIncreaseCount = () => {\n        if (this.state.quantity + 1 > this.maxQuantity)\n            this.updateQuantity(this.maxQuantity);\n\n        else if (this.state.quantity+1 <= this.maxQuantity && this.inStock) {\n            this.updateQuantity(this.state.quantity+1);\n        }\n    };\n\n    handleDecreaseCount = () => {\n        if (this.state.quantity > 1 && this.inStock) {\n            this.updateQuantity(this.state.quantity-1);\n        }\n    };\n\n    handleSelectChange = (event) => {\n        const quantity = parseInt(event.target.value);\n        this.updateQuantity(quantity);\n    };\n\n    handleInputChange = (event) => {\n        const value = parseInt(event.target.value);\n        if (value < 1)\n            this.updateQuantity(1);\n\n        else if (value > this.maxQuantity)\n            this.updateQuantity(this.maxQuantity);\n\n        else if (value > 1 && (value <= this.maxQuantity && this.inStock)) {\n            this.updateQuantity(value);\n        }\n    };\n\n    handleBlur = event => {\n        const value = parseInt(event.target.value);\n        if(value < 1 || !value)\n            this.updateQuantity(1);\n    }\n\n    updateQuantity(quantity) {\n        this.setState({quantity});\n        if (this.quantityInput) this.quantityInput.value = quantity;\n        this.props.onQuantityChange && this.props.onQuantityChange(quantity);\n    }\n\n    render() {\n        const isAvailable = this.maxQuantity !== 0 && this.inStock != false;\n        const options = 8;\n\n        return (\n            <div className={cx(\n                    s.root, \n                    this.props.thin && s.thin,\n                    this.props.className, \n                    !isAvailable && s.disabled\n                )}>\n                <div className={s.controls}>\n                        <div\n                            className={s.control}\n                            disabled={this.state.quantity === 1}\n                            onClick={this.handleDecreaseCount}>\n                                <div>&ndash;</div>\n                        </div>\n                    <div className={s.quantity}>\n                        <input \n                            defaultValue={isAvailable ? this.state.quantity : 0}\n                            ref={(input) => { this.quantityInput = input; }}\n                            onChange={this.handleInputChange.bind(this)}\n                            onBlur={this.handleBlur.bind(this)}\n                            onClick={() => this.quantityInput.select()}\n                            pattern=\"[0-9]\"\n                        >\n                        </input>\n                    </div>\n                        <div\n                            className={s.control}\n                            onClick={this.handleIncreaseCount}>\n                                <div>+</div>\n                        </div>\n                </div>\n                <div className={s.mobileSelectContainer}>  \n                    <select \n                        aria-label=\"Quantity\"\n                        name=\"quantity\" \n                        className={s.mobileSelect} \n                        onChange={this.handleSelectChange.bind(this)} value={this.state.quantity}>\n                        {isAvailable ? \n                            Array.apply(null, Array(options)).map((item, index) => (\n                                <option \n                                    key={index+1}\n                                    value={index+1}>\n                                        {index+1}\n                                </option>\n                            ))\n                            :\n                            <option \n                                value={0}>\n                                    {0}\n                            </option>\n                        }\n                    </select>\n                </div>\n            </div>\n        );\n    }\n}\n\nexport default ProductQuantity;\n"]}},"_buildHash":"650e1cb78b973a4de31bfb56a31bfadb","hash":"891ea4a3cfe8360ff4b2997b1b0d7ec2","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[292,299],"loc":{"start":{"line":11,"column":22},"end":{"line":11,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-prototype-of","range":[300,347],"loc":{"start":{"line":11,"column":22},"end":{"line":11,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/classCallCheck","range":[447,485],"loc":{"start":{"line":15,"column":23},"end":{"line":15,"column":70}}},{"type":"RequireHeaderDependency","range":[439,446],"loc":{"start":{"line":15,"column":23},"end":{"line":15,"column":70}}},{"type"