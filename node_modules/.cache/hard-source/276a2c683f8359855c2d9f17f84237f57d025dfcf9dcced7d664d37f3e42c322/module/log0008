rt { PropTypes as pt } from 'prop-types';\nimport { COLOR_BLACK } from 'Middleware/constants/colors';\nimport s from './ProductView.scss';\nimport cx from 'classnames';\nimport withStyles from 'isomorphic-style-loader/lib/withStyles';\nimport { observer, inject } from 'mobx-react';\nimport striptags from 'striptags';\n\nimport { getThisVariantImage, getVariantKey } from 'Middleware/services/products';\nimport { getNativeProductUrl } from 'Middleware/utils/url';\nimport { SIZE_EXTRA_SMALL } from 'Middleware/constants/icon';\nimport { COLOR_WHITE } from 'Middleware/constants/colors';\n\nimport Row from 'Common/Row';\nimport Link from 'Common/Link';\nimport Button from 'Common/Button';\nimport Separator from 'Common/Separator';\nimport Trashcan from 'Icons/Trashcan.jsx';\n\nimport ProductPrice from 'Components/Product/ProductPrice';\nimport ProductQuantity from 'Components/Product/ProductQuantity';\nimport ProductSwatches from 'Components/Product/ProductSwatches';\nimport ProductTags from 'Components/Product/ProductTags';\nimport ProductRating from 'Components/Product/ProductRating';\nimport ProductAddButton from 'Components/Product/ProductAddButton';\nimport ProductImageViewer from 'Components/Product/ProductImageViewer';\n\n@withStyles(s)\n@inject(\n    'categoryStore', \n    'contentStore',\n    'cartStore'\n)\n@observer\nclass ProductView extends Component {\n\n    static propTypes = {\n        product: pt.object,\n        showClose: pt.bool,\n        disableAddToCart: pt.bool\n    };\n\n    static defaultProps = {\n        selectedVariant: 0,\n        showClose: true,\n        disableAddToCart: false,\n    }\n\n    state = {\n        quantity: 1,\n    }\n\n    componentWillReceiveProps(nextProps) {\n        if (this.props.selectedVariant !== nextProps.selectedVariant) {\n            this.setState({\n                selectedVariant: false, \n                selectedVariantId: false, \n                selectedVariantName: false\n            });\n        }\n    }\n\n    shouldComponentUpdate(nextProps, nextState) {\n        if (nextProps.product == this.props.product && this.props.selectedVariant == nextProps.selectedVariant && this.state.selectedVariant == nextState.selectedVariant && this.state.quantity == nextState.quantity)\n          return false;\n        return true;\n    }\n\n    handleQuantityChange = (num) => {\n        this.setState({quantity: num});\n    }\n\n    close = () => {\n        this.props.categoryStore.toggleQuicklook({});\n    }\n\n    selectedVariant = (variant) => {\n        this.setState({\n            selectedVariant: variant.name, \n            selectedVariantId: variant.id, \n            selectedVariantName: variant.fullName, \n            activeVariant: variant.id\n        });\n    }\n\n    render() {\n        const { product, disableAddToCart, contentStore: { content: { shop } } } = this.props;\n\n        let descriptionLength = 340;\n        let variantLink = '';\n\n        let selectedVariant;\n        \n        if (!this.props.selectedVariant && typeof product.variants[0].swatches != \"undefined\") {\n            selectedVariant = product.variants[0].id;\n        }\n        else selectedVariant = this.props.selectedVariant;\n\n        //TODO: state should not be written in render.\n        if (!this.state.selectedVariant) this.state.selectedVariant = selectedVariant;\n\n        if (!product) return null;\n        \n        const images = getThisVariantImage(product, this.state.selectedVariant);\n        const selectedProduct = getVariantKey(this.state.selectedVariant, product);\n        \n        if (product.variants.length > 1) {\n            descriptionLength = 290;\n            variantLink = this.state.selectedVariant;\n        }\n        if(!this.props.showClose)\n            descriptionLength = 520;\n\n        const description = product.description.length > 200 ? striptags(product.description.substring(0,descriptionLength), \"<style>\")+ \"...\" : product.description;\n        const detailLink = getNativeProductUrl(product.handle, variantLink);\n\n        // Use a named function instead of const component = () =>\n        // to prevent unneeded DOM re-renders\n        function renderTopSection() {\n            return (\n            <React.Fragment>\n                <Link to={detailLink}>\n                    <div className={s.title}>\n                        {product.title}\n                    </div>\n                </Link>\n                <div className={s.skuContainer}>\n                    <div className={cx(s.sizeLabel, (selectedProduct.productVolume || product.productVolume) && s.size)}>\n                        {selectedProduct.productVolume || product.productVolume}\n                    </div>\n                    <div>\n                        Item #{selectedProduct ? selectedProduct.sku : product.sku}\n                    </div>\n                </div>\n                <div className={s.sku}>\n                    {product.summary}\n                </div>\n                <div className={s.reviewContainer}>\n                    <Link to={detailLink}>\n                    <ProductRating\n                        key={product.id}\n                        productId={product.id} />\n                    </Link>\n                </div>\n                <ProductPrice\n                    className={s.price}\n                    amount={selectedProduct.price}\n                    compareAtPrice={selectedProduct.compareAtPrice}\n                    sku={selectedProduct.sku}\n                />\n            </React.Fragment>\n        )};\n\n        return (\n            <div className={s.root}>\n                <div className={s.tabletHeading}>\n                    {renderTopSection()}\n                </div>\n                <div className={s.leftColumn}>\n                    <div className={s.tagContainer}>\n                        <ProductTags product={product} />\n                    </div>\n                    <ProductImageViewer \n                        className={s.viewerSmall} \n                        centered \n                        images={images} \n                        product={product.title}\n                        arrows={true} \n                        activeVariant={this.state.selectedVariantId} />\n                </div>\n                <div className={s.info}>\n                    <div className={s.heading}>\n                        {renderTopSection()}\n                    </div>\n                    {product.variants.length > 1 && \n                        <div>\n                            <Separator className={s.margin} />\n                                <div className={s.swatchContainer}>\n                                    <ProductSwatches \n                                        label=\"Color\" \n                                        noURLChange \n                                        large \n                                        compactMobile\n                                        product={product} \n                                        selectedVariant={this.selectedVariant} \n                                        defaultVariant={this.state.selectedVariant} />\n                                </div>\n                            <Separator className={s.margin} />\n                        </div>\n                    }\n                    <div \n                        className={cx(s.description, product.variants.length <= 1 && s.longDescription)} \n                        dangerouslySetInnerHTML={{__html: description}}>\n                    </div>\n                    <div className={s.buttonContainer}>\n                        {!disableAddToCart && \n                            <div className={s.quantityContainer}>\n                                <ProductQuantity  \n                                    stock={selectedProduct ? selectedProduct.inStock : product.inStock}\n                                    onQuantityChange={this.handleQuantityChange.bind(this)} \n                                    className={cx(s.button)}\n                                    id={selectedProduct ? selectedProduct.id : product.id} />\n                            </div>\n                        }\n                        {product.type !== 'bundle' && \n                        !disableAddToCart && \n                            <ProductAddButton \n                                product={product}\n                                quantity={this.state.quantity}\n                                selectedVariant={this.state.selectedVariant}\n                                variant={selectedProduct}\n                                mediumSize={true}\n                            />\n                        }\n                        <div className={s.detailButton}>\n                            <Button \n                                color={COLOR_WHITE} \n                                className={s.button}\n                                to={detailLink}>\n                                    {shop[0].detailsLink}\n                            </Button>\n                            <Link\n                                to={detailLink}\n                                underlined\n                                className={s.phoneDetailLink}>\n                                    {shop[0].detailsLink}\n                            </Link>\n                        </div>\n                    </div>\n                </div>\n                {this.props.showClose && <div className={s.closeIconContainer}>\n                    <Trashcan\n                        size={SIZE_EXTRA_SMALL}\n                        className={s.closeIcon} \n                        color={s.closeIconColor}\n                        onClick={this.close.bind(this)}\n                    />\n                </div>}\n            </div>\n        );\n    }\n}\n\nexport default ProductView;\n"]}},"_buildHash":"6e2a44ad4cb54403b64e4abb00464171","hash":"eb104c2aa64a3a77a1e9028560ace85d","_las