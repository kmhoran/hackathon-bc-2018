g,\n        viewportWidth: pt.number,\n        sliderSettings: pt.object,\n        contentStore: pt.object.isRequired,\n        promoStore: pt.object.isRequired,\n        promoId: pt.string,\n    };\n\n    static defaultProps = {\n        sliderSettings: {\n            prevArrow: <Arrows CSSOverrides={s.arrowSymbol} left spaced />,\n            nextArrow: <Arrows CSSOverrides={s.arrowSymbol} right spaced />,\n            speed: 500,\n            // dotsClass: cx(s.slickDots, 'slick-dots'),\n            customPaging: (() => {\n                return <span><a></a></span>\n            }),\n            swipe: true,\n            swipeToSlide: true,\n            touchMove: false,\n            initialSlide: 0,\n        },\n    };\n\n    state = {\n        appIsMounted: false\n    }\n\n    componentDidMount() {\n        requestAnimationFrame(() => {\n            this.setState({ appIsMounted: true });\n        });\n    }\n\n    shouldComponentUpdate(nextProps, nextState) {\n        const { viewportWidth } = this.props;\n    //check for media changes\n        let newMedia = '';\n        if (viewportWidth !== nextProps.viewportWidth) {\n            if (viewportWidth <= MEDIA_PHONE + 10) newMedia = 'isPhone';\n            else if (viewportWidth >= MEDIA_DESKTOP - 10) newMedia = 'isDesktop';\n            else newMedia = 'isTablet';\n            if (newMedia !== this.currentMedia) {\n                this.currentMedia = newMedia;\n                return true;\n            }\n            return false;\n        }\n        return true;\n     }\n\n    render() {\n        const { heading, subheading, promoId,\n            viewportWidth: viewportMode,\n            interfaceStore, promoStore,\n            productsStore: { productsById, productOrVariantById },\n            contentStore: { content: { shop } },\n        } = this.props;\n        let { isGWPActive, currentGWPChoices: ids } = promoStore;\n        const { isPhone, isComputer: isDesktop } = interfaceStore;\n        const { buttonViewProduct: buttonText, buttonSelect } = shop[0];\n        const PhoneSize = ({ children }) => {\n            return viewportMode <= MEDIA_PHONE ? children : null;\n        };\n        let { sliderSettings } = this.props;\n\n        const { appIsMounted } = this.state;\n\n// check to see if this can be populated with a disabled state.\n        if (!ids || !isGWPActive) {\n            try {\n                ids = toJS(promoStore.promosByPromoId[promoId].actions[0].gwpProducts).map(gwpProduct => gwpProduct.id);\n                if (ids.length < 1) return false;\n            } catch (e) {\n                __BROWSER__ && console.log('unable to display disabled promo state in selector');\n                return false;\n            }\n        }\n\n        const products = ids.map(id => productOrVariantById(id));\n\n        const isMoreThanOne = products.length > 1 ? true : false;\n        const isMoreThanTwo = products.length > 2 ? true : false;\n        const isLessThanTwo = products.length <= 1 ? true : false;\n\n        sliderSettings = {\n            ...sliderSettings,\n            dots: isMoreThanTwo,\n            infinite: false,\n            arrows: (() => {\n                if (isMoreThanOne && isPhone) return true;\n                else if (isMoreThanTwo) return true;\n                return false;\n            })(),\n            slidesToShow: (() => {\n                const gridSize = (() => {\n                    if (isPhone) return 1;\n                    if (isDesktop)\n                        if (products.length <= 4 ) return products.length;\n                        else return 4;\n                    return 2;\n                })();\n                return products.length > 1 ? gridSize : 1;\n            })(),\n            slidesToScroll: 1,\n        };\n\n//only one product\n        if (!products[0] || !products[0].title) return (\n            <Section innerClassName={s.root} contentClassName={s.content}>\n                Error: Cannot find product id {