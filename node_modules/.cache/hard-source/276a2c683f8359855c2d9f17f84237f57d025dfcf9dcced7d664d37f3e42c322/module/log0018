    } = this.config;\n\n        return (\n            <Container contentArea>\n                <div className={s.root}>\n                    <div className={s.closeIconContainer}>\n                        <Trashcan\n                            size={SIZE_EXTRA_SMALL}\n                            className={s.closeIcon}\n                            color={s.closeIconColor}\n                            onClick={() => this.handleGoBack()} />\n                    </div>\n                    <div className={s.wrapper}>\n                        <div className={s.title}>\n                            <h2>{signInHeading}</h2>\n                            <p>{signInInstructions}</p>\n                        </div>\n                        <Form\n                            items={getLoginItems(this, s, {\n                                emailLabel,\n                                passwordLabel,\n                            })}\n                            onSubmit={this.handleSubmit}\n                            errors={this.state.errors}\n                            showLabels={true}\n                            onChange={items => this.handleChange(items)}\n                            labelsPosition={'top'}\n                            largeInput={true}\n                            floatingLabel={true}\n                            noMargin={true}>\n                            <div className={s.forgotPasswordWrapper}>\n                                <Link\n                                    className={s.forgotPasswordLink}\n                                    to={routes.FORGOT_PASSWORD}>\n                                        {getSafe(() => forgotPasswordLink.fields.name)}\n                                </Link>\n                            </div>\n                            <div className={s.button}>\n                                <Button isSubmit wide>\n                                    {signInButton}\n                                </Button>\n                  