oducts, \"asc\");\n\n        const filteredCount = Object.keys(filteredProducts).length;\n        const productsCount = search ? filteredCategories.length : Object.keys(filteredCategories).length;\n\n        let\n            total = 1,\n            count = 0,\n            row = 1,\n            data = [],\n            key,\n            productSkus = [],\n            product,\n            productItems;\n\n        for (let i = 0, len = Object.keys(filteredProducts).length; i < len; i++) {\n                key = Object.keys(filteredProducts)[i];\n                product = filteredProducts[key];\n                count++;\n                total++;\n\n                if(count == columns+1) {\n                    count = 1;\n                    row++;\n                }\n\n                // Insert promotion into the grid\n                if(category && this.props.category.showPromo && this.props.category.promo) {\n                    if(total == 8 && Object.keys(filters).length < 1) {\n                        data.push (\n                            <LazyLoad\n                                key={Math.random()}\n                                offset={5}\n                                type={1}>\n                                <CatalogPromo\n                                    viewportWidth={this.state.viewportWidth}\n                                    {...this.props.category.promo.fields}\n                                />\n                            </LazyLoad>\n                        );\n                    }\n                }\n                if(count == 1) {\n                    data.push (\n                        <React.Fragment\n                            key={\"key-grid-row-\" + row} />\n                    )\n                }\n                if(isComputer && count == 1) {\n                    this.state.viewportWidth > MEDIA_MOBILE && data.push (\n                        <TransitionGroup key={\"quickview-root-\" + row}>\n                            {filteredProducts.map(prod => (\n                                (quicklook[row] && quicklook[prod.handle]) &&\n                                    <CSSTransition\n                                        key={\"quickview\" + row + prod.handle}\n                                        classNames={ {\n                                            appear: s.quicklookAnimationAppear,\n                                            appearActive: s.quicklookAnimationEnterAppear,\n                                            enter: s.quicklookAnimationEnter,\n                                            enterActive: s.quicklookAnimationEnterActive,\n                                            exit: s.quicklookAnimationExit,\n                                            exitActive: s.quicklookAnimationExitActive,\n                                        } }\n                                        timeout={{ enter: 800, appear: 0, exit: 400 }}>\n                                                    <div key={prod.handle + row} className={s.quicklook}>\n                                                        <ProductQuicklook\n                                                            slug={prod.handle}\n                                                            row={row}\n                                                            selectedVariant={quicklook.variant}\n                                                            product={prod}\n                                                        />\n                                                    </div>\n                                    </CSSTransition>\n                                )\n                            )}\n                        </TransitionGroup>\n                    );\n                }\n\n\n                productSkus.push(product.sku);\n\n                data.push(\n                    <div\n                        className={s.productItemContainer}\n                        key={`${row} ${product.id} product-container`}>\n                        <LazyLoad key={this.props.category} offset={10}>\n                                <ProductItem\n                                    key={`${product.id} product-item`}\n                                    onClick={this.handleProductClick}\n                                    product={product}\n                                    row={row}\n                                    viewportWidth={this.state.viewportWidth}\n                                    config={this.props.config}\n                                    showQuicklook={true}\n                                />\n                        </LazyLoad>\n                    </div>\n                )\n\n        };\n\n        productItems = data;\n\n        if (search) {\n            var retryLink = this.props.config.searchRetryMessage.split('{')[1].split('}')[0];\n            var retryMessage = this.props.config.searchRetryMessage.split('}')[1];\n        }\n\n        this.props.analyticsStore.trackTopProducts([productSkus[0], productSkus[1], productSkus[2]]);\n\n        return (\n            <div className={s.list} ref=\"grid\">\n                {search &&\n                    <div className={s.searchHeader}>\n                        {searchTotal ?\n                            <div className={s.searchHeading}>\n                                {this.props.config.youSearchedFor.replace('%s', search)}\n                            </div>\n                            :\n                            <React.Fragment>\n                                <div className={s.searchHeading}>           {this.props.config.noResultsFound.replace('%s', search)}\n                                </div>\n                                <div className={s.noSearchResults}>\n                                    <span className={s.retrySearch} onClick={() => this.props.interfaceStore.searchToggle(true)}>{retryLink}</span>{retryMessage}\n                                </div>\n                            <