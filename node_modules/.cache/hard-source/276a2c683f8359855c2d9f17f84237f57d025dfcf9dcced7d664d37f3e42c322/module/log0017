\n        type: pt.string,\n        defaultValue: pt.string,\n        labelPosition: pt.oneOf([\n            'left',\n            'top',\n            'right'\n        ]),\n        disabled: pt.bool,\n        editable: pt.bool,\n        autoFocus: pt.bool,\n        phone: pt.bool,\n        ssn: pt.bool,\n        icon: pt.oneOf([\n            pt.object,\n            pt.bool\n        ]),\n        tabindex: pt.string,\n        storeEntry: pt.object\n    };\n\n    static defaultProps = {\n        showLabel: false,\n        floatingLabel: true,\n        labelPosition: 'top',\n        wide: false,\n        frame: true,\n        textAlign: 'left',\n        required: false,\n        invalid: false,\n        large: false,\n        type: 'text',\n        disabled: false,\n        editable: true,\n        defaultValue: '',\n        autoFocus: false,\n        phone: false,\n        ssn: false,\n        maxLength: null,\n        icon: null,\n        tabindex: '',\n        storeEntry: null\n    };\n\n    constructor(props) {\n        super(props);\n\n        this.state = {\n            value: props.defaultValue\n        };\n    }\n\n    componentWillReceiveProps(nextProps) {\n        if (nextProps.defaultValue !== this.state.value) {\n            this.setState({ value: nextProps.defaultValue });\n        }\n    }\n\n    handleClick = event => {\n        this.props.onClick && this.props.onClick(event);\n    };\n\n    handleChange = (event, title) => {\n        const value = event.target && event.target.value;\n        this.handlePlaceholder;\n        if (this.props.storeEntry) {\n            this.props.storeEntry.form.handleChange(value, this.props.storeEntry.key, title);\n        }\n        else this.setState({ value });\n        this.props.onChange && this.props.onChange(value);\n    };\n\n    handleBlur = event => {\n        this.props.onBlur && this.props.onBlur();\n    };\n\n    handleFocus = event => {\n        this.props.onFocus && this.props.onFocus();\n    };\n\n    handlePlaceholder = event => {\n        const value = event.target && event.target.value;\n        this.props.onKeyUp && this.props.onKeyUp(value);\n    }\n\n    formatPhoneNumber(s) {\n        let x = s.replace(/\\D/g, '').match(/(\\d{0,3})(\\d{0,3})(\\d{0,4})/);\n        x = !x[2] ? x[1] : '(' + x[1] + ') ' + x[2] + (x[3] ? '-' + x[3] : '');\n        return x;\n    }\n\n    formatSsn(s) {\n        let x = s.replace(/\\D/g, '').match(/(\\d{0,3})(\\d{0,2})(\\d{0,4})/);\n        x = !x[2] ? x[1] : x[1] + '-' + x[2] + (x[3] ? '-' + x[3] : '');\n        return x;\n    }\n\n    render() {\n        const { wrapperClassName, labelPosition, floatingLabel, wide, showLabel,\n            required, textAlign, className, editable, frame, disabled, maxLength, \n            type, autoFocus, placeholder, large, phone, ssn, icon, tabindex, id, storeEntry } = this.props;\n\n        const value = ssn ? this.formatSsn(this.state.value) : phone ? this.formatPhoneNumber(this.state.value) : storeEntry ? storeEntry.value : this.state.value;\n        const error = storeEntry ? storeEntry.error : this.props.error;\n        const invalid = storeEntry ? storeEntry.error : this.props.invalid;\n        const title = (storeEntry && storeEntry.label) ? storeEntry.label : this.props.title;\n\n        return (\n            <div\n                className={cx(s.root, wrapperClassName, {\n                    [s.wide]: wide,\n                    [s.leftLabelPosition]: labelPosition === 'left',\n                    [s.rightLabelPosition]: labelPosition === 'right',[s.floatingLabelWrapper]: floatingLabel\n                })}\n            >\n                <div className={s.controls}>\n                    <input\n                        formNoValidate\n                        style={{\n                            textAlign\n                        }}\n                        className={cx(!large && [s.input], [s.blackFrame]: large,\n                            className, {\n                            [s.blackFrame]: frame && !invalid,\n                            [s.redFrame]: frame && invalid,\n                            [s.nonEditable]: !editable\n                        })}\n                        id={id}\n                        ref=\"input\"\n                        type={type}\n                        placeholder={placeholder || (showLabel ? ' ' : title)}\n                        onChange={event => this.handleChange(event, title)}\n                        onBlur={this.handleBlur}\n                        onFocus={this.handleFocus}\n                        onKeyUp={this.handlePlaceholder}\n                        onClick={this.handleClick}\n                        disabled={disabled}\n                        readOnly={!editable}\n                        value={value}\n                        autoFocus={autoFocus}\n                        autoComplete={type === 'password' ? 'new-password' : type}\n                        maxLength={maxLength}\n                        tabIndex={tabindex}\n                    />\n                    {\n                        showLabel && <label className={cx(s.label, {\n                            [s.hasOffset]: labelPosition !== 'top',\n                            [s.floatingLabel]: floatingLabel,\n                            [s.placeholderActive]: this.state.value && floatingLabel,\n                        })}\n                        >\n                            {title}\n                            {required && <span title='required'>&nbsp;</span>}\n                        </label>\n                    }\n                    {icon && <div className={s.icon}>{icon}</div>}\n                    <span className={floatingLabel && s.highlight}></span>\n                    <div className={cx('message', s.message, {\n                        [s.error]: invalid,\n                        [s.errorFloating]: floatingLabel,\n                    })}\n                    >\n                        {error}\n                    </div>\n       