s from 'isomorphic-style-loader/lib/withStyles';\nimport { observer, inject } from 'mobx-react';\nimport { getSafe } from 'Middleware/utils/object';\nimport s from './Header.scss';\nimport cx from 'classnames';\n\nimport { debounce } from 'lodash';\nimport { SIZE_SMALL, SIZE_NAV } from 'Middleware/constants/icon';\nimport { assetPath } from 'Root/config';\nimport TransitionGroup from 'react-transition-group/TransitionGroup';\nimport CSSTransition from 'react-transition-group/CSSTransition';\n\nimport CategoryNavigation from './CategoryNavigation';\nimport CartLink from 'Components/Header/CartLink';\nimport AccountLink from './AccountLink';\n\nimport CountrySelector from 'Common/CountrySelector';\nimport Link from 'Common/Link';\nimport Image from 'Common/Image';\nimport Container from 'Common/Container';\nimport ShoppingWith from 'Common/ShoppingWith';\nimport BehindTheCounter from 'Common/BehindTheCounter';\nimport IconMenu from 'Icons/Controls/Menu.jsx';\nimport IconSearch from 'Icons/Search.jsx';\n\n@withStyles(s)\n@inject(\n    'interfaceStore',\n    'contentStore',\n    'navigationStore',\n    'userStore',\n    'attributionStore'\n)\n@observer class Header extends Component {\n\n    static propTypes = {\n        onMenuClick: pt.func,\n        onCartClick: pt.func,\n        path: pt.string\n    };\n\n    state = {\n        hasError: false,\n        appIsMounted: false,\n        fadeDelay: true,\n        hoverActive: false,\n    }\n\n    componentWillMount() {\n        this.handleScroll();\n    }\n\n    componentDidMount() {\n        this.scrollListener = debounce(this.handleScroll,10);\n        window.addEventListener('scroll', this.scrollListener, { passive: true });\n\n        requestAnimationFrame(() => {\n            this.setState({ appIsMounted: true });\n            setTimeout(() => this.setState({ fadeDelay: false }), 3000);\n            setTimeout(() => this.setState({ hoverActive: true }), 4400);\n        });\n    };\n\n    componentWillUnmount() {\n        window.removeEventListener('scroll', this.scrollListener);\n    };\n\n    componentDidCatch(error, info) {\n        this.setState({\n            hasError: true\n        });\n    }\n\n    handleScroll = () => {\n        if(__BROWSER__ && window.pageYOffset >= 360 ) {\n            if(!this.props.interfaceStore.stickyHeader) {\n                this.props.interfaceStore.searchOpen && this.props.interfaceStore.searchToggle();\n                this.props.interfaceStore.stickyScrollHeader(false);\n            }\n        }\n        else {\n            if (this.props.interfaceStore.stickyHeader) {\n                this.props.interfaceStore.searchOpen && this.props.interfaceStore.searchToggle(false);\n                this.props.interfaceStore.fixedScrollHeader();\n            }\n        }\n    };\n\n    handleMenuIconClick = event => {\n        this.props.onMenuClick && this.props.onMenuClick(event);\n    };\n\n    handleCartIconClick = event => {\n        this.props.onCartClick && this.props.onCartClick(event);\n    };\n\n    handleSearchIconClick = event => {\n        let toggle = true;\n        if(this.props.searchOpen)\n            toggle = false;\n\n        this.props.interfaceStore.searchToggle(toggle);\n    };\n\n    renderMenuIcon() {\n        return (\n            <div\n                className={s.hamburger}\n                onClick={this.handleMenuIconClick}\n            >\n            <IconMenu\n                size={SIZE_SMALL}\n            />\n            </div>\n        );\n    }\n\n    render() {\n\n        if(this.state.hasError) {\n            return false;\n        }\n\n        const {\n            stickyHeader,\n            searchOpen,\n        } = this.props.interfaceStore;\n\n        const { consultant } = this.props.attributionStore;\n        const consultantShopping = this.props.attributionStore.consultantShopping;\n\n        const contentStore = this.props.contentStore;\n        const navigationStore = this.props.navigationStore;\n\n        const