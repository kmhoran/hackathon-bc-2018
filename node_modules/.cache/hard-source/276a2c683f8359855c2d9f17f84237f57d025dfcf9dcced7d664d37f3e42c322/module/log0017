mport { routes } from 'Root/config';\nimport s from './LoginStep.scss';\nimport cx from 'classnames';\n\nimport Section from 'Common/Section';\nimport Input from 'Common/Input';\nimport Button from 'Common/Button';\nimport Link from 'Common/Link';\nimport MarkdownWrapper from 'Components/MarkdownWrapper';\nimport IconSpinner from 'Icons/Spinner.jsx';\n\nconst form = {\n    email: {\n        value: '',\n        error: false,\n    },\n    firstName: {\n        value: '',\n        error: false,\n    },\n    lastName: {\n        value: '',\n        error: false,\n    },\n    password: {\n        value: '',\n        error: false,\n    },\n    confirmation: {\n        value: '',\n        error: false,\n    },\n};\n\n@withStyles(s)\n@inject('contentStore', 'interfaceStore', 'messagingStore', 'userStore', 'enrollmentStore')\n@observer\nclass LoginStep extends Component {\n    constructor(props) {\n        super(props);\n        this.debounceCheckEmailExists = debounce(this.checkEmailExists, 220);\n    }\n\n    state = {\n        errors: null,\n        form: cloneDeep(form),\n        checkingEmailExists: false,\n        completeEmail: false,\n        lastUpdated: new Date(),\n        isConsultantEmail: false,\n    };\n\n    @computed\n    get config() {\n        return getSafe(\n            () => this.props.contentStore.extensions.enrollment.data.configEnrollment[0],\n        );\n    }\n\n    handleInputChange(name, value) {\n        const entry = {\n            ...this.state.form[name],\n            error: false,\n            value,\n        };\n        let checkingEmailExists = false;\n        let completeEmail = this.state.completeEmail;\n        if (name === 'email') {\n            if (validEmail(value)) {\n                checkingEmailExists = true;\n            } else {\n                completeEmail = false;\n            }\n        }\n        this.setState({\n            form: {\n                ...this.state.form,\n                [name]: entry,\n            },\n            checkingEmailExists,\n            lastUpdated: new Date(),\n            completeEmail,\n        });\n\n        if (checkingEmailExists) this.debounceCheckEmailExists(value);\n    }\n\n    handleLoginSubmit = async event => {\n        event && event.preventDefault();\n        const { invalidEmailOrPassword } = this.config.formErrors;\n        if (this.validateInput()) {\n            this.props.interfaceStore.showProgress('login');\n            const loggedIn = await this.props.userStore.login({\n                email: this.state.form.email.value,\n                password: this.state.form.password.value,\n            });\n\n            if (loggedIn.success) {\n                this.props.messagingStore.clearErrors();\n                this.props.next();\n            }\n\n            if (!loggedIn.success) {\n                const error = loggedIn.message || 'There was an error.';\n\n                if (error === 'Invalid login or password.') {\n                    this.state.form.password.error = invalidEmailOrPassword;\n                    this.state.form.password.value = '';\n                    this.setState({\n                        form: {\n                            ...this.state.form,\n                        },\n                    });\n                } else {\n                    this.props.messagingStore.addMessage(error, { type: 'error' });\n                }\n            }\n\n            this.props.interfaceStore.hideProgress('login');\n        }\n    };\n\n    handleSaveAccount = async event => {\n        event && event.preventDefault();\n        const { invalidEmail } = this.config.formErrors;\n        if (this.validateInput()) {\n            const signedUp = await this.props.userStore.createAccount({\n                firstname: this.state.form.firstName.value,\n                lastname: this.state.form.lastName.value,\n                email: this.state.form.email.value,\n                password: this.state.form.password.value,\n                confirmation: this.state.form.password.value,\n            });\n\n            if (signedUp.success) {\n                this.props.messagingStore.clearErrors();\n                this.props.next();\n            }\n\n            if (!signedUp.success) {\n                try {\n                    const message =\n                        typeof signedUp.message === 'object'\n                            ? signedUp.message[Object.keys(signedUp.message)[0]]\n                            : signedUp.message;\n\n                    if (message === '\"Email\" is not a valid hostname.') {\n                        this.state.form.email.error = invalidEmail;\n                        this.setState({\n                            form: {\n                                ...this.state.form,\n                            },\n                        });\n                    } else {\n                        this.props.messagingStore.addMessage(message, { type: 'error' });\n                    }\n                } catch (e) {\n                    this.props.messagingStore.addMessage('There was an error.', { type: 'error' });\n                }\n            }\n        }\n    };\n\n    handleFormSubmit = e => {\n        if (e.key === 'Enter') {\n            if (this.state.emailExists) this.handleLoginSubmit();\n            e.preventDefault();\n        }\n    };\n\n    validateInput() {\n        const { form, completeEmail, emailExists } = this.state;\n        const { formErrors } = this.config;\n        const { passwordLength, requiredField, inValidEmail, minLength } = formErrors;\n        let errors = 0;\n        // if email is complete and email exists\n        if (completeEmail && emailExists) {\n            // if password length is less than 6\n            if (form.password.value.length < 6) {\n                form.password.error = !form.password.value ? requiredField : passwordLength;\n                form.password.value = '';\n                form.confirmation.value = '';\n                errors++;\n            }\n        }\n        // if email is complete and email does not exist\n        if (completeEmail && !emailExists) {\n            // if first name length is less than 2\n            if (form.firstName.value.length < 2) {\n                form.firstName.error = !form.firstName.value ? requiredField : minLength;\n                errors++;\n            }\n            // if last name length is less than 2\n            if (form.lastName.value.length < 2) {\n                form.lastName.error = !form.lastName.value ? requiredField : minLength;\n                errors++;\n            }\n            // if password length is less than 6\n            if (form.password.value.length < 6) {\n                form.password.error = !form.password.value ? requiredField : passwordLength;\n                form.password.value = '';\n                form.confirmation.value = '';\n                errors++;\n            }\n            // if password doesn't match\n            if (form.password.value.length < 6 || form.password.value !== form.confirmation.value) {\n                form.confirmation.error = !form.confirmation.value\n                    ? requiredField\n                    : mismatchPassword;\n                form.password.value = '';\n                form.confirmation.value = '';\n                errors++;\n            }\n        }\n        // if email is not a valid email\n        if (!validEmail(form.email.value)) {\n            form.email.error = !form.email.value ? requiredField : inValidEmail;\n            errors++;\n        }\n        this.setState({});\n        if (errors > 0) return false;\n        return true;\n    }\n\n    checkEmailExists = async value => {\n        this.setState({\n            completeEmail: false,\n            emailExists: false,\n        });\n        const result = await this.props.userStore.checkEmailExists(value);\n        let completeEmail = false;\n        let emailExists = false;\n\n        if (\n            validEmail(value) &&\n            value === this.state.form.email.value &&\n            Date.now() >= this.state.lastUpdated\n        ) {\n            completeEmail = true;\n            if (result.success) {\n                emailExists = true;\n            }\n        }\n        this.setState({\n            completeEmail,\n            emailExists,\n            checkingEmailExists: false,\n            lastUpdated: new Date(),\n            isConsultantEmail: getSafe(() => result.customerDetails.GroupId === CONSULTANT),\n        });\n    };\n\n    render() {\n        const {\n            form,\n            completeEmail,\n            emailExists,\n            isConsultantEmail,\n            checkingEmailExists,\n        } = this.state;\n\n        const {\n            loginHeading,\n            loginSubheading,\n            loginSignInCtaText,\n            loginSaveCtaText,\n            loginNewAccountMessage,\n            forgotPasswordLink,\n            formLabels,\n            loginPersonalInfoText,\n            loginSignInInfoText,\n            loginRequiredFields,\n        } = this.config;\n\n        const { loginEmail, loginPassword } = formLabels;\n\n        return (\n            <Section className={s.root} contentClassName={s.contentContainer}>\n                <h1 className={s.stepHeading}>{loginHeading}</h1>\n                <p className={s.stepHeading}>{loginSubheading}</p>\n                <form className={s.formContainer} onKeyPress={e => this.handleFormSubmit(e)}>\n                    <div className={s.topformInputs}>\n                        <Input\n                            wrapperClassName={s.input}\n                            defaultValue={form.email.value}\n                            onChange={e => this.handleInputChange('email', e)}\n                            error={form.email.error}\n                            invalid={form.email.error}\n                            showLabel\n                            required\n                            type={'email'}\n                            icon={\n                                <IconSpinner\n                                    className={cx(\n                                        s.spinner,\n                                        checkingEmailExists && s.spinnerActive,\n                                    )}\n                                    size={{ width: 20, height: 20 }}\n                                    color={'black'}\n                                    fill={'black'}\n                                />\n                            }\n                            title={loginEmail}\n                        />\n                        {!completeEmail && (\n                            <div className={s.buttonContainer} onClick={() => this.validateInput()}>\n                                <Button style={{ margin: '0 auto' }} disabled regular>\n                                    {loginSignInCtaText}\n                                </Button>\n                            </div>\n                        )}\n                        {completeEmail &&\n                            emailExists && (\n                                <div className={s.bottomForm}>\n                                    <Input\n                                        wrapperClassName={s.input}\n                                        defaultValue={form.password.value}\n                                        onChange={e => this.handleInputChange('password', e)}\n                                        error={form.password.error}\n                                        invalid={form.password.error}\n                                        showLabel\n                                        required\n                                        type={'password'}\n                                        title={loginPassword}\n                                    />\n                                    <div className={s.forgotPasswordWrapper}>\n                                        <Link\n                                            className={s.forgotPasswordLink}\n                                            to={routes.FORGOT_PASSWORD}\n                                        >\n                                            {getSafe(() => forgotPasswordLink.fields.name)}\n                                        </Link>\n                                    </div>\n                                    <div className={s.buttonContainer}>\n                                        {emailExists && (\n                                            <Button\n                                                style={{ margin: '0 auto' }}\n                                                type=\"submit\"\n                                                regular\n                                                onClick={() => this.handleLoginSubmit()}\n                                            >\n                                                {loginSignInCtaText}\n                                            </Button>\n                                        )}\n                                    </div>\n                                </div>\n                            )}\n                    </div>\n                    {completeEmail &&\n                        !emailExists && (\n                            <React.Fragment>\n                                <div className={s.welcomeMessage}>\n                                    <div>{loginNewAccountMessage}</div>\n                                </div>\n                                <div className={s.bottomFormInputs}>\n                                    <div className={s.bottomHeader}>{loginRequiredFields}</div>\n                                    <div className={s.infoContainer}>\n                                        <MarkdownWrapper\n                                            wrapperClassName={s.sectionHeader}\n                                            content={loginPersonalInfoText}\n                                        />\n                                        <div className={s.infoForm}>\n                                            <Input\n                                                wrapperClassName={s.input}\n                                                defaultValue={form.firstName.value}\n                                                onChange={e =>\n                                                    this.handleInputChange('firstName', e)\n                                                }\n                                                error={form.firstName.error}\n                                                invalid={form.firstName.error}\n                                                showLabel\n                                                required\n                                                type={'text'}\n                                                title={formLabels.firstName}\n                                                tabindex=\"1\"\n                                            />\n                                            <Input\n                                                wrapperClassName={s.input}\n                                                defaultValue={form.lastName.value}\n                                                onChange={e =>\n                                                    this.handleInputChange('lastName', e)\n                                                }\n                                                error={form.lastName.error}\n                                                invalid={form.lastName.error}\n                                                showLabel\n                                                required\n                                                type={'text'}\n                                                title={formLabels.lastName}\n                                                tabindex=\"2\"\n                                            />\n                                        </div>\n                                    </div>\n                                    <div className={s.signInContainer}>\n                                        <MarkdownWrapper\n                                            wrapperClassName={s.sectionHeader}\n                                            content={loginSignInInfoText}\n                                        />\n                                        <div className={s.infoForm}>\n                                            <Input\n                                                wrapperClassName={s.input}\n                                                onChange={e =>\n                                                    this.handleInputChange('password', e)\n                                                }\n                                                error={form.password.error}\n                                                invalid={form.password.error}\n                                                defaultValue={form.password.value}\n                                                showLabel\n                                                required\n                                                type={'password'}\n                                                title={formLabels.password}\n                                                tabindex=\"5\"\n                                            />\n                                            <Input\n                                                wrapperClassName={s.input}\n                                                onChange={e =>\n                                                    this.handleInputChange('confirmation', e)\n                                                }\n                                                error={form.confirmation.error}\n                                                invalid={form.confirmation.error}\n                                                defaultValue={form.con