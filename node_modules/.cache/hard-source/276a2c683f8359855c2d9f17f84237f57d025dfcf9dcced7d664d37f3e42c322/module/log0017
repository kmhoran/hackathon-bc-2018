 compState: 1,\n                    navState: this.getNavStates(1, this.props.steps.length),\n                    urlPath: this.props.steps[1].urlPath,\n                });\n            }\n        });\n        this.resizeListener = throttle(this.updateDimensions, 100);\n        window.addEventListener('resize', this.resizeListener);\n    }\n\n    componentWillUnmount() {\n        this.resizeListener = throttle(this.updateDimensions, 100);\n        window.removeEventListener('resize', this.resizeListener);\n    }\n\n    updateDimensions = () => {\n        this.setState({ viewport: window.innerWidth });\n    };\n\n    // set nav state\n    setNavState(next) {\n        const attribution = this.store.attribution || 1;\n        const url = `${window.location.origin}${\n            window.location.pathname\n        }?consultantID=${attribution}`;\n\n        this.setState({\n            navState: this.getNavStates(next, this.props.steps.length),\n        });\n\n        if (next < this.props.steps.length) {\n            this.setState({\n                compState: next,\n            });\n            window.history.pushState('', '', `${url}&step=${this.props.steps[next].urlPath}`);\n            // instead of adding check marks when user clicks to next, add check marks by checking if user completed the form (server side?)\n            // handle either parent component or child component (each step)?\n            // await this.store.trackCompletions(prev);\n            // await this.store.save();\n        }\n        // this.props.sessionStore.saveKey('step', next, { localized: false });\n    }\n\n    // update header nav states via classes so they can be styled via css\n    getNavStates(index, length) {\n        const styles = [];\n\n        for (let i = 0; i < length; i++) {\n            if (i < index) {\n                styles.push('done');\n            } else if (i === index) {\n                styles.push('active');\n            } else {\n                styles.push('todo');\n            }\n        }\n        return {\n            current: index,\n            styles,\n        };\n    }\n\n    goToStep(evt) {\n        if (evt.target == undefined) {\n            this.setNavState(evt);\n        } else if (\n            evt.target.value === this.state.compState ||\n            evt.target.value === 0 ||\n            (this.store.isLogged && evt.target.value === 1)\n        ) {\n            evt.preventDefault();\n            evt.stopPropagation();\n            return;\n        }\n        evt.persist();\n        if (\n            evt.target.value === this.props.steps.length - 1 &&\n            this.state.compState === this.props.steps.length - 1\n        ) {\n            this.setNavState(this.props.steps.length);\n        } else {\n            this.setNavState(evt.target.value);\n        }\n    }\n\n    // get the classname of steps\n    getClassName(i) {\n        return `${this.state.navState.styles[i]}`;\n    }\n\n    // render the steps as stepsNavigation\n    renderSteps() {\n        const isMobile = this.state.viewport <= 999;\n        return this.props.steps.map((step, i) => {\n            const classname = cx({\n                [s['progress-active']]: this.getClassName(i) === 'active',\n                [s['progress-done']]: this.getClassName(i) === 'done',\n                [s['progress-todo']]: this.getClassName(i) === 'todo',\n            });\n            const title =\n                isMobile && this.getClassName(i) === 'active'\n                    ? `${i + 1}. ${this.props.steps[i].name}`\n                    : isMobile\n                        ? i + 1\n                        : this.props.steps[i].name;\n            // const enabled = this.store.steps[i] && this.store.isLogged;\n            const enabled = this.store.isLogged;\n            return (\n                <li\n                    key={i}\n                    value={i}\n                    className={classname}\n                    onClick={evt => enabled && this.goToStep(evt)}\n                >\n                    {this.store.steps[i] && (\n                        <Checked\n                            className={s.checked}\n                            color={'green'}\n                            size={{ width: 16, height: 16 }}\n                        />\n                    )}\n                    {title}\n                </li>\n            );\n        });\n    }\n\n    // move forward via next button\n    next() {\n        if (this.state.compState < this.props.steps.length - 1) {\n            this.setNavState(this.state.compState + 1);\n        }\n    }\n\n    // move back via previous button\n    previous() {\n        if (this.state.compState > 1) {\n            this.setNavState(this.state.compState - 1);\n        }\n    }\n\n    render() {\n        const cloneExtensions = {\n            goToStep: c => {\n                this.goToStep(c);\n            },\n            next: () => {\n                this.next();\n            },\n            previous: () => {\n                this.previous();\n            },\n        };\n\n        const componentPointer = this.props.steps[this.state.compState].component;\n        const compToRender = React.cloneElement(componentPointer, cloneExtensions);\n\n        return (\n            <React.Fragment>\n                <ul className={s.progress}>{this.renderSteps()}</ul>\n                <div className={s.stepContainer}>\n                    <div className={s.stepWrapper}>{compToRender}</div>\n                </div>\n                <div className={s.footerButtons}>\n                    <Button\n                        disabled={this.state.compState === 1}\n                        wide\n                        onClick={() => {\n                            this.previous();\n                        }}\n                    >\n                        Previous\n                    </Button>{' '}\n                    <Button\n                        disabled={this.state.compState === 5}\n                        wide\n                        onClick={() => {\n                            this.next();\n                        }}\n                    >\n                        Next\n                    </Button>{' '}\n                    <Button\n                        wide\n                        onClick={() => {\n                            this.store.reset();\n                        }}\n                    >\n                        Reset Store\n                    </Button>\n                </div>\n            </React.Fragment>\n        );\n    }\n}\n\nSteps.propTypes = {\n    steps: pt.arrayOf(\n        pt.shape({\n            urlPath: pt.string,\n            name: pt.string.isRequired,\n            component: pt.element.isRequired,\n        }),\n    ).isRequired,\n    startAtStep: pt.number,\n};\n\nexport default Steps;\n"]}},"_buildHash":"9692cb8ddfb3552487f60a2296def0c2","hash":"3b3f456abdc00ba30eda5bcc195b4cfb","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[293,300],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/defineProperty","range":[301,339],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"RequireHeaderDependency","range":[431,438],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-prototyp