   Object.keys(categories.categoryFilters[category ? category.magentoId : 1]).forEach(section =>\n                        categories.categoryFilters[category ? category.magentoId : 1][section].forEach((item) => {\n                            if (item.categoryId == parseInt(queryFilter)) {\n                                applyFilters.push({\n                                    id: item.categoryId,\n                                    name: item.name,\n                                    value: true,\n                                    parent: item.parent,\n                                });\n                            }\n                        })\n                    )\n                )\n            }\n\n            const ShopContainer = await loadContainer(\"ShopContainer\");\n\n            stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disableHover: true,\n            });\n            stores.interfaceStore.hideProgress(\"page-load\");\n\n            return {\n                chunks: ['ShopContainer'],\n                component: <ShopContainer\n                    category={category.name}\n                    slug={params.categorySlug || null}\n                    search={query.search || null}\n                    defaultFilters={applyFilters}\n                />\n            };\n        },\n    },\n\n    {\n        path: `${routes.PRODUCT}/:slug`,\n        async action({ next }) {\n            return await next();\n        },\n        children: [{\n            path: `*`,\n            async action({ params, path, stores, next }) {\n                let variant = false, SKU = null, id = null;\n                if (path.includes(\"variant-\")) variant = path.split(\"variant-\")[1];\n\n                if (stores.productsStore.productsBySlug[params.slug]) {\n                    SKU = stores.productsStore.productsBySlug[params.slug].sku;\n                    id = store