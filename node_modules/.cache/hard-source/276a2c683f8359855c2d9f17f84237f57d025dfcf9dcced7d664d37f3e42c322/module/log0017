\n                stores.navigationStore.to({ url:'/' });\n            }\n            else {\n                if (__BROWSER__) {\n                    const utmString = window.sessionStorage.getItem('utmString');\n                    utmString ?\n                    window.history.replaceState({}, {}, `${routes.ORDER_CONFIRM}?${utmString}`) :\n                    window.history.replaceState({}, {}, routes.ORDER_CONFIRM);\n                    stores.cartStore.completeOrder({orderId: query.order, email: query.email});\n                }\n            }\n\n            return {\n                chunks: ['OrderConfirmationPage'],\n                component: <OrderConfirmationPage order={query.order} email={query.email} />,\n            }\n        }\n    },\n\n    {\n        path: routes.NEVER_LIST,\n        async action({ stores }) {\n            const [ NeverListPage ] = await Promise.all([\n                loadContainer('NeverListPage'),\n                stores.contentStore.fetchContentfulConfig(asyncModels.neverList),\n            ]);\n\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disableHover: true,\n            });\n            return {\n                chunks: ['NeverListPage'],\n                component: <NeverListPage />,\n            };\n        },\n    },\n\n    {\n        path: routes.HOST_SOCIAL,\n        async action({ stores }) {\n            const [ HostSocialPage ] = await Promise.all([\n                loadContainer('HostSocialPage'),\n                stores.contentStore.fetchContentfulConfig(asyncModels.hostSocial),\n            ]);\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disableHover: true,\n            });\n            return {\n                chunks: ['HostSocialPage'],\n                component: <HostSocialP