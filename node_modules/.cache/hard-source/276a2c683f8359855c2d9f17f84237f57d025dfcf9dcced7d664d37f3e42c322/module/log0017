 `*`,\n                action: data => data.route.parent.page(data)\n            },\n        ],\n        page: async ({ params, stores }) => {\n\n            stores.interfaceStore.showProgress('page-load');\n            const [ ourTeamStore, OurTeamContainer ] = await Promise.all([\n                loadContainer('OurTeamStore'),\n                loadContainer('OurTeamContainer'),\n            ]);\n            await ourTeamStore.init();\n            stores.interfaceStore.hideProgress('page-load');\n\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disableHover: true,\n            });\n\n            return {\n                chunks: ['OurTeamStore', 'OurTeamContainer'],\n                component: (\n                    <Provider ourTeamStore={ourTeamStore}>\n                        <OurTeamContainer\n                            articleID={params.articleID || null}\n                        />\n                    </Provider>\n                ),\n            };\n        },\n    },\n\n    {\n        path: routes.INGREDIENT_GLOSSARY,\n        async action ({ stores }) {\n            stores.interfaceStore.showProgress('page-load');\n\n            const [ ingredientsStore, IngredientsContainer ] = await Promise.all([\n                loadContainer('IngredientsStore'),\n                loadContainer('IngredientsContainer'),\n            ]);\n            await ingredientsStore.init();\n            stores.interfaceStore.hideProgress('page-load');\n\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disableHover: true,\n            });\n\n            return {\n                chunks: ['IngredientsStore', 'IngredientsContainer'],\n                component: (\n                    <Provider ingredientsStore={ingredientsStore}>\n                        <IngredientsContainer />\n                    </Provider>\n                ),\n            };\n        },\n    },\n    {\n        path: routes.ENROLLMENT,\n        async action({ next, stores }) {\n            stores.interfaceStore.showProgress(\"page-load\");\n\n            if (__BROWSER__ && stores.contentStore.flags('enrollment') && stores.contentStore.flags('contentDrafts')) {\n                const [EnrollmentContainer, enrollmentStore] = await Promise.all([\n                    loadContainer('EnrollmentContainer'),\n                    loadContainer('EnrollmentStore'),\n                    stores.contentStore.fetchContentfulConfig(asyncModels.enrollment),\n                ]);\n                await stores.interfaceStore.setHeaderStyle({\n                    transparentHeader: false,\n                    hideCategoryNavigation: false,\n                    disableHover: true,\n                    disabledCategoryBorder: false,\n                });\n                stores.interfaceStore.hideProgress(\"page-load\");\n\n                return {\n                    chunks: ['EnrollmentContainer', 'EnrollmentStore'],\n                    component: (\n                        <Provider enrollmentStore={enrollmentStore}>\n                            <EnrollmentContainer />\n                        </Provider>\n                    ),\n                };\n            }\n            stores.interfaceStore.hideProgress(\"page-load\");\n            return await next();\n        },\n    },\n\n    {\n        path: routes.GET_THE_LOOK,\n        async action({ next }) {\n            return await next();\n        },\n        children: [\n            {\n                path: `/:path`,\n                action: data => data.route.parent.go(data)\n            },\n            {\n                path: `*`,\n                action: data => data.route.parent.go(data)\n            },\n        ],\n        go: async ({ params, stores, next }) => {\n            stores.interfaceStore.showProgress(\"page-load\");\n\n            const [ GetTheLookContainer, GetTheLookStore ] = await Promise.all([\n                loadContainer('GetTheLookContainer'),\n                loadContainer('GetTheLookStore'),\n            ]);\n            await GetTheLookStore.init();\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: false,\n                hideCategoryNavigation: false,\n                disableHover: true,\n                disabledCategoryBorder: false,\n            });\n            stores.interfaceStore.hideProgress(\"page-load\");\n\n            return GetTheLookStore.config\n                ? {\n                    chunks: ['GetTheLookContainer', 'GetTheLookStore'],\n                    component: (\n                        <Provider getTheLookStore={GetTheLookStore}>\n                            <GetTheLookContainer path={params.path || ''} />\n                        </Provider>\n                    ),\n                }\n                : await next();\n        },\n    },\n\n    {\n        path: '*',\n        async action({ path, stores, query, isInitialRender, localizedPath, context }) {\n            const pagePath = path.replace(/\\\\/g, '').replace(/\\//g, '').toLowerCase();\n\n            /**\n             * Contentful custom page lookup\n             * Searches through the local URL index\n             * If there is a match, it fetches the dynamic page container and\n             * the detailed information for that page.\n             */\n\n            const newestContent = stores.contentStore.content;\n            const locale = stores.localeStore.activeLocale.code;\n\n            let contentKey = false;\n            let pageConfig = {};\n\n            for (let page in newestContent.page) {\n                if (parseContentURL(page) == path || parseContentURL(page) + '/' == path) {\n                    contentKey = page;\n                    pageConfig = newestContent.page[page];\n                }\n            }\n\n            // Check to see if we have data locally for that page\n            // If not, run an async request for that page\n            if (pageConfig.url && (typeof pageConfig.displayName === 'undefined' || !pageConfig.displayName || pageConfig.activeLocale !== locale)) {\n                stores.interfaceStore.showProgress('page-load');\n                await stores.contentStore.getPage({\n                    locale,\n                    page: pageConfig.url,\n                    firstLoad: isInitialRender,\n                });\n                pageConfig = stores.contentStore.content.page[contentKey];\n            }\n\n            if (contentKey && pageConfig && !pageConfig.disabled) {  // Is is a CMS page?\n                let widgetsToLoad = [];\n                let widgetFetchers = [];\n                let widgetPreload = [];\n\n                if (pageConfig.heroWidget) widgetsToLoad = [pageConfig.heroWidget];\n\n                if (pageConfig.widgets) {\n                    widgetsToLoad = [\n                        ...widgetsToLoad,\n                        ...pageConfig.widgets\n                    ];\n                }\n\n                if (widgetsToLoad && widgetsToLoad.length) {\n                    widgetFetchers = widgetsToLoad.map(widget =>\n                        getSafe(() => widget.sys.contentType.sys.id) &&\n                        getSafe(() => widgets[widget.sys.contentType.sys.id.replace('widget', '')]())\n                    );\n                }\n\n                const response = await Promise.all([containers['DynamicPage'](), ...widgetFetchers]);\n                const DynamicPage = response[0].default;\n\n                widgetPreload = widgetsToLoad.map(widget =>\n                    getSafe(() => widget.sys.contentType.sys.id)\n                    && widget.sys.contentType.sys.id.replace('widget', '')\n                );\n\n                await stores.interfaceStore.setHeaderStyle({ ...pageConfig });\n\n                stores.interfaceStore.hideProgress('page-load');\n\n                return {\n                    chunks: ['DynamicPage', ...widgetPreload],\n                    component: (\n                        <DynamicPage\n                            id={contentKey}\n                            firstLoad={__SERVER__ || isInitialRender} />\n                    )\n                };\n            }\n\n            /**\n             * PWS Lookup\n             * Queries the Phoenix endpoint to check whether\n             * the URL corresponds with a consultant key or id\n             * If a match is found, it loads the PWS container with the\n             * information returned on that key.\n             */\n\n            stores.interfaceStore.showProgress('page-load');\n            const consultantId = path.includes(routes.PWS) ? query.id : null;\n            const consultantName = !consultantId && path.split('/')[1];\n\n            const consultant = await stores.attributionStore.lookupPWS({\n                key: consultantName,\n                id: consultantId,\n            });\n            const social = path.split('social/')[1] || path.split('socials/')[1] || null;\n\n            if (social) stores.attributionStore.setSocial(social);\n\n            if (__BROWSER__ && consultant && query.goto) {\n                query.goto = decodeURIComponent(query.goto);\n                const [ goto, ...nestedParams ] = query.goto.split(/\\?/g);\n                const newParams = omit(safelyUnnestQueryString(query.goto), 'goto');\n\n                // fix for queries with multiple `?`\n                // Can not return new query, must mutate existing.\n                for (const key in newParams){\n                    query[key] = newParams[key];\n                }\n\n                const redirect301 = map301s(goto, stores);\n                const rewrite = `${redirect301.redirect || goto}`;\n                context.onSetMeta('robots', 'noindex');\n\n                return {\n                    redirect: rewrite,\n                    replace: `${appendLocale(localizedPath)}${rewrite}`,\n                };\n                return;\n            }\n\n            if (consultant && __BROWSER__) {\n                if (!path.split(consultantName)[1].includes(stores.attributionStore.pwsContactURL)) {\n                    return { redirect: '/' };\n                }\n\n                const [ PWSContainer ] = await Promise.all([\n                    loadContainer('PWSContainer'),\n                    stores.contentStore.fetchContentfulConfig({\n                        ...asyncModels.PWS,\n                        route: path,\n                    }),\n                ]);\n\n                if (__BROWSER__ && consultantId) window.history.replaceState('', '', consultant.pwsKey);\n\n                await stores.interfaceStore.setHeaderStyle({\n                    transparentHeader: true,\n                    hideCategoryNavigation: false,\n                    disableHover: false\n                });\n                context.onSetMeta('robots', 'noindex');\n                stores.interfaceStore.hideProgress('page-load');\n\n                stores.analyticsStore.pwsLookup(consultant, social);\n\n                return {\n                    chunks: ['PWSContainer'],\n                    component: (\n                        <PWSContainer\n                            consultant={consultant}\n                            social={social} />\n                    )\n                }\n            }\n\n            // Check for redirect mappings\n            const redirect301 = map301s(path, stores);\n            if (redirect301.redirect) return {\n                redirect: redirect301.redirect,\n                replace: appendLocale(localizedPath) + (redirect301.replace || ''),\n            };\n\n            /**\n             * 404\n             * If we don't find a PWS or Contentful page,\n             * resolve to a not found page.\n             */\n\n            stores.interfaceStore.hideProgress('page-load');\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disableHover: true,\n            });\n            context.onSetMeta('robots', 'noindex');\n\n            return {\n                component: <NotFoundPage />\n            }\n        }\n    }\n];\n\nfunction map301s(path, stores) {\n    path = decodeURI(path);\n    if (path.charAt(0) !== '/') path = `/${path}`;\n    if (path.includes('-1.html')) path = path.split('-1.html')[0];\n    if (path.includes('.html')) path = path.split('.html')[0];\n\n    let newPath, replace;\n    if (path == '/skin-care') newPath = '/products/skin-care';\n    if (path == '/cosmetics') newPath = '/products/makeup';\n    if (path == '/kids') newPath = '/products/family';\n    if (path == '/body-hair') newPath = '/products/bath-body';\n\n    if (path.indexOf('/help/#returns') == 0) newPath = '/help/returns';\n    if (path.indexOf('/help/#shipping') == 0) newPath = '/help/shipping';\n\n    const slug = path.split('/').pop();\n    if (stores.productsStore.productsBySlug[slug]) newPath = '/product/' + slug;\n    if (newPath == '/product/flawless-in-five') newPath = '/flawless-in-five';\n\n    const redirects = getSafe(() => stores.contentStore.content.configRedirects[0].list);\n    if (redirects) {\n        Object.keys(redirects).forEach(key => {\n            if (path == key) {\n                if (redirects[key].includes('?')) {\n                    const split = redirects[key].split('?');\n                    newPath = '/';\n                    replace = redirects[key];\n                }\n                else\n                    newPath = redirects[key];\n            }\n        });\n    }\n\n    return {\n        redirect: newPath,\n        replace: replace || newPath\n    }\n}\n\nfunction appendLocale(path) {\n    try {\n        const pathCode = path.split('/')[1].split('/')[0];\n        let found;\n        locales.map((locale) => {\n            if (locale.code.toLowerCase().includes(pathCode))\n                found = locale.code.toLowerCase();\n        });\n\n        if (found) return `/${found}`;\n    }\n    catch(e) {\n        console.error(e);\n    }\n    return '';\n}\n\nfunction validatePage(page, stores) {\n    const config = getSafe(() => stores.contentStore.extensions[page.id].data[page.models[0]][0]);\n\n    if (config && config.hide) return false;\n    return true;\n}\n\n// Wraps container in a provider.\n// TODO: refactor to be usable with any type of route, not just legacy apps.\nasync function handleLegacyPage({ params, stores, query, baseUrl, path, next }, container, loadables = [], c) {\n    stores.interfaceStore.showProgress(\"page-load\");\n\n    if (__SERVER__) return await next();\n\n    // verify that the Magento server-side session is valid else the page can't render\n    const loggedIn = await stores.userStore.verifyLoggedIn();\n    if (!loggedIn) {\n        !stores.contentStore.flags('nogento') && stores.userStore.logout();\n        stores.navigationStore.to({ url: routes.HOME });\n    }\n\n    const [legacyAppStore, Container] = await Promise.all([\n        loadContainer(\"LegacyAppStore\"),\n        loadContainer(container),\n        ...loadables,\n    ]);\n\n    const url = baseUrl + (() => path.length > 1 ? path : '')();\n\n    let html;\n\n// magento view container relies on html content from source to display. Therefore\n// the ui should wait for its availability.\n    if (c.container === 'MagentoViewContainer') {\n        const newHTML = JSON.parse(JSON.stringify(stores.myAccountStore.previousHTML));\n        if (stores.myAccountStore.postUrl.length > 1\n            && stores.myAccountStore.isLoadingPost\n            && newHTML && newHTML.length > 0\n            && hostnameStrip(stores.myAccountStore.postUrl, 'magento', 'POST').indexOf(c.api) > -1\n        ) {\n            stores.myAccountStore.isLoadingPost = false;\n            stores.myAccountStore.previousHTML = '';\n        } else {\n            stores.interfaceStore.showProgress('page-load');\n            html = await legacyAppStore.getPage(url);\n        }\n    }\n\n    await stores.interfaceStore.setHeaderStyle({\n        transparentHeader: false,\n        hideCategoryNavigation: false,\n        disableHover: true\n    });\n\n    return {\n        chunks: ['LegacyAppStore', container],\n        component: (\n            <Provider\n            legacyAppStore={legacyAppStore}\n            >\n                <Container\n                    url={url}\n                    html={html || false}\n                    c={c}\n                />\n            </Provider>\n        ),\n    };\n}\n\nasync function loadContainer(name) {\n    const container = await containers[name]();\n    return await container.default;\n}\n\nexport { routeList, loadContainer };"]}},"_buildHash":"edd1d8506ce773f3669747732648c6c9","hash":"8b9990c2bef026f6240c3fcd9698f9e8","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/json/stringify","range":[163,201],"loc":{"start":{"line":8,"column":17},"end":{"line":8,"column":64}}},{"type":"RequireHeaderDependency","range":[155,162],"loc":{"start":{"line":8,"column":17},"end":{"line":8,"column":64}}},{"type":"RequireHeaderDependency","range":[276,283],"loc":{"start":{"line":12,"column":16},"end":{"line":12,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/toArray","range":[284,315],"loc":{"start":{"line":12,"column":16},"end":{"line":12,"column":56}}},{"type":"RequireHeaderDependency","range":[397,404],"loc":{"start":{"line":16,"column":26},"end":{"line":16,"column":76}}},{"type":"CommonJsRequireDependency","request":"babel-r