is.props.selectedVariant) {\n                this.setState({selectedVariantId: this.props.selectedVariant});\n            }\n            if (window.location.hash.length > 1) {\n                const selector = `.${window.location.hash.substr(1)}`;\n                const main =  document.querySelector(selector);\n                if (typeof main != \"undefined\" && main)\n                    window.scrollTo(0, main.getBoundingClientRect().top + window.pageYOffset);\n                else\n                    return false;\n            }\n            this.props.analyticsStore.productViewed(this.props.selectedVariant || getSafe(() => this.props.productsStore.productsBySlug[this.props.slug].id));\n        });\n    }\n\n    componentWillReceiveProps(nextProps) {\n        if (this.props.slug !== nextProps.slug || this.props.selectedVariant !== nextProps.selectedVariant) {\n            const product = this.props.productsStore.productsBySlug[this.props.slug];\n            if(typeof product != \"undefined\" && __BROWSER__) {\n                this.setState({\n                    selectedVariant: false,\n                    selectedVariantId: false,\n                    selectedVariantName: false\n                });\n                this.context.setTitle(product.title);\n            }\n        }\n\n        this.localeSwitch = autorun(() => {\n            this.props.localeStore.activeLocale;\n            if (!nextProps.productsStore.normalizedBySKU[nextProps.selectedVariant])\n                __BROWSER__ && requestAnimationFrame(() => {\n                    this.setState({\n                        selectedVariant: false,\n                        selectedVariantId: false,\n                        selectedVariantName: false\n                    });\n                });\n        });\n    }\n\n    componentWillUnmount() {\n        window.removeEventListener(\"resize\", this.resizeListener);\n    }\n\n    updateDimensions = () => {\n        const windowWidth = window.innerWidth;\n        windowWidth != this.state.width && this.setState({ width: windowWidth});\n    }\n\n    selectedVariant = (variant) => {\n        this.state.variant != variant.id && this.setState({\n            selectedVariant: variant.name,\n            selectedVariantId: variant.id,\n            selectedVariantName: variant.fullName,\n            activeVariant: variant.id\n        });\n        this.props.analyticsStore.productViewed(variant.id);\n    }\n\n    renderProductInfo(selectedProduct) {\n        const { contentStore, promoStore, productsStore } = this.props;\n        const config = this.pageConfig;\n        const memberPromotion = this.memberPromotion;\n\n        return (\n            <ProductInfo\n                product={productsStore.productsBySlug[this.props.slug]}\n                variant={selectedProduct}\n                viewportWidth={this.state.width}\n                config={config}\n                memberPromotion={memberPromotion}\n                quantityDisplay={!promoStore.isItemPromoEligible(selectedProduct.id, 'GWP')}\n                defaultVariant={this.state.selectedVariant}\n                selectedVariant={this.selectedVariant} />\n        );\n    }\n\n    render() {\n        const\n            product = this.props.productsStore.productsBySlug[this.props.slug];\n\n        const config = this.pageConfig;\n\n        if(typeof product != \"undefined\" && config) {\n            const { activeLocale } = this.props.localeStore;\n\n            let selectedVariant;\n\n            if (!isEmpty(product)) {\n                this.context.setTitle(product.title);\n                this.context.onSetMeta('description', `${product.title} ${product.sku}`);\n            }\n\n            if(!this.props.selectedVariant && typeof product.variants[0].swatches != \"undefined\") {\n                selectedVariant = product.variants[0].id;\n            }\n            else\n                selectedVariant = this.props.selectedVariant;\n\n            if(!this.state.selectedVariant)\n                this.state.selectedVariant = selectedVariant;\n\n            if (!product)\n                return null;\n\n            const images = getThisVariantImage(product, this.state.selectedVariant);\n            const selectedProduct = getVariantKey(this.state.selectedVariant, product);\n            const disableScroll = (this.state.width < 1000 || (images && images.length <= 1 )) ? true : false;\n            const preloadImages = preloadProductImages(product, this.props.interfaceStore.viewportWidth);\n            const hasIngredients = !!getSafe(() => selectedProduct.ingredients || product.ingredients || product.bundleIds.length > 0);\n            const hasKeyIngredients = !!getSafe(() => selectedProduct.ingredients.keyIngredients || product.ingredients.keyIngredients);\n\n            return (\n\n                <div className={s.root} key={'product-page-' + product.id}>\n\n                <PreOrderPopup\n                    config={this.memberPromotion}\n                    product={this.props.productsStore.productsBySlug[this.props.slug]}\n                />\n\n                <Container\n                    hasLeftPadding={false}\n                    hasRightPadding={false}>\n                    <div className={s.hideMobile}>\n                        <NavigationBreadcrumbs \n                            className={s.navigationBreadcrumbs} \n                            title={product.title} \n                            links={this.breadcrumbLinks} \n                        />\n                    </div>\n\n                        <div id=\"pdp-sticky-container\" className={s.productHeader}>\n                            <div className={s.leftColumn}>\n                                <div className={s.infoContainerMobile}>\n                                    <ProductBasics\n                                        variant={selectedProduct}\n                                        parent={product} />\n                                </div>\n   