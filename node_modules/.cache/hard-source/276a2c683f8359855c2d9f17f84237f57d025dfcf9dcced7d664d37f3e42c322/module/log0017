t() {\n        window.removeEventListener(\"resize\", this.updateDimensions);\n        this.timeouts.forEach(clearTimeout);\n    };\n\n    updateDimensions = () => {\n        let windowWidth = window.innerWidth;\n\n        if(windowWidth != this.state.viewportWidth)\n            this.setState({ viewportWidth: windowWidth});\n    }\n\n    handleProductClick = slug => {\n        this.props.productsStore.setActiveProduct({ slug });\n    };\n\n    sortBy(filter) {\n        this.setState({ sortProductsBy: filter });\n        this.setState({ showSortDropdown: false });\n    };\n\n    handleSortClickOutside = event => {\n        this.setState({ showSortDropdown: false });\n    };\n\n    toggleSortDropdown() {\n        this.setState({showSortDropdown: !this.state.showSortDropdown});\n    }\n\n    renderCount(filtered, total) {\n            if(filtered == total)\n                return <div>\n                            {this.props.config.allItemsText.replace(\"%\", total)}\n                        </div>;\n\n            return <div>\n                {this.props.config.xOfYItems.replace(\"%\", filtered).replace(\"@\", total)}\n            </div>;\n    }\n\n    @computed get filteredCategories() {\n        const \n            { category, search } = this.props,\n            { productsByCategoryId, productsBySearch } = this.props.productsStore;\n\n        return category ? productsByCategoryId[category.magentoId] : productsBySearch;\n    }\n\n    @computed get filteredProducts() {\n        const { filters } = this.props;\n\n        return filters != {} ? this.filteredCategories.filter(product => filterItems(product.categoryList, filters)) : this.filteredCategories;\n    }\n\n    @computed get breadcrumbLinks() {\n        const { categoryStore, filters } = this.props;\n        const lastCategory = toJS(categoryStore.uniqueCategoryLinksById[categoryStore.lastCategory]);\n        const isSingleFilter = filters.Category && Object.values(filters.Category).length === 1;\n        const filter = isSingleFilter ? Object.values(filters.Category)[0] : {};\n\n        return([\n            isSingleFilter ? lastCategory : omit(lastCategory, 'to'),\n            filter,\n        ])\n    }\n\n    renderProducts(isFiltering) {\n        const { filters, category, search } = this.props;\n        const { categoryFilters, quicklook } = this.props.categoryStore;\n        const { productsByCategoryId, productsBySearch, searchTotal } = this.props.productsStore;\n        const { isComputer, appIsMounted } = this.props.interfaceStore;\n        let columns = 3;\n\n        let filteredCategories = category ? productsByCategoryId[category.magentoId] : productsBySearch;\n        let filteredProducts = filters != {} ? filteredCategories.filter(product => filterItems(product.categoryList, filters)) : filteredCategories;\n\n        if(!category && !search)return null;\n\n        let subCategories = category ? categoryFilters[category.magentoId][\"Category\"] : [];\n\n        if(this.state.sortProductsBy)\n            this.state.sortProductsBy.callback(filteredProducts, this.state.sortProductsBy.order, subCategories);\n        else\n            sorting.byName(filteredProducts, \"asc\");\n\n        const filteredCount = Object.keys(filteredProducts).length;\n        const productsCount = search ? filteredCategories.length : Object.keys(filteredCategories).length;\n\n        let \n            total = 1, \n            count = 0, \n            row = 1, \n            data = [], \n            key, \n            product, \n            productItems;\n\n        for (let i = 0, len = Object.keys(filteredProducts).length; i < len; i++) {\n                key = Object.keys(filteredProducts)[i];\n                product = filteredProducts[key];\n                count++;\n                total++;\n\n                if(count == columns+1) {\n                    count = 1;\n                    row++;\n                }\n\n                // Insert promotion into the grid\n                if(category && this.props.category.showPromo && this.props.category.promo) {\n                    if(total == 8 && Object.keys(filters).length < 1) {\n                        data.push (\n                            <LazyLoad \n                                key={Math.random()} \n                                offset={5} \n                                type={1}>\n                                <CatalogPromo \n                                    viewportWidth={this.state.viewportWidth}\n                                    {...this.props.category.promo.fields}\n                                />\n                            </LazyLoad>\n                        );\n                    }\n                }\n                if(count == 1) {\n                    data.push (\n                        <React.Fragment\n                            key={\"key-grid-row-\" + row} />\n                    )\n                }\n                if(isComputer && count == 1) {\n                    this.state.viewportWidth > MEDIA_MOBILE && data.push (\n                        <TransitionGroup key={\"quickview-root-\" + row}>\n                            {filteredProducts.map(prod => (\n                                (quicklook[row] && quicklook[prod.handle]) && \n                                    <CSSTransition\n                                        key={\"quickview\" + row + prod.handle}\n                                        classNames={ {\n                                            appear: s.quicklookAnimationAppear,\n                                            appearActive: s.quicklookAnimationEnterAppear,\n                                            enter: s.quicklookAnimationEnter,\n                                            enterActive: s.quicklookAnimationEnterActive,\n                                            exit: s.quicklookAnimationExit,\n                                            exitActive: s.quicklookAnimationExitActive,\n                                        } }\n                                        timeout={{ enter: 800, appear: 0, exit: 400 }}>\n                                                    <div key={prod.handle + row} className={s.quicklook}>\n                                                        <ProductQuicklook \n                                                            slug={prod.handle} \n                                                            row={row} \n                                                            selectedVariant={quicklook.variant}\n                                                            product={prod}\n                                                        />\n                                                    </div>\n                                    </CSSTransition>\n                                )\n                            )}\n                        </TransitionGroup>\n                    );\n                }\n                \n                data.push(\n                    <div \n                        className={s.productItemContainer} \n                        key={`${row} ${product.id} product-container`}>\n                        <LazyLoad key={this.props.category} offset={10}>\n                                <ProductItem\n                                    key={`${product.id} product-item`}\n                                    onClick={this.handleProductClick}\n                                    product={product}\n                                    row={row}\n                                    viewportWidth={this.state.viewportWidth}\n                                    config={this.props.config}\n                                    showQuicklook={true}\n                                />\n                        </LazyLoad>\n                    </div>\n                )\n    \n        };\n        \n        productItems = data;\n\n        if (search) {\n            var retryLink = this.props.config.searchRetryMessage.split('{')[1].split('}')[0];\n            var retryMessage = this.props.config.searchRetryMessage.split('}')[1];\n        }\n\n        return (\n            <div className={s.list} ref=\"grid\">\n                {search &&\n                    <div className={s.searchHeader}>\n                        {searchTotal ?\n                            <div className={s.searchHeading}>\n                                {this.props.config.youSearchedFor.replace('%s', search)}\n                            </div>\n                            :\n                            <React.Fragment>\n                                <div className={s.searchHeading}>           {this.props.config.noResultsFound.replace('%s', search)}\n                                </div>\n                                <div className={s.noSearchResults}>\n                                    <span className={s.retrySearch} onClick={() => this.props.interfaceStore.searchToggle(true)}>{retryLink}</span>{retryMessage}\n                                </div>\n                            </React.Fragment>\n                        }\n                    </div>}\n                <div className={s.topBar}>\n                    <div className={s.topBarLeft}>\n                        {this.renderCount(filteredCount,productsCount)}\n                    </div>\n                    <div className={s.topBarMiddle}>\n                    {isFiltering && \n                            <FilterBreadcrumbs filters={filters} search={search} />}\n                    </div>\n                    <div className={s.topBarRight}>\n                            <SortDropdown \n                                config={this.props.config} \n                                onSortClick={sort => this.sortBy(sort)}\n                                default={this.state.sortProductsBy}\n                             />\n                    </div>\n                </div>\n                <div className={cx(s.list, filteredCount && filteredCount < 4 && s.padBottom)}>\n                    {appIsMounted && productItems}\n                </div>\n            </div>\n        );\n    }\n\n    renderSEO() {\n        const markdownSeo = getSafe( () => this.props.category.markdownSeo.fields);\n        return markdownSeo\n            && (\n                <React.Fragment>\n                    <hr />\n                    { generateWidget('Basic', markdownSeo) }\n                </React.Fragment> \n            );\n    }\n\n    render() {\n        const { filters, search, category } = this.props;\n        const { filterCount } = this.props.categoryStore;\n        const isFiltering = !!filterCount;\n\n        console.log(category)\n\n        if(!category && !search)\n            return null;\n\n        return (\n            <div className={s.root} id=\"product-list\" ref=\"productlist\">\n                <StickyShopHeader {...this.props } grid={this} />\n                <div className={cx(s.navigationBreadcrumbWrapper)}>\n                   { !search && <NavigationBreadcrumbs links={this.breadcrumbLinks}/>}\n                </div>\n                <div className={s.breadcrumbContainer}>\n                    {isFiltering && <FilterBreadcrumbs search={search} filters={filters} />}\n                </div>\n                {this.renderProducts(isFiltering)}\n                {this.renderSEO()}\n            </div>\n        );\n    }\n}\n\nexport default ProductGrid;\n"]}},"_buildHash":"54d88a5b6befb12c1d458a4e4e9b454d","hash":"ee98792c981e7f948ae27dcfc869519a","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"