');\n\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disableHover: true,\n            });\n\n            return {\n                chunks: ['PressStore', 'PressContainer'],\n                component: (\n                    <Provider pressStore={pressStore}>\n                        <PressContainer\n                            articleID={params.articleID || null}\n                            query={query}\n                        />\n                    </Provider>\n                ),\n            };\n        },\n    },\n\n    {\n        path: routes.LOGIN,\n        async action({ stores }) {\n            stores.interfaceStore.showProgress('page-load');\n            const [ LoginPage ] = await Promise.all([\n                loadContainer('LoginPage'),\n                stores.contentStore.fetchContentfulConfig(asyncModels.login),\n            ]);\n            stores.interfaceStore.hideProgress('page-load');\n\n            return {\n                chunks: ['LoginPage'],\n                component: <LoginPage />,\n            }\n        }\n    },\n\n    {\n        path: routes.SIGNUP,\n        async action({ stores }) {\n            if (stores.userStore.isLogged) {\n                stores.navigationStore.to({ url: routes.ACCOUNT });\n                return false;\n            }\n            stores.interfaceStore.showProgress('page-load');\n            const [ SignupPage ] = await Promise.all([\n                loadContainer('SignupPage'),\n                stores.contentStore.fetchContentfulConfig(asyncModels.signup),\n            ]);\n            await stores.interfaceStore.setHeaderStyle({\n                transparentHeader: false,\n                hideCategoryNavigation: false,\n                disableHover: true,\n                disabledCategoryBorder: false,\n            });\n            stores.interfaceStore.hideProgress('page-load');\n\n        