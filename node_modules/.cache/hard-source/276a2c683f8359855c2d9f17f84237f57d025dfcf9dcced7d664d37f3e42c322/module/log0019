rom './ProductDetails.scss';\nimport cx from 'classnames';\nimport { observer, inject } from 'mobx-react';\nimport { head } from 'lodash';\nimport { SIZE_EXTRA_SMALL } from 'Middleware/constants/icon';\nimport { getThisVariantImage, getVariantIdFromImage, getProductImages, getVariantKey } from 'Middleware/services/products';\nimport { MEDIA_PHONE, MEDIA_MOBILE, MEDIA_DESKTOP, MEDIA_LAPTOP } from 'Middleware/constants/media';\nimport { getNativeProductUrl } from 'Middleware/utils/url';\n\nimport ProductTags from 'Components/Product/ProductTags';\nimport ProductSwatches from 'Components/Product/ProductSwatches';\nimport ProductPrice from 'Components/Product/ProductPrice';\nimport ProductRating from 'Components/Product/ProductRating';\nimport ProductAddButton from 'Components/Product/ProductAddButton';\nimport ProductAddButtonStyles from 'Components/Product/ProductAddButton/ProductAddButton.scss';\nimport Button from 'Common/Button';\n\nimport Image from 'Common/Image';\nimport Link from 'Common/Link';\nimport Eye from 'Icons/Eye.jsx';\nimport SwipeableViews from 'Common/SwipeableViews';\n\n@withStyles(s)\n@withStyles(ProductAddButtonStyles)\n@inject(\n    'categoryStore',\n    'contentStore',\n    'promoStore',\n    'navigationStore',\n    'productsStore',\n)\n@observer class ProductItem extends Component {\n    static propTypes = {\n        product: pt.object.isRequired,\n        onClick: pt.func,\n        row: pt.number,\n        showQuicklook: pt.bool,\n        showTags: pt.bool,\n        showSwatches: pt.bool,\n        infoPadding: pt.bool,\n        useAltMobileStyle: pt.bool,\n        showAddToCart: pt.bool,\n        handleProductAdded: pt.func.isRequired,\n        buttonText: pt.string,\n        style: pt.object,\n        isItemGWPType: pt.bool,\n    };\n\n    static defaultProps = {\n        product: {},\n        row: 1,\n        showQuicklook: false,\n        showTags: true,\n        showSwatches: true,\n        infoPadding: true,\n        useAltMobileStyle: false,\n        selectedVariant: false,\n        selectedVariantId: false,\n        selectedVariantName: false,\n        viewportWidth: 1280,\n        showAddToCart: false,\n        buttonText: null,\n        style: {},\n        isItemGWPType: false,\n    };\n\n    state = {\n        activeImageIndex: 0,\n        selectedVariant: this.props.selectedVariant || false,\n    }\n\n    openQuicklook = (product, row, variant) => {\n        this.props.categoryStore.toggleQuicklook({ product, row, variant });\n    }\n\n    selectedVariant = (color) => {\n        this.setState({\n            selectedVariant: color.name,\n            selectedVariantId: color.id,\n            selectedVariantName: color.fullName },\n        );\n    }\n\n    handleSwipeImageChange = (index) => {\n        const\n            product = this.props.product,\n            images = getProductImages(product, index);\n\n        this.setState({\n            selectedVariant: images[index].variantId,\n            selectedVariantId: images[index].variantId,\n            activeImageIndex: index,\n            selectedIndex: index,\n        });\n    };\n\n    render() {\n        const { \n            contentStore: { content: { shop, productDetail } },\n            promoStore, navigationStore, productsStore,\n            product, showQuicklook, showTags, showSwatches, showAddToCart, infoPadding, handleProductAdded, buttonText, viewportWidth, style \n        } = this.props;\n\n        if (!product || typeof product.id === 'undefined')\n            {return false;}\n\n        const mediaFilter = (device, property, injectionParent) => {\n            switch (property) {\n                case 'showPrice':\n                    switch (device) {\n                        case 'phone':\n                            return viewportWidth <= MEDIA_PHONE ? true : false;\n                            break;\n                        default: return false;\n                    }\n                    break;\n                default: return false;\n            }\n            return false;\n        }\n\n        let\n            images = [],\n            staticImages = [],\n            hasSwatches,\n            selected,\n            hasMultipleImages = false,\n            isDesktop = viewportWidth > MEDIA_MOBILE,\n            variantLink = '',\n            selectedProduct = product;\n\n        if (product.variants.length > 1) {\n            hasMultipleImages = true;\n            hasSwatches = true;\n\n            if (this.state.selectedVariant) {\n                images = getProductImages(product, this.state.selectedVariant);\n                staticImages = getThisVariantImage(product, this.state.selectedVariant);\n                variantLink = this.state.selectedVariant;\n                selectedProduct = getVariantKey(this.state.selectedVariant, product);\n            } else {\n                selected = this.props.selectedVariant || product.variants[0].id;\n                selectedProduct = getVariantKey(selected, product);\n                images = getProductImages(product, selected);\n                staticImages = images;\n                if (viewportWidth < MEDIA_LAPTOP)\n                    {this.state.selectedVariant = selected;}\n            }\n        } else\n            staticImages[0] = product.images ? product.images[0] : null;\n\n        const productURL = getNativeProductUrl(product.handle, variantLink);\n\n    // if it is gwp and active then replace product button with \n        const ButtonLogic = (({ text, handleProductAdded, enableGWP, isGWPActive }) => {\n            if (isGWPActive) {return (\n                <Button\n                    className={cx(ProductAddButtonStyles.root, ProductAddButtonStyles.smallSize, s.addButton)}\n                    onClick={handleProductAdded}\n                    thin\n                >\n                    {text}\n                </Button>\n            );}\n            return (\n                <ProductAddButton\n                  product={product}\n                  variant={selectedProduct}\n                  smallSize\n                  dark\n                  callback={handleProductAdded}\n                  className={s.addButton}\n                // if it is a gwp do not actually add the product to the cart.\n                  quantity={(() => enableGWP ? 0 : 1)()}\n                  showPrice={(() => mediaFilter('phone', 'showPrice'))()}\n                  hideCart={enableGWP}\n                >\n                    {text}\n                </ProductAddButton>);\n        });\n\n        const PhoneSize = ({ children }) => {\n            const width = viewportWidth;\n            return width <= MEDIA_PHONE ? children : null;\n        };\n\n        return (\n            <div key={`product-item-${product.id}`} style={style} className={s.root} id={`product-${product.handle}`}>\n                <PhoneSize>\n                    <hr className={s.hr} />\n                </PhoneSize>\n                <Link\n                  to={productURL}\n                  name={product.title}\n                  className={cx(\n                        s.title,\n                        this.props.infoPadding && s.infoPadding,\n                        !this.props.useAltMobileStyle && s.defaultMobileStyle)}\n                />\n\n                <div className={cx(s.imageContainer, !showSwatches && s.imageContainerNoSwatches)}>\n                    {showTags &&\n                        <div>\n                            <div\n                              className={s.tagContainer}\n                            >\n                                <ProductTags product={product} />\n                            </div>\n                            <div className={s.tagContainerMobile}>\n                                <ProductTags product={product} />\n                            </div>\n                        </div>\n                    }\n                    {showQuicklook &&\n                        <div\n                    