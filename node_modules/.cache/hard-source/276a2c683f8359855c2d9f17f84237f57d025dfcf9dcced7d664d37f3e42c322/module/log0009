B;AAAA,aAAR,CAAP;AACH;;;;kBA+YU1D,gB","file":"GetTheLookWizard.jsx","sourceRoot":"/Users/millie.savalia/react-node-frontend","sourcesContent":["import React, { Component } from 'react';\nimport { PropTypes as pt } from 'prop-types';\nimport withStyles from 'isomorphic-style-loader/lib/withStyles';\nimport { observer, inject } from 'mobx-react';\nimport { computed, autorun } from 'mobx';\nimport s from './GetTheLookWizard.scss';\nimport cx from 'classnames';\nimport { getSafe }  from 'Middleware/utils/object';\nimport TransitionGroup from 'react-transition-group/TransitionGroup';\nimport CSSTransition from 'react-transition-group/CSSTransition';\nimport Portal from 'Components/Common/Portal';\nimport ScrollToMixin from 'Components/Extensions/ScrollToMixin';\nimport mixin from 'Middleware/utils/decorators/mixinComponent';\nimport { MEDIA_MOBILE, MEDIA_PHONE } from 'Middleware/constants/media';\nimport ImagePrefetch from 'Components/Extensions/ImagePrefetch';\nimport { throttle } from 'lodash';\nimport { disableBodyScroll } from 'Middleware/utils/scrollLock';\n\nimport Section from 'Common/Section';\nimport Button from 'Common/Button';\nimport SocialNetworks from 'Common/SocialNetworks';\nimport IconClose from 'Icons/Controls/Close.jsx';\nimport { setTimeout } from 'timers';\n\n@withStyles(s)\n@inject(\n    'getTheLookStore',\n    'interfaceStore',\n    'contentStore',\n    'localeStore',\n)\n@mixin(ScrollToMixin)\n@observer class GetTheLookWizard extends Component {\n\n    @computed get store() {\n        this.props.getTheLookStore.children;\n        return getSafe(() => this.props.getTheLookStore.children[this.props.handle]);\n    }\n\n    @computed get config() {\n        return getSafe(() => this.store.config);\n    }\n\n    @computed get bundleConfig() {\n        return getSafe(() => this.store.bundleConfig);\n    }\n\n    @computed get product() {\n        return getSafe(() => this.store.product);\n    }\n\n    state ={\n        stickyActive: false,\n        panelClosed: false\n    }\n\n    componentDidMount() {\n        this.scrollTopDisposer = autorun(() => {\n            this.store.step;\n            this.panel = document.getElementById('gtl-panel');\n            this.repositionScroll();\n            requestAnimationFrame(() => {\n                if (this.panel) {\n                    this.panel.scrollTop = 0;\n                    this.stickyUpdate = setInterval(() => this.handleScroll(), 120);\n                    this.state.stickyActive && this.setState({ stickyActive: false });\n                }\n            });\n        });\n        this.panelLockDisposer = autorun(() => {\n            this.panel = document.getElementById('gtl-panel');\n            if (this.props.interfaceStore.cartOpen) {\n                requestAnimationFrame(() => {\n                    if (this.panel) this.panel.scrollTop = 0;\n                });\n                if (this.props.interfaceStore.isPhone) {\n                    disableBodyScroll(this.panel);\n                }\n            }\n        });\n    }\n\n    repositionScroll(){\n        requestAnimationFrame(this.scrollToSwatch.bind(this))\n    }\n\n    componentWillUnmount() {\n        this.scrollTopDisposer();\n        this.panelLockDisposer();\n        this.stickyUpdate = () => null;\n        clearInterval(this.stickyUpdate);\n    }\n\n    setSelection(id, swatch) {\n        swatch.inStock && this.store.setStepSelection(id);\n    }\n\n    handleScroll = () => {\n        if (this.panel && (this.panel.scrollTop > 80 || !this.props.interfaceStore.isPhone || window.innerHeight > 780)) {\n            !this.state.stickyActive && this.setState({ stickyActive: true });\n        }\n    };\n\n    scrollToSwatch() {\n        if (!!this.recommendedSwatch) {\n            this.scrollToHorizontal({ \n                scrollWrapper: this.swatchesWrapper, \n                scrollElement: this.recommendedSwatch,\n                extraHeight: 180\n            });\n        }\n    }\n\n    goToStep(id) {\n        this.store.goToStep({ step: id });\n    }\n\n    nextStep = throttle((valid = true) => valid && this.store.nextStep(), 250);\n\n    renderHeading() {\n        return (\n            <div className={s.infoHeading}>\n                <div className={s.arrow}>\n                    <div>{this.config.step}&nbsp;{this.store.step + 1}</div>\n                </div>\n                <div>{this.store.stepInfo.chooseTitle}</div>\n            </div>\n        )\n    }\n\n    renderRecommendationTip() {\n        if (this.store.stepRecommendationTotal > 0)\n            return (\n                <div className={s.recommendations}>\n                    {this.config.recommendedText}\n                </div>\n            )\n    }\n\n    renderSingleSwatchMessage(config) {\n        try {\n            const message = config.includes('{') ? config.split('{')[0] : config;\n            const link = config.includes('{') ? config.split('{')[1].split('}')[0] : config;\n            return (\n                <div className={s.singleSwatchMessage}>\n                    <strong>{message}</strong>\n                    {link && <div onClick={() => this.props.getTheLookStore.backToLandingPage()} className={s.link}>{link}</div>}\n                </div>\n            );\n        }\n        catch(e) {\n            console.log(e)\n            return false;\n        }\n    }\n\n    renderStep(step) {\n        const { product } = this;\n\n        if (step >= this.store.steps) return (\n            <div className={s.reviewContainer}>\n                <div className={s.reviewHeadingPanel}>\n                    {this.config.wizardReviewHeading}\n                </div>\n                <div className={s.reviewHeader} style={{ backgroundImage: `url(${getSafe(() => this.config.reviewImage.fields.file.url)})` }} />\n                <div className={s.reviewMain}>\n                    <div className={s.reviewProductsHeading}>\n                        <div className={s.heading}>\n                            {this.bundleConfig.name}\n                        </div>\n                        <div className={s.reviewDecscription}>\n                            {this.bundleConfig.shortDescription}\n                        </div>\n                    </div>\n                    <div className={s.reviewProducts}>\n                        {this.store.productSelections.map(product => (\n                            <div key={product.productName} className={s.reviewProduct} onClick={() => this.goToStep(product.step)}>\n                                <div className={s.reviewProductImageContainer}>\n                                    <div className={s.reviewProductEdit}>\n                                        <caption>\n                                            {this.config.editIconText}\n                                        </caption>\n                                    </div>\n                                    <img src={product.reviewImage} />\n                                </div>\n                                <div className={s.reviewProductHeading}>\n                                    <div className={s.reviewProductName}>\n                                        {product.productName}\n                                    </div>\n                                    <div className={s.reviewProductVariant}>\n                                        {product.name}\n                                    </div>\n                                </div>\n                            </div>\n                        ))}\n                    </div>\n                </div>\n                {this.renderAddToCart()}\n            </div>\n        );\n\n        const { panel, interfaceStore } = this.props;\n        const data = this.store.stepInfo;\n        const selection = this.store.swatchSelection;\n        const optionalBackground = !this.props.panel &&this.store.stepInfo.optionalBackground;\n        const isSingle = data.swatches.length === 1 && data.singleItemMessage;\n        const defaultSelection = selection || data.defaultSwatch;\n\n        return (\n            <div className={s.stepContainer} key={step} ref={el => {this.el = el}}>\n                {panel && this.renderHeading()}\n                <div className={s.leftMain}>\n                    <TransitionGroup className={cx(s.imageContainer, optionalBackground && s.shiftedImage)}>\n                        <CSSTransition\n                            key={getSafe(() => defaultSelection.mainImage + defaultSelection.id)}\n                            classNames={{\n                                enter: s.fadeIn,\n                                enterActive: s.fadeInActive,\n                                exit: s.fadeOut,\n                                exitActive: s.fadeOutActive,\n                            }}\n                            timeout={{ enter: 250, exit: 250 }}>\n                            <div className={s.imageWrapper} key={getSafe(() => defaultSelection.mainImage)}>\n                                <div\n                                    style={{ backgroundImage: `url(${getSafe(() => defaultSelection.mainImage)})` }}\n                                    className={s.image} />  \n                                {optionalBackground && <div \n                            style={{ backgroundImage: `url(${optionalBackground})` }}\n                            className={s.backgroundImage} />}\n                             </div>  \n                        </CSSTransition>      \n                    </TransitionGroup>      \n                    <div>\n                        {this.renderRecommendationTip()}\n                    </div>\n                </div>\n                <div className={s.info}>\n                    {!panel && this.renderHeading()}\n                    <div\n                        dangerouslySetInnerHTML={{ __html: data.productDescription }}\n                        className={s.infoDescription} />\n                    <div className={s.infoSwatches}>\n                        <div className={s.infoSwatchesHeader}>\n                            <span className={s.infoSwatchesColor}>{data.shadeSelectionLabel || this.config.shadeSelectDefaultLabel}</span> {this.store.selections[step] && selection.name}\n                        </div>\n                        <div className={cx(s.swatchesContainer, \"touch-priority\")}>\n                            <div className={cx(s.swatches, !data.swatchColors && s.swatchesImages, \"touch-priority\")} ref={el => this.swatchesWrapper = el}>\n                                {data.swatches.map(swatch => (\n                                    <div className={cx(s.swatchWrapper, isSingle && s.singleSwatchWrapper)}>\n                                        <div\n                                            key={swatch.id + swatch.name}\n                                            ref={swatch.isSelected && (el => this.recommendedSwatch = el)}\n                                            className={cx(s.swatch, {\n                                                [s.swatchColor] : data.swatchColors,\n                                                [s.swatchImage] : !data.swatchColors,\n                                                [s.selectedImageSwatch]: swatch.isSelected && !data.swatchColors,\n                                                [s.selectedColorSwatch]: swatch.isSelected && data.swatchColors,\n                                                [s.outOfStock]: !swatch.inStock\n                                            }, \"touch-priority\", s[`swatch-step${step}`])} \n                                            onClick={() => this.setSelection(swatch.id, swatch)}>\n                                                {swatch.isRecommended &&\n                                                    <div className={s.swatchIsRecommendedIcon} />\n                                                }\n                                                {swatch.isSelected &&\n                                                    <div className={s.swatchSelectedIcon} />\n                                                }\n                                                {!swatch.inStock &&\n                                                    <div className={s.outOfStockMessage}>\n                                                        {this.config.soldOutMessage}\n                                                    </div>\n                                                }\n                                                {data.swatchColors ? \n                                                    <div className={s.swatchColorOuter}>\n                                                        <div \n                                                            className={s.swatchColorInner} \n                                                            style={{ backgroundColor: swatch.color }} /> \n                                                    </div>\n                                                :\n                                                    <img src={swatch.image} className={\"touch-priority\"} />\n                                                }\n                                                {swatch.name}\n                                        </div>\n                                        {isSingle && this.renderSingleSwatchMessage(data.singleItemMessage)}\n                                    </div>\n                                ))}\n                            </div>\n                        </div>\n                    </div>\n                    <div className={s.infoOptions}>\n                        <div>\n                            {this.renderButton()}\n                        </div>\n                    </div>\n                </div>\n                {!panel && this.renderDots()}\n            </div>\n        )\n    }\n\n    renderButton() {\n        const step = this.store.step;\n        const valid = getSafe(() => this.store.totalSelections && this.store.selections[step]);\n\n        return (\n            <Button onClick={() => this.nextStep(valid)} disabled={!valid} className={s.nextStep}>\n                {step < this.store.steps - 1 ? this.config.stepStrings[step + 2] : this.config.finalStepButton}\n            </Button>\n        );\n    }\n\n    renderDots() {\n        const steps = [];\n        for (var i = 0; i < this.store.steps; i++)\n            steps[i] = i;\n\n        return (\n            <div className={s.dots}>\n                {steps.map(num => {\n                    const disabled = num !== 0 && (!this.store.totalSelections || !this.store.completions[num]);\n\n                    return (\n                        <li\n                            key={num}\n                            className={cx(s.dot, this.store.step === num && s.dotActive, disabled && s.dotDisabled)}\n                            onClick={() => !disabled && this.goToStep(num)} />\n                    )\n                })}\n            </div>\n        );\n    }\n\n    renderAddToCart() {\n        const { price, strikeoutPrice } = this.store;\n        return (\n            <section>\n                <div className={s.addToCart}>\n                    <div className={s.addToCartHeading}>\n                        {this.config.yourCustomizedSet}\n                    </div>\n                    <div className={cx(s.addToCartHeadingPanel)}>\n                        {this.config.reviewAllSet}\n                    </div>\n                    <div className={s.addToCartPrice}>\n                        <span className={s.specialPrice}>\n                            {price}\n                        </span>\n                        {strikeoutPrice}\n                    </div>\n                    <Button cat onClick={() => this.store.addToCart()}>\n                        {this.props.interfaceStore.isPhone ? this.config.addToBagMobile : this.config.addToBag}\n                    </Button>\n                </div>\n            </section>\n        )\n    }\n\n    renderStickyNav() {\n        const show = this.state.stickyActive && this.props.interfaceStore.cartOpen && this.store.step <= this.store.steps;\n\n        const steps = [this.renderRecommendationTip(), this.renderButton(), this.renderDots()];\n\n        return (\n            <Portal visibility={true}>\n                <div className={cx(s.stickyNav, show && s.stickyNavActive)}>\n                    {this.store.step <= this.store.steps -1 && steps}\n                    {this.store.step === this.store.steps && this.renderAddToCart()}\n                </div>\n            </Portal>\n        );\n    }\n\n    closePanel() {\n        this.props.interfaceStore.closeRightPanel();\n    }\n\n    renderInner() {\n        const step = this.store.step;\n\n        return (\n            <div className={cx(s.container, this.props.panel && s.panel)}>\n                <div className={s.header}>\n                    <div className={s.headerLeft}>\n                    <Button \n                        color={'white'} \n                        onClick={() => this.store.reset()} \n                        className={s.startOver}>\n                           {this.config.startOver}\n                    </Button>\n                    </div>\n                    {step >= this.store.steps ? <div className={s.headerReview}>{this.config.wizardReviewHeading}</div> : <div className={s.headerRight}>\n                        <div className={s.headerRightTop}>\n                            {this.bundleConfig.name}\n                        </div>\n                        <div className={s.headerRightBottom}>\n                            {this.bundleConfig.shortDescription}\n                        </div>\n                    </div>}\n                </div>\n                <TransitionGroup id={this.props.panel && `gtl-panel`} className={cx(s.inner, this.store.step === 6 && s.panel_lastStep)}>\n                    <CSSTransition\n                        key={step}\n    