import ScrollToMixin from 'Components/Extensions/ScrollToMixin';\nimport mixin from 'Middleware/utils/decorators/mixinComponent';\nimport { MEDIA_MOBILE, MEDIA_PHONE, MEDIA_LAPTOP } from 'Middleware/constants/media';\nimport ImagePrefetch from 'Components/Extensions/ImagePrefetch';\nimport { throttle } from 'lodash';\nimport { disableBodyScroll } from 'Middleware/utils/scrollLock';\n\nimport Section from 'Common/Section';\nimport Button from 'Common/Button';\nimport SocialNetworks from 'Common/SocialNetworks';\nimport Image from 'Common/Image';\nimport IconClose from 'Icons/Controls/Close.jsx';\nimport IconRefresh from 'Icons/Refresh.jsx';\nimport { setTimeout } from 'timers';\nimport Swipeable from 'react-swipeable';\n\n@withStyles(s)\n@inject(\n    'flawlessStore',\n    'interfaceStore',\n    'contentStore',\n    'localeStore',\n    'productsStore',\n)\n@mixin(ScrollToMixin)\n@observer class FlawlessWizard extends Component {\n\n    constructor(props) {\n        super(props);\n\n        this.autoHideTimeout = 3000;\n        \n        this.state = {\n            stepThumbsOpen: false,\n            stickyActive: false,\n            panelClosed: false\n        }\n        this.swipedUp = this.swipedUp.bind(this);\n        this.swipedDown = this.swipedDown.bind(this);\n    }\n\n    @computed get store() {\n        return this.props.flawlessStore;\n    }\n\n    @computed get config() {\n        return this.props.flawlessStore.config;\n    }\n\n    componentDidMount() {\n        this.scrollTopDisposer = autorun(() => {\n            this.store.step;\n            this.panel = document.getElementById('flawless-panel');\n            if (this.panel) {\n                requestAnimationFrame(() => {\n                    this.panel.scrollTop = 0;\n                    this.stickyUpdate = setInterval(() => this.handleScroll(), 120);\n                    this.state.stickyActive && this.setState({ stickyActive: false });\n                });\n            }\n        });\n        this.panelLockDisposer = autorun(() => {\n            this.panel = document.getElementById('flawless-panel');\n            if (this.props.interfaceStore.cartOpen) {\n                requestAnimationFrame(() => {\n                    if (this.panel) this.panel.scrollTop = 0;\n                });\n                if (this.props.viewportMode <= MEDIA_LAPTOP) {\n                    disableBodyScroll(this.panel);\n                }\n            }\n        });\n    }\n\n    componentDidUpdate(){\n        requestAnimationFrame(this._scrollToSwatch.bind(this))\n    }\n\n    _scrollToSwatch() {\n        if(!!this.recommendedSwatch){\n            this.scrollToHorizontal({scrollWrapper: this.swatchesWrapper, scrollElement: this.recommendedSwatch });\n        }\n    }\n\n    componentWillUnmount() {\n        this.scrollTopDisposer();\n        this.panelLockDisposer();\n        this.stickyUpdate = () => null;\n        clearInterval(this.stickyUpdate);\n    }\n\n    setSelection(id, swatch) {\n        // check is in stock first\n        if (swatch.inStock) {\n            let alreadyOpen = false;\n            if (this.state.stepThumbsOpen !== false)\n              alreadyOpen = true;\n            this.setState({\n                stepThumbsOpen: true,\n            }, () => {\n                // alreadyOpen === false && document.getElementById('flawless-panel').scrollBy(0, 100);\n                this.store.setStepSelection(id);\n                setTimeout(() => {\n                    this.setState({\n                        stepThumbsOpen: false,\n                    }, () => {\n                      // document.getElementById('flawless-panel').scrollBy(0, -100);\n                    });\n                }, this.autoHideTimeout);\n            });\n        }\n    }\n\n    handleScroll = () => {\n        if (this.panel && (this.panel.scrollTop > 110 || this.props.viewportMode > MEDIA_PHONE || window.innerHeight > 780)) {\n            !this.state.stickyActive && this.setState({ stickyActive: true });\n        }\n    };\n\n    goToStep(id) {\n        this.store.goToStep({ step: id });\n\n        if (!this.props.panel && this.props.viewportMode > MEDIA_LAPTOP) {\n            this.scrollTo({\n                hash: `#flawless-wizard`,\n                animate: true,\n                duration: 400,\n                extraHeight: 20,\n            });\n        }\n    }\n    \n    swipedUp(e, deltaY, isFlick) {\n        this.setState({\n            stepThumbsOpen: true,\n        }, () => {\n            // document.getElementById('flawless-panel').scrollBy(0, 100);\n        });\n    }\n\n    swipedDown(e, deltaY, isFlick) {\n        this.setState({\n            stepThumbsOpen: false,\n        }, () => {\n            // document.getElementById('flawless-panel').scrollBy(0, -100);\n        });\n    }\n\n    nextStep = throttle(() => {\n        this.store.nextStep();\n    }, 350);\n\n    renderRecommendationTip() {\n        if (this.store.stepRecommendationTotal > 0)\n            return (\n                <div className={s.recommendations}>\n                    {this.config.recommendedText.replace('%', this.store.bundle[0].productName)}\n                </div>\n            );\n    }\n\n    renderStep(step) {\n        const { product } = this.props.flawlessStore;\n        const {\n            promoCallout1Toggle,\n            promoCallout1RedFlagText,\n            promoCallout2Image,\n            promoCallout2Text,\n            promoCallout1MiddleText,\n            promoCallout1ParenthesesText\n        } = this.config;\n        const { isMobile } = this.props.interfaceStore;\n\n        const InfoHeader = (({ displayFor }) => {\n            \n            if (displayFor !== 'mobile' && isMobile) return null;\n            if (displayFor === 'mobile' && !isMobile) return null;\n            \n\n            return (\n                <div className={s.descriptionArea}>\n                    <div className={s.infoSwatchesHeader}>\n                        <div className={s.headerArrowDownSmall} />\n                        {this.store.selections[step] ? \n                            selection.name :\n                            this.config.makeYourSelection\n                        }\n                    </div>\n                    <div className={s.swatchDescription}>\n                        {this.store.selections[step] && selection.description}\n                    </div>\n                    <div>\n                        {!isMobile && this.renderRecommendationTip()}\n                    </div>\n                </div>\n            );\n        });\n\n        if (step >= this.store.steps) return (\n            <div className={s.reviewContainer + ' ' + (promoCallout1Toggle ? s.enablePromoCallout2 : s.disablePromoCallout2)}\n                key={step}\n                style={{ height: this.config.promoCallout1Toggle ? '1090px' : '1050px' }}>\n                <div className={s.reviewHeader} style={{ backgroundImage: `url(${getSafe(() => this.config.reviewPageImage.fields.file.url)})` }} />\n                <div className={s.addToCartHeading}>\n                    {this.config.yourCustomizedSet}\n                </div>\n                <div className={s.reviewProducts}>\n                    {this.store.productSelections.map(product => (\n                        <div key={product.id} className={s.reviewProduct} onClick={() => this.goToStep(product.step)}>\n                            <div className={s.reviewProductImageContainer}>\n                                <div className={s.reviewProductEdit}>\n                                    {this.config.editIconText}\n                                </div>\n                                <img src={product.reviewImage} />\n                            </div>\n                            <div className={s.reviewProductHeading}>\n                                <div className={s.reviewProductName}>\n                                    {product.productName}\n                                </div>\n                                <div className={s.reviewProductVariant}>\n                                    {product.name}\n                                </div>\n                            </div>\n                        </div>\n                    ))}\n\n                    <div className={s.promoCallout2ContainerMobile}>\n                        <div className={s.promoCallout2CardMobile}>\n                            <div className={s.promo2ImageContainer}>\n                                <img className={s.promoCallout2Image} src={getSafe(() => promoCallout2Image.fields.file.url)} />\n                            </div>\n\n                            <div className={s.promo2CardInfo}>\n                                <span className={s.promo1Flag}>{promoCallout1RedFlagText}</span>\n                                <div className={s.promoCallout2Text}><p>{promoCallout2Text}</p></div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                {this.renderAddToCart()}\n            </div>\n        );\n\n        const { panel } = this.props;\n        const data = this.store.stepInfo;\n        const selection = this.store.selections[step] ? this.store.swatchSelection\n        : {\n            mainImage: getSafe(() => this.store.config.imageDefault.fields.file.url),\n        };\n\n\n        return (\n            <div className={s.stepContainer} key={step} ref={el => {this.el = el}}>\n                \n                <div className={s.leftContentContainer}>\n                    <InfoHeader displayFor='mobile'/>\n                    <TransitionGroup className={s.imageContainer}>\n                        <CSSTransition\n                            key={selection.mainImage}\n                            classNames={{\n                                enter: s.fadeIn,\n                                enterActive: s.fadeInActive,\n                                exit: s.fadeOut,\n                                exitActive: s.fadeOutActive,\n                            }}\n                            timeout={{ enter: 300, exit: 300 }}>\n                                <div\n                                    key={selection.mainImage}\n                                    style={{ backgroundImage: `url(${selection.mainImage})` }}\n                                    className={s.image} />\n                        </CSSTransition>\n                    </TransitionGroup>\n                    {isMobile && this.renderRecommendationTip()}\n                    {!isMobile &&\n                        <div\n                        dangerouslySetInnerHTML={{ __html: data.productDescription }}\n                        className={s.infoDescription} \n                        />\n                    }\n                </div>\n\n                <div className={s.info}>\n                    <div className={s.infoSwatches}>\n                        <InfoHeader displayFor='desktop'/>\n                        <div className={cx(s.swatchesContainer, \"touch-priority\")}>\n                            <div className={cx(s.swatches, \"touch-priority\")} ref={el => this.swatchesWrapper = el}>\n                                {data.swatches.map(swatch => (\n                                    <div\n                                        key={swatch.id}\n                                        ref={swatch.isSelected && (el => this.recommendedSwatch = el)}\n                                        className={cx(s.swatch, s[`swatch-step${step}`], \"touch-priority\", swatch.isSelected && s.selectedSwatch, !swatch.inStock && s.outOfStock)} onClick={() => this.setSelection(swatch.id, swatch)}>\n                                        {swatch.isRecommended &&\n                                            <div className={s.swatchIsRecommendedIcon} />\n                                        }\n                                        {swatch.isSelected &&\n                                   