);\n        this.geolocate = this.geolocate.bind(this);\n    }\n\n    componentDidUpdate() {\n        this.props.autocomplete && this.props.autocomplete.addListener('place_changed', this.fillInAddress);\n    }\n\n    fillInAddress() {\n        const place = this.props.autocomplete.getPlace();\n        let streetNumber = '', route;\n\n        for (let i = 0; i < place.address_components.length; i++) {\n            const result = place.address_components[i];\n            const addressType = result.types[0];\n\n            switch (addressType) {\n                case 'street_number': \n                    streetNumber = result['short_name'];\n                    break;\n                case 'route':\n                    route = result['long_name'];\n                    break;\n                case 'locality':\n                    this.props.form.handleChange(result['long_name'], 'city', this.props.form.city.label);\n                    break;\n                case 'administrative_area_level_1':\n                    this.props.form.handleChange(result['long_name'], 'state', this.props.form.state.label);\n                    break;\n                case 'country':\n                    this.props.form.handleChange(result['long_name'], 'country', this.props.form.country.label);\n                    break;\n                case 'postal_code':\n                    this.props.form.handleChange(result['short_name'], 'zip', this.props.form.zip.label);\n                    break;\n                default: break;\n            }\n\n        this.props.form.handleChange(streetNumber + ' ' + route, 'address', this.props.form.address.label);\n        this.props.form.forceSessionSave();\n        }\n    }\n\n    geolocate() {\n        if (__BROWSER__ && window.navigator && window.navigator.geolocation) {\n            window.navigator.geolocation.getCurrentPosition(position => {\n                const geolocation = {\n                    lat: position.coords.latitude,\n                    lng: position.coords.longitude\n                };\n                const circle = new window.google.maps.Circle({\n                    center: geolocation,\n                    radius: position.coords.accuracy\n                });\n                this.props.autocomplete.setBounds(circle.getBounds());\n            });\n        }\n    }\n\n    render() {\n        const { newAddress, saveAddress } = this.props.content;\n        const form = this.props.form;\n\n        const options = [{\n            name: 'first',\n            selectable: true\n        }, {\n            name: 'second',\n            selectable: true\n        }, {\n            name: 'third',\n            selectable: true\n        }];\n\n        return (\n            <div className={cx(s.root, this.props.useShipping && s.collapsed)}>\n                <div>\n                    <Select title={newAddress} options={options} />\n                </div>\n                <div className={s.row}>\n                    <Input \n                        storeEntry={form.firstName}\n                        wrapperClassName={s.inputSmall}\n                        showLabel={true}\n                        required={true} />\n                    <Input\n                        storeEntry={form.lastName} \n                        wrapperClassName={s.inputSmall}\n                        showLabel={true}\n                        required={true} />\n                </div>\n                <Input\n                    storeEntry={form.company} \n                    wrapperClassName={s.input}\n                    showLabel={true} />\n                <Input\n                    storeEntry={form.address}\n                    id={`address${this.props.parent}`} \n                    onFocus={this.geolocate}\n                    wrapperClassName={s.input}\n                    showLabel={true}\n                    required={true} />\n                <Input\n                    storeEntry={form.apt} \n                    wrapperClassName={s.input}\n                    showLabel={true} />\n                <div className={s.row}>\n                    <Input\n                        storeEntry={form.city} \n                        wrapperClassName={s.inputSmall}\n                        showLabel={true}\n                        required={true} />\n                    <Select\n                        value={form.state.value}\n                        storeEntry={form.state} \n                        wrapperClassName={cx(s.inputSmall, s.select)}\n                        options={this.stateOptions}\n                        uppercaseTopText={true}\n                        required={true} />\n                </div>\n                <div className={s.row}>\n                    <Input\n                        storeEntry={form.zip}\n                        wrapperClassName={s.inputSmall}\n                        showLabel={true}\n                        required={true} />\n                    <Select\n                        storeEntry={form.country}  \n                        wrapperClassName={cx(s.inputSmall, s.select)}\n                        options={this.countryOptions}\n                        uppercaseTopText={true}\n                        required={true} />\n                </div>\n                <Input\n                    storeEntry={form.telephone} \n                    wrapperClassName={s.inputSmall}\n                    showLabel={true} />\n                <Checkbox\n                    className={cx(this.props.checkbox, s.saveAddress)}\n                    title={saveAddress} />\n            </div>\n        );\n    }\n}\n\nexport default withStyles(s)(AddressForm);\n"]}},"_buildHash":"7c4583180a08314e0fe7489c9ba56ca4","hash":"d803c0b55dfface8c4b989794cbe39b6","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column