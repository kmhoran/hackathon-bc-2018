         error: false,\n            value: value.replace(/[^\\w\\s]/gi, ''),\n        };\n\n        this.setState({\n            form: {\n                ...cloneDeep(form),\n                [name]: entry,\n            },\n        });\n    }\n\n    resetSearch = () => {\n        \n        this.setState({\n            results: [],\n            loading: false,\n            showingResults: false,\n            form: cloneDeep(form),\n        });\n        requestAnimationFrame(() => {\n            this.scrollTo({\n                hash: `#consultant-search-form`,\n                duration: 300,\n                extraHeight: 80,\n                animate: true,\n            });\n        });\n    }\n\n    handleSearch = async (event) => {\n        event && event.preventDefault();\n        \n        this.setState({ loading: true });\n\n        let results;\n        const limit = 1000;\n        if (this.state.form.names.value)\n            results = await this.props.attributionStore.searchConsultants(this.state.form.names.value, limit);\n        else\n            results = await this.props.attributionStore.searchConsultantsByLocation(this.state.form.location.value, limit);\n\n        this.setState({\n            results: results ? results.items : [],\n            loading: false,\n            showingResults: true,\n        });\n        \n        setTimeout(() => {\n            requestAnimationFrame(() => {\n                this.scrollTo({\n                    hash: `#consultant-search-results`,\n                    duration: 400,\n                    extraHeight: 80,\n                    animate: true,\n                });\n            })\n        }, 250);\n    }\n\n    render() {\n        const {\n            findConsultantHeading,\n            connectConsultantHeading,\n            connectConsultantBody,\n            searchButton,\n            connectButtonLink,\n            inputFirstLastName,\n            inputLocation\n        } = this.config;\n        const { loading, res