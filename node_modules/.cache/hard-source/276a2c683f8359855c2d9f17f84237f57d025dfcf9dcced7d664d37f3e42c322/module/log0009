 return getSafe(() => this.props.contentStore.extensions.enrollment.data.configEnrollment[0]);\n    }\n\n    @computed get consultant() {\n        return getSafe(() => this.props.userStore.consultant);\n    }\n\n    componentDidMount() {\n        if (this.consultant) {\n            this.setState({\n                showConsultant: true\n            });\n        }\n    }\n\n    handleDefaultClick = () => {\n        this.setState({\n            showMentorSearch: false,\n            showConsultant: false,\n            consultant: {\n                id: 0,\n                fullName: '',\n                imageUrl: '',\n                location: ''\n            }\n        });\n    }\n\n    handleMentorClick = () => {\n        this.setState({\n            showMentorSearch: !this.state.showMentorSearch,\n        });\n    }\n\n    handleInputChange(name, value) {\n        const entry = {\n            ...this.state.form[name],\n            error: false,\n            value: value,\n        };\n\n        let searching = false;\n        if (name === 'names' && value.length >= 3) {\n            searching = true;\n            this.searchConsultants(value);\n        }\n        if (name === 'location' && value.length >= 3) {\n            searching = true;\n            this.searchConsultantsByLocation(value);\n        }\n\n        this.setState({\n            form: {\n                ...this.state.form,\n                [name]: entry,\n            },\n            showResults: true,\n            searching\n        });\n    }\n\n    handleClickOutside = () => {\n        this.setState({\n            searchNameResults: {\n                results: [],\n                time: Date.now()\n            },\n            searchLocationResults: {\n                results: [],\n                time: Date.now()\n            },\n            showResults: false\n        });\n    }\n\n    async searchConsultants(entry) {\n        const results = await this.props.attributionStore.searchConsultants(entry);\n        if(!results || !results.items) {\n            this.setState({\n                searching: false,\n                searchNameResults: {\n                    time: Date.now(),\n                    results: [],\n                }\n            });\n        }\n        else if (Date.now() > this.state.searchNameResults.time) {\n            this.setState({\n                searching: false,\n                searchNameResults: {\n                    time: Date.now(),\n                    results: results.items,\n                    showResults: true,\n                }\n            });\n        }\n        else {\n            this.setState({\n                searching: false\n            });\n        }\n    }\n\n    async searchConsultantsByLocation(entry) {\n        const results = await this.props.attributionStore.searchConsultantsByLocation(entry);\n        if(!results || !results.items) {\n            this.setState({\n                searching: false,\n                searchLocationResults: {\n                    time: Date.now(),\n                    results: [],\n                }\n            });\n        }\n        else if (Date.now() > this.state.searchLocationResults.time) {\n            this.setState({\n                searching: false,\n                searchLocationResults: {\n                    time: Date.now(),\n                    results: results.items,\n                    showResults: true,\n                }\n            });\n        }\n        else {\n            this.setState({\n                searching: false\n            });\n        }\n    }\n\n    setConsultant = (data) => {\n        this.setState({\n            showConsultant: true,\n            consultant: {\n                id: data.accountID,\n                fullName: data.fullName,\n                imageUrl: data.imageUrl,\n                location: data.location,\n            }\n        });\n        // this.props.enrollmentStore.setConsultant({\n        //     id: data.accountID,\n        //     fullName: data.fullName,\n        //     imageUrl: data.imageUrl,\n        //     location: data.location,\n        // });\n        this.resetSearchInput();\n    }\n\n    resetSearchInput() {\n        this.setState({\n            searchNameResults: {\n                results: [],\n                time: Date.now(),\n            },\n            searchLocationResults: {\n                results: [],\n                time: Date.now(),\n            },\n            form: {\n                ...this.state.form,\n                names: {\n                    value: '',\n                    error: false,\n                },\n                location: {\n                    value: '',\n                    error: false,\n                },\n            },\n            showResults: false,\n        });\n    }\n\n    renderDefaultMentor() {\n        const { mentorImagePlaceholder, mentorSelectText1, mentorSelectText2, mentorSelectCtaText, mentorChooseCtaText } = this.config;\n        return (\n            <React.Fragment>\n                <div className={s.mentorSelect}>\n                    <div className={s.mentorText}>\n                        <span className={s.title}>{mentorSelectText1}</span> <span className={s.text}>{mentorSelectText2}</span>\n                    </div>\n                    <div className={s.mentorFields}>\n                        <Button\n                            regular\n                            onClick={() => { this.props.next() }}\n                        >\n                            {mentorSelectCtaText}\n                        </Button>\n                        <span className={s.orText}>or</span>\n                        <Button\n                            regular\n                            color={COLOR_WHITE}\n                            onClick={() => { this.handleMentorClick() }}\n                        >\n                            {mentorChooseCtaText}\n                        </Button>\n                    </div>\n                </div>\n            </React.Fragment>\n        );\n    }\n\n    renderMentorSearch() {\n        const { mentorSearchText, mentorImagePlaceholder, mentorNoSearchResults, mentorChooseText, formLabels } = this.config;\n        const { form, searchNameResults, searchLocationResults, showResults } = this.state;\n        const hasNameResults = searchNameResults.results.length > 0 && true;\n        const hasLocationResults = searchLocationResults.results.length > 0 && true;\n\n        return (\n            <React.Fragment>\n                <div className={s.mentorSelect}>\n                    <div className={s.mentorText}>\n                        <span className={s.title}>{mentorSearchText}</span>\n                    </div>\n                    <div className={s.mentorFields}>\n                        <ClickOutHandler onClickOut={this.handleClickOutside}>\n                            <Input \n                                wrapperClassName={s.input}\n                                defaultValue={form.names.value}\n                                onChange={e => this.handleInputChange('names', e)}\n                                error={form.names.error}\n                                invalid={form.names.error}\n                                showLabel\n                                title={formLabels.mentorName}\n                            />\n                            {\n                                (hasNameResults || form.names.value) && showResults && <div className={s.searchResultsContainer}>\n                                    {searchNameResults.results.map((item, index) => (\n                                        <div \n                                            key={item.accountID}\n                                            onClick={() => this.setConsultant(item)}\n                                            tabIndex={index} \n                                            className={s.searchResult}\n                                        >\n                                            <div \n                                                style={item.imageUrl ? { backgroundImage: `url('${item.imageUrl}')` } : { backgroundImage: `url(${getSafe(() => mentorImagePlaceholder.fields.file.url)})` }} \n                                                className={s.image} \n                                            />\n                                            <div className={s.nameLocation}>\n                                                <div className={s.name}>\n                                                    {item.fullName}\n                                                </div>\n                                                <div className={s.location}>\n                                                    {item.location}\n                                                </div>\n                                            </div>\n                                        </div>\n                                    ))}\n                                    {!hasNameResults && form.names.value.length >= 3 &&\n                                        <div className={s.searchResult}>\n                                            {mentorNoSearchResults}\n                                        </div>\n                                    }\n                                </div>\n                            }\n                        </ClickOutHandler>\n                        <span className={s.orText}>or</span>\n                        <ClickOutHandler onClickOut={this.handleClickOutside}>\n                            <Input\n                                wrapperClassName={s.input}\n                                defaultValue={form.location.value}\n                                onChange={e => this.handleInputChange('location', e)}\n                                error={form.location.error}\n                                invalid={form.location.error}\n                                showLabel\n                                title={formLabels.mentorLocation}\n                            />\n                            {\n                                (hasLocationResults || form.location.value) && showResults && <div className={s.searchResultsContainer}>\n                                    {searchLocationResults.results.map((item, index) => (\n                                        <div \n                                            key={item.accountID}\n                                            onClick={() => this.setConsultant(item)} \n                                            tabIndex={index} \n                                            className={s.searchResult}\n                                        >\n                                            <div \n                                                style={item.imageUrl ? { backgroundImage: `url('${item.imageUrl}')` } : { backgroundImage: `url(${getSafe(() => mentorImagePlaceholder.fields.file.url)})` }} \n                                                className={s.image} \n                                            />\n                                            <div className={s.nameLocation}>\n                                                <div className={s.name}>\n                                                    {item.fullName}\n                                                </div>\n                                                <div className={s.location}>\n                                                    {item.location}\n                                                </div>\n                                            </div>\n                                        </div>\n                                    ))}\n                                    {!hasLocationResults && form.location.value.length >= 3 &&\n                                        <div className={s.searchResult}>\n                                            {mentorNoSearchResults}\n                                        </div>\n                                    }\n                                </div>\n                            }\n                        </ClickOutHandler>\n                    </div>\n                    <div \n                        className={s.defaultLink}\n                        onClick={() => this.handleMentorClick()}\n                    >\n                        {mentorChooseText}\n                    </div>\n                </div>\n            </React.Fragment>\n        );\n    }\n\n    renderConsultant() {\n        const { consultant } = this.state;\n        const { mentorSelectCtaText } = this.config;\n        return (\n            <React.Fragment>\n                <div className={s.mentorSelected}>\n                    <div className={s.mentorInfo}>\n                        <div className={s.mentorName}>{consultant.fullName}</div>\n                        <div className={s.mentorLocation}>{consultant.location}</div>\n                    </div>\n                    <div className={s.mentorButtons}>\n                        <Button\n                            regular\n                            onClick={() => { this.props.next() }}\n                        >\n                            {mentorSelectCtaText}\n                        </Button>\n                        <Button\n                            regular\n                            color={COLOR_WHITE}\n                            onClick={() => { this.handleDefaultClick() }}\n                        >\n                            X\n                        </Button>\n                    </div>\n                </div>\n            </React.Fragment>\n        )\n    }\n\n    render() {\n        const { \n            mentorHeading,\n            mentorSubheading,\n            mentorMarkdown,\n            mentorSelectText1,\n            mentorSelectText2,\n            mentorSearchText,\n            mentorSelectCtaText,\n            mentorChooseCtaText,\n            mentorChooseText,\n            mentorImagePlaceholder,\n            mentorNoSearchResults,\n            formLabels\n        } = this.config;\n\n        const { form, searchNameResults, searchLocationResults, showResults, searching, showMentorSearch, showConsultant, showDefaultMentor, consultant } = this.state;\n        const { firstname } = getSafe(() => this.props.userStore.userInfo);\n        \n        return (\n            <React.Fragment>\n                <Section className={s.root} contentClassName={s.contentContainer}>\n                    <h1 className={s.stepHeading}>\n                        Hi {firstname}, {mentorHeading}\n                    </h1>\n                    <p className={s.stepHeading}>{mentorSubheading}</p>\n                    <div className={s.mentorContainer}>\n                        <img className={s.mentorImage} src={`${consultant.imageUrl ? consultant.imageUrl : mentorImagePlaceholder.fields.file.url}`} alt=\"mentor\" />\n                        { showConsultant ? this.renderConsultant() : (showMentorSearch ? this.renderMentorSearch() : this.renderDefaultMentor()) }\n                    </div>\n                </Section>\n                <div className={s.markdown}>\n                    <MarkdownWrapper wrapperClassName={s.sectionHeader} content={mentorMarkdown} />\n                </div>\n            </React.Fragment>\n        );\n    }\n}\n\nexport default MentorStep;"]}},"_buildHash":"4abe7e7677c38257b2d3192d78c670b2","hash":"e767a4199b397b99f21e6425731d3c33","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start"