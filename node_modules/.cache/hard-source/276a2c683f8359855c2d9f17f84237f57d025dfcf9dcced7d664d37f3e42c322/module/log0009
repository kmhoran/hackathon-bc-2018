rt { computed } from 'mobx';\nimport { routes, assetPath } from 'Root/config';\nimport { getSafe }  from 'Middleware/utils/object';\n\nimport Footer from 'Components/Footer';\nimport Link from 'Common/Link';\nimport Button from 'Common/Button';\nimport Input from 'Common/Input';\nimport Checkbox from 'Common/Checkbox';\n\nimport CheckoutInterstitialAccount from './CheckoutInterstitialAccount';\nimport CheckoutInterstitialAttribution from './CheckoutInterstitialAttribution';\n\n@withStyles(s)\n@inject(\n    'attributionStore',\n    'contentStore',\n    'checkoutStore',\n    'messagingStore',\n    'interfaceStore',\n    'cartStore',\n    'userStore',\n    'analyticsStore'\n)\n@observer class CheckoutInterstitialContainer extends Component {\n    constructor(props) {\n        super(props);\n        this.stepOneComplete = false;\n        this.stepTwoComplete = false;\n    }\n\n    static pageName = 'checkoutInterstitial';\n\n    static contextTypes = {\n        setTitle: PropTypes.func.isRequired,\n    };\n\n    state = {\n        errors: null,\n        form: {},\n        selectingConsultant: false,\n    };\n\n    componentDidMount() {\n        this.props.interfaceStore.fadeInAll();\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.checkout.data.configCheckout[0]);\n    }\n\n    updateTitle() {\n        this.context.setTitle(getSafe(() => this.config.displayName));\n    }\n\n    handleLoginSubmit = async (data) => {\n        this.props.interfaceStore.showProgress(\"login\");\n        const loggedIn = await this.props.userStore.login(data);\n\n        if (!loggedIn.success) {\n            const error = loggedIn.message || \"Invalid username or password.\";\n            this.props.messagingStore.addMessage(error, { type: 'error' });\n        }\n        this.props.interfaceStore.hideProgress(\"login\");\n    };\n\n    renderConsultantSelector() {\n        const {\n            labelPleaseSelectOption,\n            checkboxAttributeToConsultant,\n            checkboxDoNotAttributeToConsultant\n        } = this.config;\n\n\n        if(!this.state.selectingConsultant)\n            return (\n                <React.Fragment>\n                    {labelPleaseSelectOption}<br /><br />\n                    <Checkbox title={checkboxAttributeToConsultant}></Checkbox>\n                    <Checkbox title={checkboxDoNotAttributeToConsultant} />\n                    <br /><br /><br />\n                    <Button onClick={() => this.setState({ selectingConsultant: true })}>{buttonAccountContinue}</Button>\n                </React.Fragment>\n            );\n        return (\n            <React.Fragment>\n                {labelPleaseSelectOption}<br /><br />\n                <Checkbox title={checkboxDoNotAttributeToConsultant}></Checkbox>\n                    <Input \n                        className={s.inputEmail}\n                        showLabel={true}\n                        required\n                        type={'email'} \n                        title={'Search a consultant'} /><br />\n                <Button onClick={() => this.setState({ selectingConsultant: true })}>Continue</Button>\n            </React.Fragment>\n        );\n    }\n\n    trackStep(stepOne) {\n        if (stepOne && !this.stepOneComplete) {\n            this.props.analyticsStore.trackCheckoutStep(1);\n            this.stepOneComplete = true;\n        } else if (!stepOne && !this.stepTwoComplete) {\n            this.props.analyticsStore.trackCheckoutStep(2);\n            this.stepTwoComplete = true;\n        }\n    }\n\n    render() {\n        const { consultantId } = this.props.checkoutStore;\n        const { isConsultant, isEmployee } = this.props.userStore;\n        const {\n            headingAccount,\n            headingShoppingWithConsultant,\n            headingYourConsultant\n        } = this.config;\n\n        if (isConsultant || isEmployee) {\n            this.props.checkoutStore.checkout();\n            return false;\n        }\n\n        const activeLeft = !this.props.userStore.isLogged && !this.props.userStore.isGuest;\n        const activeRight = this.props.userStore.isLogged || this.props.userStore.isGuest;\n\n        this.updateTitle();\n        this.trackStep(activeLeft);\n\n        return (\n            <React.Fragment>\n                <div className={s.root}>\n                    <div className={s.header}>\n                        <Link to='/'\n                            className={s.logo} \n                            image={`${assetPath}/images/logo-full.svg`} />\n                    </div>\n                    <div className={s.content}>\n                        <div className={s.left}>\n                            <div className={cx(s.overlay, !activeLeft && s.activePanel)} />\n                            <div className={s.headingBox}>\n                                <div className={s.boxHeading}>\n                                    {headingAccount}\n                                </div>\n                                <CheckoutInterstitialAccount />\n                            </div>\n                        </div>\n                        <div className={s.right}>\n                            <div className={cx(s.overlay, !activeRight && s.activePanel)} />\n                            <div className={s.headingBox}>\n                                <div className={s.boxHeading}>\n                                    {!consultantId ? \n                                    headingShoppingWithConsultant :\n                                    headingYourConsultant}\n                                </div>\n                                <CheckoutInterstitialAttribution />\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div className={s.footerWrapper}>\n                    <Footer minimal={true} />\n                </div>\n            </React.Fragment>\n        );\n 