n@withStyles(s)\n@inject(\n    'contentStore',\n    'interfaceStore',\n    'attributionStore',\n)\n@mixin(ScrollToMixin)\n@observer class FindConsultantResults extends Component {\n\n    static defaultProps = {\n        results: [],\n    }\n\n    state = {\n        page: 1,\n    }\n\n    componentWillReceiveProps(nextProps) {\n        if (nextProps.results !== this.props.results) this.setState({ page: 1 });\n    }\n\n    @computed get perPage() {\n        const { showShopButton } = this.config;\n        const { isPhone } = this.props.interfaceStore;\n        return isPhone ? 20 : showShopButton ? 19 : 20;\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.findConsultant.data.configFindConsultant[0]);\n    }\n\n    @computed get pageResults() {\n        return drop(this.props.results, (this.state.page - 1) * this.perPage).slice(0, this.perPage);\n    }\n\n    @computed get pageCount() {\n        return getSafe(() => Math.ceil(this.props.results.length/this.perPage));\n    }\n\n    resetSearch = () => {\n        this.props.resetSearch && this.props.resetSearch();\n    }\n\n    linkify = (item) => {\n        try {\n            const trySearchAgain = <span className={s.link} onClick={() => this.resetSearch()}>{item.split('{')[1].split('}')[0]}</span>;\n\n            return <div>{item.split('{')[0]}{trySearchAgain}{item.split('}')[1]}</div>;\n        }\n        catch(e) {\n            console.error(e);\n            return <div />;\n        }\n    }\n\n    goToPage(page) {\n        this.setState({ page });\n        requestAnimationFrame(() => {\n            this.scrollTo({\n                hash: `#consultant-search-results`,\n                duration: 400,\n                extraHeight: 80,\n                animate: true,\n            });\n        });\n    }\n\n    render() {\n        const { \n            labelSearchResults,\n            captionSearchResults,\n            captionNoSearchResults,\n            labelSearchCount,\n            labelShopWith,\n            shopButton,\n            shopButtonCta,\n            labelPageCount,\n            linkTryDifferentSearch,\n            placeholderResultImage,\n            showShopButton\n        } = this.config;\n        const { results } = this.props;\n        const { isMobile, isPhone } = this.props.interfaceStore;\n        const hasResults = results.length ? true : false;\n\n        const resultsCaption = this.linkify(captionSearchResults);\n        const noResultsCaption = this.linkify(captionNoSearchResults);\n        const placeholderImageUrl = getSafe(() => placeholderResultImage.fields.file.url);\n\n        const paginationConfig = {\n            pageCount: this.pageCount,\n            goToPage: page => this.goToPage(page),\n            page: this.state.page,\n            pageTrim: isMobile ? 4 : 6,\n            pageText: labelPageCount,\n        };\n\n        return (\n            <Section column={true}>\n                <div className={s.heading} id='consultant-search-results'>\n                    <h2>\n                        {labelSearchResults}\n                    </h2>\n                    {hasResults ? resultsCaption : noResultsCaption}\n                </div>\n                {(this.pageResults.length !== 0 || showShopButton) &&\n                <div className={s.resultsContainer}>\n                    {hasResults && <div className={s.topBar}>\n                        {labelSearchCount.replace('%', results.length || 0)}\n                        <Pagination {...paginationConfig} />\n                    </div>}\n                    <div className={cx(s.results, !hasResults && s.noResults)}>\n                        {showShopButton && <div className={s.result}>\n                            <Link to={shopButtonCta || '/'}>\n                                <div className={s.specialResult}>\n                                <ReactMarkdown source={shopButton} escapeHtml={false} />\n                                    <IosArrowThinRight\n                                        color={'#fff'}\n                                    />\n                                </div>\n                            </Link>\n                        </div>}\n                        {this.pageResults.map((result, key) => (\n                            <div className={s.result} key={result.accountID + result.fullName}>\n                                <img \n                                    className={s.resultImage}\n                                    onClick={() => this.props.attributionStore.goToPWS(result.accountID)}\n                                    src={result.imageUrl || placeholderImageUrl}\n                                    onError={e => e.target.setAttribute(\"src\", placeholderImageUrl)}/>\n                                <div className={s.resultPWS}>\n                                    <div onClick={() => this.props.attributionStore.goToPWS(result.accountID)}>\n                                        {labelShopWith} \n                                        <IosArrowThinRight size={{ width: 28, height: 28 }} />\n                                    </div>\n                                </div>\n                                <div className={s.resultName}>\n                                    {result.fullName}\n                                </div>\n                                <div className={s.resultLocation}>\n                                    {result.location}\n                                </div>\n                            </div>\n                        ))}\n                    </div>\n                </div>}\n                {hasResults && <div className={s.footer}>\n                    <Pagination {...paginationConfig} />\n                    <span className={s.link} onClick={() => this.resetSearch()}>\n                        {linkTryDifferentSearch}\n                    </span>\n                </div>}\n            </Section>\n        );\n    }\n\n}\n\nexport defaul