n        if (isMobile) {\n            this.panelOpen = true;\n            this.renderWizard();\n        } else {\n            this.scrollTo({\n                hash: `#flawless-wizard`,\n                animate: true,\n                duration: 500,\n                extraHeight: 20,\n            });\n        }\n    }\n\n    componentWillUnmount() {\n        this.flawlessStoreInstance = null;\n    }\n\n    playVideo() {\n        this.setState({ videoOpen: true });\n    }\n\n    closeVideo() {\n        this.setState({ videoOpen: false });\n    }\n\n    renderTutorialLink() {\n        const { tutorialsLink } = this.config;\n\n        return (\n            <Link to={getSafe(() => tutorialsLink.fields.url)} underlined={true}>\n                {getSafe(() => tutorialsLink.fields.name)}\n            </Link>\n        );\n    }\n\n    renderWizard() {\n        const { interfaceStore } = this.props;\n        const { isPhone, isMobile } = interfaceStore;\n\n        //on phone\n        if (isMobile && this.panelOpen) {\n            interfaceStore.setRightPanel(\n                this.renderWizardContent(),\n                'fif-1'\n            );\n            interfaceStore.openRightPanel();\n        }\n\n        //on a non phone\n        return isMobile ? null :\n            (() => {\n                if (interfaceStore.cartOpen && interfaceStore.panelContentName === 'fif-1') interfaceStore.closeRightPanel();\n                return this.renderWizardContent();\n            })();\n    };\n\n    renderWizardContent() {\n        return (\n            <FlawlessWizard \n            panel={this.props.interfaceStore.isMobile}\n            viewportMode={this.state.viewportMode} \n            flawlessStore={this.flawlessStoreInstance} \n            />\n        );\n    }\n\n    render() {\n        const {\n            heading,\n            subheading,\n            bodyText,\n            mobileCallToAction,\n            mobileCallToActionContinue,\n            desktopCallToAction,\n        