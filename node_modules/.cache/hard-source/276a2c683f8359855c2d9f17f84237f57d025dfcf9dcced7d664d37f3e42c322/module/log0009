pplyCustom,\n            buttonApply: this.config.boBButtonApply,\n            yourRewards: this.config.boBYourRewards,\n            currencySymbol: this.config.boBCurrencySymbol\n        };\n    }\n\n    @computed get giftContent() {\n        return { \n            enterMessage: this.config.giftEnterMessage,\n            options: this.config.giftOptions,\n            textareaCaption: this.config.giftTextareaCaption,\n            textareaPlaceholder: this.config.giftTextareaPlaceholder\n        };\n    }\n\n    @computed get orderReviewContent() {\n        return { \n            terms: this.config.orderReviewTerms,\n            labels: this.config.orderReviewLabels\n        };\n    }\n\n    scrollTo(node) {\n        __BROWSER__ && setTimeout(() => {\n            this.props.interfaceStore.isMobile && window.scrollBy({ \n                top: this.refs[node].getBoundingClientRect().top, \n                behavior: 'smooth' \n            });\n        }, 0);\n    }\n\n    changeStep(nextStep) {\n        const newState = { step: nextStep };\n        if (!this.state.visited[nextStep]) newState.visited = { ...this.state.visited, [nextStep]: true };\n        this.setState(newState);\n        if (nextStep === 1) this.scrollTo('address');\n        else if (nextStep === 2) this.scrollTo('method');\n        else if (nextStep === 3) this.scrollTo('review');\n    }\n\n    handleAddressContinue() {\n        this.changeStep(2);\n    }\n\n    handlePaymentContinue() {\n        this.changeStep(3);\n    }\n\n    handlePurchase() {\n        console.log('purchase');\n    }\n\n    handleBillingCheckbox(value) {\n        this.setState({ useShipping: !!value });\n    }\n\n    render() {\n\n        const { \n            orderReviewGiftButtonNextStep, \n            shippingBillingButtonNextStep,\n            shippingPaymentMethodButtonNextStep,\n            billingInformationHeader,\n            shippingInformationHeader,\n            shippingMethodHeader,\n            orderReviewHeader,\n   