Styles from 'isomorphic-style-loader/lib/withStyles';\nimport s from './CheckoutInterstitialAccount.scss';\nimport cx from 'classnames';\nimport { observer, inject } from 'mobx-react';\nimport { computed } from 'mobx';\nimport { routes } from 'Root/config';\nimport { validEmail } from 'Middleware/utils/validators';\nimport { debounce } from 'lodash';\nimport { getSafe }  from 'Middleware/utils/object';\nimport { CONSULTANT, EMPLOYEE } from 'Middleware/constants/userStates';\n\nimport Link from 'Common/Link';\nimport Button from 'Common/Button';\nimport Input from 'Common/Input';\nimport Checkbox from 'Common/Checkbox';\nimport IconSpinner from 'Icons/Spinner.jsx';\n\nconst form = {\n    email: {\n        value: '',\n        error: false,\n    },\n    password: {\n        value: '',\n        error: false,\n    },\n};\n\n@withStyles(s)\n@inject(\n    'attributionStore',\n    'contentStore',\n    'interfaceStore',\n    'messagingStore',\n    'localeStore',\n    'cartStore',\n    'userStore',\n)\n@observer class CheckoutInterstitialAccount extends Component {\n\n    constructor(props) {\n        super(props);\n\n        this.debounceCheckEmailExists = debounce(this.checkEmailExists, 220);\n        this.debounceSaveButton = debounce(this.handleSaveAccount, 120);\n    }\n\n    state = {\n        errors: null,\n        form,\n        completeEmail: false,\n        guestEmail: false,\n        checkingEmailExists: false,\n        lastUpdated: new Date(),\n        isSpecialEmail: false\n    };\n\n    componentWill() {\n        autorun(() => {\n            this.props.localeStore.activeLocale;\n            this.setState({ form: cloneDeep(form) });\n        })\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.checkout.data.configCheckout[0]);\n    }\n\n    handleLoginSubmit = async () => {\n        const { errorInvalidLoginOrPassword } = this.config;\n        if (this.validateInput()) {\n            this.props.interfaceStore.showProgress(\"login\");\n            const loggedIn = await this.props.userStore.login({\n                email: this.state.form.email.value,\n                password: this.state.form.password.value,\n            });\n\n            if (!loggedIn.success) {\n                const error = loggedIn.message || 'There was an error.';\n\n                if (error === \"Invalid login or password.\") {\n                    this.state.form.password.error = errorInvalidLoginOrPassword;\n                    this.setState({\n                        form: {\n                            ...this.state.form,\n                        },\n                    });\n                } else {\n                    this.props.messagingStore.addMessage(error, { type: 'error' });\n                }\n            }\n            this.props.interfaceStore.hideProgress(\"login\");\n        }\n    };\n\n    handleSaveAccount = async () => {\n        const { errorInvalidEmail } = this.config;\n\n        if (this.validateInput()) {\n            const response = await this.props.userStore.createAccount({\n                firstname: 'Guest',\n                lastname: 'Shopper',\n                email: this.state.form.email.value,\n                password: this.state.form.password.value,\n                confirmation: this.state.form.password.value,\n            });\n\n            if (!response.success) {\n                try {\n                    const message = typeof response.message === 'object' ? response.message[Object.keys(response.message)[0]] : response.message;\n                    if (message == '\"Email\" is not a valid hostname.') {\n                        this.state.form.email.error = errorInvalidEmail;\n                        this.setState({ form: this.state.form });\n                    }\n                    else\n                        this.props.messagingStore.addMessage(message, { type: 'error' });\n                }\n                catch(e) {\n                    this.props.messagingStore.addMessage('There was an error.', { type: 'error' });\n                }\n            }\n        }\n    }\n\n    handleFormSubmit = (e) => {\n        if (e.key === 'Enter') {\n            if (this.state.emailExists) this.handleLoginSubmit();\n            else this.handleSaveAccount();\n            e.preventDefault();\n        }\n    }\n    \n    handleInputChange(name, value, regex = false) {\n        const entry = {\n            ...this.state.form[name],\n            error: false,\n            value: value,\n        };\n\n        let checkingEmailExists = false;\n        let completeEmail = this.state.completeEmail;\n        if (name === 'email') {\n            if (validEmail(value))\n                checkingEmailExists = true;\n            else\n                completeEmail = false;\n        }\n        this.setState({\n            form: {\n                ...this.state.form,\n                [name]: entry,\n            },\n            checkingEmailExists,\n            lastUpdated: new Date(),\n            completeEmail,\n        });\n\n        if (checkingEmailExists) this.debounceCheckEmailExists(value);\n    }\n\n    validateInput() {\n        const form = this.state.form;\n        const {\n            errorRequiredField,\n            errorMinPasswordLength,\n            errorInvalidEmail\n        } = this.config;\n\n        let errors = 0;\n        if (this.state.completeEmail) {\n            if (form.password.value.length < 6) {\n                this.state.form.password.error = !form.password.value ? errorRequiredField : errorMinPasswordLength;\n                errors++;\n            }\n        }\n\n        if (!validEmail(form.email.value)) {\n            this.state.form.email.error = !form.email.value ? errorRequiredField : errorInvalidEmail;\n            errors++;\n        }\n        this.setState({});\n        if (errors > 0) return false;\n        return true;\n    }\n\n    checkEmailExists = async (value) => {\n        this.setState({ \n            completeEmail: false,\n            emailExists: false,\n        });\n        const result = await this.props.userStore.checkEmailExists(value);\n        let completeEmail = false;\n        let emailExists = false;\n        const reservedEmail = this.isReservedEmail(value);\n\n        if (validEmail(value) && value === this.state.form.email.value && Date.now() >= this.state.lastUpdated) {\n            completeEmail = true;\n            if (result.success) {\n                emailExists = true;\n            }\n            if (reservedEmail) {\n                emailExists = true;\n            }\n        }\n\n        this.setState({ \n            completeEmail,\n            emailExists,\n            checkingEmailExists: false,\n            lastUpdated: new Date(),\n            isSpecialEmail: getSafe(() => result.customerDetails.GroupId === CONSULTANT || result.customerDetails.GroupId === EMPLOYEE) || reservedEmail,\n        });\n    }\n\n    isReservedEmail(value) {\n        try {\n            const end = `@${value.toLowerCase().split('@')[1]}`;\n            return this.config.reservedEmails.filter(email => end === email.toLowerCase()).length;\n        }\n        catch(e) {\n            return;\n        };\n    }\n\n    setGuestEmail() {\n        if (validEmail(this.state.form.email.value))\n            this.props.userStore.setGuest(this.state.form.email.value, 'test');\n    }\n\n    render() {\n        const { \n            form, \n            completeEmail, \n            emailExists, \n            isSpecialEmail,\n            checkingEmailExists\n        } = this.state;\n        const {\n            messageWelcome,\n            messageNewAccountWelcome,\n            messageMemberProgramWelcome,\n            labelAccountForm,\n            buttonAccountContinue,\n            labelEmail,\n            labelPassword,\n            buttonContinueAsGuest,\n            buttonSaveAccount,\n            buttonSignIn,\n            buttonForgotPassword\n        } = this.config;\n        const { hasMemberOnlyItems } = this.props.cartStore;\n\n        if(this.props.userStore.isLogged || this.props.userStore.isGuest)\n            return (\n                <div className={s.loggedIn}>\n                    <div className={s.heading}>\n                        {messageWelcome}\n                    </div>\n                    {this.props.userStore.userInfo.email || this.props.userStore.guest.email}\n                </div>\n            );\n        else\n            return (\n                <form className={s.root} onSubmit={this.handleFormSubmit.bind(this)} onKeyPress={e => this.handleFormSubmit(e)}>\n                    <div className={s.mainCaption}>\n                        {labelAccountForm}\n                    </div>\n                    <Input \n                        onChange={e => this.handleInputChange('email', e)} \n                        className={s.inputEmail}\n                        showLabel={true}\n                        defaultValue={form.email.value}\n                        error={form.email.error}\n                        invalid={form.email.error}\n                        required\n                        type={'email'} \n                        icon={<IconSpinner className={cx(s.spinner,checkingEmailExists && s.spinnerActive)} size={{ width: 20, height: 20 }} color={'black'} fill={'black'} />}\n                        title={labelEmail} />\n                    {!completeEmail && \n                        <div className={s.buttonContainer}>\n                            <div onClick={() => this.validateInput()}>\n                                <Button disabled={true} regular>{buttonAccountContinue}</Button>\n                            </div>\n                        </div>\n                    }\n                    {completeEmail && (\n                        <div className={s.bottomForm}>\n                            {!emailExists && <div className={s.welcomeMessage}>\n                                <div>{hasMemberOnlyItems ? messageMemberProgramWelcome : messageNewAccountWelcome}</div>\n                            </div>\n                            }\n                            <Input \n                                onChange={e => this.handleInputChange('password', e)} \n                                className={s.inputEmail}\n                                showLabel={true}\n                                defaultValue={form.password.value}\n                                error={form.password.error}\n                                invalid={form.password.error}\n                                required\n                                type={'password'} \n                                title={labelPassword} />\n                            <div className={s.buttonContainer}>\n                                {emailExists && <Button type=\"submit\" regular onClick={() => this.handleLoginSubmit()}>{buttonSignIn}</Button>}\n                                {!emailExists && <Button type=\"submit\" regular onClick={() => this.debounceSaveButton()}>{buttonSaveAccount}</Button>}\n                                {!hasMemberOnlyItems && !isSpecialEmail && <Button regular onClick={() => this.setGuestEmail()} color={'white'}>{buttonContinueAsGuest}</Button>}\n                            </div>\n                            {emailExists && <div className={s.forgotPassword}>\n                                <Link to={routes.FORGOT_PASSWORD}>\n                                    {buttonForgotPassword}\n                                </Link>\n                            </div>}\n                        </div>\n                    )}\n                </form>\n            );\n    }\n}\n\nexport default CheckoutInterstitialAccount;\n"]}},"_buildHash":"e0f7cb82c18c7aeda2ea2c943c058ded","hash":"a0f3a9a4da84c1bf8e439ecc294d50f7","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","