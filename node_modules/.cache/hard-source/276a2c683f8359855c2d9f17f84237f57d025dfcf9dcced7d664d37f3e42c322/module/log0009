        const MobileSize = ({ children }) => {\n            return viewportWidth <= MEDIA_MOBILE ? children : null;\n        };\n        const LargerThanMobileSize = ({ children }) => {\n            return viewportWidth > MEDIA_MOBILE ? children : null;\n        };\n        const iconPrefs = (() => {\n            const color = MobileSize(true) === null ? '#000000' : '#1B2B45';\n            return {\n                    color,\n                    thickness: 5,\n            };\n        })();\n        const duration = 400;\n        const defaultStyle = {\n            transition: `opacity ${duration}ms ease-in-out, transform ${duration}ms ease-in-out`,\n            opacity: '0'\n        };\n        const transitionStyles = {\n            entered: { transform: 'translateX(0%)', opacity: '.9' },\n        };\n        let defaultContentStyle = {\n            transition: `opacity ${duration}ms ease-in-out, transform ${duration}ms ease-in-out`,\n            transform: 'scale(.8) translateX(-60%)',\n            opacity: '0'\n        };\n        let contentTransitionStyles = {\n            entered: { transform: 'scale(1) translateX(-50%)', opacity: '1' },\n        };\n        if (LargerThanMobileSize(true) !== null) {\n            defaultContentStyle = {\n                transition: `opacity ${duration}ms ease-in-out, transform ${duration}ms ease-in-out`,\n                transform: 'scale(.8) translateX(-60%) translateY(-60%)',\n                opacity: '0'\n            };\n            contentTransitionStyles = {\n                entered: { transform: 'scale(1) translateX(-50%) translateY(-50%)', opacity: '1' },\n            };\n        }\n\n//api extractions\n        const image = getSafe(() =>\n            campaign.imageBG.fields.file.url);\n        const logo = getSafe(() =>\n            campaign.logo.fields.file.url);\n        let { headline, bodyText, emailPlaceholder, invalidEmailMessage, submitButton, emailDisplayed, locationInfo } = campaign;\n\n        let AddressDisplayStyles = {};\n        if (error) AddressDisplayStyles = {visibility: 'hidden'};\n        let AddressDisplay = () => \n        <span className={s.emailPop__addressSection} style={AddressDisplayStyles}>\n            <MobileSize>\n                {locationInfo}<br />\n                <a href={\"mailto:\" + emailDisplayed}>{emailDisplayed}</a>\n            </MobileSize>\n            <LargerThanMobileSize>\n                <hr />{locationInfo} | <a href={\"mailto:\" + emailDisplayed}>{emailDisplayed}</a>\n            </LargerThanMobileSize>\n        </span>;\n        const customSubmit = MobileSize(true) === null ?\n            <div className={s.emailPop__submitButtonDesktop}>{submitButton}</div> : <div className={s.emailPop__submitButtonMobile}><IosArrowThinRight \n            color={'#FFFFFF'}\n                size={SIZE_EXTRA_LARGE}\n            /></div>;\n        let EmailInputArea = () => <EmailSignup\n            successHandler={() => this.handleSuccess()}\n            errorHandler={() => this.handleError()}\n            placeholder={emailPlaceholder}\n            success={'n/a'}\n            invalid={invalidEmailMessage}\n            customSubmit={customSubmit}\n            source='Modal'\n        />;\n\n        if (success) {\n            headline = campaign.headlineSuccess;\n            bodyText = campaign.successText;\n            EmailInputArea = () => <div \n            className={s.emailPop__successButton}\n            onClick={() => handleClick()}\n            >\n                {campaign.successButton}\n            </div>;\n        }\n\n        // console.log(campaign);\n\n        return (\n            <React.Fragment>\n                <div className={s.emailBG}>\n                    <Transition in={!!show} timeout={duration}>\n                        {state => (\n                            <React.Fragment>\n                                <div\n                                    className={s.emailBG__triangle_bottom}\n                               