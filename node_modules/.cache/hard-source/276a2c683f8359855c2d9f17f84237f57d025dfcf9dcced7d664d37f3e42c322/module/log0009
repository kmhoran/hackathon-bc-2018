nimport GetTheLookWizard from './GetTheLookWizard/GetTheLookWizard.jsx';\n\n@inject('contentStore', 'getTheLookStore', 'interfaceStore')\n@withStyles(s)\n@mixin(ScrollToMixin)\n@observer\nclass GetTheLookContainer extends Component {\n    static contextTypes = {\n        setTitle: pt.func.isRequired,\n        onSetMeta: pt.func.isRequired\n    };\n\n    constructor(props) {\n        super(props);\n        console.log('BCCOM-2804');\n\n        this.state = {\n            selectedProductIndex: -1,\n            selectedModelIndex: 0,\n            path: this.props.path || '',\n        };\n\n        this.resetProductIndex = this.resetProductIndex.bind(this);\n        this.setSelectedProductIndex = this.setSelectedProductIndex.bind(this);\n        this.scrollToBody = this.scrollToBody.bind(this);\n        this.onSliderChange = this.onSliderChange.bind(this);\n        this.openWizard = this.openWizard.bind(this);\n        this.setSelectedModelIndex = this.setSelectedModelIndex.bind(this);\n        this.goBackToPanelView = this.goBackToPanelView.bind(this);\n        this.handleHeroCta = this.handleHeroCta.bind(this);\n    }\n\n    get safeProductIndex() {\n        return this.state.selectedProductIndex < 0 ? 0 : this.state.selectedProductIndex;\n    }\n\n    get isProductSelected() {\n        return this.state.selectedProductIndex >= 0;\n    }\n\n    get selectedBundle() {\n        return this.config.bundles[this.safeProductIndex].fields;\n    }\n\n    @computed\n    get config() {\n        return getSafe(\n            () => this.props.contentStore.extensions.getTheLook.data.configGetTheLook[0],\n        );\n    }\n\n    updateTitle() {\n        this.context.setTitle(getSafe(() => this.config.displayName) || null);\n    }\n    \n    setPageMeta() {\n        setSEOMeta(this.context, getSafe(() => this.config.configSEO.fields));\n    }\n\n    setSelectedModelIndex(selectedModelIndex) {\n        this.setState({ selectedModelIndex });\n    }\n\n    setSelectedProductIndex(selectedProductIndex) {\n        this.setState({ selectedProductIndex });\n    }\n\n    resetProductIndex() {\n        this.setState({ selectedProductIndex: -1 });\n    }\n\n    onSliderChange(e) {\n        if (!this.props.interfaceStore.isPhone) {\n            this.resetProductIndex();\n        }\n        this.setSelectedModelIndex(parseInt(e.target.value));\n    }\n\n    scrollToBody() {\n        this.scrollTo({\n            hash: '#scroll-to-id-hash',\n            extraHeight: 65,\n        });\n    }\n\n    goBackToPanelView() {\n        this.props.getTheLookStore.backToLandingPage();\n    }\n\n    handleHeroCta() {\n        if(this.state.path){\n            this.goBackToPanelView();\n        }\n        this.scrollToBody();\n    }\n\n    openWizard() {\n        const { bundles, models } = this.config;\n\n        const bundle = bundles[this.safeProductIndex].fields;\n        const selection = models[this.state.selectedModelIndex].fields;\n\n        this.props.getTheLookStore.preselectBundle(bundle.handle, [selection.preselectionId]);\n        this.setState({ path: bundle.handle });\n        this.scrollToBody();\n        this.openPanel();\n    }\n\n    componentDidMount() {\n        this.updateTitle();\n        this.setPageMeta();\n        this.mobileDisposer = autorun(() => {\n            if (\n                this.props.path &&\n                this.props.interfaceStore.isPhone &&\n                this.props.interfaceStore.appIsMounted\n            )\n                this.openPanel();\n            else {\n                this.props.interfaceStore.closeRightPanel();\n            }\n        });\n    }\n\n    async openPanel() {\n        if (this.props.interfaceStore.isPhone) {\n            const getTheLookStore = await loadContainer('GetTheLookStore');\n            const content = (\n                <Provider getTheLookStore={getTheLookStore}>\n                    <GetTheLookWizard handle={this.selectedBundle.handle} panel />\n                </Provider>\n            );\n            this.asyncPanelsDisposer = autorun(() => {\n                this.props.interfaceStore.setRightPanel(content);\n                this.props.interfaceStore.openRightPanel();\n            });\n        }\n    }\n\n    componentWillReceiveProps(nextProps) {\n        if (nextProps.path && this.props.interfaceStore.isPhone) this.openPanel();\n        if (nextProps.path !== this.state.path) {\n            this.setState({ path: nextProps.path });\n            this.scrollToBody();\n        }\n    }\n\n    componentWillUnmount() {\n        if (typeof this.asyncPanelsDisposer === 'function') this.asyncPanelsDisposer();\n        this.mobileDisposer();\n    }\n\n    renderContainer() {\n        const { selectedProductIndex, selectedModelIndex, path } = this.state;\n\n        const { interfaceStore } = this.props;\n\n        const {\n            heroMarkdown,\n            heroBackgroundImage,\n            heroPhoneBackgroundImage,\n            heroCta,\n            models,\n            sliderTitle,\n            bundles,\n            textToggleShadePrompt1,\n            textToggleShadePrompt2,\n            textOpenWizardCta,\n            customHex,\n        } = this.config;\n\n        const sliderLabels = models.map(model => model.fields.title);\n\n        const selectedModelAssets = models[selectedModelIndex].fields.closeUpImage;\n        const productModelAssets = models.map(\n            model => model.fields.closeUpImage[this.safeProductIndex],\n        );\n        const productNames = bundles.map(bundle => bundle.fields.name);\n        const selectedProductBundle = getSafe(() => bundles[this.safeProductIndex].fields);\n\n        const RangeComponent = (\n            <RangeSlider\n                step={1}\n                min={0}\n                max={3}\n                defaultValue={selectedModelIndex}\n                labels={sliderLabels}\n                handleChange={this.onSliderChange}\n            />\n        );\n\n\n        //TODO: refactor to fetch data in Panel Component\n        const innerContentProps = {\n            //static\n            selectedModel :  models[selectedModelIndex],\n            selectedProductIndex :  this.safeProductIndex,\n            rangeComponent :  RangeComponent,\n            textOpenWizardCta :  textOpenWizardCta,\n            textToggleShadePrompt1 :  textToggleShadePrompt1,\n            textToggleShadePrompt2 :  textToggleShadePrompt2,\n            selectedModelIndex :  selectedModelIndex,\n            delayRender: 1100,\n            // callback\n            onClose :  this.resetProductIndex,\n            openWizardCb :  this.openWizard,\n            scrollToCb :  this.scrollToBody,\n            setSelectedModelIndex :  this.setSelectedModelIndex,\n            // dynamic per panel\n            palette :  selectedProductBundle.palette,\n            leftCarouselMedia :  productModelAssets,\n            rightCarouselMedia :  selectedModelAssets,\n            briefOverview :  selectedProductBundle.briefOverview,\n            headerTitle :  selectedProductBundle.name,\n            subHeaderText :  selectedProductBundle.shortDescription,\n        }\n\n        const PanelComponent = (\n            <CSSTransition\n                key={'panels'}\n                classNames={{\n                    enter: s.modelTransitionEnter,\n                    enterActive: s.modelTransitionActive,\n                    exit: s.modelTransitionLeave,\n                    exitActive: s.modelTransitionLeaveActive,\n                }}\n                timeout={{ enter: 700, appear: 0, exit: 0 }}\n            >\n                <div>\n                    <div className={cx(s.rangeSliderWrapper, {[[s.hideOnProductSelect]]: this.isProductSelected })}>\n                        <h2>{sliderTitle}</h2>\n                        { RangeComponent }\n                    </div>\n\n                    <Panels\n                        selectedProductIndex={this.state.selectedProductIndex}\n                        setSelectedProductIndex={this.setSelectedProductIndex}\n                        resetProductIndex={this.resetProductIndex}\n                        modelAssets={selectedModelAssets}\n                        productLabels={productNames}\n                        bundles={bundles}\n                        innerContentProps={innerContentProps}\n                    />\n                </div>\n            </CSSTransition>\n        );\n\n        const WizardComponent = (\n            <CSSTransition\n                key={'wizard'}\n                classNames={{\n                    enter: s.modelTransitionEnter,\n                    enterActive: s.modelTransitionActive,\n                    exit: s.modelTransitionLeave,\n                    exitActive: s.modelTransitionLeaveActive,\n                }}\n                timeout={{ enter: 700, appear: 0, exit: 0 }}\n            >\n                <GetTheLookWizard handle={path} />\n            </CSSTransition>\n        );\n\n        const BodyComponent = (\n            <TransitionGroup>\n                {path && !interfaceStore.isPhone ? WizardComponent : PanelComponent}\n            </TransitionGroup>\n        );\n\n        return (\n            <div className={s.root}>\n                <GetTheLookHero\n                    heroMarkdown={heroMarkdown}\n                    heroBackgroundImage={heroBackgroundImage.fields.file.url}\n                    heroPhoneBackgroundImage={heroPhoneBackgroundImage.fields.file.url}\n                    scrollToCallback={this.handleHeroCta}\n                    heroCta={heroCta}\n                    customHex={customHex}\n                />\n                \n                <div id={'scroll-to-id-hash'} />\n\n                { BodyComponent }\n\n            </div>\n        );\n    }\n\n    render() {\n        const { models, bundles } = this.config;\n        \n        return models && bundles\n            ? this.renderContainer()\n            : false;\n    }\n}\n\nexport default GetTheLookContainer;\n"]