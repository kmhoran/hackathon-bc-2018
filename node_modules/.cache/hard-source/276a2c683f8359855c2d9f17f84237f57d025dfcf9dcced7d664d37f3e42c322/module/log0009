 hasAttribution: false,\n        socials: [],\n        socialSelection: false,\n        showingResults: false,\n        loadingSocials: false\n    };\n\n    componentDidMount() {\n        this.disposer = autorun(() => {\n            const { consultantId } = this.props.checkoutStore;\n            if (consultantId) this.getSocials(consultantId);\n        });\n    }\n\n    componentWillUnmount() {\n        this.disposer();\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.checkout.data.configCheckout[0]);\n    }\n    \n    handleInputChange(name, value, regex = false) {\n        const entry = {\n            ...this.state.form[name],\n            error: false,\n            value: value,\n        };\n\n        let searching = false;\n        if (name === 'searchInput') {\n            searching = true;\n            this.searchConsultants(value);\n        }\n        else if (name === 'checkedAttribution' && value) {\n            this.state.form.checkedNoAttribution.value = false;\n        }\n        else if (name === 'checkedNoAttribution' && value) {\n            this.state.form.checkedAttribution.value = false;\n        }\n        else if (name === 'checkedNoAttribution' && !value && !this.state.form.checkedAttribution.value) {\n            this.props.attributionStore.reset();\n        }\n\n        this.setState({\n            form: {\n                ...this.state.form,\n                [name]: entry,\n            },\n            showingResults: true,\n            searching\n        });\n    }\n\n    async searchConsultants(entry) {\n        const results = await this.props.attributionStore.searchConsultants(entry);\n        if(!results || !results.items) \n            this.setState({ \n                searching: false,\n                searchResults: {\n                    time: Date.now(),\n                    results: [],\n            }});\n\n        else if (Date.now() > this.state.searchResults.time) {\n            this.setState({ \n                searching: false,\n                searchResults: {\n                    time: Date.now(),\n                    results: results.items,\n                    showingResults: true,\n            }});\n        }\n        else \n            this.setState({ searching: false });\n    }\n\n    setConsulant(data) {\n        this.props.checkoutStore.setConsultant({\n            id: data.accountID,\n            fullName: data.fullName,\n            imageUrl: data.imageUrl,\n            location: data.location,\n        });\n        this.setState({ \n            hasAttribution: false, \n            socials: [],\n        });\n        this.resetSearchInput();\n    }\n\n    getSocials = async (id) => {\n        this.setState({ loadingSocials: true });\n        const data = await this.props.attributionStore.getSocials(id);\n        let found;\n\n        if (this.props.attributionStore.socialId)\n            found = getSafe(() => find(data, { partyId: parseInt(this.props.attributionStore.socialId) }));\n\n        if (found) this.setState({ socialSelection: {\n            id: found.partyId,\n            name: found.socialName,\n            selectable: true,\n        }});\n        this.setState({ \n            socials: data,\n            loadingSocials: false\n        });\n    }\n\n    handleSocialChange(options, selected) {\n        const social = find(options, { name: selected });\n        this.props.attributionStore.setSocial(social ? social.id : null);\n        this.setState({ socialSelection: social });\n    }\n\n    resetSearchInput() {\n        this.setState({\n            searchResults: {\n                results: [],\n                time: Date.now(),\n            },\n            form: {\n                ...this.state.form,\n                searchInput: {\n                    value: '',\n                    error: false,\n                },\n            },\n            showingResults: false,\n         });\n    }\n\n    handleClickOutside = () => {\n        this.setState({ showingResults: false });\n    }\n\n    renderSearch() {\n        const { form, searchResults, showingResults, searching } = this.state;\n        const {\n            labelFindConsultant,\n            labelNoSearchResults,\n            resultImagePlaceholder\n        } = this.config;\n        const hasResults = searchResults.results.length > 0 && true;\n\n        function icons() {\n            return (\n                <div className={s.iconContainer}>\n                    <IconSpinner className={cx(s.spinner, searching && s.spinnerActive)} size={{ width: 20, height: 20 }} color={'black'} fill={'black'} />\n                    <IconSearch size={{ width: 16, height: 16 }}/>\n                </div>\n            )\n        }\n\n        return (\n            <div className={s.searchContainer}>\n                <ClickOutHandler onClickOut={this.handleClickOutside}>\n                <Input \n                    onChange={e => this.handleInputChange('searchInput', e)}\n                    defaultValue={form.searchInput.value}\n                    className={s.searchInput}\n                    showLabel={true}\n                    required\n                    title={labelFindConsultant}\n                    icon={icons()} />\n                {(hasResults || form.searchInput.value) && showingResults && <div className={s.searchResultsContainer}>\n                    {searchResults.results.map((item, index) => (\n                        <div key={item.accountID} onClick={() => this.setConsulant(item)} tabIndex={index} className={s.searchResult}>\n                            <div style={item.imageUrl ? { backgroundImage: `url('${item.imageUrl}')` } : { backgroundImage: `url(${getSafe(() => resultImagePlaceholder.fields.file.url)})` }} className={s.img} />\n                            {item.fullName}\n                            <div className={s.location}>\n                                {item.location}\n                            </div>\n                        </div>\n                    ))}\n                    {!hasResults && \n                        <div className={s.searchResult}>\n                            {labelNoSearchResults}\n                        </div>\n                    }\n                </div>}\n                </ClickOutHandler>\n            </div>\n        )\n    }\n\n    setNoConsultant() {\n        this.setState({\n            form: {\n                ...this.state.form,\n                checkedNoAttribution: {\n                    value: 'checked',\n                    error: false,\n                },\n                checkedAttribution: {\n                    value: '',\n                    error: false,\n                },\n            }\n        });\n        this.props.checkoutStore.reset();\n    }\n\n    checkout = () => {\n        this.props.checkoutStore.checkout();\n    }\n\n    continue = () => {\n        const { form } = this.state;\n        \n        if (!form.checkedAttribution.value) {\n            this.checkout();\n        }\n        else\n            this.setState({ selectingConsultant: true });      \n    }\n\n    chooseAnotherConsultant = () => {\n        this.props.checkoutStore.reset();\n        this.state.form.checkedAttribution.value = true;\n        this.state.form.checkedNoAttribution.value = false;\n        this.setState({});\n    }\n\n    render() {\n        const { form, searchResults, socials, loadingSocials } = this.state;\n        const { consultantId, consultant } = this.props.checkoutStore;\n        const { isPhone } = this.props.interfaceStore;\n        const {\n            labelSelectASocial,\n            labelNoSocial,\n            placeholderNoSocials,\n            checkboxAttributeToConsultant,\n            checkboxDoNotAttributeToConsultant,\n            labelPleaseSelectOption,\n            buttonContinueToCheckout,\n            buttonNoConsultant,\n            buttonChooseAnotherConsultant,\n            buttonChooseAnotherConsultantShort,\n            buttonAttributionContinue,\n            resultImagePlaceholder,\n        } = this.config;\n\n        let socialOptions = false;\n        if (socials && socials.length) {\n            socialOptions = socials.map((social) => ({\n                name: social.socialName,\n                id: social.partyId,\n                selectable: true,\n            }));\n            socialOptions = [\n                ...socialOptions,\n            ];\n        };\n\n        const component = this;\n        function socialDropdown() {\n            return (\n                <div className={s.social}> \n                {!loadingSocials &&                               \n                    <div className={s.socialOptions}>\n                        {!socialOptions && <h3>{placeholderNoSocials}</h3>}\n                        {socialOptions &&<Select\n                                title={labelSelectASocial}\n                                onChange={e => component.handleSocialChange(socialOptions, e)}\n                                options={socialOptions}\n                                defaultValue={getSafe(() => component.state.socialSelection.name)}\n                            />}\n                    </div>\n                }\n                </div>\n            );\n        }\n\n        if (consultantId) {\n            return (\n                <div className={s.root}>\n                    <div className={s.consultant}>\n                        <div style={consultant.imageUrl ? { backgroundImage: `url('${consultant.imageUrl}')` } : { backgroundImage: `url('${getSafe(() => resultImagePlaceholder.fields.file.url)}')`, backgroundSize: 'contain' }} className={s.img} />\n                        <div className={s.consultantInfo}>\n                            <div className={s.consultantName}>\n                                {consultant.fullName}\n                            </div>\n                            {consultant.location}\n                            <hr />\n\n                            <div onClick={() => this.chooseAnotherConsultant()} className={s.linkNoConsultant}>\n                                {!isPhone && buttonChooseAnotherConsultant || buttonChooseAnotherConsultantShort}\n                            </div>\n                            \n                            <div onClick={() => this.setNoConsultant()} className={s.linkNoConsultant}>\n                                {buttonNoConsultant}\n                            </div>\n                            {!isPhone && socialDropdown()}\n                        </div>\n                    </div>\n                    {isPhone && socialDropdown()}\n                    <div className={cx(s.buttonContainer, s.buttonFinal)}>\n                        <Button regular onClick={() => this.checkout()}>\n                             {buttonContinueToCheckout}\n                        </Button>\n                    </div>\n                </div>\n            )\n        }\n\n        return (\n            <div className={s.root}>\n                <div className={s.label}>{labelPleaseSelectOption}</div>\n                <Checkbox \n                    className={s.checkbox}\n                    title={checkboxAttributeToConsultant} \n                    checked={form.checkedAttribution.value}\n                    onChange={e => this.handleInputChange('checkedAttribution', e)} />\n                {!form.checkedAttribution.value && <Checkbox \n                    className={s.checkbox}\n                    title={checkboxDoNotAttributeToConsultant} \n                    checked={(this.props.attributionStore.shoppingWithHQ) || form.checkedNoAttribution.value}\n                    onChange={e => this.handleInputChange('checkedNoAttribution', e)} />}\n                {form.checkedAttribution.value && this.renderSearch()}\n                <div className={s.buttonContainer}>\n                    <Button \n                        regular\n                        disabled={(!this.props.attributionStore.shoppingWithHQ && !form.checkedNoAttribution.value) || (this.props.attributionStore.shoppingWithHQ && form.checkedAttribution.value)}\n                        onClick={() => this.continue()}>\n                        {buttonAttributionContinue}\n                    </Button>\n                </div>\n            </div>\n        )\n    }\n}\n\nexport default CheckoutInterstitialAttribution;\n"]}},"_buildHash":"457eaab178d07eda06f709f336118c97","hash":"d05c373cdb2fe4d42ee9382c1c35aec2","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[296,303],"loc":{"start":{"line":11,"column":26},"end":{"line":11,"column":76}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/toConsumableArray","range":[304,345],"loc":{"start":{"line":11,"column":26},"end":{"line":11,"column":76}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/defineProperty","range":[452,490],"loc":{"start":{"line":15,"column":23},"end":{"line":15,"column":70}}},{"type":"RequireHeaderDependency","range":[444,451],"loc":{"start":{"line":15,"column":23},"end":{"line":15,"column":70}}},{"type":"RequireHeaderDependency","range":[576,583],"loc":{"start":{"line":19,"column":16},"end":{"line":19,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/extends","range":[584,615],"loc":{"start":{"line":19,"column":16},"end":{"line":19,"column":56}}},{"type":"CommonJsRequireDependency","request":"b