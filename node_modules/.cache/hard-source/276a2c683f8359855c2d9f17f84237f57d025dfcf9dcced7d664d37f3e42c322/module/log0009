point);\n        window.removeEventListener('touchmove', this._enableWaypoint);\n        window.removeEventListener('resize', this._checkBreakpoint);\n    }\n\n\n    _handleWaypointEnter = (elementId, e) => {\n        if (!this.state.isWaypointDisabled) {\n            this.setState({\n                focusedSectionId: elementId,\n                waypointPosition: e.previousPosition,\n            });\n        }\n    }\n\n    _enableWaypoint = () => {\n        if (this.state.isWaypointDisabled) {\n            this.setState({ isWaypointDisabled: false });\n        }\n    }\n\n    _checkBreakpoint = () => {\n        const activeBreakpoint = throttleBreakpoints(window.innerWidth, this.state.activeBreakpoint);\n        if (!!activeBreakpoint) {\n            this.setState({ activeBreakpoint });\n        }\n    }\n\n    _handleMobileNavChange = () => {\n        if (this.navMenu && this.focusedNavItem) {\n            const windowWidth = window.innerWidth;\n            const navItemRect = this.focusedNavItem.getBoundingClientRect();\n\n            const isLeft = navItemRect.x < 0;\n            const isRight = navItemRect.right > windowWidth;\n\n            if (isLeft || isRight) {\n                const scrollDelta = windowWidth - navItemRect.width;\n                this.navMenu.scrollLeft += isRight ? scrollDelta : -scrollDelta;\n            }\n        }\n    }\n\n    changeViewport = (elementId) => {\n        this.setState({ focusedSectionId: elementId, isWaypointDisabled: true });\n        setTimeout(() => this.refs[elementId].scrollIntoView({ block: 'start', behavior: 'instant' }), 0);\n    }\n\n    render() {\n        let ContentBlocks = [];\n        let NavItems = [];\n\n        const {\n            _handleWaypointEnter,\n        } = this;\n\n        const {\n            content,\n            navMenuClassName,\n        } = this.props;\n\n        const {\n            pageLeftOffset,\n            navTopOffset,\n            focusedSectionId,\n            activeBreakpoint,\n        } = this.state;\n\n        const getWaypoints = (isNavHeader, id) => {\n            if (isNavHeader) {\n                return (\n                    <React.Fragment>\n                        <div ref={id} className={s.scrollToAnchor} />\n                        <div className={s.waypointAnchor}>\n                            <Waypoint\n                              scrollableAncestor={'window'}\n                              onEnter={_handleWaypointEnter.bind(this, id)}\n                            />\n                        </div>\n                    </React.Fragment>\n                );\n            }\n        };\n\n        const ContentBlock = props =>\n            (<li\n              className={s.listItem}\n              key={props.id}\n            >\n                {getWaypoints(props.isNavHeader, props.id)}\n                <Section\n                  className={props.blockClassName}\n                  innerClassName={cx(s.blockInner, props.blockClassName)}\n                  contentClassName={cx(s.contentBlockBody, props.classname)}\n                >\n                    {props.children}\n                </Section>\n            </li>);\n\n        const NavItem = props =>\n            (<li\n              className={cx(props.navItemClassName, s.navItemFixedWidth, {\n                    [props.navItemSelectedClassName]: focusedSectionId === props.id,\n                })}\n              ref={(focusedNavItem) => { if (focusedSectionId === props.id) { this.focusedNavItem = focusedNavItem; } }}\n              key={props.id}\n              onClick={this.changeViewport.bind(this, props.id)}\n            >\n                {props.id === '0-9' ? <span> # </span> : <span> {props.title} </span>}\n            </li>);\n\n\n        content.forEach((contentChild) => {\n            ContentBlocks.push(ContentBlock(contentChild.props));\n            if (contentChild.props.isNavHeader) {\n                NavItems.push(NavItem(contentChild.props));\n            }\n        });\n\n        return (\n            <div>\n                <div className={s.panel}>\n                    <Sticky\n                      className={s.sticky}\n                      activeClass={s.activeNode}\n                      bottomBoundary=\"#page-wrapper\"\n                      innerZ={5}\n                    >\n                        <div className={s.navWrapper} >\n                            <div\n                              className={cx(s.navBlock, navMenuClassName)}\n                              ref={(navMenu) => { this.navMenu = navMenu; }}\n                            >\n                                <ul>\n                                    {NavItems}\n                                </ul>\n                            </div>\n                        </div>\n                    </Sticky>\n                </div>\n                <div\n                  id={'page-wrapper'}\n                >\n                    <ul className={cx(s.list, navMenuClassName)}>\n                        {ContentBlocks}\n                    </ul>\n                </div>\n            </div>\n        );\n    }\n}\n\nexport default IngredientsStickyNav;\n"]}},"_buildHash":"0d38f3636d573bbc27ff9ecc672ae86c","hash":"33cd0e1ef888fbcf3c73470614b9fd65","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/defineProperty","range":[114,152],"loc":{"start":{"line":7,"column":23},"end":{"line":7,"column":70}}},{"type":"RequireHeaderDependency","range":[106,113],"loc":{"start":{"line":7,"column":23},"end":{"line":7,"column":70}}},{"type":"RequireHeaderDependency","range":[244,251],"loc":{"start":{"line":11,"column":22},"end":{"line":11,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/