LocaleURL } from 'Middleware/utils/url';\nimport { routes, analytics, liveUrl, accountLinks, locales } from 'Root/config'\nimport Location from 'Middleware/utils/Location';\nimport { parseContentURL } from 'Middleware/utils/url';\n\nimport localeStore from 'Stores/localeStore';\nimport interfaceStore from 'Stores/interfaceStore';\nimport categoryStore from 'Stores/categoryStore';\nimport contentStore from 'Stores/contentStore';\nimport userStore from 'Stores/userStore';\nimport attributionStore from 'Stores/attributionStore';\n\nconst DEFAULT_BACK_PATH = '/';\n\nclass NavigationStore {\n\n    @observable history;\n    @observable navigation;\n    @observable hash;\n\n    constructor() {\n        this.history = [{\n            path: DEFAULT_BACK_PATH,\n            isBackPoint: true\n        }];\n        this.navigation = [];\n        this.hash = this.updatedHash;\n    }\n\n    @action load = (data) => {\n        this.navigation = parseNavigation(data);\n    }\n\n    @action to = ({ url, reload = false, assign = false, newWindow = false } = {}) => {\n        if (reload && __BROWSER__) {\n            window.location.replace(url);\n            return true;\n        }\n        if (assign && newWindow && __BROWSER__) {\n            window.open(url);\n            return true;\n        }\n\n        if (assign && __BROWSER__) {\n            window.location.assign(url);\n            return true;\n        }\n\n        Location.push(url);\n\n        interfaceStore.searchClose();\n        categoryStore.toggleQuicklook({});\n\n        if(!url.includes(routes.PRODUCTS)) {\n            categoryStore.setCurrentCategory(null);\n        }\n\n        const path = typeof url === \"string\" ? url : url.pathname;\n\n        this.history.push({\n            path,\n            isBackPoint: interfaceStore.cartOpen ? false : isBackPointPath(path)\n        });\n\n        interfaceStore.closeRightPanel();\n        interfaceStore.closeBottomPanel();\n    }\n\n    @action goBack = (data) => {\n        const backPath = this.previous.path != this.path ? this.previous.path : null;\n\n        this.to({ url: backPath || routes.HOME});\n    }\n\n    @action addQuery = ({queries = [], isBackPoint = false}) => {\n        if (__BROWSER__) {\n            window.history.replaceState('','',\n            setURLQuery(queries));\n\n            this.history.push({\n                path: window.location.pathname + window.location.search,\n                isBackPoint\n            });\n        }\n    }\n\n    @action changePath = ({path = '', isBackPoint = false}) => {\n        this.history.push({\n            path,\n            isBackPoint\n        });\n    }\n\n    @action cleanPath = ({ route = '' }) => {\n        const query = window.location.search;\n        const url = getLocaleURL(localeStore.activeLocale.code, route);\n        window.history.replaceState('', '', url + query);\n    }\n\n    @action addPathArgument = ({ route = '', arg = '' }) => {\n        const query = window.location.search;\n        const url = getLocaleURL(localeStore.activeLocale.code, route);\n        window.history.replaceState('', '', `${url}/${arg}${query}`);\n    }\n\n    @action localizeURL = (url) => {\n        return getLocaleURL(localeStore.activeLocale.code, url);\n    }\n\n    @action setStartPage = (path = '') => {\n        this.history.push({\n            path,\n            isBackPoint: isBackPointPath(path)\n        });\n    }\n\n    @action newPath = (path = '') => {\n        if (!path.includes(routes.PRODUCTS)) {\n            categoryStore.setCurrentCategory(null);\n        }\n\n        this.history.push({\n            path,\n            isBackPoint: isBackPointPath(path)\n        });\n        this.postNavigationEvents();\n    }\n\n    @action postNavigationEvents = () => {\n        __BROWSER__ && requestAnimationFrame(() => {\n            if(analytics.optimizely.enabled) {\n                window.optimizely = window.optimizely || [];\n                window.optimizely.push({ type: 'activate' });\n            }\n        });\n    }\n\n    @computed get backPath() {\n        return findLast(this.history, { isBackPoint: true }).path;\n    }\n\n    @computed get previous() {\n        return this.history[this.history.length - 3] || { isBackpoint: false };\n    }\n\n    @computed get path() {\n        return last(this.history).path;\n    }\n\n    @computed get queryString() {\n        return QueryString.extract(this.path);\n    }\n\n    @computed get absolutePath() {\n        return liveUrl + last(this.history).path;\n    }\n\n    @action unlocalizedPath = (path = this.path) => {\n        return unlocalizeURL(path);\n    }\n\n    @action unlocalizedBasePath = (path = this.path) => {\n        return removeURLQueries(false, unlocalizeURL(path));\n    }\n\n    @action parseNavigation = (links) => {\n        return parseNavigation(links.filter(link => link && this.isLinkVisible(link) && link));\n    }\n\n    @action isLinkVisible = (link) => {\n        // (only update on appIsMounted render pass to avoid server side hydration issues)\n        if (link.fields.hideIf && interfaceStore.appIsMounted) {\n            let valid = true;\n            link.fields.hideIf.map((condition) => {\n                switch (condition) {\n                    case 'isConsultant':\n                        if (userStore.isConsultant) valid = false;\n                        break;\n                    case 'hasConsultant':\n                        if (attributionStore.consultantShopping) valid = false;\n                        break;\n                    case 'isEmployee':\n                        if (userStore.isEmployee) valid = false;\n                        break;\n                    default:\n                        valid = true;\n                        break;\n                }\n            });\n            if (!valid) return false;\n        }\n        return true;\n    }\n\n    // Update subscriber components when link visibility changes\n    @computed get observeLinkChanges() {\n        return [\n            interfaceStore.appIsMounted,\n            userStore.isConsultant,\n            attributionStore.consultantShopping,\n        ];\n    }\n\n    get updatedHash() {\n        if(__BROWSER__){\n            return '' || window.location.hash.substr(1);\n        }\n        return '';\n    }\n\n    @action updateHash = (newHash) => {\n        window.history.replaceState(\n            null,\n            null,\n            newHash ? `#${newHash}` : '',\n        );\n        this.hash = this.updatedHash;\n    }\n}\n\nconst navigationStore = new NavigationStore();\n\nexport default navigationStore;\nexport { NavigationStore };\n"]}},"_buildHash":"5134aae429afbc707da070efb5ca1385","hash":"23bac31cd8c8ecf589899cdcce73ec16","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/define-property","range":[150,196],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[142,149],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[297,304],"loc":{"start":{"line":12,"column":32},"end":{"line":12,"column":99}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[305,363],"loc":{"start":{"line":12,"column":32},"end":{"line":12,"column":99}}},{"type":"RequireHeaderDependency","range":[475,482],"loc":{"start":{"line":16,"column":23},"end":{"line":16,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/classCallCheck","range":[483,521],"loc":{"start":{"line":16,"column":23},"end":{"line":16,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/createClass","