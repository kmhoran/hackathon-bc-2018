moStore {\n\n        @observable promoGraph;\n        @observable promosByID;\n        @observable promosByPromoId;\n        @observable currentGWP;\n        @observable activatedPromos;\n        @observable currentGWPChoices;\n        @observable promoCartProductsByID;\n\n        @observable promoCartTotal;\n        @observable discountTotal;\n\n        @observable promoTypesDictionary;\n\n        @observable currentCartPromo;\n\n        constructor() {\n            // tracks whether the session has been loaded.\n            this.promoCartLoaded = false;\n        \n            this.promoGraph = {};\n            this.promosByID = {}; // ID as in Parent ID, not promo ID.\n            this.promosByPromoId = {};\n            this.promoLocale = '';\n\n            this.currentGWP = 0;\n            this.currentGWPChoices = [];\n\n        // dictionary of promos namespaced by phoenix region\n            this.activatedPromos = {};\n\n        // array of items as they are checked on wakeup. prevents duplicate promos on autorehydration.\n            this.autoRehyrdationCheckedPromos = [];\n\n        // a mutable copy of the cart store that displays products with adjustments based on the return data from the server engine.\n            this.promoCartProductsByID = null;\n\n        // a list of line items which are not associated with promos.\n            this.unpromofiedCartItemsById = {};\n\n            this.previousCartTotal = 0;\n            this.recentlyUpdatedId = null; // the most recently updated product in the cart. Used for determining thresholds.\n\n            this.promoCartTotal = 0; // the total received from the server engine.\n            this.discountTotal = 0; // discount total from server.\n\n        //set this to a matching key to display a custom promo message\n            this.currentCartPromo = null;\n\n            this.lastTimeStamp = 0;\n\n        // a dictionary of all the different types of promos, how they are identified, and how to trigger their functionality.\n        // BEHAVIORS -\n        //   removesFromCartTransfer - will subtract this item from the cart when transferring it over.\n\n            this.promoTypesDictionary = {\n                thresholdGWP : {\n                    traceAttribs: ['hasThreshold', 'hasGWPItems', ],\n                    behaviors: [\n                        'makesCartPromo', 'triggersProductSelector', 'activatesSelectorOnDelete', 'hasShortCartContainer',\n                        'removesChildrenOnRemove'\n                    ],\n                    detectors: ['isChild', 'thresholdMet'],\n                    description: 'Like a gwp, except that it is triggered by threshold instead of parent existence.',\n                },\n                commonGWP : {\n                    traceAttribs: ['hasGWPItems', 'notHasThreshold'],\n                    behaviors: [\n                        'triggersProductSelector', 'canDisableSelectorOnCartClose',\n                        'removesChildrenOnRemove',\n                    ],\n                    detectors: [],\n                },\n                autoAddProducts : {\n                    traceAttribs: ['hasAutoAddItems', 'hasParent'],\n                    behaviors: [\n                        'subtractsParentFromThreshold', 'removesFromCartTransfer',\n                        'subtractsParentFromSubtotalUnlessSelf', 'disablesDelete'\n                    ],\n                    detectors: [],\n                },\n                // this one is currently handled by deprecated logic, so has been commented out. In use by BoB.\n                // thresholdAutoAdd : {\n                //   traceAttribs: ['hasThreshold', 'hasAutoAddItems', 'hasParent'],\n                //   behaviors: [\n                //     'removesFromCartTransfer',\n                //   ], \n                // },\n                parentlessThresholdAutoAdd : {\n                    traceAttribs: ['hasThreshold', 'hasAutoAddItems', 'notHasParent'],\n                    behaviors: [\n                        'removesFromCartTransfer', 'removesChildrenOnRemove',\n                    ],\n                    detectors: [], \n                },\n                discountedProducts : {\n                    traceAttribs: ['hasDiscountedItems'],\n                    detectors: [],\n                },\n                unspecified : {\n                    behaviors: [],\n                    traceAttribs: [],\n                    detectors: [],\n                },\n                // ['simple-promo']: {\n                //   description: 'used for identifying unclassable promos.',\n                // }\n            };\n        // a dictionary of all the trace attributes used for identifying promo types.\n            this.traceAttributeFilters = {\n            // this tracer will identify a promo with a threshold\n                hasThreshold: ((d) => {\n                    try {\n                        const thresh = this.promosByPromoId[d].orderSubtotalThreshold;\n                        if (thresh && isNumber(thresh) && thresh > 0) return true;\n                    } catch (e) {}\n                    return false;\n                }),\n                notHasThreshold: ((d) => {\n                    return !this.traceAttributeFilters['hasThreshold'](d);\n                }),\n            // this tracer will identify a promo with gwp items.\n                hasGWPItems: ((d) => {\n                    try {\n                        if (this.promosByPromoId[d].actions[0].gwpProducts.length > 0) return true;\n                    } catch (e) {}\n                    return false;\n                }),\n            // identify promos with autoAddItems\n                hasAutoAddItems: ((d) => {\n                    try {\n                        if (this.promosByPromoId[d].actions[0].autoAddProducts.length > 0) return true;\n                    } catch (e) {}\n                    return false;\n                }),\n                hasDiscountedItems: ((d) => {\n                    try {\n                        if (this.promosByPromoId[d].actions[0].discountedProducts.length > 0) return true;\n                    } catch (e) {}\n                    return false;\n                }),\n                hasParent: ((d) => {\n                    try {\n                        if (this.promosByPromoId[d].parents.length > 0) return true;\n                    } catch (e) {}\n                    return false;\n                }),\n                notHasParent: ((d) => {\n                    return !this.traceAttributeFilters['hasParent'](d);\n                }),\n            }\n        // a dictionary of all the behaviors a promo can trigger.\n            this.promoBehaviorsDictionary = {\n                makesMiniCartPromo: {\n                    description: 'reads config for mini promo in cart. Replaces promolet when '\n                },\n                activatesSelectorOnDelete: {\n                    description: 'Keeping in mind that rearming is different - This will activate the muliple choice product selector again, if the detectors validate.'\n                }\n            }\n\n        // a dictionary of all the detectors used to determine if a given product id should trigger promo creation/activation.\n            this.promoDetectorsDictionary = {\n                isChild: ((descriptorId, type, productId) => {\n                    try {\n                        if (type === 'thresholdGWP') type = 'gwpProducts';\n                        // todo: add other static type interpretations here.\n\n                        if (this.promosByPromoId[descriptorId].actions[0][type].slice().map(item => Number(item.id)).indexOf(Number(productId)) >= 0) return true;\n                    } catch (e) {}\n                    return false;\n                }),\n                thresholdMet: ((descriptorId, type, productId) => {\n                    try {\n                        if (cartStore.total >= (this.promosByPromoId[descriptorId].orderSubtotalThreshold + productStore.productOrVariantById(productId).price)) return true;\n                    } catch (e) {}\n                    return false;\n                })\n            }\n\n            __BROWSER__ && requestAnimationFrame(() => {\n\n// handles signaling decline of flyout panel choices\n                autorun(() => {\n                    interfaceStore.cartOpen;\n                    if (!interfaceStore.cartOpen && interfaceStore.isPhone) {\n                        if (this.promoTypeHasBehavior(\n                        this.getPromoType(this.currentGWP), 'canDisableSelectorOnCartClose')) {\n                            this.deactivateCurrentGWP();\n                        }\n                    }\n                });\n\n// handles resetting promo store when switching locales\n                autorun(() => {\n                    localeStore.activeLocale;\n                    this.deactivateCurrentGWP();\n                })\n\n// handles threshold promo detection changes\n                autorun(() => {\n                    //observe subtotal for threshold based products.\n                    cartStore.total;\n                    \n                    const buckets = this.getActivePromoBucketsOfType('autoAddProducts');\n                    // handles cart manipulations that involves adding items to the cart automatically.\n                    if (cartStore.total > this.previousCartTotal) {\n\n                    // handles promo logic for items which are cart based + threshold based.\n                        Object.keys(buckets).forEach((promoId) => {\n                            const { bucket, details } = buckets[promoId];\n                            \n                            const childIds = details.actions[0].autoAddProducts\n                                .slice()\n                                .map(child => Number(child.id));\n\n                            let selectedBucket;\n                            toJS(bucket).filter((i, index) => {\n                                this.logCollector('checking for auto add promo');\n                                return i.active\n                                && this.isUserRequirementsMet(details)\n                                && this.isThresholdPromoRequirementsMet(i, details, (() => {\n                                    if (!this.checkForProductInActivePromos(childIds[0], 'child')) {\n                                        selectedBucket = index;\n                                        return { adjust: true };\n                                    }\n                                })())\n                            }).forEach((bucketInstance) => {\n                                try {\n                                    childIds.forEach((childId, i) => {\n                                        const isNotLastChild = i < childIds.length - 1;\n                                        cartStore.addProduct({\n                                            product: productsStore.productsById[childId],\n                                            quantity: 1,\n                                            isPromo: true,\n                                            mockPromoSend: isNotLastChild,\n                                            hideCart: true,\n                                            // disablePromoSearching: isNotLastChild,\n                                            // disablePromoFinding: isNotLastChild,\n                                        });\n                                        this.addChildToBucket(productsStore.productsById[childId], \n                                            bucket[selectedBucket]);\n\n                                    });\n                                    this.savePromos();\n\n                                } catch (e) {}\n                            });\n                        })\n\n                    //handles triggering threshold exclusive activated promos.\n                        this.getPromosOfType('thresholdGWP')\n                            .map((id) => ({ id, descriptor: this.promosByPromoId[id] }))\n                            .filter(({id, descriptor}) => this.isThresholdPromoRequirementsMet({}, descriptor, {\n                                new: true,\n                            })\n                            && !this.activatedPromos[id])\n                            .forEach(({id, descriptor}) => {\n                                this.setGWPActive(id);\n                                // if the requirements for this descriptor are set then activate it.\n                            });\n\n                        //handles triggering threshold exclusive activated promos.\n                        this.getPromosOfType('parentlessThresholdAutoAdd')\n                            .map((id) => ({ id, descriptor: this.promosByPromoId[id] }))\n                            .filter(({id, descriptor}) => this.isThresholdPromoRequirementsMet({}, descriptor, {\n                            new: true,\n                            })\n                            && !this.activatedPromos[id]\n                            && this.isUserRequirementsMet(descriptor)\n                            ).forEach(({id, descriptor}) => {\n\n                                // if this is a promo product and is being created, then set up a new bucket.\n                                const newBucket = this.createPromoBucket({\n                                    promoType: 'parentlessThresholdAutoAdd',\n                                    parentQuantity: 0,\n                                    children: [...descriptor.actions[0].autoAddProducts\n                                        .slice()\n                                        .map((product) => parseInt(product.id))\n                                    ],\n                                    childQuantity: descriptor.actions[0].autoAddProducts.slice().length,\n                                });\n\n                                const bucketId = this.saveBucket(id, newBucket);\n\n                                this.savePromos();\n                                \n                                descriptor.actions[0].autoAddProducts\n                                .slice().forEach((product) => {\n                                    cartStore.addProduct({\n                                        product: productsStore.productOrVariantById(product.id),\n                                        quantity: 1,\n                                        isPromo: true,\n                                        disablePromoFinding: true, // prevent endless loop.\n                                        hideCart: true,\n                                    });\n                                });\n                                \n                            });\n                    }\n\n\n            // handles cart tasks that involve auto removing things from the cart.\n                    // logic applied to autoAdd type products of threshold variety.\n                    if (cartStore.total < this.previousCartTotal) {\n                        Object.keys(buckets).forEach((promoId) => {\n                            const { bucket, details } = buckets[promoId];\n\n                            const childIds = details.actions[0].autoAddProducts\n                            .slice()\n                            .map(child => Number(child.id));\n\n                            bucket.filter((i) => {\n                                return !i.active && !this.isThresholdPromoRequirementsMet(i, details);\n                            })\n                            .forEach((bucketInstance) => {\n                                try {\n                                    childIds.forEach((childId, i) => {\n\n                                        const childCartItem = cartStore.productsById[childId];\n                                        if (childCartItem) {\n                                            this.removeProductFromPromo(childCartItem, {\n                                                promo: bucketInstance,\n                                                type: 'child',\n                                                removeAction: 'preserveAndReArm',\n                                            });\n                                            this.savePromos();\n                                        }\n                                    });\n                                } catch (e) {console.log(e)}\n                            });\n                        });\n\n                        // handles removing of promos that are threshold based when under the\n                        // required threshold. consider trying to refactor above into this.\n                        this.getPromoTypesWithTraceAttrib('hasThreshold').forEach((promoType) => {\n                            this.getPromosOfType(promoType).forEach((descriptorId) => {\n                                if (this.activatedPromos[descriptorId]) {\n                                    this.activatedPromos[descriptorId].forEach((instance) => {\n                                        const descriptor = this.promosByPromoId[descriptorId];\n                                        if (!this.isThresholdPromoRequirementsMet({}, descriptor, {\n                                            adjust: (() => {\n                                                if (instance.children.indexOf(this.recentlyUpdatedId) > -1) {\n                                                    return false;\n                                                }\n                                                return true;\n                                            })(),// only adjust if the removed/modified product is the chosen child product.\n                                        })) {\n                                            this.removeBucket(descriptorId);\n                                        }\n                                    });\n                                }\n                            });\n                        })\n\n\n                        //handles deactivating product selector for threshold exclusive activated promos.\n                        if (this.isGWPActive) {\n                            this.logCollector('check for gwp disable');\n                            if (this.getPromoType(this.currentGWP) === 'thresholdGWP') {\n                                this.logCollector('is threshold based');\n                                if ( !this.isThresholdPromoRequirementsMet({}, this.promosByPromoId[this.currentGWP], {\n                                    adjust: true,\n                                })) {\n                                    this.deactivateCurrentGWP();\n                                }\n                            }\n                        }\n\n                    }\n                    this.previousCartTotal = cartStore.total;\n                });\n\n                autorun(() => {\n                    userStore.userInfo;\n                    contentStore.content;\n                    this.groupPricingRules = getSafe(() => contentStore.content.shop[0].groupPricingRules);\n                });\n\n                autorun(() => {\n                    cartStore.productsById;\n                    this.autoRehydrateActivePromos();\n                });\n\n                autorun(() => {\n                    productsStore.products.length && this.initPromoDB();\n                })\n                \n            });\n        }\n\n        savePromos = () => {\n            sessionStore.savePromos(this);\n        }\n\n        @computed get products() {\n            return this.promoCartProductsByID ? sortByDate(values(JSON.parse(JSON.stringify(this.promoCartProductsByID)))) : false;\n        }\n\n        @computed get isGWPActive() {\n            if (this.currentGWP !== 0\n            && this.currentGWPChoices.length > 0)\n                return true;\n            return false;\n        }\n\n        @action clear = () => {\n            this.currentGWP = 0;\n            this.currentGWPChoices = [];\n            this.activatedPromos = {};\n            this.promoCartProductsByID = null;\n            this.unpromofiedCartItemsById = {};\n            this.previousCartTotal = 0;\n            this.recentlyUpdatedId = null;\n            this.promoCartTotal = 0;\n            this.discountTotal = 0;\n            this.currentCartPromo = null;\n            this.lastTimeStamp = 0;\n\n            this.savePromos();\n\n        }\n\n        @action initPromoDB = async() => {\n            if (this.promoLocale === localeStore.activeLocale.code) return false;\n            this.promoLocale = toJS(localeStore.activeLocale.code);\n\n            const promoData = await getPromoDB(localeStore.activeLocale, contentStore.flags('catalogCaching'));\n\n            try {\n                this.promosByID = promoData.promosbyParentProductId;\n                this.promosByPromoId = promoData.promosByPromoId;\n            } \n            catch (e) {\n                console.error('catalog is missing promos. Try rebuilding.');\n            }\n        }\n\n    //loaded data from content store\n        @action load = async (data, origin = 'api') => {\n            if (!data.promosByID && !this.promosByID) return false;\n            else if (origin === 'api' || Object.keys(toJS(this.promosByID)).length < 1) {\n                this.promosByID = data.promosByID;\n                this.promosByPromoId = data.promosByPromoId;\n            }\n\n            // this.promoGraph = data.promoGraph;\n            this.promoCartProductsByID = data.promoCartProductsByID;\n            try {\n                if (Object.keys(data.activatedPromos).length > 0) {\n                    this.currentGWP = data.currentGWP;\n                    this.currentGWPChoices = data.currentGWPChoices;\n                    this.activatedPromos = data.activatedPromos;\n                }\n            } catch (e) {}\n            if (origin === 'api') {\n            } else {\n                this.promoCartLoaded = true;\n                this.checkForExpiredBucketGroups();\n                this.checkCart();\n            }\n        }\n\n    // as a precautionary measure, this runs actively on an empty promo storage,\n    // to see if it can recreate promos to prevent promo loss.\n        autoRehydrateActivePromos = () => {\n            try {\n                if (Object.keys(this.activatedPromos).length < 1 && this.promoCartLoaded) {\n                    this.promosByPromoId && Object.keys(this.promosByPromoId).forEach((descriptorId) => {\n                        const promoType = this.getPromoType(descriptorId);\n                        Object.keys(cartStore.productsById).forEach((productId) => {\n                            if (this.isItemPromoEligible(productId, promoType)) {\n                                if (this.autoRehyrdationCheckedPromos.slice().indexOf(productId) < 0) {\n\n                                    this.autoRehyrdationCheckedPromos.push(productId);\n                                    \n                                    // enable gwp triggering for this promo.\n                                    this.setGWPActive(descriptorId);\n\n                                    this.checkItem(cartStore.productsById[productId], 'verify', true);\n\n                                }\n                            }\n                        });\n                    });\n                }\n            }\n            catch(e) {\n                console.log(e);\n            }\n        }\n\n        checkForExpiredBucketGroups = () => {\n            Object.keys(this.activatedPromos).forEach((activePromoId) => {\n                if (this.isBucketGroupExpired(activePromoId)) {\n                    messagingStore.addMessage('A promotion was removed from the cart, because it is expired.', { type: 'error' });\n                    delete this.activatedPromos[activePromoId];\n                    this.logCollector('removing expired bucket');\n                }\n            });\n        }\n\n        isBucketGroupExpired = (bucketId) => {\n            try {\n                const descriptor = this.promosByPromoId[bucketId];\n                if (descriptor.endDateUTC !== null && new Date(descriptor.endDateUTC).getTime() > new Date().getTime()) {\n                    return false;\n                }\n                if (descriptor.endDateUTC === null) {\n                    return false;\n                }\n                return true;\n            } catch (e) { return false; }\n        }\n\n        @action changeQuantity = (promoPack, quantity, id) => {\n\n        // items are bound together. if you increase the quantity of the parent so\n        // increases the child.\n            if (promoPack) {\n                if (promoPack.relationship === 'parent') {\n                    promoPack.promo.parentQuantity += quantity;\n                    promoPack.promo.childQuantity += quantity;\n                // TODO: extend for more complex gwp integration. only handles one to one.\n                    cartStore.changeQuantity({\n                        id: promoPack.promo.children[0], \n                        quantity,\n                    });\n                    analyticsStore.promoItemChanged(promoPack.promo.children[0], quantity, promoPack);\n                }\n            } else {\n                if (this.promoCartProductsByID) {\n                    analyticsStore.promoItemChanged(id, quantity);\n                    return this.promoCartProductsByID[id].quantity += quantity;\n                } else {\n                    return null;\n                }\n            }\n        }\n\n        @action removeProductFromPromo = (product, promoPack) => {\n            this.logCollector(`removeProductFromPromo ${JSON.stringify(promoPack)}`);\n            if (!promoPack || !product) return false;\n            const { promo, removeAction } = promoPack;\n            const promoCopy = JSON.parse(JSON.stringify(promo));\n            let postCall = null;\n            \n        //the number by which to adjust the base product.\n            let quantityAdjust = 0;\n            let totalProductQuantity = cartStore.productsById[product.id].quantity;\n\n        // make promo adjustments\n\n        // case in which product is the same as one in a promo. A detached product which needs to be promo aware.\n            if (promoPack.relationship === 'parent' || promoPack.type === 'parent') {\n                quantityAdjust = totalProductQuantity - (totalProductQuantity - \n                    (promo.parentQuantity / promo.parents.length)\n                );\n                \n            // don't set to 0 for more complex promos. you are thinking that we are removing the parent, but no - there could be more than one parent.\n                promo.parentQuantity -= product.quantity;\n                // promo.parentQuantity = 0;\n\n            // remove parent from promo item.\n                let i = promo.parents.indexOf(product.id);\n                if (i !== -1) promo.parents.splice(i, 1);\n\n            // also remove the child product.\n                if (promo.childQuantity > 0) {\n                    postCall = () => promo.children.forEach((childId, i) => {\n                            this.removeProductFromPromo(cartStore.productsById[childId], {\n                                promo: JSON.parse(JSON.stringify(promo)),\n                                type: 'child',\n                            });\n                    });\n                }\n\n        // mirror of above for children\n            } else if (promoPack.type === 'child') {\n                quantityAdjust = totalProductQuantity - (totalProductQuantity - \n                    (promo.childQuantity / promo.children.length)\n                );\n\n                // TODO: fix this for removing stackable promos.\n                // promo.childQuantity -= product.quantity;\n                promo.childQuantity -= 1;\n                \n                let i = promo.children.indexOf(product.id);\n                if (i !== -1) promo.children.splice(i, 1);\n            }\n\n        // case of discounted simple promos\n            else if (promoPack.type === 'simple-promo') {\n                quantityAdjust = totalProductQuantity;\n\n            // remove parent from promo item.\n                let i = promo.parents.indexOf(product.id);\n                if (i !== -1) promo.parents.splice(i, 1);\n            }\n\n        // \n            if (promo.type !== 'simple-promo') {\n                if (promo.childQuantity < 1 || promo.parentQuantity < 1 ) {\n                    if (removeAction === 'preserveAndReArm') {\n                        promo.active = true;\n                    } else {\n                        const promoId = this.removePromoWhichMatches(promo);\n                        if (this.promoTypeHasBehavior(promo.promoType, 'activatesSelectorOnDelete')) {\n                            this.setGWPActive(promoId);\n                        }\n                    }\n                }\n            } else if (promo.parentQuantity < 1)\n                this.removePromoWhichMatches(promo);\n\n            analyticsStore.promoItemRemoved(product.id, promoPack);\n\n    // make the adjustments to the cart\n            if (quantityAdjust === totalProductQuantity) {\n                cartStore.removeProduct({\n                    product,\n                    promoCheck: false,\n                });\n            } else if (quantityAdjust > 0) {\n                if (quantityAdjust >= cartStore.productsById[product.id].quantity) {\n                    cartStore.removeProduct({\n                        product,\n                        promoCheck: false,\n                    });\n                } else {\n                    cartStore.changeQuantity({ \n                        id: product.id,\n                        quantity: -quantityAdjust,\n                    });\n                }\n            } else {\n                console.error('quantity to remove is greater than what exists.', quantityAdjust, totalProductQuantity);\n            }\n\n            this.savePromos();\n\n            if (postCall) postCall();\n        }\n\n    // called by pdp add button to determine where the promo page is located\n        @action navigationForProductID = (id) => {\n            return `/${toJS(this.promosByID[id]).url}`;\n        }\n\n    // called by product container to determine special functionality\n    // TODO: needs to be extended for more complex types like 2 product gwp.\n        @action isItemPromoEligible = (id, promoType) => {\n            try {\n                if (promoType) {\n\n                // this section is deprecated. Needs to be refactored.\n                    if (promoType === 'GWP' && this.promosByID[id]) {\n                        if (this.promosByID[id].actions.filter((action) => action.gwpProducts.length > 0 ).length > 0) return true;\n                        return false;\n                    }\n                    if (promoType === 'BOGO' && this.promosByID[id]) {\n                        if (this.promosByID[id].actions.filter((action) => action.autoAddProducts.length > 0 ).length > 0) return true;\n                        return false;\n                    }\n\n                // new dynamic detectors here\n                    const possibleMatches = this.getPromosOfType(promoType);\n                    \n                    if (possibleMatches.filter((descriptorId) => {\n                        for (let i = 0; i < this.promoTypesDictionary[promoType].detectors.length; i += 1) {\n                            const detector = this.promoTypesDictionary[promoType].detectors[i];\n                            if (!this.promoDetectorsDictionary[detector](descriptorId, promoType, id)) return false;\n                            return true;\n                        }\n                    }).length > 0) return true;\n\n                }\n\n            \n\n            // fallback OG behavior\n                return id in this.promosByID;\n            }\n            catch(e) {\n                return false\n            }\n        }\n\n    //called by cart store on verify cart. will process all items through promo engine.\n        @action checkCart = async (products = cartStore.productsById, mutate = true) => {\n            const modCart = this.formatCartForPromoServer(products);\n            let response;\n\n        // delete any items that are expired\n            this.checkForExpiredBucketGroups();\n\n            try {\n                response = await this.sendCart(modCart);\n                if (response) {\n\n                    // reset promo cart before processing response\n                    if (mutate) {\n                        if (Object.keys(cartStore.productsById).length < 1) return false;\n                        this.promoCartProductsByID = JSON.parse(JSON.stringify(cartStore.productsById));\n                    }\n\n                    // process response\n                    this.processServerResponse(response.data);\n\n                    //\n                    this.promoCartTotal = response.data.orderSubTotal;\n                    this.discountTotal = response.data.orderSubtotalDiscountValue;\n                }\n\n            } catch (e) {\n                console.error('issue connecting with promo server', e);\n            }\n        }\n\n    // fuzzy removal of matching promo using deep comparison. As a convenience, it will\n    // return the id of the promo.\n        removePromoWhichMatches = (promo) => {\n            this.logCollector(`removePromoWhichMatches ${JSON.stringify(promo)}`);\n            const promos = this.activatedPromos;\n            for (let i = 0; i < Object.keys(promos).length; i += 1) {\n                const descriptorId = Object.keys(promos)[i];\n                for (let j = 0; j < promos[Object.keys(promos)[i]].length; j += 1) {\n                    if (JSON.stringify(promo) === JSON.stringify(promos[Object.keys(promos)[i]][j])) {\n                        if (promo.active) {\n                            this.inactivateBucket(promo);\n                        }\n\n                        promos[Object.keys(promos)[i]].splice(j, 1);\n                        if (promos[Object.keys(promos)[i]].length < 1) delete promos[Object.keys(promos)[i]];\n                        this.activatedPromos = JSON.parse(JSON.stringify(promos));\n                        this.savePromos();\n                        return descriptorId;\n                    }\n                }\n            }\n            return false;\n        }\n\n        removeBucket = (bucketId) => {\n            if (this.promoTypeHasBehavior(\n                this.getPromoType(bucketId), \n                'removesChildrenOnRemove')) {\n                const instanceCopy = JSON.parse(JSON.stringify(this.activatedPromos[bucketId][0]));\n                if (instanceCopy.children.length > 0) {\n                    const childId = instanceCopy.children[0];\n                    if (cartStore.productsById[childId].quantity > 1) {\n                        cartStore.changeQuantity({\n                            id: childId,\n                            quantity: -1,\n                            disablePromoSearching: true,\n                        });\n                    } else {\n                        cartStore.removeProduct({\n                            product: cartStore.productsById[childId],\n                        });\n                    }\n                }\n            } \n\n            delete this.activatedPromos[bucketId];\n\n            this.savePromos();\n        }\n\n        sendCart = async (cart, options = {}) => {\n            const timestamp =  Date.now();\n            const rawJSON = await processCart(cart, {\n                ...options,\n                timestamp,\n            });\n            if (timestamp > this.lastTimeStamp) {\n                this.lastTimeStamp = timestamp;\n                return rawJSON;\n            } else {\n            }\n        }\n\n        processServerResponse = (data) => {\n            if (data && data.modifiedProducts \n            && data.modifiedProducts.length > 0) {\n                const { modifiedProducts } = data;\n\n                if (Array.isArray(modifiedProducts) && modifiedProducts.length > 0) {\n                    modifiedProducts.forEach((promoDetails) => {\n                        const cartItem = this.promoCartProductsByID[promoDetails.id];\n                        const price = ((cartItem.price \n                        * (cartItem.quantity - promoDetails.quantity))\n                        + (promoDetails.lineItemPrice\n                        * promoDetails.quantity)) / cartItem.quantity;\n                        this.promoCartProductsByID[promoDetails.id].promoPrice = true;\n                        this.promoCartProductsByID[promoDetails.id].price = price;\n                    });\n                }\n                return this.promoCartProductsByID;\n            }\n            return false;\n        }\n\n        formatCartForPromoServer = (cartProducts) => {\n\n            const { activeLocale } = localeStore;\n\n            const products = this.removePromos(cartProducts, (product => this.createPromoServerItem(product)));\n\n\n\n            const orderSubtotal = products.reduce((acc, item) => acc + (item.lineItemPrice * item.quantity), 0);\n\n            return {\n                accountTypeId: parseInt(userStore.phoenixGroupId),\n                countryId: parseInt(activeLocale.phoenixCountry),\n                orderTypeId: 1, // 1 = online order \n                products,\n                orderSubtotal,\n                // promoCodes: [\n                //   \"string\",\n                // ]\n            };\n        }\n\n        // takes a list of productsById and a formatter and returns a cloned list with the promo items stripped out.\n        removePromos(products, formatter) {\n            // clone the input\n            products = JSON.parse(JSON.stringify(products));\n\n            const ret = Object.keys(products).map((id) => {\n                let { quantity } = products[id];\n                let remove = false;\n            // removes quantity for free items.\n                try {\n                    Object.keys(this.activatedPromos).forEach((promoId) => {\n                        this.activatedPromos[promoId].forEach((bucketInstance) => {\n                            if (toJS(bucketInstance).children.includes(Number(id))) {\n\n                                // TODO: if this is needed for stackable promos (hopefully not.)\n                                // then this needs to be added onto for higher quantities.\n                                quantity -= 1;\n\n                                if (quantity < 1) {\n                                    remove = true;\n                                }\n                            }\n                        });\n                    });\n                } catch (e) {}\n                return remove ? 'remove' : formatter({...products[id], quantity});\n            }).filter(i => {\n                return i !== 'remove';\n            });\n            return ret;\n        }\n\n        createPromoServerItem = ({id, product, quantity}) => {\n            return {\n                id,\n                lineItemPrice: this.applyGroupPricingRules({\n                    price: cartStore.productsById[id].price,\n                    sku: product.sku,\n                }),\n                quantity,\n            }\n        }\n\n// takes a promo actions object and returns what it is.\n        getPromoType = (promoId) => {\n            try {\n                return Object.keys(this.promoTypesDictionary).filter((promoType) => {\n                    //run tracing filters to see if this is the type of promo.\n                    for (let i = 0; i < this.promoTypesDictionary[promoType].traceAttribs.length; i += 1) {\n                        const tracer = this.promoTypesDictionary[promoType].traceAttribs[i];\n                        if (!this.traceAttributeFilters[tracer](promoId)) {\n                            return false;\n                        }\n                    };\n                    return true;\n                })[0];\n            } catch (e) { this.logCollector(e) }\n\n            return false;\n        }\n\n    // receives a promo type string and returns all the promo buckets that are currently active.\n        getActivePromoBucketsOfType(promoType = 'autoAddProducts') {\n            const ret = {};\n            try {\n                this.getPromosOfType(promoType).forEach((promoId) => {\n                    if (this.activatedPromos[promoId]) {\n                        ret[promoId] = {\n                            details: this.promosByPromoId[promoId],\n                            bucket: this.activatedPromos[promoId],\n                        };\n                    }\n                });\n            }\n            catch(e) {\n                verbose & console.log(e);\n            }\n            return ret;\n        }\n\n    // recevies a promo type string such as 'autoAddProducts' and return all the promos id's for the matching promo descriptors\n    // of this type.\n        getPromosOfType(promoType) {\n            const promoCharacteristics = this.promoTypesDictionary[promoType];\n            \n            try {\n                return promoCharacteristics ? Object.keys(this.promosByPromoId).filter((promoConfig) => {\n                    //run tracing filters to find this promo.\n                    for (let i = 0; i < promoCharacteristics.traceAttribs.length; i += 1) {\n                        const tracer = promoCharacteristics.traceAttribs[i];\n                        if (!this.traceAttributeFilters[tracer](promoConfig)) {\n                            return false;\n                        }\n                    };\n                    return true;\n                }) : false;\n            }\n            catch(e) {\n                verbose && console.log(e);\n            }\n        }\n\n        // known edge cases that will fail:\n        // - if the threshold is being checked against a promo which has the behavior 'subtractsParentFromSubtotalUnlessSelf' and both promos share a mutual parent matcher.\n        isThresholdPromoRequirementsMet(bucketInstance, descriptor, options = {}) {\n\n            let parentId, childId;\n            let parentPriceSubtract = 0;\n            let childPriceSubtract = 0;\n\n            let requiredThreshold = descriptor.orderSubtotalThreshold;\n\n            this.logCollector(`checking for threshold`);\n            this.logCollector(bucketInstance);\n            this.logCollector(descriptor);\n\n            try {\n                if (options.new) {\n                    parentId = bucketInstance.id;\n                } else {\n                    parentId = descriptor.parents[0].id;\n                }\n            } catch (e) {}\n\n            // if (!options.adjust || options.new) {\n            if (!options.adjust && !options.new) {\n                // used to subtract quant 1 of this product from threshold.\n                try { childId = descriptor.actions[0].autoAddProducts[0].id } catch(e) {};\n                try { childId = descriptor.actions[0].gwpProducts[0].id } catch(e) {};\n            }\n            \n            // get a list of all the products but remove the free promo children.\n            // TODO: extend this to also return the removed promos, so that parent quantity calulation\n            // can be performed against.\n            const products = this.removePromos(cartStore.productsById, (product => product));\n            \n            // get the subtotal\n            let orderSubtotal = products.reduce((acc, item) => acc + (this.applyGroupPricingRules({\n                price: item.price,\n                sku: productsStore.productOrVariantById(item.id).sku,\n            }) * item.quantity), 0);\n            \n            try { \n                if (Object.keys(toJS(cartStore.productsById)).indexOf(String(childId)) > -1) {\n                    childPriceSubtract = productsStore.productsById[childId].price; \n                }\n            } catch(e) {}\n            \n            \n            // if this promo needs its parent subtracted.\n            try {\n                const parent = productsStore.productsById[parentId];\n                // TODO: see above\n                // parentPriceSubtract = parent.quantity * parent.price;\n                parentPriceSubtract = parent.price;\n                orderSubtotal -= parentPriceSubtract;\n                \n            } catch(e) {}\n\n            // some promo types have a weird threshold which has the price of the \n            // parent added to the threshold price.\n            if (this.promoTypeHasBehavior(bucketInstance.promoType, 'subtractsParentFromThreshold')) {\n                requiredThreshold -= parentPriceSubtract;\n            }\n\n            // remove all active parents from the subtotal\n            Object.keys(this.promosByPromoId).forEach((descriptorId) => {\n                if (this.promoTypeHasBehavior(\n                    this.getPromoType(descriptorId), 'subtractsParentFromSubtotalUnlessSelf')) {\n                        let parent\n                        try {\n                            // see if this promo's parents exist in the cart\n                            parent = cartStore.productsById[this.promosByPromoId[descriptorId].parents[0].id];\n                        } catch(e) {}\n\n                        const subtractExistingParentPrice = () => {\n                            if (parent) {\n                                this.logCollector(`original subtotal: ${orderSubtotal} before removing product id: ${parent.id} with price: ${parent.price}`);\n\n                                orderSubtotal -= parent.price;\n                            }\n                        };\n\n                        try {\n                            if (descriptor\n                            .parents\n                            .slice()\n                            .map(parent => Number(parent.id))\n                            .indexOf(parent.id) < 0) {\n                                subtractExistingParentPrice();\n                            } else { this.logCollector('does not subtract from subtotal for self'); }\n                        } catch (e) {subtractExistingParentPrice();}\n                    }\n            });\n\n            this.logCollector(`checking threshold: ${requiredThreshold} against subtotal: ${orderSubtotal}`);\n\n            if (requiredThreshold <= orderSubtotal) {\n                return true;\n            }\n            return false;\n        }\n\n        isUserRequirementsMet(descriptor) {\n            const accountTypesAllowed = toJS(descriptor.accountTypesAllowed);\n\n            return accountTypesAllowed.some((user, i) => \n                user === userStore.phoenixGroupId\n            ) ? true : accountTypesAllowed.length <= 0;\n        }\n\n    // called by cart store when an item is added to cart\n    // performs client side matching logic on an individual item.\n        @action checkItem = (product, operation, save) => {\n        // don't check an item if the session has not yet been loaded.\n            if (!this.promoCartLoaded) {\n                return false;\n            }\n\n        // verify valid product\n            if (product && product.id) {\n\n                const promo = this.promosByID && this.promosByID[product.id];\n                // if this is a promo product and is being created, then set up a new bucket.\n                if (promo && ( operation === 'create' || operation === 'verify')) {\n                    const promoType = this.getPromoType(promo.promotionId);\n                    const newBucket = this.createPromoBucket({\n                        id: product.id,\n                        promoType,\n                        parentQuantity: 1,\n                        ...(() => {\n                                if (promoType === 'autoAddProducts') {\n                                        // check if this is a threshold based promo.\n                                        if ((promo.orderSubtotalThreshold && this.isThresholdPromoRequirementsMet({ id: product.id, promoType }, promo, {new: true}))\n                                        // or check if this is an autoAdd.\n                                        || !promo.orderSubtotalThreshold) {\n                                            let count = promo.actions[0].autoAddProducts.slice().length;\n                                            return {\n                                                children: [...promo.actions[0].autoAddProducts\n                                                    .slice()\n                                                    .map((product) => parseInt(product.id))\n                                                ],\n                                                childQuantity: count,\n                                            };\n                                        // this is the case where there is a threshold to this\n                                        // autoadd, but it has not been fullfilled.\n                                        } else if (promo.orderSubtotalThreshold \n                                            && !this.isThresholdPromoRequirementsMet({ id: product.id, promoType }, promo, {new: true})) {\n                                                return {\n                                                    active: true,\n                                                };\n                                        }\n                                }\n                        })(),\n                    });\n                    const bucketId = this.saveBucket(promo.promotionId, newBucket);\n\n                // set gwp active to enable fancy ui stuff.\n                    if (newBucket.promoType === 'commonGWP')\n                        this.setGWPActive(promo.promotionId, bucketId);\n\n                    if (newBucket.promoType === 'autoAddProducts') {\n                            if (promo.orderSubtotalThreshold\n                            && (\n                                this.isThresholdPromoRequirementsMet(newBucket, promo, { new: true })\n                                && this.isUserRequirementsMet(promo)\n                            )\n                            || !promo.orderSubtotalThreshold) {\n                                promo.actions[0].autoAddProducts.slice().forEach((child) => {\n                                    const promoProduct = productsStore.productOrVariantById(parseInt(child.id));\n\n                \tif (typeof promoProduct === 'undefined') {\n                  \tconsole.error('trying to add promo for nonexistent product id:', parseInt(promo.actions[0].autoAddProducts[0].id), toJS(promo));\n                  \treturn false;\n                \t}\n                \tif (operation !== 'verify') {\n                                        cartStore.addProduct({\n                                            product: promoProduct,\n                                            quantity: 1,\n                                            isPromo: true,\n                                            hideCart: true,\n                                            disablePromoFinding: true, // prevent endless loop.\n                                        });\n                                    }\n                            });\n                        }\n                    }\n                }\n\n            //child logic for gwp\n                else if (this.isGWPActive) {\n                    const promoType = this.getPromoType(this.currentGWP);\n\n                    if (this.isItemPromoEligible(product.id, promoType)) {\n\n                        if (promoType === 'thresholdGWP') {\n                            this.saveBucket(this.currentGWP, this.createPromoBucket({\n                                children: [product.id],\n                                promoType\n                            }));\n                            this.deactivateCurrentGWP();\n                        }\n\n                    // assuming common gwp\n                        else {\n                            const bucket = this.findActiveBucketForChild(product.id);\n                            if (bucket && this.isUserRequirementsMet(this.promosByPromoId[this.currentGWP])) {\n                            // adds the ghost parent product.\n                                cartStore.addProduct({\n                                    product: productsStore.productsById[bucket.parents[0]],\n                                    quantity: 1,\n                                    hideCart: true,\n                                    disablePromoFinding: true,\n                                });\n\n                                this.addChildToBucket(product, bucket, { isGWP: true });\n                                this.deactivateCurrentGWP();\n                            }\n                        }\n                    }\n                }\n\n                    \n                //TODO: check all active promos. If they are filled, close them.\n            }\n\n            save && this.savePromos();\n        }\n\n        // Takes string of promoType. Returns bool.\n        promoTypeHasBehavior = (promoType, behavior) => {\n            try {\n                return this.promoTypesDictionary[promoType].behaviors.indexOf(behavior) > -1;\n            } catch (e) {\n                return false;\n            }\n        }\n\n        getPromoTypesWithTraceAttrib = (traceAttrib = '') => {\n            return Object.keys(this.promoTypesDictionary)\n                .filter((promoType) => this.promoTypesDictionary[promoType].traceAttribs\n                        .indexOf(traceAttrib) > -1 );\n        }\n\n        goToPromoPage = ({ url }) => {\n            if (url) {\n                if (url !== navigationStore.path) {\n                    navigationStore.to({ url });\n                }\n            }\n        }\n\n    //looks for active bucket that is compatible with this child possibility\n        findActiveBucketForChild = (productId) => {\n            const promoID = this.scanThroughPromosForChild(productId);\n        //this is a possible child. see if bucket exists.\n            if (promoID) {\n                const bucketGroup = this.activatedPromos[promoID];\n                if (Array.isArray(toJS(bucketGroup)) && bucketGroup.length > 0) {\n                    return bucketGroup.filter((bucket) => bucket.active)[0];\n                }\n            }\n            return false;\n        }\n\n        // TODO: refactor this to be simpler.\n        // looks to see if this id is part of an active promo.\n        // return true;\n        checkForProductInActivePromos = (productId, type = 'child', returns = 'bucket') => {\n            for (let i = 0; i < Object.keys(this.activatedPromos).length; i += 1) {\n                const id = Object.keys(this.activatedPromos)[i];\n                for (let j = 0; j < this.activatedPromos[id].length; j += 1) {\n                    const currentPromo = this.activatedPromos[id][j];\n                    if (type === 'parent' || type === 'both') {\n                        if (currentPromo.parents.slice().indexOf(Number(productId)) >= 0)\n                            return returns === 'bucket' ? currentPromo : id;\n                    }\n                    if (type === 'child' || type === 'both') {\n                        if (currentPromo.children.slice().indexOf(Number(productId)) >= 0)\n                            return returns === 'bucket' ? c