AAA;AAAA;AAAA;AAAA;AAAA,uCAAsB,yBAAcA,IAAd,CAAtB;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,a;;;;;;;;;;;;;kGAEY,mBAAOW,MAAP;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uCAAwB,iCAA0BA,MAA1B,CAAxB;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,a;;;;;;;;;;;;;kGAEP,mBAAOA,MAAP;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uCAAwB,4BAAqBA,MAArB,CAAxB;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,a;;;;;;;;;;;;;kGAEK,mBAAOA,MAAP;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uCAAwB,iCAA0BA,MAA1B,CAAxB;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,a;;;;;;;;;;eAEF,UAAC7C,KAAD;AAAA,mBAAW,uBAAaE,KAAb,CAAmB,SAAnB,IAAgC,4BAAiBF,KAAjB,CAAhC,GAA0D,+BAAwBA,KAAxB,CAArE;AAAA,S;;;;;AAI/B,IAAMqD,YAAY,IAAIlF,SAAJ,EAAlB;;kBAEekF,S;QACNlF,S,GAAAA,S","file":"userStore.js","sourceRoot":"/Users/millie.savalia/react-node-frontend","sourcesContent":["\nimport { observable, computed, action, autorun, toJS } from 'mobx';\nimport { getSessionItem, saveSessionItem } from 'Middleware/services/session';\n\n// Old authentication: To be removed\nimport { \n    loginUser as loginUserMagento, \n    logoutUser as logoutUserMagento, \n    checkIsLogged as checkIsLoggedMagento, \n    createAccount as createAccountMagento, \n    resetPasswordEmail as resetPasswordEmailMagento, \n    resetPassword as resetPasswordMagento, \n    checkPasswordToken as checkPasswordTokenMagento, \n    checkEmailExists as checkEmailExistsMagento\n} from 'Middleware/api/magento/account';\n////////////////////////////////////\n\n// New authentication: To be kept\nimport { \n    login as loginFirebase, \n    logout as logoutFirebase, \n    createAccount as createFirebaseAccount, \n    subscribeIsLogged, \n    writeUserData,\n    checkEmailExists \n} from 'Middleware/services/user';\n////////////////////////////////////\n\nimport { getSafe } from 'Middleware/utils/object';\nimport requestPWS, { getSocials } from 'Middleware/api/phoenix';\nimport { cloneDeep } from 'lodash';\nimport { CLIENT, MEMBER, CONSULTANT, EMPLOYEE } from 'Middleware/constants/userStates';\nimport { analytics } from 'Root/config'\n\nimport contentStore from 'Stores/contentStore';\nimport sessionStore from 'Stores/sessionStore';\nimport interfaceStore from 'Stores/interfaceStore';\nimport productsStore from 'Stores/productsStore';\nimport localeStore from 'Stores/localeStore';\nimport cartStore from 'Stores/cartStore';\nimport attributionStore from 'Stores/attributionStore';\nimport analyticsStore from 'Stores/analyticsStore';\n\nclass UserStore {\n\n    @observable userInfo;\n    @observable isLogged;\n    @observable guest;\n\n    constructor() {\n        this.userInfo = {};\n        this.isLogged = false;\n        this.guest = {};\n\n        this.groupPricingRules = {};\n        __BROWSER__ && requestAnimationFrame(() => {\n            if (sessionStore.getKey('user')) this.verifyLoggedIn();\n            this.guest = sessionStore.getKey('guest-account') || {};\n            autorun(() => sessionStore.saveKey('guest-account', this.guest));\n            this.mountReactions();\n        });\n    }\n\n    mountReactions() {\n        autorun(() => {\n            if (this.isLogged) {\n                sessionStore.saveKey('user', this.userInfo);\n            }\n        });\n    }\n\n    @computed get user() {\n        return getSafe(() => JSON.parse(JSON.stringify(this.userInfo)));\n    }\n\n    @computed get groupId() {\n        return getSafe(() => this.userInfo.groupid);\n    }\n\n    @computed get userId() {\n        return getSafe(() => (this.userInfo.magentoid));\n    }\n\n    @computed get phoenixGroupId() {\n        try {\n            const id = new Map([\n                [9, 1], // distributor\n                [10, 4], // employee\n                [1, 3], // general\n                [0, 7], // guest\n                [8, 2], // preferred\n                [7, 3], // retail\n            ]).get(parseInt(this.groupId));\n            if (id) {\n                return id;\n            } else throw '';\n        } catch (e) {\n            return 7; // must be a guest eh?\n        }\n    }\n\n    @computed get hasOpenSocial() {\n        return getSafe(() => this.socials.length > 0);\n    }\n\n    @computed get consultantId() {\n        return getSafe(() => this.userInfo.consultantId);\n    }\n\n    @computed get consultant() {\n        return getSafe(() => this.userInfo.consultant);\n    }\n\n    @computed get socials() {\n        return getSafe(() => this.userInfo.socials);\n    }\n\n    @computed get isConsultant() {\n        return this.groupId === CONSULTANT;\n    }\n\n    @computed get isMember() {\n        return this.groupId === MEMBER;\n    }\n\n    @computed get isClient() {\n        return this.groupId === CLIENT;\n    }\n\n    @computed get isEmployee() {\n        return this.groupId === EMPLOYEE;\n    }\n\n    @computed get pwsUrl() {\n        return getSafe(() => this.consultant.pwsKey);\n    }\n\n    @computed get fullName() {\n        const firstName = getSafe(() => this.userInfo.firstname);\n        const lastName = getSafe(() => this.userInfo.lastname);\n        return (firstName && lastName) ? `${firstName} ${lastName}` : '';\n    }\n\n    @computed get email() {\n        return getSafe(() => this.userInfo.email);\n    }\n\n    @computed get isGuest() {\n        return getSafe(() => this.guest.email);\n    }\n\n    @action verifyLoggedIn = async (force = false) => {\n        // New authentication: To be kept\n        if (contentStore.flags('nogento')) {\n            subscribeIsLogged(async auth => {\n                if (!auth) return false;\n                const login = await auth();\n                \n                if (login && login.email) {\n                    this.addLoginState(login);\n                    return true;\n                }\n                else {\n                    this.addLogoutState();\n                }\n            });\n        }\n        // Old authentication: To be removed\n        else {\n            const login = await checkIsLoggedMagento();\n\n            if (login && login.customerdetails) {\n                if (!this.isLogged || force) this.addLoginState(login.customerdetails);\n                return true;\n            }\n\n            this.addLogoutState();\n            return false;\n        }\n        /////////////////////////////\n    }\n\n    @action login = async (form, options = {}) => {\n        // New authentication: To be kept\n        if (contentStore.flags('nogento')) {\n            if (options.progress) interfaceStore.showProgress('user-login');\n\n            this.verifyLoggedIn();\n            const response = await loginFirebase(form.email, form.password);\n            interfaceStore.hideProgress('user-login');\n            if (response.email) {\n                return {\n                    success: true,\n                };\n            }\n            else return {\n                ...response,\n                success: false,\n            }\n        }\n        // Old authentication: To be removed\n        else {\n            if (options.progress) interfaceStore.showProgress('user-login');\n\n            // Force trigger logout to clear any existing sessions\n            // because Magento tends not to always clear existing sessions\n            const loggingOut = await logoutUserMagento();\n\n            const login = await loginUserMagento(form.email, form.password);\n            interfaceStore.hideProgress(\"login\");\n            if (login && getSafe(() => login.customerdetails.firstname)) {\n                this.addLoginState(login.customerdetails);\n            }\n            return login;\n        }\n        /////////////////////////////\n    }\n\n    @action addLoginState = async (account) => {\n        const userInfo = {\n            name: account.firstname || '',\n            ...account,\n            groupid: account.groupid || 0,\n            consultantId: account.encoreid || 0,\n        };\n        this.isLogged = true;\n        account.attribution && attributionStore.hydrateAttribution(account.attribution);\n        this.userInfo = cloneDeep(userInfo);\n        this.guest = {};\n        attributionStore.updateAttribution();\n\n        // Temporarily disable due to API issue with the cartSave endpoint\n        // cartStore.loadUserCart(account.cart);\n        interfaceStore.hideProgress('user-login');\n\n        saveSessionItem(\"user\", {\n            loggedIn: true,\n            ...userInfo\n        });\n\n        if (this.isConsultant || this.isEmployee) {\n            attributionStore.reset();\n            if (this.isConsultant) {\n                await this.lookupConsultantDetails();\n                await this.lookupOpenSocials();\n            }\n        }\n    }\n\n    @action processConsultantData = (consultant) => {\n        return {\n            ...consultant,\n            fullName: `${consultant.firstName} ${consultant.lastName}`,\n            url: consultant.key || '/',\n        };\n    }\n\n    @action setConsultant = (consultant) => {\n        this.userInfo = cloneDeep({\n            ...this.userInfo,\n            consultant: this.processConsultantData(consultant),\n        });\n    }\n\n    @action setGuest = (email, password) => {\n        this.guest = {\n            email,\n            password\n        };\n        this.guest = cloneDeep(this.guest);\n    }\n\n    @action clearGuest = () => {\n        this.guest = {};\n    }\n\n    @action lookupConsultantDetails = async () => {\n        try {\n            const result = await requestPWS({ id: this.consultantId });\n            if (result && result.data && result.data.id && result.data.active) {\n                const data = {\n                    ...result.data,\n                };\n                this.setConsultant(data);\n                return toJS(this.consultant);\n            }\n        }\n        catch(err) {\n            console.log(err);\n            return false;\n        }\n    }\n\n    @action lookupOpenSocials = async () => {\n        try{\n            const result = await getSocials(this.consultantId, localeStore.activeLocale.phoenixCountry);\n            if (result) {\n                this.setSocials(result);\n                return toJS(this.socials);\n            }\n        }\n        catch(err) {\n            console.log(err);\n            return false;\n        }\n    }\n\n    @action setSocials = (socials) => {\n        this.userInfo = cloneDeep({\n            ...this.userInfo,\n            socials,\n        });\n    }\n\n    @action logout = async (load = true) => {\n        // New authentication: To be kept\n        if (contentStore.flags('nogento')) {\n            if (load) interfaceStore.showProgress('user-logout');\n\n            const loggingOut = await logoutFirebase(() => {\n                attributionStore.reset();\n                this.addLogoutState();\n                requestAnimationFrame(() => {\n                    cartStore.clear();\n                    sessionStore.clearAllKeyInstances('cart');\n                    sessionStore.clearAllKeyInstances('promoStorage');\n                });\n                interfaceStore.hideProgress('user-logout');\n                analyticsStore.reset();\n            });\n        }\n        // Old authentication: To be removed\n        else {\n            if (load) {\n                interfaceStore.showProgress('user-logout');\n            }\n\n            const verifyLoggedIn = await this.verifyLoggedIn();\n            const loggingOut = await logoutUserMagento();\n            if (loggingOut || !verifyLoggedIn) {\n                attributionStore.reset();\n                this.addLogoutState();\n                requestAnimationFrame(() => {\n                    cartStore.clear();\n                    sessionStore.clearAllKeyInstances('cart');\n                    sessionStore.clearAllKeyInstances('promoStorage');\n                });\n            }\n            interfaceStore.hideProgress('user-logout');\n\n            analyticsStore.reset();\n        }\n        /////////////////////////////\n    }\n\n    @action addLogoutState = async () => {\n        this.isLogged = false;\n        this.userInfo = {};\n        saveSessionItem(\"user\", {\n            loggedIn: false\n        });\n    }\n\n    @action createAccount = async (params, autologin = true) => {\n        const input = {\n            ...params,\n            storeid: localeStore.activeLocale.magentoStoreView,\n            sailthru_subscription_source: 'account_creation',\n        };\n\n        // New authentication: To be kept\n        if (contentStore.flags('nogento')) {\n            interfaceStore.showProgress(\"login\");\n            const response = await createFirebaseAccount(input);\n            interfaceStore.hideProgress(\"login\");\n    \n            if (response && response.email) {\n                this.addLoginState(response);\n                return {\n                    success: true\n                };\n            }\n            return {\n                success: false,\n                message: response.message || 'Error'\n            };\n        }\n        // Old authentication: To be removed\n        else {\n            interfaceStore.showProgress(\"login\");\n            const create = await createAccountMagento(input);\n            if (create && create.success) {\n                if (autologin) {\n                    const login = await this.login(input);\n                    if(login.success) {\n                        const loggedIn = await this.verifyLoggedIn();\n                        interfaceStore.hideProgress(\"login\");\n                        return login;\n                    }\n                    else {\n                        interfaceStore.hideProgress(\"login\");\n                        return false;\n                    }\n                }\n                else return create;\n            }\n            else {\n                interfaceStore.hideProgress(\"login\");\n                return create;\n            }\n        }\n        /////////////////////////////\n    }\n\n    @action saveInfo = async (data) => await writeUserData(data);\n\n    @action requestPasswordReset = async (params) => await resetPasswordEmailMagento(params);\n\n    @action resetPassword = async (params) => await resetPasswordMagento(params);\n\n    @action checkPasswordToken = async (params) => await checkPasswordTokenMagento(params);\n    \n    @action checkEmailExists = (email) => contentStore.flags('nogento') ? checkEmailExists(email) : checkEmailExistsMagento(email);\n\n}\n\nconst userStore = new UserStore();\n\nexport default userStore;\nexport { UserStore };\n"]}},"_buildHash":"859f25355c178eea7432968059c06897","hash":"4a464d276d954f4e61f406fa7899f4fe","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/define-property","range":[144,190],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[136,143],"loc":{"start":{"line":8,"column":22},"end":{"line":8,"column":77}}},{"type":"RequireHeaderDependency","range":[275,282],"loc":{"start":{"line":12,"column":16},"end":{"line":12,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/extends","range":[283,314],"loc":{"start":{"line":12,"column":16},"end":{"line":12,"column":56}}},{"type":"RequireHeaderDependency","range":[389,396],"loc":{"start":{"line":16,"column":19},"end":{"line":16,"column":55}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[397,424],"loc":{"start":{"line":16,"column":19},"end":{"line":16,"column":55}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/asyncToGenerator","range":[520,560],"loc":{"start":{"line":20,"column":25},"end":{"line":20,"column":74}}},{"type":"Require