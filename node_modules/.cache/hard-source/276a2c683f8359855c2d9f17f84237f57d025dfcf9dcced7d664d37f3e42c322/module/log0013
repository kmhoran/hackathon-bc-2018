| null;\n        function process(pieces, type) {\n            return pieces\n                    .map(piece => ({\n                        ...piece,\n                        type,\n                        filters: [\n                            type === 'digital' ? 1 : 0,\n                            new Date(piece.date).getFullYear(),\n                        ],\n                    }))\n                    .filter(piece => !piece.disabled);\n        }\n\n        if (items) {\n            this.items = [\n                ...process(items.pressPieceDigital, 'digital'),\n                ...process(items.pressPiecePrint, 'print'),\n            ];\n            this.page = custom || items.configPress[0] || null;\n            this.generateFilters();\n            this.setSelected(this.selectedID);\n        }\n    }\n\n    @computed get selected() {\n        return this.items.filter(item => item.slug === this.selectedID)[0];\n    }\n\n    @action setSelected = (id, urlChange) => {\n        if (this.itemsById[id]) {\n            if (this.selectedID !== id) {\n                this.selectedID = id;\n\n                if (__BROWSER__ && !urlChange) {\n                    navigationStore.addPathArgument({ route: routes.PRESS, arg: id });\n                }\n            }\n        }\n        else this.resetSelected();\n    }\n\n    @action resetSelected = () => {\n        if (this.selectedID) {\n            this.selectedID = null;\n            if (__BROWSER__) navigationStore.cleanPath({ route: routes.PRESS });\n        }\n    }\n\n    @computed get itemsById() {\n        return keyBy(this.items, 'slug');\n    }\n\n    @computed get filteredItems() {\n        return this.activeFilters ? this.items.filter(item => filterItems(item.filters, this.activeFilters)) : this.items;\n    }\n\n    @computed get itemsByYear() {\n        const sorted = this.filteredItems.sort((a, b) => new Date(b.date) - new Date(a.date));\n        \n        const byYear = {};\n        sorted.forEach((item) => {\n            const year = new Date(item.date).getFullYear();\n            if (!byYear[year]) byYear[year] = [];\n            byYear[year].push(item);\n        });\n\n        return byYear;\n    }\n\n    @action generateFilters = () => {\n        const filters = {};\n\n        const pressType = this.page.filterPressType || 'Press Type';\n        const year = this.page.filterYear || 'Year';\n\n        filters[pressType] = {\n            0: { id: 0, name: this.page.typePrint, parent: pressType },\n            1: { id: 1, name: this.page.typeDigital, parent: pressType },\n        };\n\n        const years = {};\n        this.items.forEach((item) => {\n            const date = new Date(item.date).getFullYear();\n            years[date] = { id: date, name: date.toString(), parent: year };\n        });\n        filters[year] = years;\n\n        if (!isEqual(this.filters, filters)) {\n            this.resetFilters(false);\n            this.filters = filters;\n        }\n    }\n\n    @action toggleFilter = (data, urlUpdate = true) => {\n        const newFilters = JSON.parse(JSON.stringify(this.activeFilters));\n\n        if (typeof newFilters[data.parent] === 'undefined') {\n            newFilters[data.parent] = {};\n        }\n\n        if (data.value) {\n            newFilters[data.parent][data.id] = data;\n        }\n        else {\n            delete newFilters[data.parent][data.id];\n            if (isEmpty(newFilters[data.parent]))\n                delete newFilters[data.parent];\n        }\n\n        this.activeFilters = newFilters;\n        urlUpdate && this.updateQueries();\n        this.resetSelected();\n    }\n\n    @action updateQueries = () => {\n        const keys = [];\n\n        Object.keys(this.activeFilters).forEach((section) => {\n            Object.keys(this.activeFilters[section]).forEach((filter) => {\n                keys.push(this.activeFilters[section][filter].id);\n            })\n        })\n\n        navigationStore.addQuery({\n            queries: keys, \n            isBackPoint: true\n        });\n\n        if (!keys.length) this.resetFilters(true);\n    }\n\n    @action resetFilters = (hard = true) => {\n        this.activeFilters = {};\n        if (__BROWSER__ && hard) {\n            removeURLQueries(true);\n            this.resetSelected();\n        }\n    }\n\n    @action decodeFilterQuery = (query) => {\n        if (query.filter && query.filter.length) {\n            const all = this.allFilters;\n            query.filter.map(value => all[value] && this.toggleFilter({\n                ...all[value],\n                value: true,\n            }, false));\n        }\n        else this.resetFilters();\n    }\n\n    @computed get allFilters() {\n        const all = {};\n        Object.keys(this.filters).map((section) => {\n            Object.keys(this.filters[section]).map((filter) => {\n                all[this.filters[section][filter].id] = this.filters[section][filter];\n            });\n        });\n\n        return all;\n    }\n\n    @computed get filterCount() {\n        const filters = this.activeFilters;\n\n        return Object.keys(filters).reduce(\n            (total, section) => total + Object.keys(filters[section]).reduce(\n                (total, filter) => total + Boolean(filters[section][filter].value), 0), \n            0);\n        \n    }\n\n    @computed get hasFilters() {\n        return Boolean(this.filterCount);\n    }\n\n    @action resetScroll = () => {\n        const element = document.querySelector('#press-body-container');\n        if (element) {\n            const position = window.pageYOffset + element.getBoundingClientRect().top;\n            if(window.pageYOffset > position) {\n                const offset = 100;\n                window.scrollTo(0, position - offset);\n            }\n        }\n    }\n}\n\nconst pressStore = new PressStore();\n\nexport default pressStore;\nexport { PressStore };\n"]}},"_buildHash":"5ee3bbde960b57457d683a0e7405140e","hash":"95837cdedad99a