computed } from 'mobx';\nimport { observer, inject } from 'mobx-react';\nimport s from './ProductAddButton.scss';\nimport cx from 'classnames';\nimport Button from 'Common/Button';\nimport Cart from 'Components/Cart';\nimport { COLOR_WHITE, COLOR_BLACK } from 'Middleware/constants/colors';\nimport ProductPrice from 'Components/Product/ProductPrice';\nimport { getSafe } from 'Middleware/utils/object';\n\n@withStyles(s)\n@inject(\n    'cartStore',\n    'contentStore',\n    'productsStore',\n    'promoStore',\n)\n@observer class ProductAddButton extends Component {\n\n    static propTypes = {\n        quantity: pt.number,\n        product: pt.object,\n        variant: pt.object,\n        mediumSize: pt.bool,\n        smallSize: pt.bool,\n        className: pt.string,\n        thin: pt.bool,\n        dark: pt.bool,\n        hideCart: pt.bool,\n        callback: pt.func,\n        actionOverride: pt.bool,\n        showPrice: pt.bool,\n        disabled: pt.bool,\n    };\n\n    static defaultProps = {\n        quantity: 1,\n        mediumSize: false,\n        smallSize: false,\n        thin: false,\n        dark: false,\n        hideCart: false, //disable showing the cart when adding product.\n        callback: null,\n        actionOverride: false,\n        showPrice: false,\n        disabled: false,\n    };\n\n    handleAddProduct = () => {\n        const\n            { cartStore, promoStore,\n                product, variant, quantity, selectedVariant, hideCart, callback, actionOverride } = this.props;\n        const disablePromoFinding = actionOverride ? true : false;\n\n        cartStore.addProduct({\n            product,\n            quantity,\n            selectedVariant: selectedVariant || (variant && variant.id) || null,\n            variant: variant || product,\n            callback,\n            hideCart,\n            disablePromoFinding,\n        });\n    };\n\n    @computed get maxed() {\n        this.props.productsStore.productsById;\n        this.props.cartStore.productsById;\n        const maxQuantity = this.props.cartStore.getItemMaxQuantity(this.props.selectedVariant || this.props.product.id);\n        return maxQuantity <= 0 ? true : false;\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.content.productDetail[0]);\n    }\n\n    render() {\n        const { product, variant, mediumSize, smallSize, thin, actionOverride, cartStore, promoStore, showPrice, children} = this.props;\n        const config = this.config;\n        const inStock = variant ? variant.inStock : product.inStock;\n        const { maxed } = this;\n        let { dark, disabled } = this.props;\n        if (maxed) disabled = true;\n\n        // use a named function to prevent full re-rendering on state changes\n        function renderPriceButton(message) {\n            return (\n                <div className={s.flexWrapper}>\n                    <ProductPrice\n                        className={cx(s.textElement, s['button__textElement--price'])}\n                        amount={product.price}\n                        compareAtPrice={product.compareAtPrice}\n                        sku={variant.sku || product.sku}\n                        small\n                        whiteFont\n                        lightFont\n                        inline\n                    />\n                    <div className={cx(s.textElement, s['button__textElement--seperator'])} />\n                    <div className={cx(s.textElement, s['button__textElement--message'])}>{message}</div>\n                </div>\n            );\n        };\n\n        const action = actionOverride ? { to: promoStore.navigationForProductID(product.id) } : { onClick: this.handleAddProduct };\n        const addToCartButton = maxed ? config.maximumInCart : config.addToCart;\n\n        let message = '';\n        if (inStock) {\n            if (actionOverride) {\n                message = config.freeGiftButton\n            } else if (product.preOrderCTA) {\n                message = config.buttonPreOrder;\n            } else {\n                message = addToCartButton;\n            }\n        } else { message = config.outOfStock; }\n\n        if (typeof children === 'string') message = children;\n\n        return (\n            <React.Fragment>\n                <Button\n                    className={cx(s.root, mediumSize && s.mediumSize, smallSize && s.smallSize, this.props.className && this.props.className,\n                        {[s['button--priceShow']]: showPrice},\n                    )}\n                    disabled={disabled || !inStock}\n                    color={(() => {\n                        if (dark) return COLOR_BLACK;\n                        else if (smallSize) return COLOR_WHITE;\n                        else return COLOR_BLACK;\n                    })()}\n                    mobile={smallSize || false}\n                    thin={thin}\n                    {...action}\n                >\n                    {showPrice ? renderPriceButton(message) :\n                        <span>{message}</span>\n                    }\n                </Button>\n                {children && typeof children !== 'string' && children}\n                {actionOverride &&\n                    <a className={s.noFreePick} href='javascript:void(0)' onClick={this.handleAddProduct}>{config.freeGiftDeclineLink}</a>\n                }\n            </React.Fragment>\n        );\n    }\n}\n\nexport default ProductAddButton;\n"]}},"_buildHash":"3a91aad2740ee3da70c51655df0fb244","hash":"fabf9c51f987e5e10cbff6c9d7c716a2","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line"