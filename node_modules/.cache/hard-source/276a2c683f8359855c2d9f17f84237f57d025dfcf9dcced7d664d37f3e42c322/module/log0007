n\n@withStyles(s)\n@mixin(ScrollToMixin)\nclass ImageViewer extends Component {\n    static propTypes = {\n        className: pt.string,\n        large: pt.bool,\n        product: pt.string\n    };\n\n    static defaultProps = {\n        className: '',\n        images: [],\n        large: false,\n        product: false,\n        activeVariant: 0\n    };\n\n    state = {\n        activeImageIndex: 0,\n        imagePreviewsLoaded: true,\n        fullscreen: false,\n        width: 1280,\n        appIsMounted: false,\n    };\n\n    componentDidMount() {\n        if(this.props.large) {\n            if(__BROWSER__) {\n                this.scrollListener = throttle(this.handleScroll,40);\n                this.resizeListener = this.updateDimensions;\n                window.addEventListener('scroll', this.scrollListener, { passive: true });\n                window.addEventListener(\"resize\", this.resizeListener);\n            }\n            this.updateDimensions();\n        }\n        \n        requestAnimationFrame(() => {\n            if(this.props.activeVariant) \n                this.setState({appIsMounted: true});\n        })\n    }\n\n    componentWillUnmount = () => {\n        window.removeEventListener('scroll', this.scrollListener);\n        window.removeEventListener(\"resize\", this.resizeListener);\n    }\n\n    updateDimensions = () => {\n        if(__BROWSER__) {\n            let windowWidth = window.innerWidth;\n            this.setState({width: windowWidth});\n        }\n    }\n\n    handleScroll = () => {\n        if(this.state.width > MEDIA_MOBILE && this.props.large && this.refs.imageviewer) {\n            let scrollTop = window.pageYOffset,\n                pos, latestIndex = 0, ref;\n\n            for(let index = 0; index < this.props.images.length; index++) {\n                    ref = `image-${index}`;\n                    if(this.refs[ref]) {\n                        pos = this.refs[ref].getBoundingClientRect().bottom + scrollTop - this.refs[ref].offsetHeight;\n                        if(scrollTop <= pos ) {\n                            latestIndex = index;\n                            break;\n                        }\n                        else if (index === this.props.images.length - 1) {\n                            latestIndex = index;\n                        }\n                    }\n            };\n\n            if (this.state.activeImageIndex != latestIndex && this.refs.imageviewer) {\n                    this.setState({ activeImageIndex: latestIndex});\n            }\n        }\n    }\n\n    componentWillReceiveProps(nextProps) {\n        if (this.props.product !== nextProps.product) {\n            // in case we navigate to the page directly from another product, we need to reset the state of the image index otherwise it could show a blank image after having loaded another image stack\n            this.state.activeImageIndex = 0;\n        }\n        // if parent changes the active image index (e.g. swatch change), update the local activeImageIndex state\n        if(this.props.activeVariant !== nextProps.activeVariant && nextProps.images.length !== this.props.images.length) {\n            this.handleMainImageIndexChange(0);\n            this.setState({ activeImageIndex: 0});\n        }\n        if(this.props.activeVariant !== nextProps.activeVariant) {\n            if(this.refs.imageviewer) {\n                this.refs.imageviewer.style.opacity = 0.7;\n                const comp = this.refs.imageviewer;\n                setTimeout(() => {\n                    if(comp) comp.style.opacity = 1;\n                }, 300);\n            }\n        }\n    }\n\n    handleImagePreviewLoading = () => {\n        this.imagesLoading++;\n\n        if (this.state.imagePreviewsLoaded && this.imagesLoading > 0) {\n            this.setState({ imagePreviewsLoaded: false });\n        }\n    };\n\n    handlerImagePreviewLoaded = () => {\n        this.imagesLoading--;\n\n        if (!this.state.imagePreviewsLoaded && this.imagesLoading === 0) {\n            this.setState({ imagePreviewsLoaded: true });\n        }\n    };\n\n    handleMainImageIndexChange = index => {\n        //this.setState({ activeImageIndex: index });\n        let extraHeight = 40;\n        if(index == 0)\n            extraHeight += 60;\n\n        if(this.props.large) { \n            const single = this.props.images.length < 2;\n            if(index != this.state.activeImageIndex || (this.state.activeImageIndex > 0 && single))\n                this.scrollTo({\n                    hash: `#image-${single ? 0 : index}`,\n                    animate: true,\n                    duration: 600,\n                    extraHeight: extraHeight\n                });\n        }\n    };\n\n    handleMainImageIndexChangePagination = index => {\n        event.stopPropagation();\n        event.preventDefault();\n        \n        this.setState({ activeImageIndex: index });\n    };\n\n    imagesLoading = 0;\n\n    renderPreviews() {\n        const { activeImageIndex, imagePreviewsLoaded } = this.state;\n\n        const Thumb = (props) => (props.video ? <img {...props} src={`${assetPath}/images/play.svg`}  /> : <Image {...props} />);\n\n        const previews = map(this.props.images, (image, index) =>\n                <Thumb \n                    key={`${(image.video || image.src) + this.state.appIsMounted + index}-preview`}\n                    src={image.src}\n                    video={image.video}\n                    className={cx(s.previewImage, this.props.large && s.previewImageLarge, {\n                        [s.active]: activeImageIndex === index\n                    })}\n                    onClick={() => this.handleMainImageIndexChange(index)}\n                />);\n\n        if(this.props.images.length > 1) {\n            return (\n                <Sticky \n                    top={30} \n                    bottomBoundary='#pdp-sticky-container'>\n                        <div className={cx(s.previews, this.props.large && s.previewsLarge)}>\n                            {previews}\n                        </div>\n                </Sticky>\n            );\n        }\n        else\n            return <div className={cx(s.previews, this.props.large && s.previewsLarge)}>\n                    {previews}\n                </div>;\n    }\n\n    renderPagination() {\n        const { activeImageIndex, imagePreviewsLoaded } = this.state;\n\n        if(this.props.images.length > 1) {\n            return (\n                <div className={cx(s.owlPagination)}>\n                    {map(this.props.images, (image, index) =>\n                        <div\n                            key={index}\n                            className={cx(s.owlPage, !this.props.large && s.owlPageSmall, {\n                                [s.active]: activeImageIndex === index\n                            })}\n                            onClick={() => this.handleMainImageIndexChangePagination(index)}\n                        >\n                        </div>\n                    )}\n                </div>\n            );\n        }\n    }\n\n    increaseIndex = () => {\n        let next = this.state.activeImageIndex + 1;\n        if(next > this.props.images.length - 1)\n            next = 0;\n\n        this.setState({ activeImageIndex: next });\n    };\n\n    decreaseIndex = () => {\n        let next = this.state.activeImageIndex - 1;\n        if(next < 0)\n            next = this.props.images.length - 1;\n\n        this.setState({ activeImageIndex: next });\n    };\n\n    showFullscreen = () => {\n        this.setState({ fullscreen: true });\n    }\n\n    handleCloseFullscreen = () => {\n        this.setState({ fullscreen: false });\n    }\n\n    renderArrows(children) {\n        return (\n            <div className={s.arrowContainer}>\n                <Arrows onClick={() => this.decreaseIndex()} className={s.arrow} left />\n                    {children}\n                <Arrows onClick={() => this.increaseIndex()} className={s.arrow} right />\n            </div>\n        )\n    }\n\n    renderViewerSwipe() {\n        return (\n            <SwipeableViews\n                resistance\n                index={this.state.activeImageIndex}\n                className={s.viewer}\n                onChangeIndex={this.handleMainImageIndexChangePagination}\n                springConfig={{\n                    stiffness: 250,\n                    damping: 36\n                }}\n            >\n                {this.renderContentImages(true)}\n            </SwipeableViews>\n        );\n    }\n\n    renderViewerScroll() {\n        return (\n            <div>\n                {this.renderContentImages()}\n            </div>\n        );\n    }\n\n    renderContentImages(isSwipe) {\n        let ref;\n        isSwipe ? ref =  'imageMobile' : ref = 'image';\n\n        return map(this.props.images, (image, index) => {\n            if (isSwipe && index > this.state.activeImageIndex + 1 || isSwipe && index < this.state.activeImageIndex - 1) return false;\n\n            return (\n                <div \n                    key={image.src + this.state.appIsMounted + index} \n                    className={cx(s.centered, this.props.large && this.state.width > MEDIA_MOBILE && s.centeredLarge)} \n                    id={`${ref}-${index}`} \n                        ref={`${ref}-${index}`} >\n                        {image.video ? \n                            <video \n                                width=\"100%\" \n                                loop \n                                autoPlay\n                                muted \n                                playsInline \n                                src={image.src} /> \n                        : \n                            <Image \n                                className={cx(this.props.large ? \"productImageLarge\" : \"productImage\")} \n                                key={image.src + \"-image\"} \n                                src={image.src} />\n                        }\n                </div>\n            )\n        })\n    }\n\n    render() {\n        const { overlay } = this.props;\n\n        return (\n            <React.Fragment>\n                {overlay && <div className={s.overlay}>\n                    {overlay}\n                    <div className={s.fullscreenButton} onClick={this.showFullscreen}>\n                        <IconZoom\n                            size={{ width: 18, height: 18 }}\n                            color={'#000'}\n                            onClick={this.handleClickOutside}\n                        />\n                    </div>\n                </div>}\n                <div ref=\"imageviewer\" className={cx(s.root, this.props.className, this.props.large && s.large)}>\n                    {this.props.large && <div className={s.previewContainer}>\n                        {this.renderPreviews()}\n                    </div>}\n                    {this.state.width < MEDIA_MOBILE || !this.props.large ? \n                    (<div className={s.swipeContainer} >\n                        {this.props.images.length > 1 && this.props.arrows ? \n                            this.renderArrows(this.renderViewerSwipe())\n                        :\n                            this.renderViewerSwipe()\n                        }\n                        {this.renderPagination()}\n                    </div>) :\n                    (<div className={s.scrollContainer}>\n                        {this.renderViewerScroll()}\n                    </div>)}\n                    <TransitionGroup>\n                    {this.state.fullscreen && \n                        <CSSTransition\n                        classNames={ {\n                            enter: s.fullscreenEnter,\n                            enterActive: s.fullscreenEnterActive,\n                            exit: s.fullscreenExit,\n                            exitActive: s.fullscreenExitActive,\n                        } }\n                        timeout={{ enter: 400, appear: 0, exit: 400 }}>\n                            <FullscreenViewer \n                                onClose={this.handleCloseFullscreen}\n                                viewer={this.renderViewerSwipe()}\n                                pagination={this.renderPagination()}>\n                            </FullscreenViewer>\n                        </CSSTransition>\n                    }\n                    </TransitionGroup>\n                </div>\n            </React.Fragment>\n        )\n    }\n}\n\nexport default ImageViewer;\n"]}},"_buildHash":"62ae65cd2431ceeeca027848a54c3583","hash":"022e714545ed57f161c87988ceede012","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/defineProperty","range":[114,152],"loc":{"start":{"line":7,"column":23},"end":{"line":7,"column":70}}},{"type":"RequireHeaderDependency","range":[106,113],"loc":{"start":{"line":7,"column":23},"end":{"line":7,"column":70}}},{"type":"RequireHeaderDependency","range":[238,245],"loc":{"start":{"line":11,"column":16},"end":{"line":11,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/extends","range":[246,277],"loc":{"start":{"line":11,"column":16},"end":{"line":11,"column":56}}},{"type":"RequireHeaderDependency","range":[355,362],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-prototype-of","range":[363,410],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/classCallCheck","range":[510,548],"loc":{"start":{"line":19,"column":23},"end":{"line":19,"column":70}}},{"type":"RequireH