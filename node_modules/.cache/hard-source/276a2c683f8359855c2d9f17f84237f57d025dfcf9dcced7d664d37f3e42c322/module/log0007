ort withStyles from 'isomorphic-style-loader/lib/withStyles';\nimport s from './Navigation.scss';\nimport cx from 'classnames';\n\nimport { SIZE_EXTRA_SMALL, SIZE_SMALL } from 'Middleware/constants/icon';\nimport { MEDIA_MOBILE, MEDIA_LAPTOP } from 'Middleware/constants/media';\nimport { isEmpty } from 'lodash';\nimport { routes, navigation, developerTools, accountLinks } from 'Root/config';\nimport { throttle, debounce } from 'lodash';\nimport { parseNavigation } from 'Middleware/services/navigation';\nimport { observer, inject } from 'mobx-react';\nimport { getSafe }  from 'Middleware/utils/object';\n\nimport SearchBar from 'Components/SearchBar';\nimport CountrySelector from 'Common/CountrySelector';\nimport ShoppingWith from 'Common/ShoppingWith';\nimport BehindTheCounter from 'Common/BehindTheCounter';\nimport Link from 'Common/Link';\nimport Collapsible from 'Common/Collapsible';\nimport CollapsibleLink from 'Common/CollapsibleLink';\nimport IconClose from 'Icons/Controls/Close.jsx';\nimport IconSearch from 'Icons/Search.jsx';\nimport NavigationFooter from 'Components/Footer/NavigationFooter';\nimport InputProxy from 'Common/InputProxy';\n\n\n@withStyles(s)\n@inject(\n    'contentStore',\n    'navigationStore',\n    'userStore',\n    'attributionStore',\n    'cartStore'\n)\n@observer class NavigationPanel extends Component {\n\n    constructor() {\n        super();\n        this.handleSearchIconClick = debounce(this.handleSearchIconClick, 50);\n    }\n\n    static propTypes = {\n        style: PropTypes.object,\n        onClick: PropTypes.func,\n    };\n\n    static defaultProps = {\n        style: {}\n    };\n\n    state = {\n        width: 200,\n        searchOpen: false,\n        appIsMounted: false,\n    }\n\n    componentDidMount() {\n        this.resizeListener = throttle(this.updateDimensions,100);\n        window.addEventListener(\"resize\", this.resizeListener);\n        this.updateDimensions();\n        requestAnimationFrame(() => this.setState({ appIsMounted: true }));\n    }\n\n    componentWillUnmount() {\n        window.removeEventListener('resize', this.handleScroll);\n    };\n\n    updateDimensions = () => {\n        let windowWidth = window.innerWidth;\n        window.innerWidth != this.state.width && this.setState({width: windowWidth});\n    }\n\n    handleLinkClick = () => {\n        this.props.onClick && this.props.onClick();\n    };\n\n    handleSearchIconClick = () => {\n        this.setState({searchOpen: true});\n    };\n\n    handleSearchClose = () => {\n        this.setState({searchOpen: false});\n    };\n\n    handleSearchLinkClicked = () => {\n        this.props.onClick && this.props.onClick();\n        this.setState({searchOpen: false});\n    };\n\n    getSpecialLinks() {\n        let links = [];\n        const config = getSafe(() => this.props.contentStore.content.hamburgerNavigation[0]);\n\n        parseNavigation(config.mobileSecondaryCategoryLinks.fields.links)\n        .map((link, index) => {\n            links.push([link.name, link.to]);\n        });\n\n        return links;\n    }\n\n    renderLinks() {\n        const { navigation } = this.props.navigationStore;\n\n        const config = getSafe(() => this.props.contentStore.content.hamburgerNavigation[0]);\n\n        if(navigation && navigation.length) {\n            const links = navigation.map((link, index) => \n                link && (\n                    isEmpty(link.links)\n                    ? <div key={link.name} onClick={this.handleLinkClick}><Link\n                        to={link.to}\n                        name={link.name}\n                        key={link.name}\n                        className={cx(s.link, s.category)}\n                        custom \n                        wrapperClassName={s.linkWrapper}\n                    /></div>\n                    : <div key={link.name} onClick={this.handleLinkClick}>\n                        <CollapsibleLink\n                            {...link}\n                            to={link.to}\n                            name={link.name}\n                            key={link.name}\n                            custom \n                            className={s.link}\n                        />\n                    </div>\n                ));\n\n            return [\n                ...links,\n                ];\n        }\n    }\n\n    render() {\n        const { className } = this.props;\n\n        this.props.navigationStore.observeLinkChanges;\n            \n        const config = getSafe(() => this.props.contentStore.content.hamburgerNavigation[0]);\n\n        if (!config) return null;\n\n        const languageLabel = getSafe(() => this.props.contentStore.content.header[0].languageLabel);\n\n        let navigationLinks = {};\n        let navigation = {};\n\n        if (this.state.width < MEDIA_LAPTOP) navigationLinks[config.mobileSecondaryCategoryLinks.fields.heading] = this.getSpecialLinks();\n\n        config.links.map(linkList => {\n            navigation[linkList.fields.heading] = [];\n            if (getSafe(() => linkList.fields.links))\n                this.props.navigationStore.parseNavigation(linkList.fields.links).map(link => {\n                    link && navigation[linkList.fields.heading].push([link.name, link.to]);\n                })\n        });\n        navigationLinks = Object.assign(navigationLinks, navigation);\n\n        const RenderChildren = ({children}) => \n            <React.Fragment>\n                {children}\n            </React.Fragment>\n    \n        const ConditionallyRender = (conditional = false) => \n            conditional ? RenderChildren : () => null;\n\n        const IsConsultant = ConditionallyRender(this.props.userStore.isConsultant);\n        const IsConsultantShopping = ConditionallyRender(this.state.appIsMounted && this.props.attributionStore.consultantShopping);\n\n        const AccountComponent = ((props) => {\n            return this.props.userStore.isLogged ? (\n                <div className={s.accountContainer}>\n                    {accountLinks\n                        .filter((link, key) => link.header)\n                        .map((link, key) => (\n                            <Link \n                                key={key}\n                                className={s.account} \n                                onClick={this.handleLinkClick}\n                                to={link.to}>\n                                {link.name}\n                            </Link>\n                        )\n                    )}\n                </div>\n            ) : (\n                <div className={s.accountContainer}>\n                    <Link \n                        className={s.account} \n                        onClick={this.handleLinkClick}\n                        to={config.signInLink.fields.url}>\n                        {config.signInLink.fields.name}\n                    </Link>\n                    <Link \n                        className={s.account} \n                        onClick={this.handleLinkClick}\n                        to={routes.SIGNUP}>\n                        {config.createAccountLink}\n                    </Link>\n                </div>\n            );\n        })\n\n        return (\n            <div\n                className={cx(className, s.root)}\n                style={this.props.style}\n            >\n                <div className={s.topContainer}>\n                    <div className={s.searchIconContainer} >\n                        <InputProxy onClick={this.handleSearchIconClick.bind(this)}>\n                            <IconSearch\n                                    className={s.iconSearch}\n                                    size={SIZE_SMALL}\n                                    />\n                        </InputProxy>\n\n                        </div>\n                    <div className={s.closeContainer}>\n                        <IconClose\n                            size={SIZE_EXTRA_SMALL}\n                            className={s.close}\n                            onClick={this.handleLinkClick}\n                        />\n                    </div>\n                </div>\n                {this.state.searchOpen ? \n                <div className={s.searchContainer}>\n                    <SearchBar \n                        onLinkClicked={this.handleSearchLinkClicked.bind(this)} \n                        onCloseSearch={this.handleSearchClose.bind(this)} />\n                </div>\n                :\n                <div>\n                    <IsConsultant>\n                        <div className={s.optionsContainer}>\n                            <BehindTheCounter />\n                        </div>\n                    </IsConsultant>\n                    <IsConsultantShopping>\n                        <div className={s.shoppingWith}>\n                            <ShoppingWith onClick={this.handleLinkClick} />\n                        </div>\n                    </IsConsultantShopping>\n                <div className={s.optionsContainer}>\n\n                    <AccountComponent />\n\n                    <div className={s.countryContainer}>\n                        <CountrySelector label={languageLabel} />\n                    </div>\n                </div>\n                <div className={s.inner}>\n                    <div className={cx(s.links, s.row)}>\n                        {this.renderLinks()}\n                    </div>\n                    <div className={cx(s.secondaryLinks)}>\n                    {Object.keys(navigationLinks).map((heading, key) => (\n                        <Collapsible \n                            key={heading}\n                            open={(key == 0 && this.state.width > MEDIA_MOBILE) && true}\n                            trigger={heading}\n                            triggerClassName={s.collapsible} \n                            triggerOpenedClassName={s.collapsibleExpanded} \n                            contentInnerC