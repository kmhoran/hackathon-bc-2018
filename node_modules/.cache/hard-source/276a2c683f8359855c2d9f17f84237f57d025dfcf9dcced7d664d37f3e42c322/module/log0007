             ...this.state.form,\n                recipients: recipients\n            }\n        });\n\n        this.validateRecipient(recipients[index], index);\n\n    }\n\n    validateRecipient(recipient, index){\n        if (!(index === (this.state.form.recipients.length - 1))) return;\n\n        if (!recipient.name.value) {\n            return;\n        }\n        if (!validEmail(recipient.email.value)) {\n            return;\n        }\n\n        this.setState({\n            addAnother: true,\n        });\n    }\n\n    validateRecipients(ignoreUpdate) {\n        const { form } = this.state\n        const newForm = cloneDeep(form);\n\n        let errors = 0;\n        form.recipients.forEach((recipient, i) => {\n            if (!form.recipients[i].name.value) {\n                newForm.recipients[i].name.error = this.config.invalidNameError;\n                errors++;\n            }\n            if (!validEmail(form.recipients[i].email.value)) {\n                newForm.recipients[i].name.error = this.config.invalidEmailError;\n                errors++;\n            }\n        });\n\n        !ignoreUpdate && this.setState({\n            form: newForm,\n        });\n\n        this.setState({\n            addAnother: (errors === 0),\n        });\n\n        if (errors > 0) return false;\n        return true;\n    }\n\n    validateInput(ignoreUpdate) {\n        const { form } = this.state;\n        const newForm = cloneDeep(form);\n\n        let errors = 0;\n        if (!form.name.value) {\n            newForm.name.error = this.config.invalidNameError;\n        }\n\n        if (!validEmail(form.email.value)) {\n            newForm.email.error = this.config.invalidEmailError;\n            errors++;\n        }\n\n        form.recipients.forEach((recipient, i) => {\n            if (!form.recipients[i].name.value) {\n                newForm.recipients[i].name.error = this.config.invalidNameError;\n                errors++;\n            }\n            if (!validEmail(form.recipients[i].email.value)) {\n                newForm.recipients[i].name.error = this.config.invalidEmailError;\n                errors++;\n            }\n        });\n\n        if (!form.captcha.value) {\n            newForm.captcha.error = this.config.invalidCaptchaError;\n            errors++;\n        }\n\n        !ignoreUpdate && form.recipients.length < 4 && this.setState({\n            form: newForm,\n        });\n\n        if (errors > 0) return false;\n        return true;\n    }\n\n    buildMessage(recipient){\n        const { form } = this.state;\n        const { productSlug, productName, productDesc, productImage} = this.props;\n\n        return `Hi ${recipient.name.value},<br /><br />${form.name.value} wants you to check out this product at\n        Beautycounter:<br/><br/><a target=\"_\" href=\"${getProductUrl(productSlug)}\">${productName}</a>\n        <br/><br/>${form.name.value} says:<br/>${form.message.value}<br /><br />`\n    }\n\n    handleSubmit = async () => {\n        const valid = this.validateInput(false);\n        const event = this.config.sailthruSendProductToFriendEvent;\n        if (valid) {\n            try {\n                this.setState({ loading: true });\n                const {\n                    name,\n                    email,\n                    message,\n                    recipients\n                } = this.state.form;\n                const {\n                    emailSubject,\n                    successMessage\n                } = this.config;\n                const { productSlug, productName, productDesc, productImage} = this.props;\n                let subj = emailSubject.replace('userName', name.value).replace('productName', productName);\n\n                const promises = [];\n                recipients.forEach((recipient) => {\n                    let vars = {\n                        senderName: name.value,\n                        senderEmail:email.value,\n                        recipientName: recipient.name.value,\n                        recipientEmail: recipient.email.value,\n                        productName: productName,\n                        productUrl: getProductUrl(productSlug),\n                        productDesc: productDesc,\n                        productImage: productImage,\n                        subj: subj,\n                        message: message.value\n                    }\n                    promises.push(sendEmailPDP(this.buildMessage(recipient), email.value, event, vars));\n                });\n                const response = await Promise.all(promises);\n                this.setState({\n                    loading: false,\n                    successMessage: successMessage,\n                });\n\n                setTimeout(() => {\n                    this.props.onClose && this.props.onClose(event);\n                }, 2000);\n            }\n            catch(e) {\n                console.log(e);\n                this.setState({loading: false});\n                return false;\n            }\n        }\n        else\n            return false;\n    }\n\n\n    handleEmailBlur() {\n        const { form } = this.state;\n        const newForm = cloneDeep(form);\n\n        if (!validEmail(form.email.value)) {\n            newForm.email.error = this.config.invalidEmailError;\n            this.setState({\n                form: newForm,\n            });\n        }\n    }\n\n    handleRecipientEmailBlur(i) {\n        const { form } = this.state;\n        const newForm = cloneDeep(form);\n\n        if (!validEmail(form.recipients[i].email.value)) {\n            newForm.recipients[i].email.error = this.config.invalidEmailError;\n            this.setState({\n                form: newForm,\n            });\n            return;\n        }\n\n        this.validateRecipients(true);\n    }\n\n    handleRecipientNameBlur(i) {\n        this.validateRecipients(true);\n    }\n\n    addRecipient() {\n        if (this.state.form.recipients.length >= 4) retur