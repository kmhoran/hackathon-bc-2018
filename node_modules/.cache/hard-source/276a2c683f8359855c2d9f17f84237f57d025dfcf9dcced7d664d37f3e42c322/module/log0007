\n        error: false,\n        fetching: false,\n        errorMessage: \"\"\n    }\n\n    handleSubmission =  event => {\n        var input = this.refs.email.value;\n\n        if(validEmail(input)) {\n            this.setState({ \n                fetching: true, \n                error: false \n            });\n            const self = this;\n\n            async function getData(email) {\n\n                let request = await staticService(`${magento.URL}/newsletter/subscriber/new/?add_me_to_bc=${email}&storeView=${self.props.localeStore.activeLocale.magentoStoreView}`);\n                let response = await request.json();\n                \n                if(response.success) {\n                    if (self.props.successHandler) self.props.successHandler();\n                    self.setState({ \n                        success: true, \n                        fetching: false, \n                        error: false,\n                        errorMessage: ''\n                    });\n                    self.props.analyticsStore.signUp(email, self.props.source);\n                }\n                else {\n                    self.setState({ \n                        fetching: false,\n                        error: true, \n                        errorMessage: response.message = 'This email address is already assigned to another user.' ?  self.props.taken : response.message \n                    });\n                }\n            }\n            getData(input);\n        }\n        else {\n            this.setState({ \n                error: true, \n                errorMessage: null, \n                fetching: false \n            });\n            // if (this.props.errorHandler) this.props.errorHandler(input);\n        }\n        event.preventDefault();\n        return false;\n    }\n\n    clearErrors() {\n        this.state.error && this.setState({ error: false });\n    }\n\n    render() {\n        const { error, success } = this.state;\n        let SubmitButton = () => <AngleRightIcon\n            color={'#fff'} \n            size={{ width: 16, height: 16 }}\n        /> \n        if (this.props.customSubmit) SubmitButton = () => this.props.customSubmit;\n\n        return (\n                <div>\n                    {!this.state.success ?\n                    <div>\n                        <form className={s.root} onSubmit={this.handleSubmission.bind(this)}>\n                            <input \n                                ref=\"email\"\n                                type=\"text\" \n                                aria-label={this.props.placeholder}\n                                label={this.props.placeholder}\n                                onChange={() => this.clearErrors()}\n                                placeholder={this.props.placeholder} \n                                className={cx(s.input, error && s.inputError)} \n                            />\n                            <div className={cx(s.button, error && s.buttonError)} onClick={this.handleSubmission.bind(this)}>\n                                {this.state.fetching ? \n                                    <IconSpinner \n                                        color={'#fff'} \n                                        size={{width: 18, height: 18}} />\n                                    :\n                                    <SubmitButton />\n                                }\n                            </div>\n                        </form>\n                    </div>\n                    :\n                        <div className={s.successContainer}>\n                        <Checked\n                            color={s.checkmarkColor}\n                            className={s.checkmark}\n                            size={{ width: 16, height: 16 }} /> \n                                {this.props.success}\n                        </div>\n                    }\n                    {this.state.error &&\n                        <div className={s.errorMessage}>\n                            {this.state.errorMessage || this.props.invalid}\n                        </div>\n                    }\n                </div>\n            );\n        }\n}\n\nexport default EmailSignup;\n"]}},"_buildHash":"38f299db4d01bc1b86397b0c59dca263","hash":"f9c3d256135f81f53814650c6f0147da","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[110,137],"loc":{"start":{"line":7,"column":19},"end":{"line":7,"column":55}}},{"type":"RequireHeaderDependency","range":[102,109],"loc":{"start":{"line":7,"column":19},"end":{"line":7,"column":55}}},{"type":"RequireHeaderDependency","range":[225,232],"loc":{"start":{"line":11,"column":25},"end":{"line":11,"column":74}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/asyncToGenerator","range":[233,273],"loc":{"start":{"line":11,"column":25},"end":{"line":11,"column":74}}},{"type":"RequireHeaderDependency","range":[369,376],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-prototype-of","range":[377,424],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/classCallCheck","range":[524,562],"loc":{"start":{"line":19,"column":23},"end":{"line":19,"column":70}}},{"type":"RequireHeaderDependency","range":[516,523],"loc":{"start":{"line":19,"column":23},"end":{"line":19,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/createClass","range":[660,695],"loc":{"start":{"line":23,"column":20},"end":{"line":23,"column":64}}},{"type":"RequireHeaderDependency","range":[652,659],"loc":{"start":{"line":2