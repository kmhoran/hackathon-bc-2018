Action = async () => {\n        try {\n            const request = await fetch(`${firebase.apiBase}/publishCatalog?cacheRefresh=true`);\n            const response = await request.json();\n\n            if (!response || !response.success) throw(\"error\");\n            this.setState({ success: true, loading: false });\n        }\n        catch(e) {\n            this.setState({ success: false, loading: false });\n            console.log(e);\n        }\n    }\n\n    handleClickOutside = event => {\n        event.stopPropagation();\n        event.preventDefault();\n        this.props.onClose && this.props.onClose(event);\n    };\n    \n    render() {\n        const { success, loading } = this.state;\n\n        return (\n            <Portal visibility={true}>\n                <TransitionGroup>\n                {this.props.visible && <CSSTransition\n                    classNames={ {\n                        enter: s.fullscreenEnter,\n                        enterActive: s.fullscreenEnterActive,\n                        exit: s.fullscreenExit,\n                        exitActive: s.fullscreenExitActive,\n                    } }\n                    timeout={{ enter: 400, appear: 0, exit: 400 }}>\n                        <div className={cx(s.root)}>\n                            <div className={s.container}>\n                                <ClickOutHandler onClickOut={this.handleClickOutside}>\n                                    <div className={s.inner}>\n                                    <div className={cx(s.loader, loading && s.loading)}>\n                                    <svg version=\"1.1\" x=\"0px\" y=\"0px\"\n                                    viewBox=\"0 0 100 100\" enable-background=\"new 0 0 100 100\">\n                                    <circle fill=\"none\" stroke=\"#000\" stroke-width=\"4\" stroke-miterlimit=\"10\" cx=\"50\" cy=\"50\" r=\"48\"/>\n                                    <line fill=\"none\" stroke-linecap=\"round\" stroke=\"#000\" stroke-wid