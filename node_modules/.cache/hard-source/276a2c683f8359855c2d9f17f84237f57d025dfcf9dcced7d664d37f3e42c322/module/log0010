> {\n        const { pressStore, pressStore: { page, resetSelected }, localeStore, productsStore, viewportMode } = this.props;\n        let rows = {};\n        let row = 0;\n        let count = 0;\n        const quickviewTransition = {\n            appear: s.quickviewAnimationAppear,\n            appearActive: s.quickviewAnimationEnterAppear,\n            enter: s.quickviewAnimationEnter,\n            enterActive: s.quickviewAnimationEnterActive,\n            exit: s.quickviewAnimationExit,\n            exitActive: s.quickviewAnimationExitActive,\n        };\n\n        /**\n         * @param {Object} item The current item to view\n         * @param {Bool} scrollTo Should the component control initial scroll\n         * @param {Number} row the row number. used for scrollTo animation.\n         */\n\n        const QuickView = (props) => {\n            return (\n                <PressQuickview\n                    {...props}\n                    products={productsStore.productsBySKUS(props.item.featuredProducts)}\n                    defaultBG={page.defaultBackground}\n                    formatDate={localeStore.formatDate}\n                    close={resetSelected}\n                    linkDigital={page.linkDigital}\n                    linkPrint={page.linkPrint}\n                    viewportMode={viewportMode}\n                    labelFeaturedItems={page.labelFeaturedItems}\n                />\n            );\n        }\n\n        return (\n            <div key={`section-${year}`} className={s.section}>\n                <h1>{year}</h1>\n                <div className={s.items}>\n                    {items.map((item, key) => {\n                        count++;\n                        if (count === 3+1 || count === 1) {\n                            count = 1;\n                            row++;\n                            rows[row] = {};\n                            rows[row][item.slug] = true;\n                            rows[row][items[key+1] && items[key+1].slug