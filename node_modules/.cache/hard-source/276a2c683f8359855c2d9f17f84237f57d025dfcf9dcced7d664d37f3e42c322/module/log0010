s from './ResetPasswordPage.scss';\nimport { observer, inject } from 'mobx-react';\nimport { computed, autorun, toJS } from 'mobx';\nimport Container from 'Common/Container';\nimport ContentFrame from 'Common/ContentFrame';\nimport { routes } from 'Root/config';\nimport { SIZE_EXTRA_SMALL } from 'Middleware/constants/icon';\nimport { COLOR_WHITE, COLOR_BLACK } from 'Middleware/constants/colors';\nimport { getSafe, } from 'Middleware/utils/object';\nimport { removeURLQueries } from 'Middleware/utils/url';\nimport { throttle, cloneDeep } from 'lodash';\nimport ReactMarkdown from 'react-markdown';\n\nimport Section from 'Common/Section';\nimport Input from 'Common/Input';\nimport Link from 'Common/Link';\nimport Button from 'Common/Button';\nimport Separator from 'Common/Separator';\nimport Checkbox from 'Common/Checkbox';\n\nconst form = {\n    password: {\n        value: '',\n        error: false,\n    },\n    confirmation: {\n        value: '',\n        error: false,\n    },\n};\n\n@withStyles(s)\n@inject(\n    'contentStore',\n    'navigationStore',\n    'messagingStore',\n    'localeStore',\n    'userStore',\n)\n@observer\nclass ResetPasswordPage extends Component {\n    static contextTypes = {\n        setTitle: PropTypes.func.isRequired,\n        onSetMeta: PropTypes.func.isRequired\n    };\n\n    constructor(props) {\n        super(props);\n\n        this.handleInputChange = this.handleInputChange.bind(this);\n        this.submit = throttle(this.handleSubmit, 200);\n        const app = this;\n\n        // Automatically reset form on locale change\n        this.autoRunDisposer = autorun(() => {\n            if (this.props.localeStore.activeLocale.code !== this.state.locale && app) {\n                app.setState({\n                    locale: toJS(this.props.localeStore.activeLocale.code),\n                    form: cloneDeep(form),\n                });\n            }\n        });\n    }\n\n    state = {\n        form: cloneDeep(form),\n        locale: toJS(this.props.localeStore.activeLocale.code),\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.resetPassword.data.configResetPassword[0]);\n    }\n\n    componentDidMount() {\n        this.updateTitle();\n        this.setPageMeta();\n\n        if (window) {\n            removeURLQueries(true);\n            this.checkQuery.call(this, this.props.query);\n        }\n    }\n\n    checkQuery = async (query) => {\n        const response = await this.props.userStore.checkPasswordToken(query);\n        if (!response.success) {\n            this.props.messagingStore.addMessage( this.config.messageTokenExpired, { type: 'error' });\n        }\n    }\n\n    updateTitle() {\n        this.context.setTitle(getSafe(() => this.config.displayName));\n    }\n\n    setPageMeta() {\n        setSEOMeta(this.context, getSafe(() => this.config.configSEO.fields));\n    }\n\n    handleSubmit = async (event) => {\n        event && event.preventDefault();\n\n        const valid = this.validateInput();\n\n        if (valid) {\n            const params = this.getFormParams();\n            const passwordReset = await this.props.userStore.resetPassword(params);\n\n            if (passwordReset.success) {\n                this.props.messagingStore.addMessage(this.config.messageSuccess, { type: 'success' });\n                this.props.userStore.logout();\n                this.props.navigationStore.changePath({ url: routes.HOME, isBackPoint: true });\n                this.props.navigationStore.to({ url: routes.LOGIN });\n            } else {\n                this.props.messagingStore.addMessage(this.parseError(passwordReset.message),\n                    { type: 'error' });\n            }\n        }\n    };\n\n    getFormParams() {\n        // use the same key/value pair the service sends.\n        const params = { ...this.props.query };\n        Object.keys(this.state.form).forEach(key => params[key] = this.state.form[key].value);\n        return params;\n    }\n\n    validateInput() {\n        const form = this.state.form;\n\n        let errors = 0;\n        Object.keys(this.state.form).forEach((key) => {\n            if (!this.state.form[key].value && !this.state.form[key].notRequired) {\n                this.state.form[key].error = this.config.messageGenericFieldError;\n                errors++;\n            }\n        })\n        if (form.password.value !== form.confirmation.value) {\n            this.state.form.confirmation.error = this.config.messagePasswordMatchError;\n            this.state.form.password.error = true;\n            this.state.form.confirmation.value = '';\n            this.state.form.password.value = '';\n            errors++;\n        }\n        else if (form.password.value.length < 6) {\n            this.state.form.password.error = this.config.messagePasswordMinLength;\n            this.state.form.confirmation.error = true;\n            this.state.form.password.value = '';\n            this.state.form.confirmation.value = '';\n            errors++;\n        }\n\n        this.setState({});\n\n        return (errors <= 0);\n    }\n\n    parseError(error) {\n        let message = this.config.messageGenericErrorResponse;\n        if (error && /token|expired/.test(error.toLowerCase())) {\n            message = this.config.messageTokenExpired;\n        }\n        return message;\n    }\n\n    handleInputChange(name, value, regex = false) {\n        const entry = {\n            ...this.state.form[name],\n            error: false,\n            value: regex ? value.replace(regex, '') : value,\n        };\n\n        this.setState({\n            form: {\n                ...this.state.form,\n                [name]: entry,\n            },\n        });\n    }\n\n    render() {\n        const {\n            passwordLabel,\n            confirmPasswordLabel,\n            heading,\n            instruction,\n            submitButton,\n        } = this.config;\n        const { form } = this.state;\n\n        this.updateTitle();\n        this.setPageMeta();\n\n        return (\n            <Section innerClassName={s.root} contentClassName={s.content}>\n                <div className={s.header}>\n                    <h2>{heading}</h2>\n                    <div>{instruction}</div>\n                </div>\n                <form action='' onSubmit={this.handleSubmit} style={{ width: '100%' }}>\n                    <div className={s.inputBlock}>\n                        <Input\n                            onChange={e => this.handleInputChange('password', e)}\n                            wrapperClassName={s.input}\n                            error={form.password.error}\n                            invalid={form.password.error}\n                            defaultValue={form.password.value}\n                            showLabel\n                            required\n                            type={'password'}\n                            title={`${passwordLabel} *`}\n                        />\n                        <Input\n                            onChange={e => this.handleInputChange('confirmation', e)}\n                            wrapperClassName={s.input}\n                            error={form.confirmation.error}\n                            invalid={form.confirmation.error}\n                            defaultValue={form.confirmation.value}\n                            required\n                            showLabel\n                            type={'password'}\n                            title={`${confirmPasswordLabel} *`}\n                        />\n                    </div>\n                    <Button\n                        isSubmit\n                        onClick={this.submit}>\n                        {submitButton}\n                    </Button>\n                </form>\n            </Section>\n        );\n    }\n}\n\nexport default ResetPasswordPage;\n"]}},"_buildHash":"81d0c50cdff399008a0de72ff6a1ac14","hash":"1c7c520ac279d4f5252b23412c1e0432