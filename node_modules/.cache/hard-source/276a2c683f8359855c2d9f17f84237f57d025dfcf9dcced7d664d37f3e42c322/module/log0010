scrollTo && setTimeout(() => {\n            this.scrollTo({\n                hash: `#quickview-${this.props.item.slug}-${this.props.row}`,\n                animate: true,\n                duration: 600,\n                extraHeight: 95\n            });\n        }, 340);\n\n        if (__BROWSER__ && !this.props.mobile) document.addEventListener('click', this.handleClick);\n    }\n\n    componentWillUnmount() {\n        if (__BROWSER__ && !this.props.mobile) document.removeEventListener('click', this.handleClick);\n    }\n\n    componentWillReceiveProps(nextProps) {\n        if (this.props.viewportMode !== nextProps.viewportMode)\n            this.forceUpdate();\n    }\n\n    truncate(string, length) {\n        if (string.length > length)\n           return string.substring(0,length) + '...';\n        else\n           return string;\n     }\n\n    render() {\n        const { products, item, defaultBG, formatDate, linkPrint, linkDigital, labelFeaturedItems, viewportMode } = this.props;\n        const image = getSafe(() =>\n            item.image.fields.file.url);\n        const defaultImage = getSafe(() =>\n            defaultBG.fields.file.url);\n        const hasProducts = Array.isArray(products.slice()) && products.length > 0;\n        const TabletSize = ({ children }) => {\n            const width = viewportMode;\n            return width < MEDIA_DESKTOP && width > MEDIA_PHONE ? children : null;\n        };\n        const DesktopSizeOrMobile = ({ children }) => {\n            const width = viewportMode;\n            return width >= MEDIA_DESKTOP || width <= MEDIA_PHONE ? children : null;\n        };\n        const iconPrefs = (() => {\n            const color = TabletSize(true) === null ? '#285591' : '#ffffff';\n            return {\n                size: ICON_SIZE,\n                className: s.linkContainer__icon,\n                color,\n            };\n        })();\n        const ExtraPublicationMeta = () => {\n            return item.type === 'digital' ? (\n                <span>\n                    <a href={addressStrip(item.url)} className={s.textContainer__link} target='_blank' rel=\"noopener\">\n                        {addressStrip(item.url, true)}\n                    </a>\n                </span>\n            ) : null;\n        };\n        const LeftContent = () => {\n            const url = getSafe(() => item.type === 'digital' ? item.url : item.downloadablePDF.fields.file.url);\n            const aClass = item.type === 'digital' ? 'linkContainer__link--visit' : 'linkContainer__link--download';\n            const link = item.type === 'digital' ? linkDigital : linkPrint;\n            const onClick = () => url && __BROWSER__ && window.open(url, '_blank');\n            const containerStyle = {\n                cursor: url ? 'pointer' : 'auto',\n                backgroundImage: item.hasFullCover ? 'none' : `url(${defaultImage})`\n            };\n            return (\n                <div>\n                    <div className={s.imageContainer} style={containerStyle} onClick={onClick}>\n                        <img className={cx(item.hasFullCover ? s.cover : s.logo)} src={image} />\n                    </div>\n                    <div className={s.linkContainer}>\n                        <a className={cx(s.linkContainer__link, aClass)} href={url} target='_blank' rel=\"noopener\">\n                            <LinkChain {...iconPrefs} />{link}\n                        </a>\n                    </div>\n                </div>\n            );\n        };\n        const CarouselContainer = () => {\n            if (hasProducts) {\n                return (\n                    <div className={s.carouselContainer}>\n                        <h5 className={s.carouselContainer__header}>{labelFeaturedItems}</h5>\n                        <hr className={s.carouselContainer__seperator} />\n                        <ProductMiniCarousel products={products} viewportMode={viewportMode}/>\n                    </div>\n                );\