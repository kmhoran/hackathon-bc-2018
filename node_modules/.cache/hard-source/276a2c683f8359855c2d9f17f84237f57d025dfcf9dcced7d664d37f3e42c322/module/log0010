Zip: false,\n            isValidEmail: false,\n            isEmptyEmail: false,\n            formActivated: false,\n            formUnfocused: false,\n            formProcessing: false\n        };\n\n        this.handleInputChange = this.handleInputChange.bind(this);\n        this.showVideo = this.showVideo.bind(this);\n        this.hideVideo = this.hideVideo.bind(this);\n        this.handleClickScroll = this.handleClickScroll.bind(this);\n        this.invalidButtonClick = this.invalidButtonClick.bind(this);\n    }\n\n    componentWillMount() {\n        this.updateTitle();\n        this.setPageMeta();\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.advocacy.data.configAdvocacy[0]);\n    }\n\n    updateTitle() {\n        this.context.setTitle(getSafe(() => this.config.displayName));\n    }\n\n    setPageMeta() {\n        setSEOMeta(this.context, getSafe(() => this.config.configSEO.fields));\n    }\n\n    isFormValid() {\n        if(this.state.isValidName && this.state.isValidEmail && this.state.isValidZip) {\n            return false;\n        }\n\n        return true;\n    }\n\n    invalidButtonClick() {\n        if(this.state.formActivated) { this.setState({ formUnfocused: true }) }\n    }\n\n    checkValidation(inputField) {\n        const activeField = Object.keys(inputField)[0];\n\n        // Validation for name field.\n        if(activeField == 'name') {\n            inputField.name ? this.setState({ isValidName: true }) : this.setState({ isValidName: false });\n        } else if (activeField == 'zipcode') {\n            inputField.zipcode ? this.setState({ isEmptyZip: false }) : this.setState({ isEmptyZip: true });\n            validUsZipcode(inputField.zipcode) ? this.setState({ isValidZip: true }) : this.setState({ isValidZip: false });\n        } else if (activeField == 'email') {\n            inputField.email ? this.setState({ isEmptyEmail: false }) : this.setState({ isEmptyEmail: true });\n            validEmail(inputField.email) ? this.setState({ isValidEmail: true }) : this.setState({ isValidEmail: false });\n        }\n    }\n\n    handleInputChange(event) {\n        this.setState({ formActivated: true });\n        const target = event.target;\n        let value;\n\n        // Condition statement for setting a value for checkbox.\n        if(typeof(event) === 'string' && event === 'checked') {\n          value = true;\n\n        } else if (typeof(event) === 'string' && event === '') {\n          value = false;\n        } else {\n          value = target.value;\n        }\n\n        const name = typeof(event) === 'string' ? 'checked' : target.className.split(' ')[2];\n\n        this.setState({ [name]: value });\n        this.checkValidation({ [name]: value });\n    }\n\n    handleSubmit = async (e) => {\n      e && e.preventDefault();\n\n      // Check if the checkbox is checked, store the\n      // corresponding object in a variable.\n      let formSubmissionObj = {\n          'email': this.state.email,\n          'lists': this.state.checked ?\n            {\n              \"marchforth2018\": 1,\n              \"Master\": 1\n            } :\n            { \"marchforth2018\": 1},\n          'vars': {\n              'zipCode': this.state.zipcode,\n              'name': this.state.name\n          }\n      }\n\n      // Send the object and form values to the sailthru API.\n      this.setState({ formProcessing: true });\n      userSignUpEmail(formSubmissionObj).then(() => {\n        this.setState({ successSubmit: true });\n      });\n    }\n\n    submitSuccess() { return this.state.successSubmit ? s.successHideForm : '' }\n\n    submitShowMsg() { return this.state.successSubmit ? s.successShowMsg : '' }\n\n    showVideo() { this.setState({ videoActive: true }) }\n\n    hideVideo() { this.setState({ videoActive: false }) }\n\n    handleClickScroll() {\n        this.scrollTo({\n            hash: '#signUpForm',\n            duration: 500\n        });\n    }\n\n    render() {\n\n        const {\n            displayName,\n            videoThumbnail,\n            tile2Image,\n            signUpButtonText,\n            joinTheMovementButtonText,\n            tile1text,\n            tile2Text,\n            tile3Text,\n            tile4Text,\n            formSubmitSuccessMessage,\n            checkboxText,\n            inputFullName,\n            inputEmail,\n            inputZipcode,\n            requiredNote,\n            formHeader,\n            inputZipErrorMessage,\n            inputEmailErrorMessage,\n            inputRequiredErrorMessage,\n            videoId\n        } = this.config;\n\n        this.setPageMeta();\n        \n        return (\n            <section className={s.root}>\n                {/*  Tile 1 */}\n                <VideoWindow video={videoId} visible={this.state.videoActive} onClose={this.hideVideo}></VideoWindow>\n                <Section contentClassName={s.breakOnMobile}>\n                    <Section contentClassName={s.contentBase}>\n                        <MarkdownWrapper\n                            className={s.leftCol}\n                            content={tile1text} />\n                          <section className={s.buttonContainer}>\n                            <a onClick={this.handleClickScroll}>\n                              <Button className={s.signUpBtn}>{signUpButtonText}</Button>\n                            </a>\n                          </section>\n                    </Section>\n                    <Section className={s.videoThumbnail}>\n                        <section onClick={this.showVideo}>\n                            <img\n                                className={s.mediaContent}\n                                src={getSafe(() => videoThumbnail.fields.file.url)}\n                            />\n                        </section>\n                    </Section>\n                </Section>\n\n                {/*  Tile 2 */}\n                <Section contentClassName={s.breakOnMobile}\n                  className={s.highlight1}>\n                    <Section>\n                      <img\n                          className={s.imageContent}\n                          src={getSafe(() => tile2Image.fields.file.url)}\n                      />\n                    </Section>\n                    <Section contentClassName={s.contentBase}>\n                        <MarkdownWrapper\n                            className={s.leftCol}\n                            content={tile2Text} />\n                    </Section>\n                </Section>\n\n                {/*  Tile 3 */}\n                <Section\n                    contentClassName={s.breakOnMobile, s.contentCenter}>\n                    <MarkdownWrapper\n                        content={tile3Text} />\n                    <a id='signUpForm'></a>\n                </Section>\n\n                {/* Tile 4/form */}\n                <Section\n                  contentClassName={s.breakOnMobile}\n                  className={s.highlight1} >\n                    <Section\n                      contentClassName={s.contentBase}\n                      className={s.leftCol + ' ' + s.formLeftCol} >\n                        <MarkdownWrapper\n                            content={tile4Text} />\n                    </Section>\n                    {/* Form */}\n                  <Section className={s.successMsg + ' ' + this.submitShowMsg()}>\n                    <MarkdownWrapper\n                        content={formSubmitSuccessMessage} />\n                  </Section>\n                  <section className={this.submitSuccess()}>\n                    <section className={s.formHeader}>\n                        <label>{formHeader} <br/> <small>{requiredNote}</small></label>\n                    </section>\n                    <Section style={{ textAlign: 'left' }}>\n                        <form\n                            action=''\n                            onChange={this.handleInputChange}\n                            onSubmit={this.handleSubmit}\n                            className={s.joinForm}>\n                            <div className={s.block}>\n                                <div className={s.inputBlock}>\n                                  <Input\n                                      showLabel\n                                      required\n                                      defaultValue={this.state.name}\n                                      className=\"name\"\n                                      title={inputFullName}\n                                  />\n                                  <ErrorField\n                                      className={s.inputInvalid}\n                                      errors={\n                                          (!this.state.isValidName && this.state.formActivated) && this.state.formUnfocused ?\n                                          inputRequiredErrorMessage : []}\n                                   />\n                                  <Input\n                                      defaultValue={this.state.zipcode}\n                                      className=\"zipcode\"\n                                      showLabel\n                                      required\n                                      title={inputZipcode}\n                                  />\n                                  <ErrorField\n                                      className={s.inputInvalid}\n                                      errors={\n                                          (!this.state.isValidZip && this.state.formActivated) && this.state.formUnfocused ?\n                                          inputZipErrorMessage : []}\n                                    />\n                                  <ErrorField\n                                      className={s.inputInvalid}\n                                      errors={\n                                          (this.state.isEmptyZip && this.state.formActivated) && this.state.formUnfocused ?\n                                          inputRequiredErrorMessage : []}\n                                    />\n                                  <Input\n                                      defaultValue={this.state.email}\n                                      className=\"email\"\n                                      showLabel\n                                      required\n                                      title={inputEmail}\n                                  />\n                                  <ErrorField\n                                      className={s.inputInvalid}\n                                      errors={\n                                          (!this.state.isValidEmail && this.state.formActivated) && this.state.formUnfocused ?\n                                          inputEmailErrorMessage : []}\n                                  />\n                                  <ErrorField\n                                      className={s.inputInvalid}\n                                      errors={\n                                          (this.state.isEmptyEmail && this.state.formActivated) && this.state.formUnfocused ?\n                                          inputRequiredErrorMessage : []}\n                                    />\n                                </div>\n                                <div className={s.inputBlock} onClick={this.invalidButtonClick}>\n                                  <Button\n                                      isSubmit\n                                      onClick={this.submit}\n                                      className={s.joinBtn}\n                                      disabled={this.isFormValid()}\n                                      ><span style={{ display: !this.state.formProcessing ? 'block' : 'none' }}>{joinTheMovementButtonText}</span>\n                                      <span style={{ display: this.state.formProcessing ? 'block' : 'none' }}>\n                                          <Loader />\n                                      </span>\n                                  </Button>\n                                </div>\n                                <div className={s.checkbox}>\n                                    <Checkbox\n                                        onChange={e => this.handleInputChange(e)}\n                                        wrapperClassName={s.checkbox} />\n                                    <MarkdownWrapper\n                                        contentClass={s.smallText}\n                                        content={checkboxText} />\n                                </div>\n                            </div>\n                        </form>\n                    </Section>\n                  </section>\n                </Section>\n            </section>\n        );\n    }\n}\n\nexport default AdvocacyPage;\n"]}},"_buildHash":"e2fad9696b636de838755c763ccbc590","hash":"c97b06654ab295f2ab320ef1c19bd36e","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[293,300],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/defineProperty","range":[301,339],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/keys","range":[429,464],"loc":{"start":{"line":15,"column