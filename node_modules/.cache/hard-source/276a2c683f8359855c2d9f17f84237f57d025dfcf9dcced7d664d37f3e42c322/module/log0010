'productsStore',\n    'cartStore',\n    'localeStore',\n    'messagingStore',\n    'navigationStore',\n    'userStore'\n)\n@mixin(ScrollToMixin)\n@observer class MemberContainer extends Component {\n\n    constructor(props) {\n        super(props);\n\n        this.localeListener = autorun(() => {\n            this.props.localeStore.activeLocale;\n            this.closeTerms();\n        });\n\n        this.groupListener = autorun(() => {\n            if (this.props.userStore.isEmployee) {\n                this.props.navigationStore.to({ url: `${routes.HOME}?errorMessage=${this.config.notAllowedMessage || ''}` });\n            }\n        });\n    }\n\n    static contextTypes = {\n        setTitle: pt.func.isRequired,\n        onSetMeta: pt.func.isRequired\n    };\n\n    static defaultProps = {\n        renewal: false,\n    }\n\n    state = {\n        agreedTerms: false,\n        agreedCheckbox2: false,\n        readingTerms: false,\n        terms: 1, // 1 = base terms, 2 = member terms\n    }\n\n    componentWillMount() {\n        this.updateTitle();\n        this.setPageMeta();\n    }\n    \n    componentWillUnmount() {\n        this.localeListener();\n        this.groupListener();\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.becomeMember.data.configBecomeMember[0]);\n    }\n\n    updateTitle() {\n        this.context.setTitle(getSafe(() => this.config.displayName));\n    }\n\n    setPageMeta() {\n        setSEOMeta(this.context, getSafe(() => this.config.configSEO.fields));\n    }\n\n\n    handleAddProduct = () => {\n        if (!this.canProceed()) return false;\n\n        const { cartStore, productsStore, renewal, messagingStore } = this.props;\n\n        const product = productsStore.productBySKU(renewal ? specialProductSKUs.memberProgramRenew : specialProductSKUs.memberProgram);\n        \n        if (!cartStore.productsById[product.id]) {\n            cartStore.addProduct({\n                product,\n                quantity: 1,\n            });\n        } else { messagingStore.addMessage('Item already added to cart.', {type: 'error'})}\n    };\n\n    handleDetailClick() {\n        this.scrollTo({\n            hash: `#member-body`,\n            animate: true,\n            duration: 600,\n            extraHeight: 60,\n        });\n    }\n\n    handleTermsClick(val) {\n        this.handleReadTerms();\n        this.setState({ terms: val });\n    }\n\n    handleReadTerms() {\n        this.setState({ readingTerms: true });\n    }\n\n    closeTerms() {\n        this.setState({ readingTerms: false });\n    }\n\n    handleAgreeTerms(change) {\n        this.setState({ agreedTerms: change });\n    }\n\n    handleAgreeCheckbox2(change) {\n        this.setState({ agreedCheckbox2: change });\n    }\n\n    canProceed() {\n        const secondary = this.config.requireCheckbox2 ? this.state.agreedCheckbox2 : true;\n        return this.state.agreedTerms && secondary;\n    }\n\n    renderButton() {\n        const { buttonBecomeMember, buttonRenewMembership, renewPrice, becomeMemberPrice } = this.config;\n\n        return (\n            <Button onClick={() => this.handleAddProduct()} disabled={!this.canProceed()}>\n                <div>{this.props.renewal ? buttonRenewMembership : buttonBecomeMember}</div>\n                <div>{this.props.renewal ? renewPrice : becomeMemberPrice}</div>\n            </Button>\n        )\n    }\n\n    renderCheckbox() {\n        const { checkboxLabel, enableCheckbox2, checkbox2Label, memberTermsOfUseLabel, termsOfUseLabel } = this.config;\n\n        let label = reactStringReplace(checkboxLabel, '%s', (match, i) => (\n            <a key={match + i} onClick={() => this.handleTermsClick(1)}> {termsOfUseLabel} </a>\n          ));\n\n        label = reactStringReplace(label, '%x', (match, i) => (\n            <a key={match + i} onClick={() => this.handleTermsClick(2)}> {memberTermsOfUseLabel} </a>\n          ));\n\n        return (\n            <div className={s.checkboxContainer}>\n                <div className={s.checkbox}>\n                    <Checkbox checked={this.state.agreedTerms} onChange={change => this.handleAgreeTerms(change)} />\n                    <div>{label}</div>\n                </div>\n                {enableCheckbox2 && <div className={s.checkbox}>\n                    <Checkbox checked={this.state.agreedCheckbox2} onChange={change => this.handleAgreeCheckbox2(change)} />\n                    <div dangerouslySetInnerHTML={{ __html: checkbox2Label}} />\n                </div>}\n            </div>\n        )\n    }\n\n    render() {\n        const { renewal } = this.props;\n        const { headingArea, headingAreaRenewal, heroImage, bodyWidgets, moreDetails, featureSection, footerHeading, footerText, memberTermsOfUseLabel, termsOfUseLabel, termsAndConditionsDocument, memberTermsOfUseDocument } = this.config;\n        const hero = getSafe(() => heroImage.fields.file.url);\n        const { readingTerms } = this.state;\n\n        return (\n            <div className={s.root}>\n                <div id={'member-heading'} className={s.hero} style={hero ? { backgroundImage: `url(${hero})` } : {}}>\n                    <div className={s.header}>\n                        {!readingTerms && <div className={s.headerTop}>\n                            <MarkdownWrapper content={renewal ? headingAreaRenewal : headingArea} cssInherits={{ ...s }} />\n                        </div>}\n                        {readingTerms && <div className={s.termsBlock}>\n                            <div className={s.termsHeader}>\n                                <div>{this.state.terms === 1 ? termsOfUseLabel : memberTermsOfUseLabel}</div>\n                                <div onClick={() => this.closeTerms()}>Close</div>\n                            </div>\n                            <div className={s.termsBody}>\n                                <MarkdownWrapper content={this.state.terms === 1 ? termsAndConditionsDocument.fields.body : memberTermsOfUseDocument.fields.body} />\n                            </div>\n                        </div>}\n                        <div className={s.headerBody}>\n                            {!readingTerms && <div className={s.features}>\n                                <MarkdownWrapper content={featureSection} cssInherits={{ ...s }} />\n                            </div>}\n                            {this.renderCheckbox()}\n                            {this.renderButton()}\n                            <div onClick={() => this.handleDetailClick()} className={s.link}>{moreDetails}</div>\n                        </div>\n                    </div>\n                </div>\n                <div id={'member-body'}>\n                    <WidgetEngine page={{ url: 'become-member', widgets: bodyWidgets }} />\n                </div>\n                <Section customBackground={{ backgroundColor: '#FBFBFB' }} contentClassName={s.bottomCall}>\n                    {!readingTerms && <h2>{footerHeading}</h2>}\n                    {!readingTerms && \n                    <div className={s.bottomText}>\n                        <br /><br />\n                        {footerText}\n                    </div>}\n                    {readingTerms && <div className={s.termsBlock}>\n                        <div className={s.termsHeader}>\n                            <div>{this.state.terms === 1 ? termsOfUseLabel : memberTermsOfUseLabel}</div>\n                            <div onClick={() => this.closeTerms()}>Close</div>\n                        </div>\n                        <div className={s.termsBody}>\n                            <MarkdownWrapper content={this.state.terms === 1 ? termsAndConditionsDocument.fields.body : memberTermsOfUseDocument.fields.body} />\n                        </div>\n                    </div>}\n                    <div className={cx(s.bottomButtons, !readingTerms && s.borderTop)}>\n                        {this.renderCheckbox()}\n                     