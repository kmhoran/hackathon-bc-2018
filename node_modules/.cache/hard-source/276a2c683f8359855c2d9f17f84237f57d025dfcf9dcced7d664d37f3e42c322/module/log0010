.slug &&\n                                                <CSSTransition\n                                                    key={`quickview-anim-${row}-${qk.slug}`}\n                                                    classNames={quickviewTransition}\n                                                    timeout={{ enter: 800, appear: 0, exit: 400 }}>\n                                                        <div\n                                                            key={`quickview-${qk.slug}-${row}`}\n                                                            id={`quickview-${qk.slug}-${row}`}>\n                                                                <QuickView\n                                                                    item={qk}\n                                                                    row={row}\n                                                                    scrollTo={true}\n                                                                    expandSpacer={this.props.expandSpacer}\n                                                                    handleQuickView={this.handleQuickView}\n                                                                />\n                                                        </div>\n                                                </CSSTransition>\n                                    ))}\n                            </TransitionGroup>,\n                            <div key={`${item.slug}--container`} className={s.column}>\n                                <PressItem\n                                    item={item}\n                                    selected={id => this.setSelected(id, true)}\n                                    disabled={pressStore.selectedID && pressStore.selectedID !== item.slug || false}\n                                    expandSpacer={this.props.viewportMode > MEDIA_PHONE && this.props.expandSpacer}\n                                    handleQuickView={this.handleQuickView} />\n                            </div>,\n                        ];\n                    })}\n                </div>\n            </div>\n        );\n    }\n\n    render() {\n        const byYear = this.props.pressStore.itemsByYear;\n        const years = Object.keys(byYear).sort((a, b) => b - a);\n\n        return (\n            <div className={s.root} id={'grid-container'}>\n                {years.map(year => this.renderSection(byYear[year], year))}\n            </div>\n        );\n    }\n}\n\nexport default PressGrid;\n"]}},"_buildHash":"7d2698a8d79d5289e3ebc52132c18314","hash":"f50d8b1bdb7a348016d517207a756447","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/extends","range":[107,138],"loc":{"start":{"line":7,"column":16},"end":{"line":7,"column":56}}},{"type":"RequireHeaderDependency","range":[99,106],"loc":{"start":{"line":7,"column":16},"end":{"line":7,"column":56}}},{"type":"RequireHeaderDependency","range":[206,213],"loc":{"start":{"line":11,"column":12},"end":{"line":11,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/keys","range":[214,249],"loc":{"start":{"line":11,"column":12},"end":{"line":11,"column":56}}},{"type":"RequireHeaderDependency","range":[320,327],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-prototype-of","range":[328,375],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/classCallCheck","range":[475,513],"loc":{"start":{"line":19,"column":23},"end":{"line":19,"column":70}}},{"type":"RequireHeaderDependency","range":[467,474],"loc":{"start":{"lin