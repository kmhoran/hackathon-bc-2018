(nextProps) {\n        const { localeStore, navigationStore, url, html, legacyAppStore, c } = nextProps;\n\n        this.context.setTitle(c.name);\n\n\n        this.cleanHTML = nextProps.html;\n\n        //reset these for the new page.\n        this.buttonCleanStore = [];\n        this.checkBoxCleanStore = [];\n\n        this.setState({\n            html, message: '',\n            contentMessage: legacyAppStore.responseMessage,\n        });\n\n        this.inputCleanStore = [];\n\n        this.formActions = [];\n\n        if (this.lastLocale !== null && this.lastLocale !== localeStore.activeLocale.code) {\n            navigationStore.to({\n                url: navigationStore.localizeURL(navigationStore.path),\n            });\n        }\n        this.lastLocale = localeStore.activeLocale.code;\n     }\n\n    shouldComponentUpdate(nextProps, nextState) {\n\n        // don't update if the url doesn't match this page.\n        if (nextProps.url !== nextProps.navigationStore.path) {\n            return false;\n        }\n\n        // if (nextProps.myAccountStore.postUrl.length > 0 && !this.isForwarded) {\n        //     const url = nextProps.legacyAppStore.namespacedURL(nextProps.myAccountStore.postUrl, 'magento');\n        //     nextProps.navigationStore.to({ url });\n        //     this.isForwarded = true;\n        //     return false;\n        // }\n\n        if (nextProps.myAccountStore.previousHTML.length > 0 &&\n            this.props.html !== nextProps.myAccountStore.previousHTML\n        ) {\n            nextProps.myAccountStore.previousHTML = '';\n            nextProps.myAccountStore.isLoadingPost = false;\n            nextProps.myAccountStore.postUrl = '';\n           return true;\n        }\n\n        if (this.props.interfaceStore.isMobile !== this.isMobileTracker) {\n            this.isMobileTracker = this.props.interfaceStore.isMobile;\n            return true;\n        }\n\n        if (nextProps.myAccountStore.previousHTML.length > 0 &&\n            this.props.html !== nextProps.myAccountStore.previousHTML\n        ) {\n            nextProps.myAccountStore.previousHTML = '';\n            nextProps.myAccountStore.isLoadingPost = false;\n            nextProps.myAccountStore.postUrl = '';\n           return true;\n        }\n\n        if (this.props.interfaceStore.isMobile !== this.isMobileTracker) {\n            this.isMobileTracker = this.props.interfaceStore.isMobile;\n            return true;\n        }\n\n        if (nextProps.myAccountStore.previousHTML.length > 0 &&\n            this.props.html !== nextProps.myAccountStore.previousHTML\n        ) {\n            nextProps.myAccountStore.previousHTML = '';\n            nextProps.myAccountStore.isLoadingPost = false;\n            nextProps.myAccountStore.postUrl = '';\n           return true;\n        }\n\n\n        if (this.props.interfaceStore.isMobile !== this.isMobileTracker) {\n            this.isMobileTracker = this.props.interfaceStore.isMobile;\n            return true;\n        }\n\n        if (nextProps.myAccountStore.previousHTML.length > 0 &&\n            this.props.html !== nextProps.myAccountStore.previousHTML\n        ) {\n            nextProps.myAccountStore.previousHTML = '';\n            nextProps.myAccountStore.isLoadingPost = false;\n            nextProps.myAccountStore.postUrl = '';\n           return true;\n        }\n\n\n        if (this.props.interfaceStore.isMobile !== this.isMobileTracker) {\n            this.isMobileTracker = this.props.interfaceStore.isMobile;\n            return true;\n        }\n\n        if (this.props.navigationStore.path !== nextProps.url) return false;\n\n        // used by newsletter page.\n        // if the page is marked as no refresh and we are on that page.\n        // const accountConfig = accountLinks.filter((l) => l.to === nextProps.url)[0];\n        // if (accountConfig && accountConfig.noRefresh\n        // && this.props.url === nextProps.navigationStore.localizeURL(accountConfig.to)) {\n        //     console.log('3');\n        //     return false;\n        // }\n\n        if (this.props.myAccountStore\n        && this.props.myAccountStore.previousHTML\n        && this.props.myAccountStore.previousHTML.length > 0) {\n\n            if (this.props.html !== this.props.myAccountStore.previousHTML ) {\n                return true;\n            }\n            return false;\n        }\n\n    // avoid updating if q the html is the same but the styles are different.\n        if (this.state.activeStyles && nextState.activeStyles && Object.keys(nextState.activeStyles).length !== Object.keys(this.state.activeStyles).length\n        && nextProps.html === this.props.html) {\n            this.queuedStyles = nextState.activeStyles;\n            return false;\n    // handles the case where the user directs to another page in which styles would be undefined.\n        } else if (!nextState.activeStyles) {\n            return false;\n        }\n\n        return true;\n\n    }\n\n    async renderTrackingLink() {\n        const trackingLink = document.querySelector('.track');\n\n        if (trackingLink) {\n            const params = this.props.url.split('/');\n            if (params[params.length - 1] === '') params.length--;\n            const orderId = params[params.length - 1];\n            const { carrier, tracking_number, service_method, originZip, destination_zip } = await getTrackingDetails(orderId);\n            let carrierName, ozip = '46168';\n            if (tracking_number) {\n                switch (carrier) {\n                    case 'CAPOST': \n                        carrierName = 'canadapost';\n                        ozip = 'L9T%200A4';\n                        break;\n                    case 'PUROLA': \n                        carrierName = 'purolator';\n                        break;\n                    case 'SMPM': \n                        carrierName = 'dhlglobal';\n                        break;\n                    default: \n                        carrierName = carrier.toLowerCase();\n                        break;\n                }\n                let href = `https://beautycounter.narvar.com/beautycounter/tracking/${carrierName}?tracking_numbers=${tracking_number}&ozip=${ozip}`;\n                if (destination_zip) href += `&dzip=${destination_zip.split('-')[0]}`;\n                const service = (service_method === 'FED2' || service_method === 'FEDP') ? service_method === 'FED2' ? 'E2' : 'E1' : null;\n                if (service) href += `&service=${service}`;\n                trackingLink.href = href;\n                trackingLink.innerText = 'Track Order';\n                trackingLink.classList.remove('track');\n            } else {\n                trackingLink.innerText = 'No tracking number yet';\n            }\n        }\n    }\n\n    handleClick(e, node, formIndex) {\n        this.setState({\n            cardDataChangeVisibility: !this.state.cardDataChangeVisibility\n        }, () => {\n            if (this.state.passwordChangeVisibility === true) this.fields[formIndex - 1]['payment[sameCard]'] = 0;\n            else this.fields[formIndex - 1]['payment[sameCard]'] = 1;\n        });\n\n    }\n\n    handleChange(e, node, formIndex) {\n        if (node.attribs.name === 'change_password') {\n            this.setState({\n                passwordChangeVisibility: !this.state.passwordChangeVisibility\n            }, () => {\n                if (this.state.passwordChangeVisibility === true) this.fields.change_password = 1\n                else this.fields.change_password = 0;\n            });\n        }\n        if (node.attribs.id && node.attribs.id === 'region_id') {\n            let id = 0;\n            node.attribs.options.forEach((option) => {\n                if (option.key === e) {\n                    id = option.id;\n                }\n            });\n            this.fields[formIndex - 1]['billing[region_id]'] = id;\n            this.fields[formIndex - 1]['billing[region]'] = '';\n        }\n        let value;\n        if (e.persist) e.persist();\n        if (e.preventDefault) e.preventDefault();\n        if (e.target && e.target.value) {\n            value = e.target.value;\n            this.setState(prevState => ({\n                fields: { ...prevState.fields, [node.attribs.id]: value }\n            }));\n        }\n        else value = e;\n        if (node.attribs && node.attribs.name) {\n            this.fields[formIndex - 1][node.attribs.name] = value;\n        }\n        // this.props.legacyAppStore.updateValueForPage(e.target.value, i, 'pagename');\n    }\n\n    // wrap timeout in promise\n    timeout(ms) {\n        return new Promise(resolve => setTimeout(resolve, ms));\n    }\n\n    async handleSubmit(e, i, refresh = true) {\n        const { messagingStore, legacyAppStore: store, userStore, myAccountStore, navigationStore } = this.props;\n\n        const submitUrl = this.formActions[i];\n\n        e.preventDefault();\n\n        if (!this.forms[i].valid\n        // payment info page can have many forms. make sure to skip the validation check on edit and delete buttons.\n        && this.forms.length <= i + 1\n        ) {\n            messagingStore.addMessage('Please fill out all the fields', 'error');\n        } else if (submitUrl) {\n\n        // store fields to rehydrate\n            this.submittedFields = JSON.parse(JSON.stringify(this.fields));\n        //avoid changing password\n            // delete this.fields.change_password;\n\n            // if this form url is equal to a post form location, then look\n            // for forwarding location on that key.\n            let accountConfig = accountLinks.filter((l) => {\n                return l.api === store.namespacedURL(submitUrl, 'react', 'POST', true)\n            })[0];\n        // if no account config is found then try to select another way.\n            if (!accountConfig) {\n                accountConfig = accountLinks.filter((l) => {\n                    console.log(l.api, store.namespacedURL(submitUrl, 'react', 'POST', true));\n                    return l.api && store.namespacedURL(submitUrl, 'react', 'POST', true)\n                    .indexOf(l.api) > -1;\n                })[0];\n            }\n            if (accountConfig && accountConfig.noRefresh) {\n                refresh = false;\n            }\n        // also can set refresh false to cancel refreshing\n        //update clean html with new values\n            this.cleanHTML = this.filledHTML.innerHTML;\n\n            //preclear message.\n            // this.setState({ message: '' });\n\n            // declares to router that it should not reload this page on state update.\n            myAccountStore.isLoadingPost = true;\n            myAccountStore.isForwarded = false;\n            const cleanedUrl = store.namespacedURL(submitUrl, 'magento', 'POST');\n            myAccountStore.postUrl = cleanedUrl;\n            let html = await store.postForm(cleanedUrl, '', this.fields[i]);\n\n            // if there is a clipping error\n            if (!store.clipContent(html, 'col-main', true)\n            && accountConfig) {\n                if (accountConfig.errorCatchApi) {\n                    this.props.interfaceStore.showProgress('page-load');\n                    await this.timeout(1500);\n                    html = await store.postForm(accountConfig.errorCatchApi, '', {});\n                } else {\n                    messagingStore.addMessage(\"There was an error processing the request. Please make sure all the fields are completed.\", { type: 'error' });\n                }\n            }\n\n            this.lastCalledPath = null;\n\n        // after submitting the content then refresh user store\n            userStore.verifyLoggedIn(true);\n\n            const el = document.createElement('html');\n            el.innerHTML = html;\n            const messages = el.getElementsByClassName('messages')[0];\n            let success_message = null;\n            // this signals to the redirection code below if the logic should be skipped\n            // and we should stay on the same page.\n            let hasErrors = false;\n            // used to detect non success states on error catch pages.\n            let hasSuccess = false;\n\n            try {\n                success_message = messages.getElementsByClassName('success-msg')[0].firstChild.firstChild.firstChild.textContent;\n                messagingStore.addMessage(success_message, {type: 'success'});\n                hasSuccess = true;\n            } catch (e) {\n                try {\n                    const error_message = messages.getElementsByClassName('error-msg')[0].firstChild.firstChild.firstChild.textContent;\n                    messagingStore.addMessage(error_message, {type: 'error'});\n                    hasErrors = true;\n                } catch (e) {\n                    // this.setState({message: 'There was an error processing your request.'});\n                }\n            }\n\n            // if this address has a custom redirect then send to there.\n            let { redirectTo, errorCatchApi } = accountConfig;\n            if (redirectTo\n            && !hasErrors\n            && (!errorCatchApi || hasSuccess)\n            ) {\n            // clear previous html if there is because we wouldn't want to\n            // display a different page's html.\n                myAccountStore.previousHTML = '';\n                navigationStore.to({\n                    url: navigationStore.localizeURL(accountLinks.filter((l) => l.key === accountConfig.redirectTo)[0].to),\n                });\n            } else {\n\n                if (errorCatchApi && !hasSuccess && !hasErrors) {\n                    messagingStore.addMessage('There was an error submitting your payment information. Please check the information entered and try again.', {type: 'error'});\n                } else if (!hasErrors) { // reset all fields + update screen with new content\n                    myAccountStore.previousHTML = store.clipContent(html);\n\n                    this.fields = [];\n                    this.forms = [];\n                    this.setState({\n                        // html: ((() => {messages ? messages.innerHTML : null})() + this.cleanHTML),\n                        // fields: {},\n                        html: (() => refresh ? store.clipContent(html) : this.cleanHTML)(),\n                    });\n                }\n            }\n        } else {\n            messagingStore.addMessage('error retrieving data', 'error');\n        }\n\n    }\n\n    render() {\n        const { legacyAppStore: store, contentStore, c, navigationStore, localeStore } = this.props;\n        let { fields, html } = this.state;\n    \n    // clear the fields and forms\n        this.fields = [];\n        this.forms = [];\n\n    // remove 'your consultant is hq'\n        html = html.replace(`<div class=\"input-box\">Your consultant is <strong>Beautycounter HQ</strong></div>`, '');\n\n    // fix for getting the current state on address edit\n        let region_id = -1;\n        try {\n            const start = html.indexOf(`$('region_id').setAttribute('defaultValue',  \"`) + 46;\n            const remainder = html.slice(start, html.length);\n            region_id = remainder.slice(0,remainder.indexOf(`\"`));\n        } catch(e) {}\n\n    // fix for extracting address delete link.\n        let addressbookDeleteLink = '';\n        try {\n            const start = html.indexOf(`customer/address/delete/form_key/`);\n            const end = html.indexOf(`/id/'+addressId;`);\n            addressbookDeleteLink = html.slice(start, end);\n        } catch(e) {}\n\n    // all text fields come from dashboard configuration.\n        const updatedFields = getSafe(\n            () => contentStore.extensions['dashboard'].data.configAccount\n            // .filter((textPack) => textPack.page === c.key)[0]\n            .filter((textPack) => textPack.page === 'dashboard')[0]\n            .labels,\n        )\n        this.textFields = updatedFields ? updatedFields : this.textFields;\n\n    //clear these enumerating states on render\n        //keeps an index of the different forms so that the handlesubmit can match the right action to the right form.\n        let formCount = 0;\n\n        let buttonIndexCurrent = 0;\n        let checkBoxIndexCurrent = 0;\n        let inputIndexCurrent = 0;\n\n        const transform = (node, i) => {\n            let { attribs, type, name, children } = node;\n\n            //handle react attribute transformation\n            if (attribs !== undefined) {\n\n                attribs.key = this.keyEnum;\n                this.keyEnum = this.keyEnum + 1;\n\n                if (attribs.maxlength) {\n                    attribs.maxLength = attribs.maxlength;\n                    delete attribs.maxlength;\n                }\n                if (attribs.viewbox) {\n                    attribs.viewBox = attribs.viewbox;\n                    delete attribs.viewbox;\n                }\n                if (attribs['stroke-width']) {\n                    attribs.strokeWidth = attribs['stroke-width'];\n                    delete attribs['stroke-width'];\n                }\n                if (attribs.enctype) {\n                    attribs.encType = attribs.enctype;\n                    delete attribs.enctype;\n                }\n                if (attribs.onclick) {\n                    attribs.onClick = attribs.onclick;\n                    delete attribs.onclick;\n                }\n                if (attribs.autocomplete) {\n                    attribs.autoComplete = attribs.autocomplete;\n                    delete attribs.autocomplete;\n                }\n            }\n\n            if (attribs && attribs.class === 'my-account' && children && getSafe(() => children[0].children[1].children[0].data === 'My Orders' || children[0].children[1].children[0].data === 'Mes commandes')) {\n                const pText = getSafe(() => children[2].children[0]);\n                if (pText && !pText.next) {\n                    pText.data = __BROWSER__ && !window.location.href.includes('fr-ca') ? 'You have placed no orders!' : 'Vous n’avez pas passé de commande!';\n                    const parentAttribs = getSafe(() => pText.parent.attribs);\n                    parentAttribs.class = 'pText';\n                    children.push({\n                        name: 'div',\n                        type: 'tag',\n                        attribs: {\n                            class: 'shopButtonContainer'\n                        },\n                        children: [{\n                            name: 'button',\n                            attribs: {\n                                title: __BROWSER__ && !window.location.href.includes('fr-ca') ? 'Start Shopping' : 'Commencer à Magasiner',\n                                to: '/'\n                            }\n                        }]\n                    });\n                }\n            }\n\n            if (attribs && attribs.class && attribs.class === 'info-box') {\n                children && children.forEach(child => {\n                    if (child.name === 'h2' && child.children && \n                        (child.children[0].data === 'Shipping Method' || child.children[0].data === 'Mode d’expédition')) {\n                        children.push({\n                            type: 'tag',\n                            name: 'a',\n                            attribs: {\n                                class: 'track'\n                            }\n                        });\n                    }\n                });\n            }\n\n            if(attribs && node.name == 'a') {\n                if(attribs.class == \"ship-print-link\") {\n                    return <a href={attribs.href} target=\"_blank\" rel=\"noopener\">{node.children[0].data.split(':').join(': ')}</a>;\n                } else if (attribs.class === 'track') {\n                    return <a class={attribs.class} target=\"_blank\" rel=\"noopener\">Getting tracking number...</a>;\n                }\n            }\n            \n            //hide region input (not sure why its even there)\n            if (attribs && attribs.id === 'region') {\n                attribs.style = {\n                    display: 'none',\n                };\n                return <input {...attribs} />;\n            }\n\n            //handle credit card visibility\n            if (name === 'li' && attribs && attribs.class && attribs.class.indexOf('authnetcim_add') >= 0) {\n                if (this.state.cardDataChangeVisibility === true) {\n                    attribs.style = '';\n                }\n            }\n\n        // handling anchor for header content\n            if (name === 'h1') {\n                const { isMobile } = this.props.interfaceStore;\n                const style = isMobile ? {\n                    textAlign: 'center',\n                } : {};\n                return (\n                    <React.Fragment>\n                        <h1 style={style} {...attribs}>\n                            {isMobile ? this.textFields['mobileHeader'] : children[0].data}\n                        </h1>\n                        {isMobile ? this.renderLinks() : null}\n                    </React.Fragment>\n                );\n            }\n\n            if (attribs && attribs.id === 'giftcard_balance_lookup') {\n                return <div {...attribs}><div\n                    dangerouslySetInnerHTML={{__html: this.state.contentMessage}}\n                    /></div>;\n            }\n\n        //handle password change visibility\n            if (name === 'div' && attribs && attribs.class === 'my-account') {\n                children.forEach((child) => {\n                    if (child.name === 'form') {\n                        const select = child.children[3];\n                        if (this.state.passwordChangeVisibility === true)\n                            select.attribs.style = '';\n                    }\n                });\n            }\n            //ordering - this must go above all child elements because it creates container for holding data from children.\n            else if (type === 'tag' && name === 'form') {\n\n                attribs.className = attribs.class;\n                delete attribs.class;\n                if (attribs.action) this.formActions.push(attribs.action)\n                delete attribs.action;\n\n                //keep track of different fields and forms\n                const index = formCount;\n                if (this.fields[index] === undefined) this.fields.push({});\n                formCount = formCount + 1;\n\n                // fixes mobile safari string for style error.\n                if (typeof attribs.style === 'string') delete attribs.style;\n\n            // get the form key.\n                this.key = '';\n                children.forEach((child) => {\n                    try {\n                        if (child.attribs.name === 'form_key') {\n                            this.key = child.attribs.value;\n                        }\n                    } catch (e) {}\n                });\n\n                this.forms[index] = this.props.formStore.create({ \n                    key: this.key, \n                    persist: false,\n                });\n                console.log(this.forms[index], this.fields, this.formActions);\n\n                return (\n                    <form {...attribs} onSubmit={(e) => this.handleSubmit(e, index)} >\n                        {processNodes(node.children, transform)}\n                    </form>\n                ); \n            }\n            else if (type === 'tag' && name === 'a') {\n                attribs.className = attribs.class;\n                delete attribs.class;\n                attribs.to = store.namespacedURL(attribs.href, 'magento');\n                if (attribs.onClick && attribs.onClick.indexOf('deleteAddress') > 0) {\n                    let addressId = '';\n                    try {\n                        const start = attribs.onClick.slice(0, `return deleteAddress('`.length);\n                        const end = attribs.onClick.indexOf(`');`);\n                        addressId = attribs.onClick.slice(start.length , end);\n                    } catch(e) {}\n                    delete attribs.onClick;\n                    attribs.to = `${store.namespacedURL(addressbookDeleteLink, 'magento')}/id/${addressId}`;\n                }\n                return (\n                    <Link\n                    {...attribs}\n                    localize={false}\n                    >\n                        {attribs.title ? attribs.title : ''}\n                        {children[0] ? children[0].data : ''}\n                    </Link>\n                );\n            }\n            //un-nest inputs\n            else if (type === 'tag' && name === 'div' &&\n                attribs !== undefined && attribs.class &&\n                attribs.class === 'input-box') {\n\n            }\n\n            else if (type === 'tag' && name === 'div' && attribs !== undefined && attribs.class && attribs.class.indexOf('checkbox') >= 0) {\n                if (this.checkBoxCleanStore.length > checkBoxIndexCurrent) {\n                    node = this.checkBoxCleanStore[checkBoxIndexCurrent]\n                    attribs = node.attribs;\n                    children = node.children;\n                } else this.checkBoxCleanStore.push(node);\n                checkBoxIndexCurrent = checkBoxIndexCurrent + 1;\n\n                this.state.passwordChangeVisibility ? attribs.checked = true : attribs.checked = false;\n\n                return <Checkbox {...attribs} />;\n            }\n            else if (type === 'tag' && name === 'input') {\n                attribs.className = attribs.class;\n                delete attribs.class;\n\n                //store hidden inputs and create standard input elements\n                if (attribs.type === 'hidden') {\n                    this.fields[formCount - 1][attribs.name] = attribs.value;\n                    return convertNodeToElement(node, i, transform);\n                }\n\n                if (attribs.type === 'radio') {\n                    attribs.onClick = (e) => this.handleClick(e, node, formCount);\n\n                    if (attribs.id === 'authnetcim_sameCard_1') {\n                        attribs.checked = 'checked';\n\n                        //setup variables for this page\n                        if (this.fields[formCount - 1]['payment[sameCard]'] === undefined)\n                            this.fields[formCount - 1]['payment[sameCard]'] = 1\n                    }\n                    if (this.state.cardDataChangeVisibility) {\n                        if (attribs.id === 'authnetcim_sameCard_0') {\n                            attribs.checked = 'checked';\n                        }\n                        if (attribs.id === 'authnetcim_sameCard_1') {\n                            attribs.checked = '';\n                        }\n                    }\n\n                    attribs.className = s.radioButton;\n                    return <RadioButton {...attribs} />;\n                }\n\n                attribs.onChange = (e) => this.handleChange(e, node, formCount);\n\n                if (attribs.type === 'checkbox') {\n\n                    if (this.checkBoxCleanStore.length > checkBoxIndexCurrent) {\n                        node = this.checkBoxCleanStore[checkBoxIndexCurrent]\n                        attribs = node.attribs;\n                        children = node.children;\n                    } else this.checkBoxCleanStore.push(node);\n                    checkBoxIndexCurrent = checkBoxIndexCurrent + 1;\n\n                // fix hack for password visibility checkbox on account information\n                //TODO: wrap in localization method.\n                    if (this.props.url === navigationStore.localizeURL(accountLinks.filter((l) => l.key === 'information')[0].to)) {\n                        attribs.checked = this.state.passwordChangeVisibility ? true : false;\n                    }\n\n                    return <Checkbox {...attribs} />;\n                }\n\n                // attribs.value = store.createInput(attribs.value);\n                attribs.defaultValue = fields[node.attribs.name] ? fields[node.attribs.name] : attribs.value;\n\n                //the index must be decremented since the current form will always be one less than the next index.\n\n                if (attribs.name === 'billing[street][]'\n                    || attribs.name === 'street[]') {\n                    if (attribs.id === 'authnetcim_street_2' || attribs.id === 'street_2') attribs.name += '2';\n                    this.fields[formCount - 1][attribs.name] = attribs.value;\n                }\n                else {\n                    this.fields[formCount - 1][attribs.name] = attribs.value;\n                }\n\n                if (this.inputCleanStore.length > inputIndexCurrent) {\n                    node = this.inputCleanStore[inputIndexCurrent]\n                    const valueSave = attribs.defaultValue;\n                    attribs = node.attribs;\n                    attribs.defaultValue = valueSave;\n                    children = node.children;\n                } else this.inputCleanStore.push(node);\n                inputIndexCurrent = inputIndexCurrent + 1;\n\n            // rehydrate fields\n                const submittedFieldSet = this.submittedFields.filter(set => typeof set[attribs.id] !== 'undefined')[0];\n                attribs.value = submittedFieldSet && typeof submittedFieldSet[attribs.id] !== 'undefined' ? submittedFieldSet[attribs.id] : null;\n                if (attribs.value)\n                    attribs.defaultValue = attribs.value;\n\n\n                this.forms[formCount - 1].add([{ \n                    key: attribs.name, \n                    label: magentoInputMap[attribs.name].label,\n                    type: magentoInputMap[attribs.name].type,\n                    value: attribs.defaultValue,\n                    replace: true,\n                }]);\n                \n                attribs.storeEntry = this.forms[formCount - 1][attribs.name];\n\n                // attribs.showLabel = true;\n                // attribs.floatingLabel = true;\n                attribs.wrapperClassName = s.input;\n\n                return <Input {...attribs} />;\n            }\n            else if (type === 'tag' && name === 'select') {\n                attribs.className = attribs.class;\n                attribs.wrapperClassName = s.select;\n                delete attribs.class;\n\n                attribs.onChange = (e) => this.handleChange(e, node, formCount);\n\n                const options = [];\n                if (node.children.length > 0) {\n                    node.children.forEach((child, i) => {\n                        if (child.name === 'option') {\n                            const textChild = child.children[0];\n                            if (i === 1 && attribs.name !== 'region_id') {\n                                // fix for payment options state select\n                                if (textChild.data !== 'Please select region, state or province') {\n                                    attribs.defaultValue = textChild.data;\n                                }\n                            }\n\n                            //set the default\n                            if (typeof child.attribs.selected !== 'undefined')\n\n                                if (child.attribs.selected === 'selected'\n                                    //check if there is an exisiting form. Some form elements are dirty such as order history select.\n                                    && formCount > 0) {\n\n                                    this.fields[formCount - 1][attribs.name] = child.attribs.value;\n                                }\n           