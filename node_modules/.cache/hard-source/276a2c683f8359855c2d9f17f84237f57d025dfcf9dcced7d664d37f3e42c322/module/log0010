n        error: false\n    },\n    email: {\n        value: '',\n        error: false\n    },\n    verifyEmail: {\n        value: '',\n        error: false\n    },\n    phone: {\n        value: '',\n        error: false,\n        notRequired: true,\n    },\n    address: {\n        value: '',\n        error: false\n    },\n    address2: {\n        value: '',\n        error: false,\n        notRequired: true\n    },\n    city: {\n        value: '',\n        error: false\n    },\n    stateProvince: {\n        value: '',\n        abbreviation: '',\n        error: false\n    },\n    zipCode: {\n        value: '',\n        error: false\n    },\n    country: {\n        value: 'USA',\n        abbreviation: 'US',\n        error: false\n    },\n    shopping: {\n        value: false,\n        error: false,\n        notRequired: true\n    },\n    becomingConsultant: {\n        value: false,\n        error: false,\n        notRequired: true\n    },\n    hostingSocial: {\n        value: false,\n        error: false,\n        notRequired: true\n    },\n    becomeMember: {\n        value: false,\n        error: false,\n        notRequired: true\n    },\n    message: {\n        value: '',\n        error: false,\n        notRequired: true\n    },\n    terms: {\n        value: false,\n        error: false\n    },\n    captcha: {\n        value: false,\n        error: false\n    },\n};\n\n\n\n\n\n@withStyles(s)\n@inject(\n    'contentStore', 'interfaceStore', 'localeStore', 'navigationStore'\n)\n@mixin(ScrollToMixin)\n@observer class ConnectConsultantPage extends Component {\n\n    state = {\n        form: cloneDeep(formObj),\n        loading: false,\n        success: false,\n        stateOptions: [],\n        countryOptions: []\n    }\n\n    static contextTypes = {\n        setTitle: pt.func.isRequired,\n        onSetMeta: pt.func.isRequired\n    };\n\n    componentWillMount() {\n        this.updateTitle();\n        this.setPageMeta();\n    }\n\n    componentDidMount() {\n        cons