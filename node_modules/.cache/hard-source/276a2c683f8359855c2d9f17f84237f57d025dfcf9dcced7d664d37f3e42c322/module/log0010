'classnames';\nimport { getSafe, setSEOMeta } from 'Middleware/utils/object';\nimport Section from 'Common/Section';\nimport Input from 'Common/Input';\nimport Button from 'Common/Button';\nimport MarkdownWrapper from 'Components/MarkdownWrapper';\nimport Image from 'Common/Image';\nimport Loader from 'Common/Loader';\nimport mixin from 'Middleware/utils/decorators/mixinComponent';\nimport ScrollToMixin from 'Components/Extensions/ScrollToMixin';\nimport Slider from 'react-slick';\nimport IconClose from 'Icons/Controls/Close.jsx';\nimport Phone from 'Icons/Phone.jsx';\nimport MapPin from 'Icons/MapPin.jsx';\nimport Checked from 'Icons/Checked.jsx';\nimport ErrorField from 'Common/ErrorField';\nimport { validEmail } from 'Middleware/utils/validators';\nimport { popupShopSubscribe } from 'Middleware/api/sailthru';\nimport { routes } from 'Root/config';\n\n@withStyles(s)\n@inject(\n    'contentStore',\n    'localeStore',\n    'navigationStore',\n)\n@mixin(ScrollToMixin)\n@observer class NantucketPage extends Component {\n\n    static contextTypes = { setTitle: pt.func.isRequired, onSetMeta: pt.func.isRequired };\n\n    constructor(props) {\n        super(props);\n        this.state = {\n            isValidEmail: false,\n            isEmptyEmail: true,\n            formActivated: false,\n            formUnfocused: false,\n            formSubmit: false,\n            submitSuccess: false,\n            addGoogleClass: '',\n            addButtonClass: '',\n            sailthruError: false,\n            showErrorMessage: false,\n            centerMode: true\n        }\n\n        this.handleInputChange = this.handleInputChange.bind(this);\n        this.invalidButtonClick = this.invalidButtonClick.bind(this);\n\n        this.autoRunDisposer = autorun(() => {\n            if (this.config.hide) {\n                this.props.navigationStore.to({ url: routes.HOME });\n            }\n        });\n\n    }\n\n    componentWillMount() { this.updateTitle(); this.setPageMeta(); }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.nantucket.data.configNantucket[0]);\n    }\n\n    updateTitle() { this.context.setTitle(getSafe(() => this.config.displayName)) }\n\n    setPageMeta() {\n        setSEOMeta(this.context, getSafe(() => this.config.configSEO.fields));\n    }\n\n    toggleVisible() { return this.state.formActive ? 'block' : 'none' }\n\n    checkValidation(inputField) {\n        const activeField = Object.keys(inputField)[0];\n\n        if (activeField == 'email') {\n            inputField.email ? this.setState({ isEmptyEmail: false }) : this.setState({ isEmptyEmail: true });\n            validEmail(inputField.email) ? this.setState({ isValidEmail: true }) : this.setState({ isValidEmail: false });\n        }\n    }\n\n    handleInputChange(event) {\n        this.setState({ formActivated: true });\n        const target = event.target;\n        let value = target.value;\n\n        const name = target.className.split(' ')[2];\n\n        this.setState({ [name]: value });\n        this.checkValidation({ [name]: value });\n\n        if(this.state.isValidEmail && this.state.formActivated) {\n            this.setState({ addGoogleClass: '' });\n            this.setState({ addButtonClass: '' });\n        }\n    }\n\n    invalidButtonClick() {\n        if(this.state.formActivated) { this.setState({ formUnfocused: true }) }\n        if((!this.state.isValidEmail && this.state.formActivated)) {\n          this.setState({ addGoogleClass: s.googleMapsError });\n          this.setState({ addButtonClass: s.buttonError });\n        }\n    }\n\n    handleSubmit = async (e) => {\n        e && e.preventDefault();\n\n        let formSubmissionObj = { 'email': this.state.email }\n        this.setState({ formSubmit: true });\n\n        // Send the object and form values to the sailthru API.\n        popupShopSubscribe(formSubmissionObj, 'nantucket-store-page').then(() => {\n            this.setState({ submitSuccess: true });\n            this.setState({ sailthruError: false });\n        }).catch((e) => {\n            this.setState({ formSubmit: false });\n            this.setState({ sailthruError: true });\n            this.setState({ isValidEmail: false });\n            this.setState({ addGoogleClass: s.googleMapsError });\n            this.setState({ addButtonClass: s.buttonError });\n        });\n    }\n\n    render() {\n        this.updateTitle();\n        this.setPageMeta();\n        const {\n            updatesCtaText,\n            locationLink,\n            locationText,\n            phoneNumber,\n            slideShowPhotos,\n            visitMessage,\n            thankYouMessage,\n            successFormSubmissionText,\n            formSubmitButtonText,\n            submitButton,\n            inputLabelText,\n            emailInputTitle,\n            emailValidationMessage,\n            formDescription\n        } = this.config;\n\n        const settings = {\n            dots: true,\n            infinite: true,\n            speed: 500,\n            arrows: false,\n            autoplay: true,\n            autoplaySpeed: 2500,\n            draggable: true\n        };\n\n        return (\n            <section className={s.root}>\n                <style dangerouslySetInnerHTML={{__html: `\n                    .slick-active { background-color: rgb(255, 255, 255) !important; }\n\n                    #checkIcon--green g { fill: rgb(6, 180, 125) !important; }\n\n                    .slick-list { max-width: 1310px; }\n\n                    @media screen and (max-width: 1431px) {\n                        .slick-list img { margin-top: -130px; }\n                    }\n\n                    @media screen and (min-width: 1310px) {\n                        .slick-slide {\n                            width: 1310px !important;\n                        }\n                    }\n\n                    @media screen and (min-width: 1140px) {\n                        .slick-list img { margin-top: -130px; }\n                    }\n\n                    @media screen and (min-width: 1100px) {\n                        .slick-list { height: 600px !important; }\n                    }\n\n                    @media screen and (max-width: 1040px) {\n                        .slick-list img { margin-top: -120px; }\n                    }\n\n                    @media screen and (max-width: 890px) {\n                        .slick-list img { margin-top: -50px; }\n                    }\n\n                    @media screen and (max-width: 500px) {\n                        .slick-list img { margin-top: 0px; }\n                    }\n                `}}></style>\n\n                <section className={s.sliderContainer}>\n                    <Slider {...settings}>\n                        {slideShowPhotos.map((photo) => {\n                          return (<div key={photo.fields.file.url}>\n                            <img\n                              className={s.mediaContent}\n                              src={getSafe(() => photo.fields.file.url) + '?q=85'} />\n                          </div>);\n                        })}\n                    </Slider>\n                </section>\n\n                <div className={s.panelContent}>\n                    <section className={s.leftCol}>\n                        <MarkdownWrapper\n                          className={s.headerText}\n                          content={visitMessage}\n                        />\n\n                        <div style={{ float: 'left'}}>\n                            <div className={s.contactTabletAbove}>\n                                <p>\n                                    <MapPin className={s.globeIcon} />\n                                    <a href={locationLink} target=\"_blank\">{locationText}</a> <span className={s.contactInfoDivider}>|</span> <Phone className={s.phoneIcon}/>\n                                    <a href={\"tel:+\" + phoneNumber}>{phoneNumber}</a>\n                     