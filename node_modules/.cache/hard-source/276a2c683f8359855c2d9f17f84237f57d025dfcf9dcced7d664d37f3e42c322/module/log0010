pes = {\n        setTitle: pt.func.isRequired,\n        onSetMeta: pt.func.isRequired\n    };\n\n    static propTypes = {\n        order: pt.string.isRequired,\n        email: pt.string.isRequired,\n    };\n\n    componentWillMount() {\n        this.updateTitle();\n        this.setPageMeta();\n    }\n\n    componentDidMount() {\n        this.disposer = autorun(() => {\n            this.props.navigationStore.path;\n            if (window && !window.location.pathname.includes(routes.ORDER_CONFIRM)) {\n                requestAnimationFrame(() => {\n                    this.props.attributionStore.resetOverride();\n                    this.disposer();\n                });\n            }\n        });\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.orderConfirmation.data.configOrderConfirmation[0]);\n    }\n\n    updateTitle() {\n        this.context.setTitle(getSafe(() => this.config.displayName));\n    }\n\n    setPageMeta() {\n        setSEOMeta(this.context, getSafe(() => this.config.configSEO.fields));\n    }\n\n    render() {\n        const {order, email} = this.props;\n        const {heading, mainMessage, secondaryMessaging, createAccount, myAccountButton, continueShoppingButton} = this.config;\n        const {isLogged} = this.props.userStore;\n        const {orderProperties} = this.props.analyticsStore;\n        const showPixel = Object.keys(orderProperties).length !== 0 && orderProperties.quantity > 0 && pixel;\n\n        const bluecoreSrc = showPixel ? ('https://www.bluecore.com/api/track/purchase_pixel?token=beautycounter?' + generateQueryString({\n                    'email': email,\n                    'total': orderProperties.total,\n                    'order_id': orderProperties.order_id,\n                    'product_ids':orderProperties.skus.join(',')\n                }, true)) : '';\n\n        const discount = showPixel && orderProperties.discount\n        && parseInt(orderProperties.discount.replace(/([$.])/g, '')) !== 0\n            ? orderProperties.discount : false;\n\n        const ranSrc = showPixel ? ('https://track.linksynergy.com/ep?' + generateQueryString({\n                        'mid': getCookieClient('ranMID'),\n                        'ord': orderProperties.order_id,\n                        ...(getCookieClient('ranSiteID') ? {'tr': getCookieClient('ranSiteID')} : {}),\n                        ...(getCookieClient('ranDateSet') ? {'tr': getCookieClient('ranDateSet')} : {}),\n                        'skulist': orderProperties.products.map(p => p.sku).join(\"|\") + (discount ? '|Discount' : ''),\n                        'qlist': orderProperties.products.map(p => p.quantity).join(\"|\") + (discount ? '|0' : ''),\n                        'amtlist': orderProperties.products.map(p => p.value.replace(/(\\$|\\.)/g, '')).join(\"|\")\n                            + (discount ? '|' + discount : ''),\n                        'cur': orderProperties.currency,\n                        'img':1,\n                        'namelist': orderProperties.products.map(p => p.name.replace(/\\|/g, '-')).join(\"|\")\n                            + (discount ? '|Discount' : '')\n                }, true)) : '';\n\n        this.updateTitle();\n        this.setPageMeta();\n\n        // The server cannot pre-render this page as it is session specific\n        if (__SERVER__ || !order || !email) return false;\n\n        return (\n            <Section className={s.root} contentClassName={s.content} darkBackground>\n                <h2>{heading}</h2>\n                <div className={s.mainMessage}>\n                    {mainMessage.includes('{email}') ? mainMessage.replace('{email}', email) : mainMessage}\n                </div>\n                <div className={s.orderNumber}>\n                    {order}\n                </div>\n                {secondaryMessaging && <div className={s.secondaryMessaging}>\n                    {secondaryMessaging}\n                </div>}\n                <div className={s.buttonContainer}>\n                    {isLogged ?\n                        <Button to={routes.ACCOUNT}>{myAccountButton}</Button> :\n                        <Button to={routes.CREATE_ACCOUNT}>{createAccount}</Button>\n                    }\n                    <Button to={routes.HOME} color={'white'}>{continueShoppingButton}</Button>\n                </div>\n                <div className={s.pixel}>\n                    {pixel &&\n                     <div><img id={'bluecore_purchase_pixel'} src={bluecoreSrc}/><img id={'ran_data_pixel'} src={ranSrc}/></div>}\n                </div>\n            </Section>\n        );\n    }\n\n}\n\nexport default OrderConfirmationPage;\n"]}},"_buildHash":"dbd03e6887329532de0a8593315423c0","hash":"500ba5e632e60408e531e56f81850626","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[286,293],"loc":{"start":{"line":11,"column":16},"end":{"line":11,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/extends","range":[294,325],"loc":{"start":{"line":11,"column":16},"end":{"line":11,"column":56}}},{"type":"RequireHeaderDependency","range":[393,400],"loc":{"start":{"line":15,"column":12},"end":{"line":15,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/keys","range":[401,436],"loc":{"start":{"line":15,"column":12},"end":{"line":15,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/cor