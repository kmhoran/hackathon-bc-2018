Link';\nimport Button from 'Common/Button';\nimport Separator from 'Common/Separator';\nimport Checkbox from 'Common/Checkbox';\n\nconst form = {\n    firstname: {\n        value: '',\n        error: false,\n    },\n    lastname: {\n        value: '',\n        error: false,\n    },\n    email: {\n        value: '',\n        error: false,\n    },\n    password: {\n        value: '',\n        error: false,\n    },\n    confirmation: {\n        value: '',\n        error: false,\n    },\n    is_subscribed: {\n        value: '',\n        error: false,\n        notRequired: true,\n    },\n};\n\n@withStyles(s)\n@inject(\n    'contentStore',\n    'navigationStore',\n    'messagingStore',\n    'localeStore',\n    'userStore',\n)\n@observer\nclass SignupPage extends Component {\n    static contextTypes = {\n        setTitle: PropTypes.func.isRequired,\n        onSetMeta: PropTypes.func.isRequired\n    };\n\n    constructor(props) {\n        super(props);\n\n        this.handleInputChange = this.handleInputChange.bind(this);\n        this.submit = throttle(this.handleSubmit, 200);\n        const app = this;\n\n        // Automatically reset form on locale change\n        this.autoRunDisposer = autorun(() => {\n            if (this.props.localeStore.activeLocale.code !== this.state.locale && app) {\n                app.setState({\n                    locale: toJS(this.props.localeStore.activeLocale.code),\n                    form: cloneDeep(form),\n                });\n            }\n        });\n    }\n\n    state = {\n        form: cloneDeep(form),\n        locale: toJS(this.props.localeStore.activeLocale.code),\n    }\n\n    componentWillMount() {\n        this.updateTitle();\n        this.setPageMeta();\n        if (this.props.userStore.isLogged) {\n            this.props.navigationStore.to({ url: routes.ACCOUNT });\n        }\n    }\n\n    componentWillUnmount() {\n        this.autoRunDisposer();\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.signup.data.configSignUp[0]);\n    }\n\n    updateTitle() {\n        this.context.setTitle(getSafe(() => this.config.displayName));\n    }\n\n    setPageMeta() {\n        setSEOMeta(this.context, getSafe(() => this.config.configSEO.fields));\n    }\n\n    handleSubmit = async (event) => {\n        event && event.preventDefault();\n\n        const valid = this.validateInput();\n        if (valid) {\n            const params = this.getFormParams();\n            const account = await this.props.userStore.createAccount(params);\n            if (account.success) {\n                this.props.messagingStore.addMessage(this.config.messageRegisterSuccessful, { type: 'success' });\n                this.props.navigationStore.to({ url: routes.ACCOUNT });\n            }\n            else {\n                const message = typeof account.message === 'object' ? account.message[Object.keys(account.message)[0]] : account.message;\n                \n                this.props.messagingStore.addMessage(message ? this.parseError(message) : 'There was an error.', { type: 'error' });\n            }\n        }\n    };\n\n    // Replaces generic server side messages\n    parseError = (error) => {\n        if (error.includes('There is already an account with this email address')) {\n            const message = this.config.accountAlreadyRegistered;\n            const link = message.split('{')[1].split('}')[0];\n            const p1 = message.split('{')[0];\n            const p2 = message.split('}')[1];\n\n            const formattedError = (\n                <span>\n                    {p1}<Link className={s.link} to={routes.LOGIN}>{link}</Link>{p2}\n                </span>\n            );\n\n            return formattedError;\n        }\n        return error;\n    }\n\n    getFormParams() {\n        const params = {};\n        Object.keys(this.state.form).forEach((key) => {\n           params[key] = this.state.form[key].value;\n        });\n        return params;\n    }\n\n    validateInput() {\n        const form = this.state.form;\n\n        let errors = 0;\n        Object.keys(this.state.form).forEach((key) => {\n            if (!this.state.form[key].value && !this.state.form[key].notRequired) {\n                this.state.form[key].error = this.config.genericFieldError;\n                errors++;\n            }\n        })\n        if (form.password.value !== form.confirmation.value) {\n            this.state.form.confirmation.error = this.config.passwordMatchError;\n            this.state.form.password.error = true;\n            this.state.form.confirmation.value = '';\n            this.state.form.password.value = '';\n            errors++;\n        }\n        else if (form.password.value.length < 6) {\n            this.state.form.password.error = this.config.passwordMinLength;\n            this.state.form.confirmation.error = true;\n            this.state.form.password.value = '';\n            this.state.form.confirmation.value = '';\n            errors++;\n        }\n\n        if (!validEmail(form.email.value)) {\n            this.state.form.email.error = this.config.messageInvalidEmail;\n            errors++;\n        }\n        this.setState({});\n        if (errors > 0) return false;\n        return true;\n    }\n\n\n    handleInputChange(name, value, regex = false) {\n        const entry = {\n            ...this.state.form[name],\n            error: false,\n            value: regex ? value.replace(regex, '') : value,\n        };\n\n        this.setState({\n            form: {\n                ...this.state.form,\n                [name]: entry,\n            },\n        });\n    }\n\n    render() {\n        const {\n            displayName,\n            firstName,\n            lastName,\n            heading,\n            labelEmail,\n            labelSendUpdates,\n            labelPassword,\n            labelPasswordConfirm,\n            personalInformation,\n            requiredFields,\n            signInInformation,\n            submit,\n         } = this.config;\n        const { form } = this.state;\n\n        this.updateTitle();\n        this.setPageMeta();\n\n        return (\n            <Section innerClassName={s.root} contentClassName={s.content}>\n                <form action='' onSubmit={this.handleSubmit} style={{ width: '100%' }}>\n                <div className={s.header}>\n                    <h2>{heading}</h2>\n                    <div>{requiredFields}</div>\n                </div>\n                <div className={s.block}>\n                    <h3>{personalInformation}</h3>\n                    <div className={s.inputBlock}>\n                        <Input\n                            onChange={e => this.handleInputChange('firstname', e)}\n                            wrapperClassName={s.input}\n                            error={form.firstname.error}\n                            invalid={form.firstname.error}\n                            defaultValue={form.firstname.value}\n                            showLabel\n                            maxLength={16}\n                            required\n                            title={`${firstName} *`} />\n                        <Input\n                            onChange={e => this.handleInputChange('lastname', e)}\n                            wrapperClassName={s.input}\n                            error={form.lastname.error}\n                            invalid={form.lastname.error}\n                            defaultValue={form.lastname.value}\n                            showLabel\n                            maxLength={24}\n                            required\n                            title={`${lastName} *`} />\n                        <Input\n                            onChange={e => this.handleInputChange('email', e)}\n                            wrapperClassName={s.input}\n                            error={form.email.error}\n                            invalid={form.email.error}\n                            defaultValue={form.email.value}\n                            required\n                            maxLength={40}\n                            showLabel\n                            type={'email'}\n                            title={`${labelEmail} *`} />\n                    </div>\n                    <div className={s.checkbox}>\n                    <Checkbox\n                        checked={form.is_subscribed.value}\n                        onChange={e => this.handleInputChange('is_subscribed', e)}\n                        wrapperClassName={s.checkbox} />\n                            <ReactMarkdown source={labelSendUpdates} escapeHtml={false} />\n                    </div>\n                </div>\n                <div className={s.block}>\n                    <h3>{signInInformation}</h3>\n                    <div className={s.inputBlock}>\n                        <Input\n                            onChange={e => this.handleInputChange('password', e)}\n                            wrapperClassName={s.input}\n                            error={form.password.error}\n                            invalid={form.password.error}\n                            defaultValue={form.password.value}\n                            showLabel\n                            required\n                            type={'password'}\n                            title={`${labelPassword} *`} />\n                        <Input\n                            onChange={e => this.handleInputChange('confirmation', e)}\n                            wrapperClassName={s.input}\n                            error={form.confirmation.error}\n                            invalid={form.confirmation.error}\n                            defaultValue={form.confirmation.value}\n                            required\n                            showLabel\n                            type={'password'}\n                            title={`${labelPasswordConfirm} *`} />\n                    </div>\n                    <br />\n                </div>\n                <Button\n                    isSubmit\n                    onClick={this.submit}>{submit}\n                </Button>\n                </form>\n            </Section>\n        );\n    }\n}\n\nexport default SignupPage;"]}},"_buildHash":"2c105e05f5fb6ef20f0f991817abbddb","hash":"65969f00d62068d7c700d579218adb6c","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[293,300],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/defineProperty","range":[301,339],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/extends","range":[433,464],"loc":{"start":{"line":15,"column":16},"end":{"line":15,"column":56}}},{"type":"RequireHeaderDependency","range":[425,432],"loc":{"start":{"line":15,"column":16},"end":{"line":15,"column":56}}},{"type":"RequireHeaderDependency","range":[539,546],"loc":{"start":{"line":19,"column":19},"end":{"line":19,"column":55}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[547,574],"loc":{"start":{"line":19,"column":19},"end":{"line":19,"column":55}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/keys","range":[657,692],"loc":{"start":{"line":23,"column":12},"end":{"line":23,"column":56}}},{"type":"RequireHeaderDependency","r