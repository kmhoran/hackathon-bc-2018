LoadingOverlay from 'Common/LoadingOverlay';\nimport { recaptcha } from '../../config';\n\nconst form = {\n    question: {\n        value: '',\n        error: false,\n        notRequiredOnMedia: () => window.innerWidth <= MEDIA_PHONE,\n    },\n    email: {\n        value: '',\n        error: false,\n    },\n    phone: {\n        value: '',\n        error: false,\n        notRequired: true,\n    },\n    message: {\n        value: '',\n        error: false,\n    },\n    fullName: {\n        value: '',\n        error: false,\n    },\n    captcha: {\n        value: false,\n        error: false,\n    },\n};\n\n@withStyles(s)\n@inject(\n    'contentStore',\n    'interfaceStore',\n    'messagingStore',\n    'attributionStore',\n    'localeStore',\n)\n@mixin(ScrollToMixin)\n@observer class PWSContainer extends Component {\n\n    static contextTypes = {\n        setTitle: pt.func.isRequired,\n        onSetMeta: pt.func.isRequired\n    };\n\n    state = {\n        form: cloneDeep(form),\n        locale: toJS(this.props.localeStore.activeLocale.code),\n        socials: false,\n        socialSelection: false,\n        successMessage: false,\n        loading: false,\n    }\n\n    componentWillMount() {\n        this.updateTitle();\n        this.setPageMeta();\n    }\n\n    componentDidMount() {\n        __BROWSER__ && requestAnimationFrame(() => this.getSocials());\n\n        // Automatically reset form on locale change\n        this.autoRunDisposer = autorun(() => {\n            if (this.props.localeStore.activeLocale.code !== this.state.locale && app) {\n                this.setState({\n                    locale: toJS(this.props.localeStore.activeLocale.code),\n                    form: cloneDeep(form),\n                });\n                this.getSocials();\n            }\n        });\n    }\n\n    getSocials() {\n        const { id } = this.props.consultant;\n        const { social } = this.props;\n\n        this.props.attributionStore.getSocials(id)\n            .then((data) => {\n                let found;\n\n                if (social) {\n                    const socialId = social.includes('-') ? social.split('-')[0] : social;\n                    found = getSafe(() => find(data, { partyId: parseInt(socialId) }));\n                }\n                else if (this.props.attributionStore.socialId)\n                    found = getSafe(() => find(data, { partyId: parseInt(this.props.attributionStore.socialId) }));\n\n                if (found) this.setState({\n                    socialSelection: {\n                        id: found.partyId,\n                        name: found.socialName,\n                        selectable: true,\n                    }\n                });\n                this.setState({ socials: data });\n            });\n    }\n\n    componentWillUnmount() {\n        this.autoRunDisposer();\n    }\n\n    @computed get config() {\n        return getSafe(() => this.props.contentStore.extensions.PWS.data.configPws[0]);\n    }\n\n    updateTitle() {\n        this.context.setTitle(getSafe(() => `${this.props.consultant.firstName} ${this.props.consultant.lastName}`));\n    }\n\n    setPageMeta() {\n        setSEOMeta(this.context, getSafe(() => this.config.configSEO.fields));\n    }\n\n    handleInputChange(name, value) {\n        const entry = {\n            ...this.state.form[name],\n            error: false,\n            value: value !== -1 ? value : null,\n        };\n\n        // Re-anchor user if recaptcha changes to \n        // prevent Google's strange scroll bug in iOS\n        if (name === 'captcha' && this.props.interfaceStore.isMobile) {\n            setTimeout(() => this.scrollTo({\n                hash: `#pws-contact-form-bottom`,\n                animate: true,\n                duration: 300,\n                extraHeight: 250,\n            }), 80);\n        }\n\n        this.setState({\n            form: {\n                ...this.state.form,\n                [name]: entry,\n            },\n        });\n    }\n\n    handleSubmit = async () => {\n        const { form } = this.state;\n        const valid = this.validateInput(false);\n        const event = this.config.sailthruEvent;\n\n        if (valid) {\n            try {\n                this.setState({ loading: true });\n                const email = this.props.consultant.email;\n                const message = this.buildMessage();\n\n                const [firstName, lastName] = form.fullName.value.split(' ');\n                const prospect = {\n                    consultantId: this.props.consultant.id,\n                    firstName: firstName,\n                    lastName: typeof lastName !== 'undefined' ? lastName : ' ',\n                    countryId: this.props.localeStore.activeLocale.countryId,\n                    emailAddress: form.email.value,\n                    phone: form.phone.value,\n                    defaultLanguageId: 1,\n                };\n\n                const [sailthru, phoenix] = await Promise.all([\n                    sendEmailToConsultant(message, email, event),\n                    addProspect(prospect),\n                ]);\n\n                this.setState({\n                    loading: false,\n                    successMessage: message,\n                });\n                if (window.innerHeight < 1000) this.scrollTo({\n                    hash: `#pws-contact-form`,\n                    animate: true,\n                    duration: 400,\n                    extraHeight: 100,\n                });\n            }\n            catch (e) {\n                console.log(e);\n                return false;\n            }\n        }\n        else{\n            this.refs['pws-contact-form'].scrollIntoView({block: 'center', behavior: 'smooth'});\n            return false;\n        }\n    }\n\n    buildMessage() {\n        const { form } = this.state;\n        const { formLabels } = this.config;\n\n        return `Hi ${this.props.consultant.firstName}, <br /><br/>\n                ${formLabels.social} ${form.question.value}<br/>\n                ${formLabels.emailLeadIn} ${form.email.value}${form.phone.value && ' ' + formLabels.phoneLeadIn + ' ' + form.phone.value}<br /><br />\n                ${form.message.value}<br /><br />\n                ${formLabels.endMessage}<br />\n                ${form.fullName.value}`;\n    }\n\n    buildCustomMessage(message) {\n        const { fullName, email, url } = this.props.consultant;\n\n        return message\n            .replace(/{First Name}/g, this.state.form.fullName.value.split(' ')[0]) \n            .replace(/{Last Name}/g, this.state.form.fullName.value.split(' ')[1])\n            .replace(/{Full Name}/g, this.state.form.fullName.value)\n            .replace(/{Email}/g, this.state.form.email.value) \n            .replace(/{Message}/g, this.state.form.message.value) \n            .replace(/{Phone}/g, this.state.form.phone.value) \n            .replace(/{Question}/g, this.state.form.question.value) \n            .replace(/{Consultant First Name}/g, fullName.split(' ')[0])\n            .replace(/{Consultant Last Name}/g, fullName.split(' ')[1])\n            .replace(/{Consultant Full Name}/g, fullName)\n            .replace(/{Consultant Email}/g, email)\n            .replace(/{Consultant URL}/g, `<a href='${url}'>beautycounter.com${url}</a>`)\n            .replace(/\\\\n/g, '<br />')\n            .replace(/undefined/g, '');\n    }\n\n    validateInput(ignoreUpdate) {\n        const { form } = this.state;\n        const newForm = cloneDeep(form);\n\n        let errors = 0;\n        Object.keys(this.state.form).forEach((key) => {\n            const input = this.state.form[key];\n            const notRequiredOnMedia = input.notRequiredOnMedia && input.notRequiredOnMedia();\n\n            if (!input.value && !input.notRequired && !notRequiredOnMedia) {\n                newForm[key].error = this.config.fieldError;\n                errors++;\n            }\n        });\n\n        if (!validEmail(form.email.value)) {\n            newForm.email.error = this.config.invalidEmailError;\n            errors++;\n        }\n        if (!form.captcha.value) {\n            newForm.captcha.error = this.config.invalidCaptchaError;\n            errors++;\n        }\n        !ignoreUpdate && this.setState({\n            form: newForm,\n        });\n        if (errors > 0) return false;\n        return true;\n    }\n\n    handleSocialChange(options, selected) {\n        const social = find(options, { name: selected });\n        this.props.attributionStore.setSocial(social ? social.id : null);\n        \n        this.setState({ socialSelection: social });\n    }\n\n    render() {\n        const {\n            heroImage,\n            contactMe,\n            contactMessage,\n            shopButton,\n            shopButtonURL,\n            formLabels,\n            questions,\n            placeholderImage,\n            contactSuccessMessage,\n            contactSuccessMessageBody,\n            formSubmitCTA,\n        } = this.config;\n        const {\n            fullName,\n            firstName,\n            imageUrl,\n            location,\n            title,\n        } = this.props.consultant;\n        const {\n            form,\n            socials,\n            successMessage,\n            loading,\n        } = this.state;\n\n        this.updateTitle();\n\n        const questionList = questions && questions.length && questions.map(question => ({\n            name: question,\n            selectable: true,\n        }));\n\n        let socialOptions = false;\n        if (socials && socials.length) {\n            socialOptions = socials.map((social) => ({\n                name: social.socialName,\n                id: social.partyId,\n                selectable: true,\n            }));\n            socialOptions = [\n                {\n                    name: 'No social',\n                    id: null,\n                    selectable: true,\n                },\n                ...socialOp