        }\n        }\n        else {\n            if (this.props.interfaceStore.stickyHeader) {\n                this.props.interfaceStore.searchOpen && this.props.interfaceStore.searchToggle(false);\n                this.props.interfaceStore.fixedScrollHeader();\n            }\n        }\n    };\n\n    handleMenuIconClick = event => {\n        this.props.onMenuClick && this.props.onMenuClick(event);\n    };\n\n    handleCartIconClick = event => {\n        this.props.onCartClick && this.props.onCartClick(event);\n    };\n    \n    handleSearchIconClick = event => {\n        let toggle = true;\n        if(this.props.searchOpen)\n            toggle = false;\n\n        this.props.interfaceStore.searchToggle(toggle);\n    };\n\n    renderMenuIcon() {\n        return (\n            <div\n                className={s.hamburger}\n                onClick={this.handleMenuIconClick}\n            >\n            <IconMenu\n                size={SIZE_SMALL}\n            />\n            </div>\n        );\n    }\n    \n    render() {\n\n        if(this.state.hasError) {\n            return false;\n        }\n\n        const { \n            stickyHeader, \n            searchOpen,\n        } = this.props.interfaceStore;\n        \n        const { consultant } = this.props.attributionStore;\n        const consultantShopping = this.props.attributionStore.consultantShopping; \n\n        const contentStore = this.props.contentStore;\n        const navigationStore = this.props.navigationStore;\n\n        const config = contentStore.content.header ? contentStore.content.header[0] : null;\n\n        // Use Contentful header configuration for realtime\n        // state updates when available \n        let headerStyle;\n        const path = navigationStore.unlocalizedPath(this.props.path);\n        if(getSafe(() => contentStore.content.page[path].url == path))\n            headerStyle = contentStore.content.page[path];\n        else\n            headerStyle = this.props.interfaceStore.headerStyle;\n\n        const isHoverHeader = headerStyle.transparentHeader && !headerStyle.hideCategoryNavigation && !headerStyle.disableHover;\n\n        return (\n            <div className={cx(\n                s.rootDesktop, \n                (searchOpen || !headerStyle.transparentHeader) && s.whiteHeader, isHoverHeader && [s.hoverHeader, this.props.interfaceStore.initialRender && this.state.fadeDelay && s.forceFade, this.state.hoverActive && s.hoverActive]\n            )}>\n                <TransitionGroup>\n                        {/* Sticky header */}\n                        {stickyHeader &&\n                            <CSSTransition\n                                classNames={ {\n                                    enter: s.stickyHeaderEnter,\n                                    enterActive: s.stickyHeaderEnterActive,\n                                    exit: s.stickyHeaderExit,\n                                    exitActive: s.stickyHeaderExitActive,\n                                } }\n                                timeout={{ enter: 700, exit: 200 }}>\n                            <div className={s.stickyContainer}>\n                                <Container fullWidth>\n                                    <div className={cx(s.compact, s.spaceBetween, s.center, s.high)}>\n                                        <div className={cx(s.sides, s.equal, s.contentLeft)}>\n                                            <div\n                                                className={s.hamburger}\n                                                onClick={this.handleMenuIconClick}\n                                            >\n                                            <IconMenu\n                                                size={SIZE_SMALL}\n                                            />\n                                            </div>\n                                            <Link to='/'>\n                                                <img\n                                                    alt=\"Beautycounter\" \n                                                    className={s.logoCompact} \n                                                    src={`${assetPath}/images/logo-white.svg`} />\n                                            </Link>\n                                        </div>\n                                        <div className={cx(s.links, s.contentCentered, s.stickyCategories)}>\n                                            <CategoryNavigation limit={4} />\n                                        </div>\n                                        <div className={cx(s.equal, s.contentRight)}>\n                                            <div className={s.linkContainer}>\n                                                <Link \n                                                    className={s.link} \n                                                    to={config && config.specialLink.fields.url}\n                                                    name={config && config.specialLink.fields.name}\n                                                />\n                                            </div>\n                                            <div className={s.linkContainer}>\n                                                <AccountLink className={s.link} />\n                                            </div>\n                                            <div \n                                                className={cx(s.link, s.linkContainer)} \n                                                onClick={this.handleCartIconClick}>\n                                                    <CartLink />\n                                            </div>\n                                            <div className={s.linkContainer}>\n                                                <IconSearch\n                                                    className={s.iconSearch}\n                                           