const { children } = this.props;\n        const numItems = children.length || 1;\n        this.doSliding('next', position === numItems - 1 ? 0 : position + 1)\n    }\n\n    prevSlide = () => {\n        if (this.state.animating) return;\n        const { position } = this.state;\n        const { children } = this.props;\n        const numItems = children.length || 1;\n        this.doSliding('prev', position === 0 ? numItems - 1 : position - 1);\n    }\n\n    getOrder(itemIndex) {\n        const { position } = this.state;\n        const { children } = this.props;\n        const numItems = children.length || 1;\n        if (itemIndex - position < 0) return numItems - Math.abs(itemIndex - position);\n        return itemIndex - position;\n    }\n\n    getSlideCount(type) {\n        const { desktopSlides, phoneSlides, tabletSlides } = this.props;\n\n        const { isPhone, isComputer, appIsLoaded } = this.props.interfaceStore;\n\n        if (!type) return isComputer ? desktopSlides : isPhone ? phoneSlides : tabletSlides;\n        else return type === 'desktop' ? desktopSlides : type === 'phone' ? phoneSlides : tabletSlides;\n    }\n\n    getPercentage(type = false) {\n        const slides = this.getSlideCount(type);\n\n        return  (100 / slides);\n    }\n\n    getTransform() {\n        const { desktopSlides, phoneSlides, tabletSlides } = this.props;\n        const { appIsMounted } = this.props.interfaceStore;\n\n        if (appIsMounted) {\n            if (!this.state.sliding) {\n                return `translateX(calc(-${this.getPercentage()}%))`;\n            }\n            if (this.state.direction === 'prev') {\n                return `translateX(calc(2 * (-${this.getPercentage()}%)))`;\n            }\n            return 'translateX(0%)';\n        }\n        return false;\n    }\n\n    render() {\n        const { state, props } = this;\n        const { position } = state;\n        const { oneArrow, startingPosition, reversed } = props;\n        const transform = this.getTransform() ? { transform: this.getTransform() } : null;\n\n        let items;\n        try {\n            const cloned = this.props.children.slice();\n            const first = this.props.children.slice().splice(-1);\n            items = [ ...first, ...cloned.slice(0, cloned.length-1) ];\n        }\n        catch(e) {\n            items = this.props.children;\n        }\n\n        return (\n            <React.Fragment>\n                {/* Defaults for when JavaScript is initially unavailable */}\n                <style>\n                        {`.${s.carousel} {\n                            transform: translateX(calc(-${this.getPercentage('desktop')}%));\n                        }\n                        @media screen and (max-width: ${MEDIA_MOBILE}px) {\n                            .${s.carousel} {\n                                transform: translateX(calc(-${this.getPercentage('tablet')}%));\n                            }\n                        }\n                        @media screen and (max-width: ${MEDIA_PHONE}px) {\n                            .${s.carousel} {\n                                transform: translateX(calc(-${this.getPercentage('phone')}%));\n                            }\n                        }`}\n                </style>\n\n                <Arrows \n                    onClick={() => this.prevSlide()} \n                    CSSOverrides={cx(s.arrow, oneArrow && position === 0 && s.disabled)}\n                    left \n                    spaced />\n                <div className={s.root}>\n                    <div \n                        className={s.carousel} \n                        style={{ \n                            transition: state.sliding ? 'none' : `transform ${props.animationSpeed}s ease`,\n                            ...transform\n                        }}>\n                            {items.map((child, index) => (\n                                <div\n                                    className={cx(s.item, props.slideClass)}\n                                    key={index}\n                                    order={this.getOrder(index)}\n                                    style={{order: this.getOrder(index)}}>\n                                        {child}\n                                </div>\n                            ))}\n                    </div>\n                </div>\n                <Arrows \n                    CSSOverrides={cx(s.arrow, oneArrow && position === this.props.children.length - this.getSlideCount() && s.disabled)} \n                    onClick={() => this.nextSlide()} \n                    right \n                    spaced />\n            </React.Fragment>\n        );\n    }\n}\n\n\nexport default SimpleCarousel;\n"]}},"_buildHash":"ba16622ea252bc8319cd7785c9e9cd60","hash":"52cdb1ae8501e2e63736356176ea6f07","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/extends","range":[107,138],"loc":{"start":{"line":7,"column":16},"end":{"line":7,"column":56}}},{"type":"RequireHeaderDependency","range":[99,106],"loc":{"start":{"line":7,"column":16},"end":{"line":7,"column":56}}},{"type":"RequireHeaderDependency","range":[220,227],"loc":{"start":{"line":11,"column":26},"end":{"line":11,"column":76}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/toConsumableArray","range":[228,269],"loc":{"start":{"line":11,"column":26},"end":{"line":11,"column":76}}},{"type":"RequireHeaderDependency","range":[367,374],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-prototype-of","range":[375,422],"loc":{"start":{"line":15,"column":22},"end":{"line":15,"column":78}}},{"type":"RequireHeade