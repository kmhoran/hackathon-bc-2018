roduct.id);\n                if (ids.length < 1) return false;\n            } catch (e) {\n                __BROWSER__ && console.log('unable to display disabled promo state in selector');\n                return false;\n            }\n        }\n\n        const products = ids.map(id => productOrVariantById(id));\n\n        const isMoreThanOne = products.length > 1 ? true : false;\n        const isMoreThanTwo = products.length > 2 ? true : false;\n        const isLessThanTwo = products.length <= 1 ? true : false;\n\n        sliderSettings = {\n            ...sliderSettings,\n            dots: isMoreThanTwo,\n            infinite: false,\n            arrows: (() => {\n                if (isMoreThanOne && isPhone) return true;\n                else if (isMoreThanTwo) return true;\n                return false;\n            })(),\n            slidesToShow: (() => {\n                const gridSize = (() => {\n                    if (isPhone) return 1;\n                    if (isDesktop)\n                        if (products.length <= 4 ) return products.length;\n                        else return 4;\n                    return 2;\n                })();\n                return products.length > 1 ? gridSize : 1;\n            })(),\n            slidesToScroll: 1,\n        };\n\n//only one product\n        if (!products[0] || !products[0].title) return (\n            <Section innerClassName={s.root} contentClassName={s.content}>\n                Error: Cannot find product id {ids[0]}\n            </Section>\n        );\n\n        function renderInterfaceContent() {\n            return (\n                <Section innerClassName={s.root} contentClassName={s.content}>\n                    <span id=\"product-selector\" />\n                    <div className={s.social}>\n                        <div className={s.socialLabel}>\n                            Share:\n                        </div>\n                        <Social\n                            small\n                