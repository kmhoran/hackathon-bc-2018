n    parseError = (error) => {\n        if (error.includes('Invalid login or password.') || error.includes('Enter Valid Username.')) {\n            const message = this.config.errorInvalid;\n            const link = getSafe( () => message.split('{')[1].split('}')[0] );\n            const p1 = message.split('{')[0];\n            const p2 = message.split('}')[1];\n\n            const formattedError = (\n                <span>\n                    {p1}<Link onClick={() =>\n            this.props.messagingStore.clear()} className={s.link} to={routes.FORGOT_PASSWORD}><u>{link}</u></Link>{p2}\n                </span>\n            );\n\n            return formattedError;\n        }\n        return error;\n    }\n\n    handleGoBack = () => {\n        this.props.navigationStore.goBack();\n    }\n\n    criteoUiDetag = ({ isConsultant }) => {\n\n        window.criteo_q = window.criteo_q || [];\n        var deviceType= /iPad/.test(navigator.userAgent)?\"t\":/Mobile|iP(hone|od)|Android|BlackBerry|IEMobile|Silk/.test(navigator.userAgent)?\"m\":\"d\";\n\n        if(isConsultant) {\n            window.criteo_q.push(\n                { event: \"setAccount\", account: 42813 },\n                { event: \"setSiteType\", type: deviceType },\n                { event: \"viewHome\" },\n                { event: \"setData\", ui_detag: 'no' }\n            );\n        } else {\n            window.criteo_q.push(\n                { event: \"setAccount\", account: 42813 },\n                { event: \"setSiteType\", type: deviceType },\n                { event: \"viewHome\" },\n                { event: \"setData\", ui_detag: 'yes' }\n            );\n        }\n    }\n\n    render() {\n        const { signInHeading,\n            signInInstructions,\n            signInButton,\n            createAccountHeading,\n            createAccountInstructions,\n            createAccountButton,\n            emailLabel,\n            passwordLabel,\n            forgotPasswordLink,\n        } = this.config;\n\n        return (\n            <Container contentArea>\n                <div className={s.root}>\n                    <div className={s.closeIconContainer}>\n                        <Trashcan\n                            size={SIZE_EXTRA_SMALL}\n                            className={s.closeIcon}\n                            color={s.closeIconColor}\n                            onClick={() => this.handleGoBack()} />\n                    </div>\n                    <div className={s.wrapper}>\n                        <div className={s.title}>\n                            <h2>{signInHeading}</h2>\n                            <p>{signInInstructions}</p>\n                        </div>\n                        <Form\n                            items={getLoginItems(this, s, {\n                                emailLabel,\n                                passwordLabel,\n                            })}\n                            onSubmit={this.handleSubmit}\n                            errors={this.state.errors}\n                            showLabels={true}\n                            onChange={items => this.handleChange(items)}\n                            labelsPosition={'top'}\n                            largeInput={true}\n                            floatingLabel={true}\n                            noMargin={true}>\n                            <div className={s.forgotPasswordWrapper}>\n                                <Link\n                                    className={s.forgotPasswordLink}\n                                    to={routes.FORGOT_PASSWORD}>\n                                        {getSafe(() => forgotPasswordLink.fields.name)}\n                                </Link>\n                            </div>\n                            <div className={s.button}>\n                                <Button isSubmit wide>\n                                    {signInButton}\n                                </Button>\n                            </div>\n        