s.setState({\n                    completePhone: true,\n                    activeId: 2,\n                });\n            } else if (name === 'dob') {\n                this.setState({\n                    completeDob: true,\n                    activeId: 3,\n                });\n            } else if (name === 'ssn') {\n                this.setState({\n                    completeSsn: true,\n                    activeId: 5,\n                });\n            }\n            await this.store.trackAccountCompletions(name);\n            await this.store.save();\n            return true;\n        }\n        return false;\n    };\n\n    handleSelectChange = async (name, value) => {\n        this.props.handleInputChange && this.props.handleInputChange(name, value);\n        if (value.length) {\n            this.setState({\n                completeGoal: true,\n                activeId: 4,\n            });\n            await this.store.trackAccountCompletions(name);\n            await this.store.save();\n        }\n    };\n\n    handleSaveAccount = event => {\n        this.props.handleSaveAccount && this.props.handleSaveAccount(event);\n    };\n\n    goToEditStep = id => {\n        this.setState({\n            activeId: id,\n        });\n    };\n\n    goToConfirmation = () => {\n        this.setState({\n            activeId: 5\n        });\n    }\n\n    goToStep = e => {\n        this.setState({\n            activeId: e.target.getAttribute('value'),\n        });\n    };\n\n    resetStorage = () => {\n        this.store.reset();\n        this.setState({\n            activeId: 1,\n            completePhone: false,\n            completeDob: false,\n            completeGoal: false,\n            completeSsn: false,\n        });\n    };\n\n    renderPhoneField = () => {\n        const { form, accountInputItems, handleInputChange, validateInput } = this.props;\n        const items = accountInputItems;\n\n        return (\n            <React.Fragment>\n                <div>{items[1].fields.textField1} </div>\n                <Input\n                    className={s.input}\n                    defaultValue={form.phone.value}\n                    onChange={e => this.props.handleInputChange('phone', e)}\n                    onBlur={() => this.validateInput('phone')}\n                    error={form.phone.error}\n                    invalid={form.phone.error}\n                    maxLength={14}\n                    phone\n                />\n                <div>{items[1].fields.textField2} </div>\n                <div style={{ fontWeight: '700' }}>{form.email.value}</div>\n            </React.Fragment>\n        );\n    };\n\n    renderDobField = () => {\n        const { form, accountInputItems, handleInputChange, validateInput } = this.props;\n        const items = accountInputItems;\n\n        return (\n            <React.Fragment>\n                <div>{items[2].fields.textField1} </div>\n                <Input\n                    wrapperClassName={s.input}\n                    defaultValue={form.dob.value}\n                    onChange={e => this.props.handleInputChange('dob', e)}\n                    onBlur={() => this.validateInput('dob')}\n                    error={form.dob.error}\n                    invalid={form.dob.error}\n                    type={'date'}\n                />\n                <div>{items[2].fields.textField2} </div>\n            </React.Fragment>\n        );\n    };\n\n    renderGoalField = () => {\n        const { form, accountInputItems, businessOptions } = this.props;\n        const items = accountInputItems;\n\n        return (\n            <React.Fragment>\n                <div>{items[3].fields.textField1}</div>\n                <Select\n                    className={s.select}\n                    defaultValue={form.businessGoal.value}\n                    onChange={e => this.handleSelectChange('businessGoal', e)}\n                    title={items[3].fields.placeholder}\n                    options={businessOptions}\n                />\n            </React.Fragment>\n        );\n    };\n\n    renderSsnField = () => {\n        const { form, accountInputItems, businessOptions } = this.props;\n        const items = accountInputItems;\n\n        return (\n            <React.Fragment>\n                <h2 className={s.ssnHeading}>{items[4].fields.textField1}</h2>\n                <p className={s.ssnSubheading}>{items[4].fields.textField2}</p>\n                <p className={s.ssnText}>{items[4].fields.textField3}</p>\n                <div className={s.ssn}>{items[4].fields.textField4}</div>\n                <Input\n                    wrapperClassname={s.input}\n                    defaultValue={form.ssn.value}\n                    onChange={e => this.props.handleInputChange('ssn', e)}\n                    onBlur={() => this.validateInput('ssn')}\n                    error={form.ssn.error}\n                    invalid={form.ssn.error}\n                    maxLength={11}\n                    ssn\n                />\n            </React.Fragment>\n        );\n    };\n\n    renderConfirmationField = () => {\n        const { form, accountInputItems, accountSaveAccountText } = this.props;\n        const items = accountInputItems;\n\n        return (\n            <React.Fragment>\n                <div className={s.confirmField}>\n                    <p className={s.confirmText}>\n                        You can reach me by phone at{' '}\n                        <span className={s.bold}>{form.phone.value}</span> and by email at{' '}\n                        <span className={s.bold}>{form.email.value}</span>\n                    </p>\n                    <div className={s.icon} onClick={() => this.goToEditStep(1)}>\n                        <Pen />\n                    </div>\n                </div>\n                <div className={s.confirmField}>\n                    <p className={s.confirmText}>\n                        I celebrate my birthday on <span className={s.bold}>{form.dob.value}</span>\n                    </p>\n                    <div className={s.icon} onClick={() => this.goToEditStep(2)}>\n                        <Pen />\n                    </div>\n                </div>\n                <div className={s.confirmField}>\n                    <p className={s.confirmText}>\n                        As a consultant I am most excited to{' '}\n                        <span className={s.bold}>{form.businessGoal.value}</span>\n                    </p>\n                    <div className={s.icon} onClick={() => this.goToEditStep(3)}>\n                        <Pen />\n                    </div>\n                </div>\n                <div className={s.confirmField}>\n                    <p className={s.confirmText}>\n                        <Lock /> My Social Security Number is{' '}\n                        <span className={s.bold}>{form.ssn.value}</span>\n                    </p>\n                    <div className={s.icon} onClick={() => this.goToEditStep(4)}>\n                        <Pen />\n                    </div>\n                </div>\n                <div className={s.saveBtnContainer}>\n                    <Button \n                        className={s.fullBtn}\n                        wide \n                        type=\"submit\" \n                        onClick={() => this.handleSaveAccount()}\n                    >\n                        {accountSaveAccountText}\n                    </Button>\n                </div>\n            </React.Fragment>\n        );\n    };\n\n    renderPanel = () => {\n        if (this.state.activeId == 1) {\n            return this.renderPhoneField();\n        }\n        if (this.state.activeId == 2) {\n            return this.renderDobField();\n        }\n        if (this.state.activeId == 3) {\n            return this.renderGoalField();\n        }\n        if (this.state.activeId == 4) {\n            return this.renderSsnField();\n        }\n    };\n\n    renderDots = () => {\n        const {\n            form,\n            accountInputItems,\n            handleInputChange,\n            validateInput,\n            formLabels,\n            businessOptions,\n        } = this.props;\n\n        const items = accountInputItems;\n\n        return items.slice(1).map(item => {\n            const { name } = item.fields;\n            const disabled = !this.store.accountSteps[name];\n            return (\n                <div\n                    className={cx(s.selector, {\n                        [s.active]: this.state.activeId == item.fields.id,\n                        [s.done]:\n                            this.store.accountSteps[name] && this.state.activeId !== item.fields.id,\n                    })}\n                    key={item.fields.id}\n                    value={item.fields.id}\n                    onClick={e => !disabled && this.goToStep(e)}\n                />\n            );\n        });\n    };\n\n    renderFields = () => {\n        const { completePhone, completeDob, completeGoal, completeSsn } = this.state;\n        const confirmed = completePhone && completeDob && completeGoal && completeSsn;\n        return (\n            <React.Fragment>\n                <div className={s.selectors}>{this.renderDots()}</div>\n                <div className={s.panelContainer}>{this.renderPanel()}</div>\n                {confirmed && (\n                        <div className={s.confirmBtnContainer}>\n                            <Button \n                                className={s.fullBtn}\n                                wide\n                                type=\"submit\" \n                                onClick={() => this.goToConfirmation()}\n                            >\n                                Review Story\n                            </Button>\n                        </div>\n                    )}\n            </React.Fragment>\n        );\n    };\n\n    render() {\n        return (\n            <div className={s.root}>\n                {/* <Button \n                    className={s.fullBtn} \n                    fat \n                    onClick={() => this.resetStorage()}\n                >\n                    Reset store\n                </Button> */}\n                {this.state.activeId == 5 ? this.renderConfirmationField() : this.renderFields()}\n            </div>\n        );\n    }\n}\n\nexport default AccountStepMobile;\n"]}},"_buildHash":"1061cdda88a30dfa1524cc8324851b63","hash":"2d9c5667581b689ea5407cec98c963a3","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[293,300],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/defineProperty","range":[301,339],"loc":{"start":{"line":11,"column":23},"end":{"line":11,"column":70}}},{"type":"RequireHeaderDependency","range":[428,435],"loc":{"start":{"line":15,"column":19},"end":{"line":15,"column":55}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[436,463],"loc":{"start":{"line":15,"column":19},"end":{"line":15,"column":55}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/asyncToGenerator","range":[559,599],"loc":{"start":{"line":19,"column":25},"end":{"line":19,"column":74}}},{"type":"RequireHeaderDependency","range":[551,558],"loc":{"start":{"line":19,"column":25},"end":{"line":19,"column":74}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/ob