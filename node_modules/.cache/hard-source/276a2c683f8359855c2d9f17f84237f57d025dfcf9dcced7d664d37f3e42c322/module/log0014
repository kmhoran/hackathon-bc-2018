/geographies';\nimport s from './AccountStep.scss';\nimport cx from 'classnames';\n\nimport AccountStepMobile from './AccountStepMobile';\nimport Section from 'Common/Section';\nimport Input from 'Common/Input';\nimport Select from 'Common/Select';\nimport Button from 'Common/Button';\nimport MarkdownWrapper from 'Components/MarkdownWrapper';\nimport Lock from 'Icons/Lock.jsx';\n\nconst form = {\n    fullName: {\n        value: '',\n        error: false,\n    },\n    country: {\n        value: '',\n        error: false,\n    },\n    phone: {\n        value: '',\n        error: false,\n    },\n    email: {\n        value: '',\n        error: false,\n    },\n    dob: {\n        value: '',\n        error: false,\n    },\n    businessGoal: {\n        value: '',\n        error: false,\n    },\n    ssn: {\n        value: '',\n        error: false,\n    },\n};\n\n@withStyles(s)\n@inject('contentStore', 'enrollmentStore', 'localeStore', 'userStore')\n@observer\nclass AccountStep extends Component {\n    state = {\n        errors: null,\n        form: cloneDeep(form),\n        businessOptions: [],\n        completePhone: false,\n        completeDob: false,\n        completeGoal: false,\n        completeSsn: false,\n        viewport: window.innerWidth,\n    };\n\n    @computed\n    get config() {\n        return getSafe(() => this.props.enrollmentStore.config);\n    }\n\n    @computed\n    get country() {\n        return getSafe(() => this.props.localeStore.activeLocale.country);\n    }\n\n    @computed\n    get user() {\n        return getSafe(() => this.props.userStore.userInfo);\n    }\n\n    componentDidMount() {\n        const { enrollmentStore } = this.props;\n        const options = this.config.accountSelectOptions;\n        const businessOptions = Object.keys(options).map(e => ({\n            name: options[e],\n            selectable: true,\n        }));\n\n        const fullName =\n            this.user.firstname && this.user.lastname\n                ? `${this.user.firstname} ${this.user.lastname}`\n                : 'Tara Toy';\n        const email = this.user.email ? `${this.user.email}` : 'tara@beautycounter.com';\n\n        this.setState({\n            form: {\n                ...this.state.form,\n                fullName: {\n                    value: fullName,\n                },\n                country: {\n                    value: this.country,\n                },\n                email: {\n                    value: email,\n                },\n            },\n            businessOptions,\n        });\n\n        this.resizeListener = throttle(this.updateDimensions, 100);\n        window.addEventListener('resize', this.resizeListener);\n    }\n\n    componentWillUnmount() {\n        this.resizeListener = throttle(this.updateDimensions, 100);\n        window.removeEventListener('resize', this.resizeListener);\n    }\n\n    updateDimensions = () => {\n        this.setState({ viewport: window.innerWidth });\n    };\n\n    handleInputChange = (name, value) => {\n        const entry = {\n            ...this.state.form[name],\n            error: false,\n            value,\n        };\n\n        if (name === 'phone') {\n            if (validPhoneNumber(value)) {\n                this.setState({\n                    completePhone: true,\n                });\n            }\n        }\n\n        if (name === 'dob') {\n            if (validDate(value)) {\n                this.setState({\n                    completeDob: true,\n                });\n            }\n        }\n\n        if (name === 'businessGoal') {\n            if (value.length) {\n                this.setState({\n                    completeGoal: true,\n                });\n            }\n        }\n\n        if (name === 'ssn') {\n            if (validSsn(value)) {\n                this.setState({\n                    completeSsn: true,\n                });\n            }\n        }\n\n        this.setState({\n            form: {\n                ...this.state.form,\n                [name]: entry,\n            },\n        });\n    };\n\n    handleSaveAccount = async event => {\n        event && event.preventDefault();\n        if (this.validateInput()) {\n            this.props.next();\n            // create input object and save to account api\n            // if save success\n            // moove to next step\n            // if save fail\n            // show error\n        }\n    };\n\n    validateInput = name => {\n        const { form } = this.state;\n        const { formErrors } = this.config;\n        const {\n            requiredField,\n            invalidPhone,\n            invalidDob,\n            invalidSsn,\n            invalidBusinessGoal,\n        } = formErrors;\n        let errors = 0;\n\n        if (name === 'phone') {\n            if (!validPhoneNumber(form.phone.value)) {\n                form.phone.error = !form.phone.value ? requiredField : invalidPhone;\n                errors++;\n            } else {\n                this.setState({\n                    completePhone: true,\n                });\n            }\n        }\n\n        if (name === 'dob') {\n            if (!validDate(form.dob.value)) {\n                form.dob.error = !form.dob.value ? requiredField : invalidDob;\n            } else {\n                this.setState({\n                    completeDob: true,\n                });\n            }\n        }\n\n        if (name === 'ssn') {\n            if (!validSsn(form.ssn.value)) {\n                form.ssn.error = !form.ssn.value ? requiredField : invalidSsn;\n            } else {\n                this.setState({\n                    completeSsn: true,\n                });\n            }\n        }\n        this.setState({});\n        if (errors > 0) return false;\n        return true;\n    };\n\n    render() {\n        const {\n            accountHeading,\n            accountSubheading,\n            accountSsnMarkdown,\n            accountInputItems,\n            accountSaveAccountText,\n            formLabels,\n        } = this.config;\n\n        const { phone, dob, ssn } = formLabels;\n\n        const items = accountInputItems;\n\n        const {\n            form,\n            businessOptions,\n            completePhone,\n            completeDob,\n            completeGoal,\n            completeSsn,\n            viewport,\n        } = this.state;\n\n        const isMobile = viewport <= 480;\n\n        return (\n            <Section className={s.root} contentClassName={s.contentContainer}>\n                <h1 className={s.stepHeading}>{accountHeading}</h1>\n                <p className={s.stepHeading}>{accountSubheading}</p>\n                <div className={s.accountContainer}>\n                    {isMobile ? (\n                        <AccountStepMobile\n                            handleInputChange={this.handleInputChange}\n                            handleSaveAccount={this.handleSaveAccount}\n                            validateInput={this.validateInput}\n                            accountInputItems={accountInputItems}\n                            formLabels={formLabels}\n                            {...this.state}\n                            {...this.config}\n                        />\n                    ) : (\n                        <div className={s.formContainer}>\n                            <div className={s.fields}>\n                                <span>{items[0].fields.textField1} </span>\n                                <span className={s.bold}>{form.fullName.value}</span>\n                                <span>{items[0].fields.textField2} </span>\n                                <span className={s.bold}>{form.country.value}</span>\n                            </div>\n\n                            <div className={s.fields}>\n                                <span>{items[1].fields.textField1} </span>\n                                <Input\n                                    wrapperClassName={s.inputWrapper}\n                                    className={s.input}\n                                    defaultValue={form.phone.value}\n                                    onChange={e => this.handleInputChange('phone', e)}\n                                    onBlur={() => this.validateInput('phone')}\n                                    error={form.phone.error}\n                                    invalid={form.phone.error}\n                                    maxLength={14}\n                                    phone\n                                />\n                                <span>{items[1].fields.textField2} </span>\n                                <span className={s.bold}>{form.email.value}</span>\n                            </div>\n\n                            {completePhone && (\n                                <div className={s.fields}>\n                                    <span>{items[2].fields.textField1} </span>\n                                    <Input\n                                        wrapperClassName={s.inputWrapper}\n                                        className={cx(s.input, s.dateInput)}\n                                        defaultValue={form.dob.value}\n                                        onChange={e => this.handleInputChange('dob', e)}\n                                        onBlur={() => this.validateInput('dob')}\n                                        error={form.dob.error}\n                                        invalid={form.dob.error}\n                                        type={'date'}\n                                    />\n                                    <span>{items[2].fields.textField2} </span>\n                                </div>\n                            )}\n\n                            {completeDob && (\n                                <div className={s.fields}>\n                                    <span>{items[3].fields.textField1}</span>\n                                    <Select\n                                        wrapperClassName={s.select}\n                                        defaultValue={form.businessGoal.value}\n                                        onChange={e => this.handleInputChange('businessGoal', e)}\n                                        title={'Select Answer'}\n                                        options={businessOptions}\n                                    />\n                                </div>\n                            )}\n\n                            {completeGoal && (\n                                <div className={cx(s.fields, s.ssnField)}>\n                                    <h1 className={s.ssnHeading}>{items[4].fields.textField1}</h1>\n                                    <p className={s.ssnSubheading}>{items[4].fields.textField2}</p>\n                                    <p className={s.ssnText}>{items[4].fields.textField3}</p>\n                                    <Lock />{' '}\n                                    <span className={s.ssn}>{items[4].fields.textField4} </span>\n                                    <Input\n                                        wrapperClassName={s.inputWrapper}\n                                        defaultValue={form.ssn.value}\n                                        onChange={e => this.handleInputChange('ssn', e)}\n                                        onBlur={() => this.validateInput('ssn')}\n                                        error={form.ssn.error}\n                                        invalid={form.ssn.error}\n                                        maxLength={11}\n                                        ssn\n                                    />\n                                </div>\n                            )}\n\n                            {completeSsn && (\n                                <div className={s.buttonContainer}>\n                                    <Button\n                                        regular\n                                        type=\"submit\"\n                                        onClick={() => this.handleSaveAccount()}\n                                    >\n                                        {accountSaveAccountText}\n                                    </Button>\n                                </div>\n                            )}\n                        </div>\n                    )}\n                </div>\n            </Section>\n        );\n    }\n}\n\nexport default AccountStep;\n"]}},"_buildHash":"eff5e3de1df783ddc2c03c5cd0b35f60","hash":"b2f69e93578eea3ec6b15607ed214c5f","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/get-own-property-descriptor","range":[123,181],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[115,122],"loc":{"start":{"line":7,"column":32},"end":{"line":7,"column":99}}},{"type":"RequireHeaderDependency","range":[282,289],"loc":{"start":{"line":11,"column":12},"end":{"line":11,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/core-js/object/keys","range":[290,325],"loc":{"start":{"line":11,"column":12},"end":{"line":11,"column":56}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/regenerator","range":[401,428],"loc":{"start":{"line":15,"column":19},"end":{"line":15,"column":55}}},{"type":"RequireHeaderDependency","range":[393,400],"loc":{"start":{"line":15,"column":19},"end":{"line":15,"column":55}}},{"type":"RequireHeaderDependency","range":[516,523],"loc":{"start":{"line":19,"column":25},"end":{"line":19,"column":74}}},{"type":"CommonJsRequireDependency","request":"babel-runtime/helpers/asyncToGenerator","range":[524,564],"loc":{"start":{"line":19,"column":25},"end":{"line":19,"column":74}}},{"ty