oot/containers';\n\nimport NotFoundPage from 'Containers/PageContainer/NotFoundPage';\nimport { widgets } from 'Components/Widgets';\n\nconst routeList = [\n    {\n        path: routes.PRODUCTS,\n        async action({ next }) {\n            return await next();\n        },\n        children: [\n            {\n                path: `/:categorySlug`,\n                action: data => data.route.parent.page(data),\n            },\n            {\n                path: `*`,\n                action: data => data.route.parent.page(data),\n            },\n        ],\n        page: async ({ params, query, stores, next }) => {\n            stores.interfaceStore.showProgress(\"page-load\");\n            let queryFilters = false;\n            let filter;\n\n            if (query) {\n                queryFilters = [];\n                for (let i = 0; i < Object.keys(query).length; i++) {\n                    filter = query[Object.keys(query)[i]];\n                    if (typeof filter == 'string') {\n                        queryFilters.push(filter);\n                    }\n                    else if (filter.length > 1) {\n                        for (let i = 0; i < filter.length; i++) {\n                            filter[i] = filter[i];\n                            queryFilters.push(filter[i]);\n                        }\n                    }\n                }\n            }\n            if (query.search) stores.productsStore.setSearch(query.search);\n\n            const categories = await stores.categoryStore;\n\n            let category = false;\n            if (typeof categories.categoriesBySlug[params.categorySlug] !== 'undefined') {\n                category = categories.\n                    categoriesBySlug[params.categorySlug];\n            }\n            else if (!query.search)\n                return await next();\n\n            await stores.categoryStore.setCurrentCategory(category.magentoId);\n\n            let applyFilters = [];\n\n            if (query && queryFilters) {\n                queryFilters.map(queryFilter =>\n                    Object.keys(categories.categoryFilters[category ? category.magentoId : 1]).forEach(section =>\n                        categories.categoryFilters[category ? category.magentoId : 1][section].forEach((item) => {\n                            if (item.categoryId == parseInt(queryFilter)) {\n                                applyFilters.push({\n                                    id: item.categoryId,\n                                    name: item.name,\n                                    value: true,\n                                    parent: item.parent,\n                                });\n                            }\n                        })\n                    )\n                )\n            }\n\n            const ShopContainer = await loadContainer(\"ShopContainer\");\n\n            stores.interfaceStore.setHeaderStyle({\n                transparentHeader: true,\n                hideCategoryNavigation: false,\n                disableHover: true,\n            });\n            stores.interfaceStore.hideProgress(\"page-load\");\n\n            return {\n                chunks: ['ShopContainer'],\n                component: <ShopContainer\n                    category={category.name}\n                    slug={params.categorySlug || null}\n                    search={query.search || null}\n                    defaultFilters={applyFilters}\n                />\n            };\n        },\n    },\n\n    {\n        path: `${routes.PRODUCT}/:slug`,\n        async action({ next }) {\n            return await next();\n        },\n        children: [{\n            path: `*`,\n            async action({ params, path, stores, next }) {\n                let variant = false, SKU = null, id = null;\n                if (path.includes(\"variant-\")) variant = path.split(\"variant-\")[1];\n\n                if (stores.productsStore.productsBySlug[params.slug]) {\n                    SKU = stores