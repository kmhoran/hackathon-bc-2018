res/localeStore';\nimport promoStore from 'Stores/promoStore';\n\nclass AnalyticsStore {\n\n    @observable viewedCheckoutStepOne;\n    @observable currency;\n    @observable title;\n    @observable _orderProperties;\n\n    @computed get orderProperties() {\n        if (this._orderProperties) {\n            return this._orderProperties;\n        } else {\n            return {};\n        }\n    }\n\n    @action setTitle(value) {\n        if (this.title && value !== this.title) this.pageViewed(value);\n        this.title = value;\n    }\n\n    @computed get properties() {\n        this.title;\n        this.currency;\n        attributionStore.consultant;\n        attributionStore.consultantInfo;\n        attributionStore.socialId;\n        localeStore.activeLocale;\n\n        const consultant_id = getSafe(() => attributionStore.consultantInfo.id) || 1;\n        const social_id = getSafe(() => attributionStore.socialId) || 0;\n        const category = __BROWSER__ && (window.location.pathname === '/' ? 'Home' : this.title);\n        const consultant_name = getSafe(() => toJS(attributionStore.consultant).fullName);\n\n        return {\n            category,\n            consultant_id,\n            consultant_name,\n            social_id,\n            currency: this.currency,\n            locale: getSafe(() => localeStore.activeLocale.code),\n            time: Date.now(),\n            chnl: 'online',\n            application: 'tiamat'\n        };\n    }\n\n    constructor() {\n        this.viewedCheckoutStepOne = false;\n        this.currency = null;\n        this.title = null;\n\n        __BROWSER__ && requestAnimationFrame(() => {\n            autorun(() => {\n                userStore.user;\n                attributionStore.consultant;\n                attributionStore.socialId;\n                attributionStore.consultantShopping;\n\n                const { consultant_id, consultant_name, social_id } = this.properties;\n\n                traits({ consultant_id, consultant_name, social_id });\n\n                if (userStore.user.hasOwnProperty('email')) {\n                    const { magentoid, email, encoreid, groupid, firstname, lastname } = userStore.user;\n                    const id = magentoid ? magentoid : encoreid;\n                    identify(id, {\n                        email,\n                        username: email,\n                        magento_id: magentoid,\n                        encore_id: encoreid,\n                        group_id: groupid,\n                        firstName: firstname,\n                        lastName: lastname,\n                        name: `${firstname} ${lastname}`,\n                        ...this.properties\n                    });\n                }\n            });\n\n            autorun(() => {\n                localeStore.activeLocale;\n                if (localeStore.activeLocale.code === 'en-US') this.currency = 'USD';\n                else this.currency = 'CAD';\n                if(window.location.pathname == \"/\") {\n                    dataLayer.push({\n                        'PageType': 'HomePage',\n                        'event': 'onHomePage',\n                        'email': userStore.email || ''\n                    },\n                    { email: userStore.email || '' },\n                    {\n                        event: \"setSiteType\",\n                        type: deviceType\n                    });\n                }\n            });\n        });\n    }\n\n    @action productAdded(product, incrementButton = false) {\n        const { quantity } = product;\n        track('Product Added', {\n            label: incrementButton ? 'Increment Button' : 'Add To Bag',\n            ...productsStore.segmentObject[product.id],\n            quantity,\n            ...this.properties\n        });\n    }\n\n    @action productRemoved(product, decrementButton = false) {\n        const { quantity } = product;\n        track('Product Removed', {\n            label: decrementButton ? 'Decrement Button' : 'Remove All',\n            ...productsStore.segmentObject[product.id],\n            quantity,\n            ...this.properties\n        });\n    }\n\n    @action trackTopProducts(productSkus) {\n        const verifyDataLayer = getSafe(() => dataLayer).length > 0;\n        if(verifyDataLayer) {\n            dataLayer.push({\n                'PageType':'ListingPage',\n                'email': userStore.email || '',\n                'event': 'onListing',\n                'ProductIDList': productSkus\n            });\n        }\n\n    }\n\n    @action async orderCompleted({ orderId }, cart) {\n        if (!cart) return false;\n        const { subtotal, grandtotal, tax, shipping_amount, discount_amount, promoid, promocode } = await getOrderDetails(orderId);\n        const fromCheckout = __BROWSER__ && getSafe(() => JSON.parse(window.sessionStorage.getItem('fromCheckout'))) || {};\n        const total = fromCheckout.total || null;\n        const { skus, quantity } = this.getSkusQty(cart.products);\n        this.trackCheckoutStep();\n        const properties = {\n            label: 'Purchase',\n            order_id: orderId,\n            total: fromCheckout && fromCheckout.total,\n            value: fromCheckout && fromCheckout.total && fromCheckout.total.toFixed(2),\n            shipping: shipping_amount,\n            tax,\n            discount: discount_amount,\n            coupon: promocode,\n            skus,\n            products: cart.products,\n            quantity,\n            ...this.properties,\n            consultant_id: (fromCheckout && fromCheckout.consultantId) || 1\n        };\n        this._orderProperties = properties;\n        track('Order Completed', properties);\n        track('Order Completed - Facebook', properties);\n\n        if (__BROWSER__ && window.optimizely) {\n            requestAnimationFrame(() => {\n                window.optimizely.push({\n                    type: 'event',\n                    eventName: 'purchase',\n                    tags: {\n                    revenue: total && (total * 100)\n                    }\n                });\n            });\n        }\n\n        const product_list = [];\n\n        if(__BROWSER__ && cart.products.length > 0) {\n            cart.products.forEach((item) => {\n                product_list.push({\n                    id: item.sku,\n                    price: item.price,\n                    quantity: item.quantity\n                });\n            });\n\n            const verifyDataLayer = getSafe(() => dataLayer).length > 0;\n\n            if(verifyDataLayer) {\n                dataLayer.push({\n                    'PageType': 'TransactionPage',\n                    'event': 'onTransaction',\n                    'email': userStore.mail || '',\n                    'ProductTransactionProducts': product_list,\n                    'TransactionID': orderId\n                });\n            }\n        }\n    }\n\n    @action criteoUiDetag(profile) {\n        window.criteo_q = window.criteo_q || [];\n        var deviceType = /iPad/.test(navigator.userAgent)?\"t\":\n            /Mobile|iP(hone|od