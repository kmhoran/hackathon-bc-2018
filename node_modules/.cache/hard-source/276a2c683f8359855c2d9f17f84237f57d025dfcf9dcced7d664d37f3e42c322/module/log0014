 from './Header.scss';\nimport cx from 'classnames';\n\nimport { debounce } from 'lodash';\nimport { SIZE_SMALL, SIZE_NAV } from 'Middleware/constants/icon';\nimport { assetPath } from 'Root/config';\nimport TransitionGroup from 'react-transition-group/TransitionGroup';\nimport CSSTransition from 'react-transition-group/CSSTransition';\n\nimport CategoryNavigation from './CategoryNavigation';\nimport CartLink from 'Components/Header/CartLink';\nimport AccountLink from './AccountLink';\n\nimport CountrySelector from 'Common/CountrySelector';\nimport Link from 'Common/Link';\nimport Image from 'Common/Image';\nimport Container from 'Common/Container';\nimport ShoppingWith from 'Common/ShoppingWith';\nimport BehindTheCounter from 'Common/BehindTheCounter';\nimport IconMenu from 'Icons/Controls/Menu.jsx';\nimport IconSearch from 'Icons/Search.jsx';\n\n@withStyles(s)\n@inject(\n    'interfaceStore',\n    'contentStore',\n    'navigationStore',\n    'userStore',\n    'attributionStore'\n)\n@observer class Header extends Component {\n\n    static propTypes = {\n        onMenuClick: pt.func,\n        onCartClick: pt.func,\n        path: pt.string\n    };\n\n    state = {\n        hasError: false,\n        appIsMounted: false,\n        fadeDelay: true,\n        hoverActive: false,\n    }\n\n    componentWillMount() {\n        this.handleScroll();\n    }\n\n    componentDidMount() {\n        this.scrollListener = debounce(this.handleScroll,10);\n        window.addEventListener('scroll', this.scrollListener, { passive: true });\n\n        requestAnimationFrame(() => {\n            this.setState({ appIsMounted: true });\n            setTimeout(() => this.setState({ fadeDelay: false }), 3000);\n            setTimeout(() => this.setState({ hoverActive: true }), 4400);\n        });\n    };\n\n    componentWillUnmount() {\n        window.removeEventListener('scroll', this.scrollListener);\n    };\n\n    componentDidCatch(error, info) {\n        this.setState({\n            hasError: true\n        });\n    }\n\n    handleScroll = () => {\n        if(__BROWSER__ && window.pageYOffset >= 360 ) {\n            if(!this.props.interfaceStore.stickyHeader) {\n                this.props.interfaceStore.searchOpen && this.props.interfaceStore.searchToggle();\n                this.props.interfaceStore.stickyScrollHeader(false);\n            }\n        }\n        else {\n            if (this.props.interfaceStore.stickyHeader) {\n                this.props.interfaceStore.searchOpen && this.props.interfaceStore.searchToggle(false);\n                this.props.interfaceStore.fixedScrollHeader();\n            }\n        }\n    };\n\n    handleMenuIconClick = event => {\n        this.props.onMenuClick && this.props.onMenuClick(event);\n    };\n\n    handleCartIconClick = event => {\n        this.props.onCartClick && this.props.onCartClick(event);\n    };\n\n    handleSearchIconClick = event => {\n        let toggle = true;\n        if(this.props.searchOpen)\n            toggle = false;\n\n        this.props.interfaceStore.searchToggle(toggle);\n    };\n\n    renderMenuIcon() {\n        return (\n            <div\n                className={s.hamburger}\n                onClick={this.handleMenuIconClick}\n            >\n            <IconMenu\n                size={SIZE_SMALL}\n            />\n            </div>\n        );\n    }\n\n    render() {\n\n        if(this.state.hasError) {\n            return false;\n        }\n\n        const {\n            stickyHeader,\n            searchOpen,\n        } = this.props.interfaceStore;\n\n        const { consultant } = this.props.attributionStore;\n        const consultantShopping = this.props.attributionStore.consultantShopping;\n\n        const contentStore = this.props.contentStore;\n        const navigationStore = this.props.navigationStore;\n\n        const config = contentStore.content.header ? contentStore.content.header[0] : null;\n\n        // Use Contentful header configuration for realtime\n        // state updates when available\n        let headerStyle;\n        const path = navigationStore.unlocalizedPath(this.props.path);\n        if(getSafe(() => contentStore.content.page[path].url == path))\n            headerStyle = contentStore.content.page[path];\n        else\n            headerStyle = this.props.interfaceStore.headerStyle;\n\n        const isHoverHeader = headerStyle.transparentHeader && !headerStyle.hideCategoryNavigation && !headerStyle.disableHover;\n\n        return (\n            <div className={cx(\n                s.rootDesktop,\n                (searchOpen || !headerStyle.transparentHeader) && s.whiteHeader, isHoverHeader && [s.hoverHeader, this.props.interfaceStore.initialRender && this.state.fadeDelay && s.forceFade, this.state.hoverActive && s.hoverActive]\n            )}>\n                <TransitionGroup>\n                        {/* Sticky header */}\n                        {stickyHeader &&\n                            <CSSTransition\n                                classNames={ {\n                                    enter: s.stickyHeaderEnter,\n                                    enterActive: s.stickyHeaderEnterActive,\n                                    exit: s.stickyHeaderExit,\n                                    exitActive: s.stickyHeaderExitActive,\n                                } }\n                                timeout={{ enter: 700, exit: 200 }}>\n                            <div className={s.stickyContainer}>\n                                <Container fullWidth>\n                                    <div className={cx(s.compact, s.spaceBetween, s.center, s.high)}>\n                                        <div className={cx(s.sides, s.equal, s.contentLeft)}>\n                                            <div\n                                                className={s.hamburger}\n                                                onClick={this.handleMenuIconClick}\n                                            >\n                                            <IconMenu\n                                                size={SIZE_SMALL}\n                                            />\n                                            </div>\n                                            <Link to='/'>\n                                                <img\n                                                    alt=\"Beautycounter\"\n                                                    className={s.logoCompact}\n                                                    src={`${assetPath}/images/logo-white.svg`} />\n                                            </Link>\n                                        </div>\n                                        <div className={cx(s.links, s.contentCentered, s.stickyCategories)}>\n                                            <CategoryNavigation limit={4} />\n                                        </div>\n                                        <div className={cx(s.equal, s.contentRight)}>\n                                            <div className={s.linkContainer}>\n                                                <Link\n                                                    className={s.link}\n                                                    to={config && config.specialLink.fields.url}\n                                                    name={config && config.specialLink.fields.name}\n                                                />\n                                            </div>\n                                            <div className={s.linkContainer}>\n                                                <AccountLink className={s.link} />\n                                            </div>\n                                            <div\n                                                className={cx(s.link, s.linkContainer)}\n                                                onClick={this.handleCartIconClick}>\n                                                    <CartLink />\n                                            </div>\n                                            <div className={s.linkContainer}>\n                                                <IconSearch\n                                                    className={s.iconSearch}\n                                                    size={SIZE_NAV}\n                                                    onClick={this.handleSearchIconClick}\n                                                    />\n                                            </div>\n                                        </div>\n                                    </div>\n                                </Container>\n                            </div>\n                            </CSSTransition>\n                        }\n                </TransitionGroup>\n\n                {/* Fixed header */}\n                <div className={cx(s.fixedContainer)}>\n                    <div className={cx(s.full, s.high, this.state.appIsMounted && consultantShopping && s.consultantPadding)}>\n                        <div className={cx(s.one, s.equal, s.contentLeft)}>\n                            <div\n                                className={s.hamburger}\n                                onClick={this.handleMenuIconClick}\n                            >\n                            <IconMenu\n                                size={SIZE_SMALL}\n                                thickness={50}\n                            />\n                            </div>\n                            <div className={cx(s.linkContainer, s.shiftLinkLeft)}>\n                                <CountrySelector label={config && config.languageLabel} />\n                            </div>\n                            <div className={s.linkContainer}>\n                                <Link\n                                    className={s.link}\n                                    to={config && config.specialLink.fields.url}\n                                    name={config && config.specialLink.fields.name}\n                                />\n                            </div>\n                        </div>\n                        <div className={cx(s.one, s.equal, s.contentCentered, s.fadeIn)}>\n                            <Link to='/'\n                                className={s.logo}\n                                image={`${assetPath}/images/logo-full.svg`}\n                            />\n                        </div>\n                        <div className={cx(s.equal, s.contentRight)}>\n                            {this.state.appIsMounted &&\n                            <div>\n                                {this.props.userStore.isConsultant &&\n                                    <div className={cx(s.behindTheCounter, isHoverHeader && s.shoppingWithHover)}>\n                                        <BehindTheCounter />\n                                    </div>\n                                }\n                                {this.props.attributionStore.consultantShopping &&\n                                    <div>\n                                        <ShoppingWith className={isHoverHeader && s.shoppingWithHover} />\n                                    </div>\n                                }\n                            </div>\n                            }\n                            <div className={s.linkContainer}>\n                                <AccountLink />\n                            </div>\n                            <div\n                                className={s.linkContainer}\n                                onClick={this.handleCartIconClick}>\n                                    <CartLink className={s.link} />\n                            </div>\n                            <div className={s.linkContainer}>\n                                <IconSearch\n                                    className={s.iconSearch}\n                                    size={SIZE_NAV}